pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "./IERC20.sol";

contract Tickets {
    struct Ticket {
        uint256 tickets;
        uint256 showId;
    }

    struct Show {
        uint256 id;
        string title;
        string info;
        string date;
        string time;
        string location;
        uint256 price;
        string currency;
        uint256 totalTickets;
        uint256 ticketsBought;
        string resource;
    }

    address private admin;
    mapping(address => Show) private organiserShowMapping;
    mapping(uint256 => Show) private showIdShowStructMapping;
    mapping(uint256 => address payable) private showIdOrganiserAddressMapping;
    mapping(uint256 => Ticket) private showIdTicketStructMapping;
    mapping(uint256 => address[]) private showIdCustomerAddressMapping;
    mapping(address => Ticket) private customerAddressTicketStructMapping;
    mapping(address => bool) public allowedTokens;
    uint256 private counterId;
    bool private paused;
    event Mint(
        address indexed organiser,
        string indexed title,
        string date,
        string time,
        uint256 price
    );
    event Buy(
        address indexed customerAddress,
        address tokenAddress,
        uint256 showId,
        uint32 totalTicketsToBuy
    );

    constructor(address _admin) {
        admin = _admin;
        counterId = 0;
    }

    //validation
    modifier onlyAdmin() {
        require(msg.sender == admin, "Only Admin can call this function.");
        _;
    }

    modifier onlyOrganiser(address organiserAddress) {
        require(
            msg.sender == organiserAddress,
            "Only organiser can call this function."
        );
        _;
    }

    modifier onlyValidTotalTicketsMint(uint256 value) {
        require(value > 0, "Invalid value, should be greater than zero.");
        _;
    }

    modifier shouldBuyAtleastOneTicket(uint256 value) {
        require(value >= 1, "Should Buy Atleast 1 Ticket!");
        _;
    }

    modifier isValidTokens(address tokenAddress) {
        console.log("is valid token :", tokenAddress);
        console.log("is valid token value :", allowedTokens[tokenAddress]);
        require(
            allowedTokens[tokenAddress] == true,
            "This token is not allowed to be used."
        );
        _;
    }

    modifier shouldNotBePaused() {
        require(paused == false, "Ticketing is paused...!");
        _;
    }

    function whoIsAdmin() external view returns (address) {
        return admin;
    }

    function mintShow(
        address payable organiserAddress,
        string memory title,
        string memory info,
        string memory date,
        string memory time,
        string memory location,
        uint256 price,
        string memory currency,
        uint256 totalTickets,
        string memory resource
    )
        external
        onlyOrganiser(organiserAddress)
        onlyValidTotalTicketsMint(totalTickets)
        shouldNotBePaused
        returns (uint256 id)
    {
        counterId = counterId + 1;
        organiserShowMapping[organiserAddress].id = counterId;
        organiserShowMapping[organiserAddress].title = title;
        organiserShowMapping[organiserAddress].info = info;
        organiserShowMapping[organiserAddress].date = date;
        organiserShowMapping[organiserAddress].time = time;
        organiserShowMapping[organiserAddress].location = location;
        organiserShowMapping[organiserAddress].price = price;
        organiserShowMapping[organiserAddress].currency = currency;
        organiserShowMapping[organiserAddress].totalTickets = totalTickets;
        organiserShowMapping[organiserAddress].ticketsBought = 0;
        organiserShowMapping[organiserAddress].resource = resource;
        showIdShowStructMapping[counterId] = organiserShowMapping[
            organiserAddress
        ];
        showIdOrganiserAddressMapping[counterId] = organiserAddress;
        emit Mint(msg.sender, title, date, time, price);
        return organiserShowMapping[organiserAddress].id;
    }

    function buyTickets(
        uint32 totalTicketsToBuy,
        uint256 showId,
        address customerAddress,
        address tokenAddress
    )
        external
        shouldBuyAtleastOneTicket(totalTicketsToBuy)
        isValidTokens(tokenAddress)
        shouldNotBePaused
    {
        Show memory showStruct = showIdShowStructMapping[showId];
        require(
            showStruct.totalTickets - showStruct.ticketsBought >=
                totalTicketsToBuy,
            "Not Enough Tickets to Buy!"
        );
        bool transferValid = _transferTicketMoney(
            customerAddress,
            tokenAddress,
            showStruct.price * totalTicketsToBuy,
            showIdOrganiserAddressMapping[showId]
        );
        require(
            transferValid == true,
            "Transfering of token failed, cant book tickets!"
        );
        showStruct.ticketsBought += totalTicketsToBuy;
        showIdShowStructMapping[showStruct.id] = showStruct;
        Ticket storage mappedTickets = showIdTicketStructMapping[showId];
        mappedTickets.tickets += totalTicketsToBuy;
        mappedTickets.showId = showId;
        customerAddressTicketStructMapping[customerAddress] = mappedTickets;
        showIdTicketStructMapping[showId] = mappedTickets;
        showIdCustomerAddressMapping[showId].push(customerAddress);
        console.log(
            "Customer with address:%s bought %s tickets:",
            customerAddress,
            totalTicketsToBuy
        );
        emit Buy(customerAddress, tokenAddress, showId, totalTicketsToBuy);
    }

    function seeShowsByOrganiser(address organiserAddress)
        external
        view
        returns (Show memory show)
    {
        return organiserShowMapping[organiserAddress];
    }

    function getTickets(address customerAddress)
        external
        view
        returns (uint256, string memory)
    {
        Ticket memory ticket = customerAddressTicketStructMapping[
            customerAddress
        ];
        Show memory show = showIdShowStructMapping[ticket.showId];
        return (ticket.tickets, show.title);
    }

    function getAllBookedCustomerAddressesForShowId(uint256 showId)
        external
        view
        returns (address[] memory)
    {
        return (showIdCustomerAddressMapping[showId]);
    }

    function getAllShows(uint256 startRange, uint256 endRange)
        public
        view
        returns (Show[] memory show)
    {
        Show[] memory shows = new Show[](endRange - startRange);
        for (uint256 i = startRange; i < endRange; i++) {
            Show storage s = showIdShowStructMapping[i];
            shows[i] = s;
        }

        return shows;
    }

    function getShow(uint256 showId) public view returns (Show memory show) {
        return showIdShowStructMapping[showId];
    }

    function setTokens(address tokenAddress, bool value) external onlyAdmin {
        allowedTokens[tokenAddress] = value;
    }

    function setPaused(bool _paused) external onlyAdmin {
        paused = _paused;
    }

    function _transferTicketMoney(
        address customerAddress,
        address tokenAddress,
        uint256 totalPrice,
        address organiserAddress
    ) internal returns (bool) {
        require(
            customerAddress == msg.sender,
            "Transaction initiated by different account, use the same account address."
        );
        console.log("Total Price:", totalPrice);
        IERC20 _token = IERC20(tokenAddress);
        _token.transferFrom(msg.sender, organiserAddress, totalPrice);
        return true;
    }
}

