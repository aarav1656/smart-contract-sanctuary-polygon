import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";

/// @custom:security-contact [email protected]
contract PistisScore is
    IPistisScore,
    Initializable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721BurnableUpgradeable,
    AccessControlUpgradeable,
    UUPSUpgradeable,
    IERC4906
{
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using StringsUpgradeable for uint256;

    CountersUpgradeable.Counter private _tokenIdCounter;

    string public contractURI;
    string private externalURL;
    string private tokenImageRenderURL;

    mapping(uint256 => TokenData) public tokens;
    mapping(address => Wallet) public wallets;
    mapping(string => Name) public names;
    mapping(bytes32 => Provider) public providers;

    /**
     * @dev Roles definitions
     */
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant PROVIDER_ROLE = keccak256("PROVIDER_ROLE");
    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
    bytes32 public constant CONTRACT_ROLE = keccak256("CONTRACT_ROLE");
    bytes32 public constant PNS_ROLE = keccak256("PNS_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(
        string memory _contractURI,
        string memory _externalURL,
        string memory _tokenImageRenderURL
    ) public initializer {
        __ERC721_init("Pistis Score", "PISTIS");
        __ERC721Enumerable_init();
        __ERC721Burnable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();

        contractURI = _contractURI;
        externalURL = _externalURL;
        tokenImageRenderURL = _tokenImageRenderURL;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(PROVIDER_ROLE, msg.sender);
        _grantRole(OPERATOR_ROLE, msg.sender);
        _grantRole(PNS_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
    }

    /// @inheritdoc IPistisScore
    function isLinkedWallet(address _wallet) public view returns (bool) {
        return wallets[_wallet].exists;
    }

    /// @inheritdoc IPistisScore
    function safeMint() public {
        if (!isLinkedWallet(msg.sender)) {
            uint256 tokenId = _tokenIdCounter.current();
            _tokenIdCounter.increment();
            _safeMint(msg.sender, tokenId);

            tokens[tokenId].exists = true;
            tokens[tokenId].addresses = [msg.sender];
            tokens[tokenId].pistisScore = 0;
            tokens[tokenId].deFiScore = 0;
            tokens[tokenId].tradFiScore = 0;
            tokens[tokenId].personalScore = 0;

            wallets[msg.sender].exists = true;
            wallets[msg.sender].token = tokenId;
        } else {
            if (tokens[wallets[msg.sender].token].burned) {
                uint256 tokenId = _tokenIdCounter.current();
                _tokenIdCounter.increment();
                _safeMint(msg.sender, tokenId);

                uint256 old_token_id = wallets[msg.sender].token;

                tokens[tokenId].exists = true;
                tokens[tokenId].addresses = tokens[old_token_id].addresses;
                tokens[tokenId].pistisScore = tokens[old_token_id].pistisScore;
                tokens[tokenId].deFiScore = tokens[old_token_id].deFiScore;
                tokens[tokenId].tradFiScore = tokens[old_token_id].tradFiScore;
                tokens[tokenId].personalScore = tokens[old_token_id]
                    .personalScore;
                tokens[tokenId].calculatedDate = tokens[old_token_id]
                    .calculatedDate;

                for (uint256 i = 0; i < tokens[tokenId].addresses.length; i++) {
                    wallets[tokens[tokenId].addresses[i]].token = tokenId;
                }

                delete tokens[old_token_id];

                emit TokenIdChanged(old_token_id, tokenId);
            } else {
                require(
                    !isLinkedWallet(msg.sender),
                    "The address is already in use"
                );
            }
        }
    }

    /// @inheritdoc IPistisScore
    function airdropMint(address _recipient) external onlyRole(OPERATOR_ROLE) {
        if (!isLinkedWallet(_recipient)) {
            uint256 tokenId = _tokenIdCounter.current();
            _tokenIdCounter.increment();
            _safeMint(_recipient, tokenId);

            tokens[tokenId].exists = true;
            tokens[tokenId].addresses = [_recipient];
            tokens[tokenId].pistisScore = 0;
            tokens[tokenId].deFiScore = 0;
            tokens[tokenId].tradFiScore = 0;
            tokens[tokenId].personalScore = 0;

            wallets[_recipient].exists = true;
            wallets[_recipient].token = tokenId;
        } else {
            if (tokens[wallets[_recipient].token].burned) {
                uint256 tokenId = _tokenIdCounter.current();
                _tokenIdCounter.increment();
                _safeMint(_recipient, tokenId);

                uint256 old_token_id = wallets[_recipient].token;

                tokens[tokenId].exists = true;
                tokens[tokenId].addresses = tokens[old_token_id].addresses;
                tokens[tokenId].pistisScore = tokens[old_token_id].pistisScore;
                tokens[tokenId].deFiScore = tokens[old_token_id].deFiScore;
                tokens[tokenId].tradFiScore = tokens[old_token_id].tradFiScore;
                tokens[tokenId].personalScore = tokens[old_token_id]
                    .personalScore;
                tokens[tokenId].calculatedDate = tokens[old_token_id]
                    .calculatedDate;

                for (uint256 i = 0; i < tokens[tokenId].addresses.length; i++) {
                    wallets[tokens[tokenId].addresses[i]].token = tokenId;
                }

                delete tokens[old_token_id];

                emit TokenIdChanged(old_token_id, tokenId);
            } else {
                require(
                    !isLinkedWallet(_recipient),
                    "The address is already in use"
                );
            }
        }
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");

        bytes memory data = abi.encodePacked(
            baseSection(_tokenId),
            attributesSection(_tokenId)
        );
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64Upgradeable.encode(data)
                )
            );
    }

    /// @inheritdoc IPistisScore
    function getTokenAddresses(uint256 _tokenId)
        public
        view
        returns (address[] memory)
    {
        return tokens[_tokenId].addresses;
    }

    /// @inheritdoc IPistisScore
    function getTokenByName(string memory _name)
        public
        view
        returns (uint256 token)
    {
        require(names[_name].exists, "Name doesn't exist");

        return names[_name].token;
    }

    /// @inheritdoc IPistisScore
    function addressInTokenExists(uint256 _tokenId, address _wallet)
        public
        view
        returns (bool)
    {
        if (wallets[_wallet].exists && wallets[_wallet].token == _tokenId) {
            return true;
        }

        return false;
    }

    /// @inheritdoc IPistisScore
    function addTokenAddress(
        uint256 _tokenId,
        address _newAddress,
        bytes memory _signature
    ) external {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");
        require(
            addressInTokenExists(_tokenId, msg.sender),
            "You aren't the token owner"
        );
        require(
            !addressInTokenExists(_tokenId, _newAddress),
            "Address already exists"
        );

        bytes32 hash = ECDSAUpgradeable.toEthSignedMessageHash(
            keccak256(
                abi.encodePacked(
                    _newAddress,
                    _tokenId,
                    "Please verify you intent to add address to Pistis token."
                )
            )
        );

        require(
            SignatureCheckerUpgradeable.isValidSignatureNow(
                _newAddress,
                hash,
                _signature
            ),
            "Signature is not valid"
        );

        tokens[_tokenId].addresses.push(_newAddress);

        wallets[_newAddress].exists = true;
        wallets[_newAddress].token = _tokenId;

        emit TokenAddressAdded(_tokenId, msg.sender, _newAddress);
    }

    /// @inheritdoc IPistisScore
    function checkTokenExists(uint256 _token) public view returns (bool) {
        return _exists(_token);
    }

    /// @inheritdoc IPistisScore
    function checkNameExists(string memory _name) public view returns (bool) {
        return names[_name].exists;
    }

    /// @inheritdoc IPistisScore
    function updateScore(
        uint256 _token,
        uint256 _pistisScore,
        uint256 _deFiScore,
        uint256 _tradFiScore,
        uint256 _personalScore
    ) external onlyRole(CONTRACT_ROLE) {
        require(
            checkTokenExists(_token),
            "ERC721Metadata: The token doesn't exist"
        );

        tokens[_token].pistisScore = _pistisScore;
        tokens[_token].deFiScore = _deFiScore;
        tokens[_token].tradFiScore = _tradFiScore;
        tokens[_token].personalScore = _personalScore;
        tokens[_token].calculatedDate = block.timestamp;

        emit MetadataUpdate(_token);
    }

    /// @inheritdoc IPistisScore
    function setPNSName(uint256 _token, string memory _name)
        public
        onlyRole(CONTRACT_ROLE)
    {
        if (bytes(tokens[_token].name).length != 0) {
            names[tokens[_token].name].exists = false;
            names[tokens[_token].name].token = 0;
        }

        tokens[_token].name = _name;

        names[_name].exists = true;
        names[_name].token = _token;

        emit MetadataUpdate(_token);
    }

    /// @inheritdoc IPistisScore
    function renouncePNSName(uint256 _tokenId) public {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");
        require(
            addressInTokenExists(_tokenId, msg.sender),
            "You aren't the token owner"
        );

        if (bytes(tokens[_tokenId].name).length != 0) {
            names[tokens[_tokenId].name].exists = false;
            names[tokens[_tokenId].name].token = 0;

            tokens[_tokenId].name = "";

            emit MetadataUpdate(_tokenId);
        }
    }

    /// @inheritdoc IPistisScore
    function revokePNSName(uint256 _tokenId) public onlyRole(PNS_ROLE) {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");

        if (bytes(tokens[_tokenId].name).length != 0) {
            names[tokens[_tokenId].name].exists = false;
            names[tokens[_tokenId].name].token = 0;

            tokens[_tokenId].name = "";

            emit MetadataUpdate(_tokenId);
        }
    }

    /// @inheritdoc IPistisScore
    function setPNSNameByDAO(uint256 _tokenId, string memory _name)
        public
        onlyRole(PNS_ROLE)
    {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");
        require(!checkNameExists(_name), "Name already exist");

        if (bytes(tokens[_tokenId].name).length != 0) {
            names[tokens[_tokenId].name].exists = false;
            names[tokens[_tokenId].name].token = 0;
        }

        tokens[_tokenId].name = _name;

        names[_name].exists = true;
        names[_name].token = _tokenId;

        emit MetadataUpdate(_tokenId);
    }

    /// @inheritdoc IPistisScore
    function updateExternalURL(string memory _newExternalURL)
        external
        onlyRole(OPERATOR_ROLE)
    {
        externalURL = _newExternalURL;
    }

    /// @inheritdoc IPistisScore
    function updateTokenImageRenderURL(string memory _newTokenImageRenderURL)
        external
        onlyRole(OPERATOR_ROLE)
    {
        tokenImageRenderURL = _newTokenImageRenderURL;
    }

    /// @inheritdoc IPistisScore
    function setContractURI(string memory _contractURI)
        external
        onlyRole(OPERATOR_ROLE)
    {
        contractURI = _contractURI;
    }

    /// @inheritdoc IPistisScore
    function addNewProvider(
        bytes32 _provider,
        address[] memory _trustedAddresses
    ) external onlyRole(PROVIDER_ROLE) {
        require(!providers[_provider].exists, "Provider already exist");

        providers[_provider].exists = true;
        providers[_provider].trustedAddresses = _trustedAddresses;
    }

    /// @inheritdoc IPistisScore
    function addProviderTrustedAddress(
        bytes32 _provider,
        address _trustedAddress
    ) external onlyRole(PROVIDER_ROLE) {
        require(providers[_provider].exists, "Provider doesn't exist");

        bool flag = false;

        for (uint256 i; i < providers[_provider].trustedAddresses.length; i++) {
            if (providers[_provider].trustedAddresses[i] == _trustedAddress) {
                flag = true;
            }
        }

        require(!flag, "Address already in trusted addresses");

        providers[_provider].trustedAddresses.push(_trustedAddress);
    }

    /// @inheritdoc IPistisScore
    function removeProviderTrustedAddress(
        bytes32 _provider,
        address _trustedAddress
    ) external onlyRole(PROVIDER_ROLE) {
        require(providers[_provider].exists, "Provider doesn't exist");

        for (uint256 i; i < providers[_provider].trustedAddresses.length; i++) {
            if (providers[_provider].trustedAddresses[i] == _trustedAddress) {
                for (
                    uint256 j = i;
                    j < providers[_provider].trustedAddresses.length - 1;
                    j++
                ) {
                    providers[_provider].trustedAddresses[j] = providers[
                        _provider
                    ].trustedAddresses[j + 1];
                }
                providers[_provider].trustedAddresses.pop();
            }
        }
    }

    /// @inheritdoc IPistisScore
    function getProviderTrustedAddresses(bytes32 _provider)
        public
        view
        returns (address[] memory)
    {
        require(providers[_provider].exists, "Provider doesn't exist");

        return providers[_provider].trustedAddresses;
    }

    /// @inheritdoc IPistisScore
    function getTokenProviderData(uint256 _tokenId, bytes32 _provider)
        public
        view
        returns (ProviderData memory)
    {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");
        require(providers[_provider].exists, "Provider doesn't exist");

        return tokens[_tokenId].providerData[_provider];
    }

    function setTokenProviderData(
        uint256 _tokenId,
        bytes32 _provider,
        uint256 _score
    ) external {
        require(_exists(_tokenId), "ERC721Metadata: The token doesn't exist");
        require(providers[_provider].exists, "Provider doesn't exist");

        bool flag = false;

        for (uint256 i; i < providers[_provider].trustedAddresses.length; i++) {
            if (providers[_provider].trustedAddresses[i] == msg.sender) {
                flag = true;
            }
        }

        require(flag, "Sender not in trusted addresses");

        tokens[_tokenId].providerData[_provider].exists = true;
        tokens[_tokenId].providerData[_provider].score = _score;
        tokens[_tokenId].providerData[_provider].calculatedDate = block
            .timestamp;
    }

    function burn(uint256 _tokenId) public override(ERC721BurnableUpgradeable) {
        require(
            addressInTokenExists(_tokenId, msg.sender),
            "You aren't the token owner"
        );

        if (bytes(tokens[_tokenId].name).length != 0) {
            names[tokens[_tokenId].name].exists = false;
            names[tokens[_tokenId].name].token = 0;
        }

        tokens[_tokenId].burned = true;
        tokens[_tokenId].name = "";

        _burn(_tokenId);
    }

    function baseSection(uint256 _tokenId) private view returns (bytes memory) {
        return
            abi.encodePacked(
                "{",
                '"description":"Pistis Score token",',
                '"external_url": "',
                externalURL,
                _tokenId.toString(),
                '",',
                '"name": "',
                bytes(tokens[_tokenId].name).length != 0
                    ? string.concat(tokens[_tokenId].name, ".pistis")
                    : string.concat("Pistis  #", _tokenId.toString(), " token"),
                '",',
                '"image":"',
                tokenImageRenderURL,
                _tokenId.toString(),
                '",'
            );
    }

    function attributesSection(uint256 _tokenId)
        private
        view
        returns (bytes memory)
    {
        return
            abi.encodePacked(
                '"attributes":',
                "[{",
                '"trait_type":"Pistis score","value":',
                tokens[_tokenId].pistisScore.toString(),
                "",
                "},{",
                '"trait_type":"DeFi score","value":',
                tokens[_tokenId].deFiScore.toString(),
                "",
                "},{",
                '"trait_type":"TradFi score","value":',
                tokens[_tokenId].tradFiScore.toString(),
                "",
                "},{",
                '"trait_type":"Personal score","value":',
                tokens[_tokenId].personalScore.toString(),
                "",
                "},{",
                '"display_type":"date","trait_type":"Last calculation date","value":',
                tokens[_tokenId].calculatedDate.toString(),
                "",
                "}]}"
            );
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(UPGRADER_ROLE)
    {}

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        require(
            from == address(0) ||
                to == address(0) ||
                addressInTokenExists(tokenId, to),
            "This a SBT token. It can't be transferred outside."
        );
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(
            ERC721Upgradeable,
            ERC721EnumerableUpgradeable,
            AccessControlUpgradeable,
            IERC165Upgradeable
        )
        returns (bool)
    {
        return
            interfaceId == bytes4(0x49064906) ||
            super.supportsInterface(interfaceId);
    }
}