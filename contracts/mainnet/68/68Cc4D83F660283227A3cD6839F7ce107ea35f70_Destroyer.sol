                sum += _vault7d[vault][i].volume;

                if (highestPrice < _vault7d[vault][i].highPrice) {
                    highestPrice = _vault7d[vault][i].highPrice;
                }
            }

            if (i == 0) {
                break;
            }
        }

        if (currentWeek > 0) {
            for (uint256 i = bin + 1; i < 7; i++) {
                if (_vault7d[vault][i].week == currentWeek - 1) {
                    sum += _vault7d[vault][i].volume;

                    if (highestPrice < _vault7d[vault][i].highPrice) {
                        highestPrice = _vault7d[vault][i].highPrice;
                    }
                }
            }
        }

        return (sum, highestPrice);
