    ///
    /// @dev Gets array of Order Ids associated with specific "Vault"
    /// @param vault address of "Vault"
    /// @return array of Order Ids
    ///
    function getOrderIdsByVault(address vault)
        public
        view
        virtual
        returns (uint256[] memory)
    {
        return _vaultOrderIds[vault];
    }

    ///
    /// @dev Gets array of "Order" structs associated with specific "Vault"
    /// @param vault address of "Vault"
    /// @param onlyActive filter only orders
    /// @return array of "Order" structs
    ///
    function getOrdersByVault(address vault, bool onlyActive)
        public
        view
        virtual
        validateVault(vault)
        vaultActive(vault)
        returns (Order[] memory)
    {
        uint256[] memory ids = _vaultOrderIds[vault];

        Order[] memory orders = new Order[](ids.length);

        for (uint256 i = 0; i < ids.length; i++) {
            if (!onlyActive) {
                orders[i] = _idOrder[ids[i]];
            } else {
                if (
                    _idOrder[ids[i]].orderStatus == OrderStatus.Created ||
                    _idOrder[ids[i]].orderStatus == OrderStatus.Executed
                ) {
                    orders[i] = _idOrder[ids[i]];
                }
            }
        }
        return orders;
    }

    ///
    /// @dev Gets array of arrays of Orders by Vault
    /// @param vaults array addresses of "Vaults"
    /// @param onlyActive filter only orders
    /// @return array of arrays of "Order" of "Vaults"
    ///
    function getOrdersByVaults(address[] memory vaults, bool onlyActive)
        public
        view
        virtual
        returns (Order[][] memory)
    {
        Order[][] memory vaultsOrders = new Order[][](vaults.length);
        for (uint256 i = 0; i < vaults.length; i++) {
            vaultsOrders[i] = getOrdersByVault(vaults[i], onlyActive);
        }
        return vaultsOrders;
    }

