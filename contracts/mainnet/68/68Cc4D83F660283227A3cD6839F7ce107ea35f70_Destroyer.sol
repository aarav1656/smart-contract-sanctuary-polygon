pragma solidity ^0.8.8;

import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";

import "./Token.sol";
import "./NFT.sol";
import "./Factory.sol";
import "./globals.sol";

///
/// @title Vault contract
/// @author Josip
/// @notice Creates "Vault" as main entity holding custom ERC20 tokens and NFTs
/// @dev Vault contracts are generated for each NFT creation or NFT import
/// Vaults serve as keepers and owners of NFTs which are fragmented in custom ERC20 tokens
/// to enable trading. NFTs are locked until user gathers total supply of related ERC20 tokens
/// and redeem NFTs from "Vault" at which point "Vault" is removed from any state storages (but still exists in EVM)
/// NOTE: Upgradeable through Beacon proxy pattern. "Vault" is created by "Factory" as BeaconProxy
///s
contract Vault is Initializable {
    ///
    /// @dev contract version
    ///
    string internal _contractVersion;

    ///
    /// @dev reference to "Exchange"
    ///
    address internal _exchange;

    ///
    /// @dev reference to "Factory"
    ///
    address internal _factory;

    ///
    /// @dev reference to "Destroyer"
    ///
    address internal _destroyer;

    ///
    /// @dev date/time creation of "Vault" (Unix)
    ///
    uint256 internal _time;

    // ERC20 custom token

    ///
    /// @dev custom ERC20 which is being traded under "Vault"
    ///
    address internal _token;

    ///
    /// @dev reference to NFT contract which is being fragmented with custom ERC20 tokens
    ///
    address internal _nft;

    ///
    /// @dev address of "Vault" creator
    ///
    address internal _creator;

    ///
    /// @dev "Vault" status (Active, Paused, Removed, Destroyed, Redeemed)
    ///
    VaultStatus internal _status;

    ///
    /// @dev first URI to first image in NFT collection, used as thumbnail
    ///
    string internal _thumbURI;

    ///
    /// @dev last price realized for each ERC20 token
    ///
    Price internal _lastPrice;

    ///
    /// @dev Emitts when Vault is created, destroyed or redeemed
    /// @param status "Vault" status
    /// @param caller action initiatior: "Vault" creator, NFT redeemer or admin
    /// @param tokenContract address of custom ERC20 token which is traded
    /// @param nftContract address of NFT contract which is being locked in "Vault"
    /// @param tokenURIs list of NFT URIs
    /// @param thumbURI URI of first NFT image to be showen as thumbnail
    ///
    event VaultEvent(
        VaultStatus indexed status,
        uint256 indexed time,
        address indexed caller,
        address tokenContract,
        address nftContract,
        string[] tokenURIs,
        string thumbURI
    );

    ///
    /// @notice Security in proxy pattern implementation
    /// @dev Used to disable calling initialize on implementation
    ///
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    ///
    /// @notice Used for initializing the state of contract
    /// @dev For proxy contracts "constructor" is swapped for "initialize"
    /// @param exchange_  sets reference for "Exchange" contract
    /// @param destroyer_ sets reference for"Destroyer" contract
    /// @param factory_ sets reference for "Factory" contract
    /// @param creator_ address of "Vault" creator
    /// @param importedNFT_ address of NFT contract being imported. address(0) if new NFT is created
    /// @param importedTokenIds_ list of tokenIds which are being imported and associated to "Vault". Empty array if new NFT is created
    /// @param newNameSymbols_ array containing naming for ERC20 and NFT creation [ERC20Name, ERC20Symbol, ERC721Name, ERC721Symbol]
    /// @param tokenURIs_ array stores URIs where NFT tokens metadata is stored (usually IPFS)
    /// @param thumbURI_ URI where NFT token image is stored (usually IPFS)
    ///
    function initialize(
        address exchange_,
        address destroyer_,
        address factory_,
        address creator_,
        address importedNFT_,
        uint256[] memory importedTokenIds_,
        string[4] memory newNameSymbols_,
        uint256 tokenTotalSupply_,
        string[] memory tokenURIs_,
        string memory thumbURI_
    ) public initializer {
        _contractVersion = "V1";
        _factory = factory_;
        _creator = creator_;
        _status = VaultStatus.Active;
        _time = block.timestamp;
        _destroyer = destroyer_;
        _exchange = exchange_;
        _thumbURI = thumbURI_;

        _token = address(
            new Token(_creator, newNameSymbols_, tokenTotalSupply_)
        );

        if (importedNFT_ == address(0) && importedTokenIds_.length == 0) {
            createNFT(newNameSymbols_, tokenURIs_);
        } else if (importedNFT_ != address(0) && importedTokenIds_.length > 0) {
            _nft = importedNFT_;
        } else {
            require(false, "Invalid creation request");
        }
        emit VaultEvent({
            status: _status,
            time: _time,
            caller: _creator,
            tokenContract: _token,
            nftContract: _nft,
            tokenURIs: tokenURIs_,
            thumbURI: thumbURI_
        });
    }

    /************************
     *********** API ********
     ************************/

    ///
    /// @dev Used for upgrading purposes
    /// @return contract version
    ///
    function getVersion() public view virtual returns (string memory) {
        return _contractVersion;
    }

    ///
    /// @dev Enum VaultStatus see ./globals.sol
    /// @return status of "Vault"
    ///
    function getStatus() public view virtual returns (VaultStatus) {
        return _status;
    }

    ///
    /// @dev Custom ERC20 token is used for fragmenting NFT in "Vault" to enable fractal trading
    /// @return address of custom ERC20 token associated with "Vault"
    ///
    function getToken() public view virtual returns (address) {
        return _token;
    }

    ///
    /// @dev creator is set on "Vault" initialization and is unchangeble
    /// @return address of "Vault" creator
    ///
    function getCreator() public view virtual returns (address) {
        return _creator;
    }

    ///
    /// @dev NFT contract tokens are ownership of "Vault" until user gathers 100%
    /// total supply of custom ERC20 token associated with "Vault"
    /// @return address of NFT contract associated with "Vault"
    ///
    function getNFT() public view virtual returns (address) {
        return _nft;
    }

    ///
    /// @dev Image is used as thumbnail in vaults gallery
    /// @return URI of NFT image
    ///
    function getThumb() public view virtual returns (string memory) {
        return _thumbURI;
    }

    ///
    /// @dev Tradeble ERC20 token is associated with "Vault" as is capped
    /// @return custom ERC20 total supply
    ///
    function getTotalSupply() public view virtual returns (uint256) {
        return Token(_token).totalSupply();
    }

    ///
    /// @dev Returns last tradeable price
    /// @return Price struct (amount, symbol, decimals)
    ///
    function getLastPrice() public view virtual returns (Price memory) {
        return _lastPrice;
    }

    ///
    /// @dev This is used as basic data for generating vaults gallery
    /// probably will be extended in future upgrades
    /// @return object with basic "Vault" data for gallery views
    /// [[NFT name, thumbURI, price symbol, token symbol], [lastprice, lastPriceDecimal], status, total supply]
    ///
    function getBasicInfo()
        public
        view
        virtual
        returns (
            string[4] memory,
            uint256[2] memory,
            VaultStatus,
            uint256
        )
    {
        string[4] memory strings;
        strings[0] = NFT(_nft).name();
        strings[1] = _thumbURI;
        strings[2] = _lastPrice.symbol;
        strings[3] = Token(_token).symbol();

        uint256[2] memory price;
        price[0] = _lastPrice.amount;
        price[1] = _lastPrice.decimals;

        return (strings, price, _status, Token(_token).totalSupply());
    }

    ///
    /// @dev This is used to set data for generating "Vault" details
    /// probably will be extended in future upgrades. Compressed strings as stack too deep error.
    /// @return generalStrings - [collectionName: is NFT collection name, thumbURI: thumbnail shown as first image,
    /// priceSymbol: currency symbol, tokenSymbol: erc20 token symbol]
    /// @return lastPrice  [lastPrice, lastPriceDecimals] last realized price in executed order associated with "Vault"
    /// @return creator address of "Vault" creator
    /// @return totalSupply custom ERC20 total supply
    /// @return nft references NFT contract associated with "Vault"
    /// @return status is VaultStatus see ./globals
    ///
    function getItemData()
        public
        view
        virtual
        returns (
            string[4] memory generalStrings,
            uint256[2] memory lastPrice,
            address creator,
            uint256 totalSupply,
            address nft,
            VaultStatus status
        )
    {
        (generalStrings, lastPrice, status, totalSupply) = getBasicInfo();
        creator = _creator;
        nft = _nft;

        return (generalStrings, lastPrice, creator, totalSupply, nft, status);
    }

    ///
    /// @notice Changes ownership of NFT from "Vault" to user
    /// @dev User can transfer ownership of NFT only if it has 100% of custom ERC20 token supply
    /// only "Destroyer" has access as it is part of it's redeem NFT process
    /// @param to address where NFTs will be transfered
    ///
    function redeemNFT(address to) public virtual {
        require(msg.sender == _destroyer, "Not authorized");
        IERC721Enumerable ierc721 = IERC721Enumerable(_nft);
        uint256 nftNum = ierc721.balanceOf(address(this));
        uint256 tokenId;
        for (uint256 i = 0; i < nftNum; i++) {
            tokenId = ierc721.tokenOfOwnerByIndex(address(this), i);
            ierc721.safeTransferFrom(address(this), to, tokenId);
        }
    }

    ///
    /// @notice Get NFTs which are in "Vault" ownership
    /// @dev uses IERC721Enumerable to iterate through collection
    /// @return token Ids of _nft which are ih "Vault" ownership
    ///
    function getTokenIds() public view virtual returns (uint256[] memory) {
        IERC721Enumerable ierc = IERC721Enumerable(_nft);
        uint256 balance = ierc.balanceOf(address(this));
        uint256[] memory tokens = new uint256[](balance);
        uint256 tokenId;
        for (uint256 i = 0; i < balance; i++) {
            tokenId = ierc.tokenOfOwnerByIndex(address(this), i);
            tokens[i] = tokenId;
        }
        return tokens;
    }

    ///
    /// @notice Sets last realized price associated to "Vault" customERC20 tokens
    /// @dev For each transaction of customERC20 tokens, "Exchange" sets price on "Vault"
    /// Only "Exchange" has access
    /// @param lastPrice struct defining price see "./globals.sol"
    ///
    function setLastPrice(Price memory lastPrice) public virtual {
        require(msg.sender == _exchange, "Not authorized");
        _lastPrice = lastPrice;
    }

    ///
    /// @notice Initiates VaultEvent for "Vault" removal request
    /// @dev This is called when admin requests "Vault" removal
    /// Only "Destroyer has access"
    /// @param sender caller of action (admin)
    ///
    function destroyVaultEvent(address sender) public virtual {
        require(msg.sender == _destroyer, "Not authorized");
        emit VaultEvent({
            status: VaultStatus.Destroyed,
            time: _time,
            caller: sender,
            tokenContract: _token,
            nftContract: _nft,
            tokenURIs: new string[](0),
            thumbURI: ""
        });
        _status = VaultStatus.Destroyed;
    }

    ///
    /// @notice Initiates VaultEvent for redeem NFT request by user
    /// @dev This is called when user request to transfer NFT ownership from "Vault" to himself
    /// Only "Destroyer has access"
    /// @param sender caller of action (user)
    ///
    function redeemVaultEvent(address sender) public virtual {
        require(msg.sender == _destroyer, "Not authorized");
        emit VaultEvent({
            status: VaultStatus.Redeemed,
            time: _time,
            caller: sender,
            tokenContract: _token,
            nftContract: _nft,
            tokenURIs: new string[](0),
            thumbURI: ""
        });
        _status = VaultStatus.Redeemed;
    }

    /***************************
     *********** HELPERS ********
     ****************************/

    ///
    /// @notice Initiates NFT creation
    /// @dev Helper method which creates NFT and assignes it to "Vault" with "Vault" as owner
    /// @param contractParams array containing naming for ERC20 and NFT creation [ERC20Name, ERC20Symbol, ERC721Name, ERC721Symbol]
    /// we use here last two parameter intended for NFT naming
    /// @param tokenURIs_ array stores URIs where NFT tokens metadata is stored (usually IPFS)
    ///
    function createNFT(
        string[4] memory contractParams,
        string[] memory tokenURIs_
    ) internal virtual {
        address nft = address(
            new NFT(address(this), contractParams, tokenURIs_)
        );

        _nft = nft;
    }
}

