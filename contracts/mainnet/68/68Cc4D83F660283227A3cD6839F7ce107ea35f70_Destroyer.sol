pragma solidity ^0.8.8;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";

///
/// @title Enumerable NFT contract with URI storage
/// @author Josip
/// @notice creates NFT as part of "Vault" creation process
/// @dev NFTs are minted directly to "Vault", which is initial owner of tokens
///
contract NFT is ERC721URIStorage, ERC721Enumerable {
    ///
    /// @dev NFT id
    ///
    uint256 internal _tokenId;

    ///
    /// @notice Used to initialize contract state
    /// @dev Minting is done immediately in constructor. Maximum NFT tokens per contract is 20.
    /// @param dest_ address of future owner ("Vault")
    /// @param contractParams_ array containing naming for ERC20 and NFT creation
    /// here we use last two [ERC20Name, ERC20Symbol, ERC721Name, ERC721Symbol]
    /// @param tokenURIs_ array stores URIs where NFT tokens metadata is stored (usually IPFS)
    ///
    constructor(
        address dest_,
        string[4] memory contractParams_,
        string[] memory tokenURIs_
    ) ERC721(contractParams_[2], contractParams_[3]) {
        require(
            tokenURIs_.length <= 20,
            "Maximum tokens per NFT contract is 20"
        );

        for (uint256 i = 0; i < tokenURIs_.length; i++) {
            mint(dest_, tokenURIs_[i]);
        }
    }

    /************************
     *********** API ********
     ************************/

    /// @inheritdoc ERC721URIStorage
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return ERC721URIStorage.tokenURI(tokenId);
    }

    /// @inheritdoc ERC721Enumerable
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /************************
     *********** HELPERS ****
     ************************/

    ///
    /// @notice Mint token to destination
    /// @dev With minting also uses associates token URI with token Id
    /// @param dest address of future owner ("Vault")
    /// @param URI where NFT tokens metadata is stored
    ///
    function mint(address dest, string memory URI) internal virtual {
        _mint(dest, _tokenId);
        _setTokenURI(_tokenId, URI);

        _tokenId++;
    }

    /// @inheritdoc ERC721Enumerable
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    /// @inheritdoc ERC721URIStorage
    function _burn(uint256 tokenId)
        internal
        virtual
        override(ERC721, ERC721URIStorage)
    {
        ERC721URIStorage._burn(tokenId);
    }
}

