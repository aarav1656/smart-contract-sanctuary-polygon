     ************************/

    ///
    /// @dev Used for upgrading purposes
    /// @return contract version
    ///
    function getVersion() public view virtual returns (string memory) {
        return _contractVersion;
    }

    ///
    /// @dev Used for upgrading purposes
    /// @return address of Beacon
    ///
    function getBeacon() public view virtual returns (address) {
        return _beacon;
    }

    ///
    /// @notice Gets Vault Id by "Vault" address
    /// @dev id values in mapping start from 1.
    /// getVaultId(address vault).equals(0) indicates non existing vault
    /// main method when validating "Vault" existance
    /// @param vault address for which we query
    /// @return "Vault" id
    ///
    function getVaultId(address vault) public view virtual returns (uint256) {
        return _vaultToId[vault];
    }

    ///
    /// @dev Gets list of "Vault" addresses managed by "Factory"
    /// @return collection of "Vault" addresses
    ///
    function getVaultsAddr() public view virtual returns (address[] memory) {
        return _vaults;
    }

    function getVaultLastPrice(address vaultAddr)
        public
        view
        virtual
        validateVault(vaultAddr)
        returns (Price memory)
    {
        return Vault(vaultAddr).getLastPrice();
    }

    ///
    /// @dev Gets Token balance by owner and full owner of token
    /// @param owner address of Token owner
    /// @param vault address of vault associated with token
    /// @return (token balance of owner for associated vault, tokenSymb, full owner of token, token total supply)
    ///
    function getERC20BalanceOwnership(address owner, address vault)
        public
        view
        virtual
        validateVault(vault)
        returns (
            uint256,
            string memory,
            address,
            uint256
        )
    {
        Token token = Token(Vault(vault).getToken());
        return (
            token.balanceOf(owner),
            token.symbol(),
            token.getFullOwner(),
            token.totalSupply()
        );
    }

    ///
    /// @dev Gets list of tokenURIs of specific vault
    /// @param vault address
    /// @return collection of tokenURIs of vault
    ///
    function getTokenURIs(address vault)
        public
        view
        virtual
        validateVault(vault)
        returns (string[] memory)
    {
        NFT nft = NFT(Vault(vault).getNFT());
        uint256 supply = nft.totalSupply();

        string[] memory tokenURIs = new string[](supply);
        for (uint256 i = 0; i < supply; i++) {
            tokenURIs[i] = nft.tokenURI(i);
        }
        return tokenURIs;
    }

    ///
    /// @dev Get NFT contract of specified vault
    /// @param vault address
    /// @return address of NFT contract
    ///
    function getNFTByVault(address vault)
        public
        view
        virtual
        validateVault(vault)
        returns (address)
    {
        return Vault(vault).getNFT();
    }

    ///
    /// @dev Gets list of NFTnames, Token Symbols and vault status by array of vaults
    /// @param vaults addresses
    /// @return array of NFTnames, Token Symbols and vault status
    ///
    function getNFTnameTokenSymbByVaults(address[] memory vaults)
        public
        view
        virtual
        returns (string[3][] memory)
    {
        string[3][] memory nftNames = new string[3][](vaults.length);
        for (uint256 i = 0; i < vaults.length; i++) {
            nftNames[i][0] = NFT(Vault(vaults[i]).getNFT()).name();
            nftNames[i][1] = Token(Vault(vaults[i]).getToken()).symbol();
            nftNames[i][2] = Strings.toString(
                uint256(Vault(vaults[i]).getStatus())
            );
        }
        return nftNames;
    }

    ///
    /// @dev Gets index in _vaults array where "Vault" address is stored
    /// used for managing vault collection vars when adding/removing "Vault"
    /// @return index of "Vault" in _vaults array
    ///
    function viewVaultIndex(address vault)
        public
        view
        virtual
        returns (uint256)
    {
        return _vaultIndex[vault];
    }

    ///
    /// @dev Gets list of "Vault" addresses belonging to specific creator
    /// @return array of vault addresses
    ///
    function viewCreatorVault(address creator)
        public
        view
        virtual
        returns (address[] memory)
    {
        return _creatorVaults[creator];
    }

    ///
    /// @dev Gets index where "Vault" is stored in creator's list of vaults
    /// used for managing vault collection vars when adding/removing "Vault"
    /// @return index of "Vault" in array returned by _creatorVaultIndex[vault]
    ///
    function viewCreatorVaultIndex(address vault)
        public
        view
        virtual
        returns (uint256)
    {
        return _creatorVaultIndex[vault];
    }

    ///
    /// @notice Main method of initiating "Vault" creation
    /// @dev As part of "Vault" creation process, supports importing existing NFTs as well as initiating
    /// creation of new NFTs and associating them to "Vault".
    /// Creates "Vault" as BeaconProxy to support "Vault" upgrades,
    /// also forwards data to "Vault" initializator for NFT and customERC20 token creation which is part of "Vault" creation chain
    /// @param newNameSymbols array containing naming for ERC20 and NFT creation [ERC20Name, ERC20Symbol, ERC721Name, ERC721Symbol]
    /// @param tokenURIs array stores URIs where NFT tokens metadata is stored (usually IPFS)
    /// @param thumbURI URI where NFT token image is stored (usually IPFS)
    /// @param importedNFT address of NFT contract being imported. address(0) if new NFT is created
    /// @param importedTokenIds list of tokenIds which are being imported and associated to "Vault". Empty array if new NFT is created
    ///
    function createVault(
        string[4] memory newNameSymbols,
        string[] memory tokenURIs,
        string memory thumbURI,
        uint256 totalSupply,
        address importedNFT,
        uint256[] memory importedTokenIds
    ) public virtual {
        address vault = address(
            new BeaconProxy(
                _beacon,
                abi.encodeWithSelector(
                    Vault(address(0)).initialize.selector,
                    _exchange,
                    _destroyer,
                    address(this),
                    msg.sender,
                    importedNFT,
                    importedTokenIds,
                    newNameSymbols,
                    totalSupply,
                    tokenURIs,
                    thumbURI
                )
            )
        );

        _vaultIndex[vault] = _vaults.length;
        _vaults.push(vault);

        _creatorVaultIndex[vault] = _creatorVaults[msg.sender].length;
        _creatorVaults[msg.sender].push(vault);

        // ids start from 1 as 0 is used for detecting no vault
        _vaultId++;
        _vaultToId[vault] = _vaultId;

        if (importedNFT != address(0) && importedTokenIds.length > 0) {
            importNFT({
                from: msg.sender,
                to: vault,
                importedTokenIds: importedTokenIds,
                importedNFT: importedNFT
            });
        }
    }

    ///
    /// @dev This is used to set data for generating "Vault" details
    /// probably will be extended in future upgrades. Compressed strings as stack too deep error.
    /// @return generalStrings - [collectionName: is NFT collection name, thumbURI: thumbnail shown as first image,
    /// priceSymbol: currency symbol, tokenSymbol: erc20 token symbol]
    /// @return lastPrice last realized price in executed order associated with "Vault"
    /// @return creator address of "Vault" creator
    /// @return totalSupply custom ERC20 total supply
    /// @return nft references NFT contract associated with "Vault"
    /// @return status is VaultStatus see ./globals
    ///
    function getVaultDetails(address vault)
        public
        view
        validateVault(vault)
        vaultActive(vault)
        returns (
            string[4] memory generalStrings,
            uint256[2] memory lastPrice,
            address creator,
            uint256 totalSupply,
            address nft,
            VaultStatus status,
            address
        )
    {
        (generalStrings, lastPrice, creator, totalSupply, nft, status) = Vault(
            vault
        ).getItemData();
        return (
            generalStrings,
            lastPrice,
            creator,
            totalSupply,
            nft,
            status,
            vault
        );
    }

    ///
    /// @notice Builds array of data objects containing "Vault" info for web views. Only active vaults.With pagination in reverse
    /// @dev Using helper methods initiates build of data objects from "Vaults". Uses _vaults as datasource
    /// Used on NFT gallery on main web page. Supports pagination
    /// @param endIndex end index for step in pagination in _vaults. endIndex < -1 states we are at first page, endindex == -1 indicates no items
    /// @param totalCount number of "Vaults" we want to get from _vaults starting from startIndex
    /// @return array of ExploreView structs { vault address, NFT name, thumbnail URI, last price realized }
    /// @return end index for next page
    ///
    function getVaultsPage(int256 endIndex, uint256 totalCount)
        public
        view
        virtual
        returns (ExploreView[] memory, int256)
    {
        (ExploreView[] memory views, int256 index) = _getVaults(
            _vaults,
            endIndex,
            totalCount
        );

        return (views, index);
    }

    ///
    /// @notice gives total count of vaults
    /// @dev This is used most for reverse pagination for end of array
    /// @return total count of created vaults
    ///
    function getCount() public view returns (uint256) {
        return _vaults.length;
    }

    ///
    /// @notice Builds array of data objects containing "Vault" info for web views belonging to specific creators. With pagination in reverse
    /// @dev Using helper methods initiates build of data objects from "Vaults". Uses _creatorVaults as datasource
    /// Used on creator gallery. Supports pagination
    /// @param creator address of creator of "Vault"
    /// @param endIndex end index for step in pagination in _vaults. endIndex < -1 states we are at first page, endindex == -1 indicates no items
    /// @param totalCount number of "Vaults" we want to get from _vaults starting from startIndex
    /// @return ExploreView structs { vault address,[NFT name, ThumbURI, lastpriceSymb, tokenSymb], last price, status, totalSupply }
    /// @return end index for next page
    ///
    function getVaultsPageByCreator(
        address creator,
        int256 endIndex,
        uint256 totalCount
    ) public view virtual returns (ExploreView[] memory, int256) {
        (ExploreView[] memory views, int256 index) = _getVaults(
            _creatorVaults[creator],
            endIndex,
            totalCount
        );

        return (views, index);
    }

    ///
    /// @dev Gets list of "Vault" addresses belonging to specific creator
    /// @param creator address of creator of "Vault"
    /// @return array of "Vault" address belonging to creator
    ///
    function getVaultsAddrByCreator(address creator)
        public
        view
        virtual
        returns (address[] memory)
    {
        return _creatorVaults[creator];
    }

    function getStatistics() public view returns (PriceVol[] memory) {
        Statistics stats = Statistics(_statistics);
        return stats.getPriceVolVaults(_vaults, block.timestamp);
    }

    ///
    /// @notice Cleans "Factory" storage vars during removal of "Vault"
    /// @dev Performs restructuring of _vaults and _creatorVaults lists which hold "Vault" addresses
    /// with help of "index" storage vars. Sets _vaultToId[vault] = 0 to indicate "Vault" does not exist
    /// Only "Destroyer" can access
    /// NOTE "Vault" is not deleted from EVM, contract exists, just is not listable in web views
    /// @param vault address of creator of "Vault"
    /// @param creator "Vault" creator
    ///
    function cleanCreatorVault(address vault, address creator) public virtual {
        require(_destroyer == msg.sender, "Not authorized");
        require(_vaultToId[vault] != 0, "Entity does not exist");

        uint256 index = _vaultIndex[vault];
        uint256 lastIndex = _vaults.length - 1;
        address lastVault = _vaults[lastIndex];

        if (_vaults.length > 1) {
            _vaults[index] = lastVault;
            _vaultIndex[lastVault] = index;
        }

        delete _vaultIndex[vault];
        _vaults.pop();

        index = _creatorVaultIndex[vault];
        lastIndex = _creatorVaults[creator].length - 1;
        lastVault = _creatorVaults[creator][lastIndex];

        if (_creatorVaults[creator].length > 1) {
            _creatorVaults[creator][index] = lastVault;
            _creatorVaultIndex[lastVault] = index;
        }

        _creatorVaults[creator].pop();
        delete _creatorVaultIndex[vault];

        delete _vaultToId[vault];
    }

    ///
    /// @notice Imports NFT contract
    /// @dev Helper method used in "Vault" creation for importing NFT contract and associating it with "Vault"
    /// This is used under assumption owner already approved "Factory" for token transfer
    /// NOTE: Imported NFT need to implement IERC721Enumerable and IERC721Metadata
    /// @param from owner of NFT tokens inside NFT contract
    /// @param importedTokenIds list of token Ids to be transfer to "Factory"
    /// @param importedNFT address of NFT contract to be imported
    ///
    function importNFT(
        address from,
        address to,
        uint256[] memory importedTokenIds,
        address importedNFT
    ) internal virtual {
        require(
            importedNFT.supportsInterface(
                type(IERC721Enumerable).interfaceId
            ) &&
                importedNFT.supportsInterface(
                    type(IERC721Metadata).interfaceId
                ),
            "Necessary interfaces not supported"
        );

        IERC721Enumerable ierc721 = IERC721Enumerable(importedNFT);

        for (uint256 i = 0; i < importedTokenIds.length; i++) {
            ierc721.transferFrom(from, to, importedTokenIds[i]);
        }
    }

    /************************
     *********** HELPERS ****
     ************************/

    ///
    /// @notice Builds data object from "Vault" information with pagination in reverse
    /// @dev endIndex < -1 states we are at first page, endindex == -1 indicates no items
    /// @param vaults list of "Vault" address we extract data from.
    /// @param endIndex end index for pagination
    /// @param totalCount number of "Vaults" we want to get from "vaults array" starting from startIndex
    /// @return array of ExploreView structs { vault address,[NFT name, ThumbURI, lastpriceSymb, tokenSymb], last price, status, totalSupply }
    ///
    function _getVaults(
        address[] memory vaults,
        int256 endIndex,
        uint256 totalCount
    ) internal view virtual returns (ExploreView[] memory, int256) {
        address[] memory v = vaults;

        if (
            endIndex == -1 ||
            endIndex >= int256(v.length) ||
            totalCount == 0 ||
            v.length == 0
        ) {
            return (new ExploreView[](0), -1);
        }
        if (endIndex < -1) {
            endIndex = int256(v.length) - 1;
        }

        int256 startIndex = endIndex - int256(totalCount) + 1;

        if (startIndex < 0) {
            startIndex = 0;
        }
        uint256 length = uint256(endIndex) - uint256(startIndex) + 1;
        ExploreView[] memory views = new ExploreView[](length);

        string[4] memory strings;
        uint256[2] memory lastPrice;
        VaultStatus status;
        uint256 totalSupply;

        uint256 index = 0;
        for (uint256 i = uint256(endIndex) + 1; i > uint256(startIndex); i--) {
            (strings, lastPrice, status, totalSupply) = Vault(v[i - 1])
                .getBasicInfo();
            ExploreView memory ev = ExploreView(
                v[i - 1],
                strings,
                lastPrice,
                status,
                totalSupply
            );
            views[index] = ev;
            index++;
        }
        return (views, startIndex - 1);
    }

    ///
    /// @notice Used as authorization for proxy upgrade
    /// @dev Only admin can upgrade proxy
    /// @param newImplementation address of contract holding new implementation
    ///
    function _authorizeUpgrade(address newImplementation)
        internal
        virtual
        override
        onlyOwner
    {}
}

