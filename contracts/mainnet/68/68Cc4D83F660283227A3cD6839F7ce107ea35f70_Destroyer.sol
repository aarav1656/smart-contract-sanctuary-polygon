pragma solidity ^0.8.8;

import "./Vault.sol";
import "./NFT.sol";
import "./Exchange.sol";
import "./Factory.sol";

///
/// @title Destroyer smart contract
/// @author Josip
/// @notice Responsible for initiating redeem NFT request from user of removal of "Vaults" on admin request
/// @dev Removes "Vaults" from "Factory" collection with cleanup process in case of admin request
/// or when user redeems NFT.
/// NOTE Contract is addition to "Factory" due to "Factory" size limit. Upgradeable.
///
contract Destroyer is UUPSUpgradeable, OwnableUpgradeable {
    ///
    /// @dev contract version
    ///
    string internal _contractVersion;

    ///
    /// @dev reference to a "Factory"
    ///
    address internal _factory;

    ///
    /// @dev reference to a "Exchange"
    ///
    address internal _exchange;

    ///
    /// @dev Throws if "Vault" does not exists
    /// @param vault address of "Vault"
    ///
    modifier validateVault(address vault) virtual {
        require(
            Factory(_factory).getVaultId(vault) != 0,
            "Entity does not exist"
        );
        _;
    }

    ///
    /// @notice Security in proxy pattern implementation
    /// @dev Used to disable calling initialize on implementation
    ///
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    ///
    /// @notice Used for initializing the state of contract V2
    /// @dev For proxy contracts "constructor" is swapped for "initialize"
    /// @param exchange_ sets reference for Exchange
    ///
    function initialize(address exchange_) public initializer {
        __Ownable_init();
        _exchange = exchange_;
        _contractVersion = "V1";
    }

    /************************
     *********** API ********
     ************************/

    ///
    /// @dev Used for upgrading purposes
    /// @return contract version
    ///
    function getVersion() public view virtual returns (string memory) {
        return _contractVersion;
    }

    ///
    /// @notice Enables user to redeem NFT from "Vault"
    /// @dev User can only redeem NFT if he has 100% of total supply of related ERC20 tokens.
    /// Method also starts cleanup process (pending orders, "Factory" storage vars).
    /// After completed "vault" is not longer part of collection in "Factory"
    /// Available only through proxy
    /// @param vault address of container holding NFT and related ERC20 tokens
    ///
    function redeemVault(address vault) public virtual validateVault(vault) {
        Vault v = Vault(vault);
        Token token = Token(v.getToken());
        require(
            token.getFullOwner() == msg.sender,
            "Caller is not owner of all tokens"
        );
        _cancelOrders(vault);

        v.redeemNFT(msg.sender);

        _cleanupStorage(vault, v.getCreator());
        v.redeemVaultEvent(msg.sender);
    }

    ///
    /// @notice Enables removal of "Vault"
    /// @dev Only admin can remove "Vault" from collection.
    /// Method also starts cleanup process (pending orders, "Factory" storage vars).
    /// After completed "vault" is not longer part of collection in "Factory"
    /// Available only through proxy
    /// @param vault address of container holding NFT and related ERC20 tokens
    ///
    function destroyVault(address vault)
        public
        virtual
        onlyOwner
        validateVault(vault)
    {
        _cancelOrders(vault);

        Vault v = Vault(vault);
        _cleanupStorage(vault, v.getCreator());
        v.destroyVaultEvent(owner());
    }

    // TODO search is over all vaults ???
    ///
    /// @notice Gets redeemble "Vaults" belonging to specific user
    /// @dev returns only "Vaults" in which user has 100% total supply of related ERC20 tokens
    /// Available only through proxy
    /// @param user address which redeemble "Vault" are available to
    /// @return "Vault" addresses for which user is eligble to redeem NFT
    ///
    function getRedeemVaultsAddrByUser(address user)
        public
        view
        virtual
        returns (address[] memory)
    {
        address[] memory vaults = Factory(_factory).getVaultsAddr();
        address[] memory redeemble;
        Token token;
        uint256 index = 0;
        for (uint256 i = 0; i < vaults.length; i++) {
            token = Token(Vault(vaults[i]).getToken());
            if (token.getFullOwner() == user) {
                redeemble[index] = vaults[i];
                index++;
            }
        }
        return redeemble;
    }

    ///
    /// @notice Sets reference for "Factory"
    /// @dev Only admin can set reference. Available only through proxy
    /// @param factory address of "Factory" contract
    ///
    function setFactory(address factory) public onlyOwner {
        _factory = factory;
    }

    /***************************
     *********** HELPERS ********
     ****************************/

    ///
    /// @notice Cancel active orders in specific "Vault"
    /// @dev Helper for initiating cleanup process for active orders inside "Vault".
    /// Used as part of "Vault" destroy or redeem process
    /// @param vault address which is holding orders
    ///
    function _cancelOrders(address vault) internal virtual {
        Exchange ex = Exchange(_exchange);
        uint256[] memory ids = ex.getOrderIdsByVault(vault);

        for (uint256 i = 0; i < ids.length; i++) {
            ex.cancelAnyOrder(vault, ids[i]);
        }
    }

    ///
    /// @notice Initiates cleanup process for storage vars
    /// @dev Used to have consistent state of "Vaults" collection in "Factory"
    /// @param vault address of which is being removed
    /// @param creator address of creator of "Vault"
    ///
    function _cleanupStorage(address vault, address creator) internal virtual {
        Factory(_factory).cleanCreatorVault(vault, creator);
    }

    ///
    /// @notice Used as authorization for proxy upgrade
    /// @dev Only admin can upgrade proxy
    /// @param newImplementation address of contract holding new implementation
    ///
    function _authorizeUpgrade(address newImplementation)
        internal
        virtual
        override
        onlyOwner
    {}
}

