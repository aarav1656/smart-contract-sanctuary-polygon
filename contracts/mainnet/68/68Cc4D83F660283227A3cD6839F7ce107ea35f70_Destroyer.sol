    /// @notice Used for initializing the state of contract
    /// @dev For proxy contracts "constructor" is swapped for "initialize"
    /// @param refTime_ sets start time for calculation to start from
    ///
    function initialize(uint256 refTime_) public initializer {
        __Ownable_init();
        _contractVersion = "V1";
        _refTime = refTime_;
    }

    /************************
     *********** API ********
     ************************/

    ///
    /// @notice adds new price and volume sample of order to contract collection
    /// @dev in datetime we pass new price and new volume to 24h and weekly rolling windows
    /// Exchange Only
    /// @param vault address associated to orders which generated price and volume
    /// @param volume of executing order
    /// @param price of executing order
    /// @param datetime of executing order
    ///
    function addPriceVol(
        address vault,
        uint256 volume,
        uint256 price,
        uint256 datetime
    ) public virtual validateVault(vault) {
        require(msg.sender == _exchange, "Not authorized");
        add7days(vault, volume, price, datetime);
        add24hours(vault, volume, price, datetime);
    }

    ///
    /// @notice gets price and volume by vaults provided with datetime
    /// @dev gets rolling window statistics back for 24h or 7 days from datetime
    /// @param vaults addresses of vaults to retrieve statistics for
    /// @param datetime point in time to fetch back 24h and 7days rolling window
    /// @return PriceVol - see PriceVol struct in ./globals
    ///
    function getPriceVolVaults(address[] memory vaults, uint256 datetime)
        public
        view
        returns (PriceVol[] memory)
    {
        require(msg.sender == _factory, "Not authorized");
        PriceVol[] memory collection = new PriceVol[](vaults.length);

        uint256 vol7d;
        uint256 vol24h;
        uint256 hprice7d;
        uint256 hprice24h;
        for (uint256 i = 0; i < vaults.length; i++) {
            (vol7d, hprice7d, vol24h, hprice24h) = getPriceVol(
                vaults[i],
                datetime
            );
            collection[i].vault = vaults[i];
            collection[i].highPrice7d = hprice7d;
            collection[i].vol7d = vol7d;
            collection[i].highPrice24h = hprice24h;
            collection[i].vol24h = vol24h;
        }

        return collection;
    }

    /***************************
     *********** HELPERS ********
     ****************************/

    ///
    /// @notice helper for fetching price and volume statistics
    /// @dev gets rolling window statistics back for 24h or 7 days from datetime
    /// @param vault address of vault to retrieve statistics for
    /// @param datetime point in time to fetch back 24h and 7days rolling window
    /// @return {vol7d - total volume in last 7 days from datetime, hprice7d - highest price in past 7 days from datetime
    /// vol24h - total volume in last 24h days from datetime, hprice24h - highest price in past 24 hours from datetime }
    ///
    function getPriceVol(address vault, uint256 datetime)
        private
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        uint256 vol7d;
        uint256 vol24h;
        uint256 hprice7d;
        uint256 hprice24h;

        (vol7d, hprice7d) = get7days(vault, datetime);
        (vol24h, hprice24h) = get24hours(vault, datetime);

        return (vol7d, hprice7d, vol24h, hprice24h);
    }

    ///
    /// @notice helper for adding volume and price statistics for 7 days rolling window
    /// @dev adds volume and price sample to specific bin of 7 days collection for specified vault.
    /// it takes into account rolling window where for new day iterations volume and price are reset to 0
