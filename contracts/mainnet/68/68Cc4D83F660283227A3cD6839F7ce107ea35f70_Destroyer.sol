    ///
    /// @notice Creates buy or sell "Order" in "Exchange"
    /// @dev Helper function for "Order" creation. Emitts "OrderEvent"
    /// @param vaultAddr address of "Vault" which "Order" is associated to
    /// @param pricePerToken is price per token in wei in which user wants to buy or sell customERC20 token
    /// @param tokenCount number of customERC20 tokens user wants to buy or sell
    /// @param orderType indicates type of "Order", buy or sell
    /// @param currency defines "Currency" used in "Order" transaction for purchase of customERC20 token
    ///
    function _createOrder(
        address vaultAddr,
        uint256 pricePerToken,
        uint256 tokenCount,
        OrderType orderType,
        Currency memory currency
    ) internal virtual {
        uint256 totalPrice = pricePerToken * (tokenCount / (10**18));
        uint256 fee = _calculateFee(totalPrice, _feePerc);

        Order memory order = Order({
            id: _orderId,
            orderType: orderType,
            orderStatus: OrderStatus.Created,
            vault: vaultAddr,
            time: block.timestamp,
            pricePerToken: pricePerToken,
            remainingTokens: tokenCount,
            feePerc: _feePerc,
            currency: currency,
            creator: msg.sender,
            exists: true
        });

        _vaultOrderIds[vaultAddr].push(_orderId);
        _userOrderIds[msg.sender].push(_orderId);
        _idOrder[_orderId] = order;

        emit OrderEvent({
            id: _orderId,
            orderType: orderType,
            orderStatus: OrderStatus.Created,
            vault: vaultAddr,
            time: block.timestamp,
            pricePerToken: pricePerToken,
            tokenCount: tokenCount,
            fee: fee,
            creator: msg.sender,
            executor: address(0),
            currencyAddr: currency.contractAddr
        });
        _orderId++;
    }

    ///
    /// @notice Executes buy or sell "Order" in "Exchange"
    /// @dev Helper function for "Order" execution. Emitts "OrderEvent"
    /// @param vaultAddr address of "Vault" which "Order" is associated to
    /// @param tokenCount number of customERC20 tokens user wants to buy or sell
    /// @param order defines "Order" which will be executed
    /// @param fee defines fee amount associated with "Order" execution
    ///
    function _executeOrder(
        address vaultAddr,
        uint256 tokenCount,
        Order memory order,
        uint256 fee
    ) internal virtual validateOrder(vaultAddr, order) {
        require(
            order.remainingTokens >= tokenCount,
            "Not enough remaining tokens in order"
        );

        order.remainingTokens -= tokenCount;
        if (order.remainingTokens == 0) {
            order.orderStatus = OrderStatus.Completed;
        } else {
            order.orderStatus = OrderStatus.Executed;
        }

        Vault v = Vault(vaultAddr);
        v.setLastPrice(
            Price(
                order.pricePerToken,
                order.currency.symbol,
                order.currency.decimals
            )
        );

        Statistics(_statistics).addPriceVol(
            order.vault,
            tokenCount,
            order.pricePerToken,
            block.timestamp
        );

        emit OrderEvent({
            id: order.id,
            orderType: order.orderType,
            orderStatus: order.orderStatus,
            vault: order.vault,
            time: block.timestamp,
            pricePerToken: order.pricePerToken,
            tokenCount: tokenCount,
            fee: fee,
            creator: order.creator,
            executor: msg.sender,
            currencyAddr: order.currency.contractAddr
        });

        _idOrder[order.id] = order;
    }

    ///
    /// @notice Cancels buy "Order" in "Exchange"
    /// @dev Helper function for buy "Order" cancellation.
    /// Reverts funds proportional to "order.remainingTokens" to buyer from "Exchange"
    /// @param vaultAddr address of "Vault" which "Order" is associated to
    /// @param order defines buy "Order" which will be cancelled
    ///
    function _cancelBuyOrder(address vaultAddr, Order memory order)
        internal
        virtual
        validateOrder(vaultAddr, order)
        validateCreator(order)
    {
        require(order.orderType == OrderType.Buy, "Order type invalid");
        _cancelOrder(order);

        IERC20Upgradeable coin = IERC20Upgradeable(order.currency.contractAddr);
        uint256 totalPrice = order.pricePerToken *
            (order.remainingTokens / (10**18));
        uint256 fee = _calculateFee(totalPrice, order.feePerc);

        // buyer needs to get funds back from exchange
        coin.safeTransfer(order.creator, totalPrice + fee);
    }

    ///
    /// @notice Cancels sell "Order" in "Exchange"
    /// @dev Helper function for sell "Order" cancellation.
    /// Reverts tokens in amount "order.remainingTokens" to seller from "Exchange"
    /// @param vaultAddr address of "Vault" which "Order" is associated to
    /// @param order defines sell "Order" which will be cancelled
    ///
    function _cancelSellOrder(address vaultAddr, Order memory order)
        internal
        virtual
        validateOrder(vaultAddr, order)
        validateCreator(order)
    {
        require(order.orderType == OrderType.Sell, "Order type invalid");
        _cancelOrder(order);

        // seller needs to get tokens back from exchange
        Token token = Token(Vault(vaultAddr).getToken());
        token.transfer(order.creator, order.remainingTokens);
    }

    ///
    /// @notice Cancels "Order" in "Exchange"
    /// @dev Helper function for "Order" cancellation. Emitts "OrderEvent"
    /// @param order defines "Order" which will be cancelled
    ///
    function _cancelOrder(Order memory order) internal virtual {
        order.orderStatus = OrderStatus.Cancelled;

        emit OrderEvent({
            id: order.id,
            orderType: order.orderType,
            orderStatus: OrderStatus.Cancelled,
            vault: order.vault,
            time: order.time,
            pricePerToken: order.pricePerToken,
            tokenCount: order.remainingTokens,
            fee: 0,
            creator: order.creator,
            executor: msg.sender,
            currencyAddr: order.currency.contractAddr
        });

        _idOrder[order.id] = order;
    }

    ///
    /// @notice Cancels buy "Order" in "Exchange"
    /// @dev Helper function for "Order" cancellation. Used in "Destroyer" processes.
    /// Reverts funds proportional to "order.remainingTokens" to buyer from "Exchange"
    /// Skips orders which cannot be reverted
    /// @param vaultAddr address of "Vault" which "Order" is associated to
    /// @param order defines buy "Order" which will be cancelled
    ///
    function _destroyBuyOrder(address vaultAddr, Order memory order)
        internal
        virtual
        validateExtCaller(order)
    {
        if (
            order.orderStatus == OrderStatus.Completed ||
            order.orderStatus == OrderStatus.Cancelled
        ) {
            return;
        }
        require(order.exists == true, "No order");
        require(vaultAddr == order.vault, "No order in entity");
        require(order.orderType == OrderType.Buy, "Order type invalid");

        _cancelOrder(order);

        IERC20Upgradeable coin = IERC20Upgradeable(order.currency.contractAddr);
        uint256 totalPrice = order.pricePerToken *
            (order.remainingTokens / (10**18));
        uint256 fee = _calculateFee(totalPrice, order.feePerc);

        // buyer needs to get funds back from exchange
        coin.safeTransfer(order.creator, totalPrice + fee);
    }

    ///
    /// @notice Cancels sell "Order" in "Exchange"
    /// @dev Helper function for sell "Order" cancellation. User in "Destroyer" processes.
    /// Reverts tokens in amount "order.remainingTokens" to seller from "Exchange"
    /// Skips orders which cannot be reverted
    /// @param vaultAddr address of "Vault" which "Order" is associated to
    /// @param order defines sell "Order" which will be cancelled
    ///
    function _destroySellOrder(address vaultAddr, Order memory order)
        internal
        virtual
        validateExtCaller(order)
    {
        if (
            order.orderStatus == OrderStatus.Completed ||
            order.orderStatus == OrderStatus.Cancelled
        ) {
            return;
        }
        require(order.exists == true, "No order");
        require(vaultAddr == order.vault, "No order in entity");
        require(order.orderType == OrderType.Sell, "Order type invalid");

        _cancelOrder(order);

        // seller needs to get tokens back from exchange
        Token token = Token(Vault(vaultAddr).getToken());
        token.transfer(order.creator, order.remainingTokens);
    }

    ///
    /// @notice Calculates fee amount
    /// @dev feePerc is uint type and to be able to use decimals we
    /// express 2.50 as 250 and divide with additional 100
    /// @param totalPrice total price for "Order"
    /// @param feePerc fee percentage expressed in 2 decimals (250 is 2.50)
    ///
    function _calculateFee(uint256 totalPrice, uint256 feePerc)
        internal
        pure
        virtual
        returns (uint256)
    {
        return (totalPrice * feePerc) / 10000;
    }

    ///
    /// @notice Used as authorization for proxy upgrade
    /// @dev Only admin can upgrade proxy
    /// @param newImplementation address of contract holding new implementation
    ///
    function _authorizeUpgrade(address newImplementation)
        internal
        virtual
        override
        onlyOwner
    {}
}

