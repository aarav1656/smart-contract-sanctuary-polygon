pragma solidity ^0.8.8;

import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";

import "./globals.sol";
import "./Token.sol";
import "./Vault.sol";
import "./Factory.sol";
import "./Statistics.sol";
import "hardhat/console.sol";

///
/// @title Exchange smart contract
/// @author Josip
/// @notice Enables creating and executing buy and sell orders
/// @dev Supports creating, executing and cancelling orders against NFT
/// related ERC20 tokens inside specific "Vault". Upgradeable.
///
contract Exchange is UUPSUpgradeable, OwnableUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    ///
    /// @dev contract version
    ///
    string internal _contractVersion;

    ///
    /// @dev reference to "Factory"
    ///
    address internal _factory;

    ///
    /// @dev reference to "Destroyer"
    ///
    address internal _destroyer;

    ///
    /// @dev reference to "Statistics"
    ///
    address internal _statistics;

    ///
    /// @dev destination where fees will be collected
    ///
    address internal _feeDest;

    ///
    /// @dev fee percentage with 2 decimals (250 is 2.50)
    ///
    uint256 internal _feePerc;

    ///
    /// @dev mapping currencyId to currency contract
    ///
    uint256 internal _currencyId;
    mapping(uint256 => Currency) internal _idCurrency;

    ///
    /// @dev mapping id to Order
    ///
    uint256 internal _orderId;
    mapping(uint256 => Order) internal _idOrder;

    ///
    /// @dev mapping vault to collection of order ids
    ///
    mapping(address => uint256[]) internal _vaultOrderIds;

    ///
    /// @dev mapping user to collection of order ids
    ///
    mapping(address => uint256[]) internal _userOrderIds;

    ///
    /// @dev defined type of the "Order"
    ///
    enum OrderType {
        Sell,
        Buy
    }

    ///
    /// @dev defines status of the "Order"
    /// Created - on order creation
    /// Executed - on order execution but not all order is fullfilled
    /// Completed - order is completely fullfilled (order.remainingTokens == 0)
    /// Cancelled - order is cancelled
    ///
    enum OrderStatus {
        Created,
        Executed,
        Completed,
        Cancelled
    }

    ///
    /// @dev defines "Order" used by "Exchange"
    /// @param id of "Order"
    /// @param orderType defines if "Order" is buy or sell order
    /// @param orderStatus defines "Order" status
    /// @param vault references "Vault" which "Order" is associated to
    /// @param time creation of "Order"
    /// @param pricePerToken defines price in wei for 1 unit of customERC20 token associated with vault
    /// @param remainingTokens indicates how much token are left in "Order" for buy/sell
    /// @param feePerc defines how much is fee percentage (with 2 decimals)
    /// @param currency struct defining currency data (decimals, name, address)
    /// @param creator address who created "Order"
    /// @param exists flag used to verify if "Order" exists in validations
    ///
    struct Order {
        uint256 id;
        OrderType orderType;
        OrderStatus orderStatus;
        address vault;
        uint256 time;
        uint256 pricePerToken;
        uint256 remainingTokens;
        uint256 feePerc;
        Currency currency;
        address creator;
        bool exists;
    }

    ///
    /// @dev Defines currency used in tradable pair "customERC20" / Currency
    /// @param contractAddr defines "Currency" contract used in "Order"
    /// @param name string indicating currency (DAI, MATIC, USDC) which customERC20 token is trading against in "Order"
    /// @param decimals number of decimals used for "Currency" in "Order"
    ///
    struct Currency {
        address contractAddr;
        string symbol;
        uint256 decimals;
    }

    ///
    /// @dev Emitts on order creation, execution and cancellation
    /// @param id of "Order"
    /// @param orderType defines if "Order" is buy or sell order
    /// @param orderStatus defines "Order" status
    /// @param vault references "Vault" which "Order" is associated to
    /// @param time creation of "Order"
    /// @param pricePerToken defines price in wei for 1 unit of customERC20 token associated with vault
    /// @param tokenCount indicates how much tokens are transfered in "Order" transaction buy/sell
    /// @param fee defines amount in "Currency" in current "Order"
    /// @param creator address who created "Order"
    /// @param executor address of user who execute existing buy/sell order
    /// @param currencyAddr address of currency used (DAI, USDC...)
    ///
    event OrderEvent(
        address indexed vault,
        address indexed creator,
        address indexed executor,
        uint256 id,
        OrderType orderType,
        OrderStatus orderStatus,
        uint256 time,
        uint256 pricePerToken,
        uint256 tokenCount,
        uint256 fee,
        address currencyAddr
    );

    ///
    /// @dev Throws if "Vault" does not exists
    /// @param vaultAddr address of "Vault"
    ///
    modifier validateVault(address vaultAddr) virtual {
        require(
            Factory(_factory).getVaultId(vaultAddr) != 0,
            "Entity does not exist"
        );
        _;
    }

    ///
    /// @notice Throws if "Vault" is not active.
    /// @dev Due to admin removing "Vault" or user redeems NFT from "Vault"
    /// @param vaultAddr address of "Vault"
    ///
    modifier vaultActive(address vaultAddr) virtual {
        require(
            Vault(vaultAddr).getStatus() == VaultStatus.Active,
            "NFT is not active for trade"
        );
        _;
    }

    ///
    /// @notice Throws if price per token, token count or fee are not valid
    /// @dev "tokenCount" is set to minimum of 1, min currency is set to 0.01
    /// Considering fee is calculated from "pricePerToken" and "tokenCount", calculated fee should be > 1 wei
    /// @param pricePerToken is price per token in wei
    /// @param tokenCount is amount of customERC20 token to sell/buy
    /// @param currencyId currency used for price
    ///
    modifier validatePrice(
        uint256 pricePerToken,
        uint256 tokenCount,
        uint256 currencyId
    ) virtual {
        require(tokenCount >= 10**18, "Quantity minimum is 1");
        require(
            pricePerToken >= 10**(_idCurrency[currencyId].decimals - 2),
            "Price per token should be greater than 0.01"
        );

        uint256 totalPrice = pricePerToken * (tokenCount / (10**18));
        uint256 fee = _calculateFee(totalPrice, _feePerc);

        require(fee > 0, "Total price lower then minimum");
        _;
    }

    ///
    /// @notice Throws if "Order" is not valid
    /// @dev due to "Order" not existing, not existing in specified "vaultAddr"
    /// or if it is already completely fullfilled or cancelled
    /// @param vaultAddr is address of "Vault" hosting the "Order"
    /// @param order referencing "Order" which is validated
    ///
    modifier validateOrder(address vaultAddr, Order memory order) virtual {
        require(order.exists == true, "No order");
        require(vaultAddr == order.vault, "No order in entity");
        require(
            order.orderStatus == OrderStatus.Created ||
                order.orderStatus == OrderStatus.Executed,
            "Invalid order status"
        );
        _;
    }

    ///
    /// @dev Throws if creator of "Order" is not caller
    /// @param order referencing "Order" which is validated
    ///
    modifier validateCreator(Order memory order) virtual {
        require(order.creator == msg.sender, "Caller is not owner");
        _;
    }

    ///
    /// @notice Throws if callers are not "Destroyer" or admin
    /// @dev "Destroyer" check is for some methods which are used as helpers by "Destroyer" in "Exchange"
    /// @param order referencing "Order" which is validated
    ///
    modifier validateExtCaller(Order memory order) virtual {
        require(
            owner() == msg.sender || _destroyer == msg.sender,
            "Not authorized"
        );
        _;
    }

    ///
    /// @dev Throws if chosen "Currency" is invalid
    /// @param id of "Currency"
    ///
    modifier validateCurrency(uint256 id) virtual {
        require(
            _idCurrency[id].contractAddr != address(0),
            "Cryptocurrency not supported"
        );
        _;
    }

    ///
    /// @notice Security in proxy pattern implementation
    /// @dev Used to disable calling initialize on implementation
    ///
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    ///
    /// @notice Used for initializing the state of contract
    /// @dev For proxy contracts "constructor" is swapped for "initialize"
    /// @param feeDest_ sets reference for fee destination
    /// @param feePerc_ sets fee percentage in 2 decimals (250 is 2.50)
    ///
    function initialize(address feeDest_, uint256 feePerc_) public initializer {
        __Ownable_init();
        _feeDest = feeDest_;
        _feePerc = feePerc_;
        _contractVersion = "V1";
    }

