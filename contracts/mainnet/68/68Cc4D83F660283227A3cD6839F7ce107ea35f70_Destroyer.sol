    /// @param from address from where tokens are transfered
    /// @param to address of token receiver
    /// @param amount token amount transfered
    ///
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        if (amount == 0) {
            return;
        }
        // check if "to" is new owner
        if (balanceOf(to) == totalSupply()) {
