    /// @dev reference to "Exchange"
    ///
    address internal _exchange;

    ///
    /// @dev reference to "Destroyer"
    ///
    address internal _destroyer;

    ///
    /// @dev reference to "Statistics"
    ///
    address internal _statistics;

    ///
    /// @dev Vault Id
    ///
    uint256 internal _vaultId;

    ///
    /// @dev mapping vaultAddr to vaultId
    ///
    mapping(address => uint256) internal _vaultToId;

    ///
    /// @dev array of "Vault" addresses
    ///
    address[] internal _vaults;

    ///
    /// @dev mapping vault to index in _vaults
