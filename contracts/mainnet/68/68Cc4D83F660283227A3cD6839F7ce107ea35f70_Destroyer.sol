    /// @param price sample of executing order
    /// @param datetime of executing order
    ///
    function add7days(
        address vault,
        uint256 volume,
        uint256 price,
        uint256 datetime
    ) private {
        uint256 diff = datetime - _refTime;
        uint256 bin = (diff / 60 / 60 / 24) % 7;

        uint256 currentWeek = diff / (7 * 24 * 60 * 60);

        if (_vault7d[vault][bin].week < currentWeek) {
            _vault7d[vault][bin].volume = 0;
            _vault7d[vault][bin].highPrice = 0;
            _vault7d[vault][bin].week = currentWeek;
        }
        _vault7d[vault][bin].volume += volume;

        if (_vault7d[vault][bin].highPrice < price) {
            _vault7d[vault][bin].highPrice = price;
