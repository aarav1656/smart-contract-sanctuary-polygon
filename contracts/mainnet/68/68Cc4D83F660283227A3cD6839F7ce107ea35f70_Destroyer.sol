    /// @param highPrice highest price in a day
    /// @param week number starting from _refTime
    ///
    struct Days7 {
        uint256 volume;
        uint256 highPrice;
        uint256 week;
    }

    ///
    /// @dev type for storing volume and highPrice for each hour
    /// @param volume of complete day
    /// @param highPrice highest price in a day
    /// @param week number starting from _refTime
    ///
    struct Hours24 {
        uint256 volume;
        uint256 highPrice;
        uint256 day;
    }

    ///
    /// @dev Throws if "Vault" does not exists
    /// @param vaultAddr address of "Vault"
    ///
    modifier validateVault(address vaultAddr) virtual {
        require(
            Factory(_factory).getVaultId(vaultAddr) != 0,
            "Entity does not exist"
        );
        _;
    }

