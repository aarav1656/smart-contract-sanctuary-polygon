    ///
    /// @dev Used for upgrading purposes
    /// @return contract version
    ///
    function getVersion() public view virtual returns (string memory) {
        return _contractVersion;
    }

    ///
    /// @notice Creates buy order for specific "Vault"
    /// @dev Method transfers calculated "Currency" amount from buyer to "Exchange" as deposit to future "Order" execution
    /// This includes amount covering purchase of tokens + calculated fee. Token count is enforced integer.
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param pricePerToken is price per token in wei which buyer wants to pay for customERC20 token associated with "vaultAddr"
    /// @param tokenCount indicating how much tokens buyer wants to buy
    /// @param currencyId indicating id of "Currency" in  which buyer wants to pay for customERC20 tokens
    ///
    function createBuyOrder(
        address vaultAddr,
        uint256 pricePerToken,
        uint256 tokenCount,
        uint256 currencyId
    )
        public
        virtual
        vaultActive(vaultAddr)
        validateCurrency(currencyId)
        validatePrice(pricePerToken, tokenCount, currencyId)
    {
        tokenCount = (tokenCount / 10**18) * 10**18;
        Currency memory currency = _idCurrency[currencyId];
        _createOrder(
            vaultAddr,
            pricePerToken,
            tokenCount,
            OrderType.Buy,
            currency
        );

        uint256 totalPrice = pricePerToken * (tokenCount / (10**18));
        uint256 fee = _calculateFee(totalPrice, _feePerc);

        // Here already approved by buyer to exchange, deposit funds + fee
        IERC20Upgradeable coin = IERC20Upgradeable(currency.contractAddr);
        coin.safeTransferFrom({
            from: msg.sender,
            to: address(this),
            value: totalPrice + fee
        });
    }

    ///
    /// @notice Creates sell order for specific "Vault"
    /// @dev Method transfers "tokenCount" from sell to "Exchange" as deposit to future "Order" execution. Token count is enforced integer.
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param pricePerToken is price per token in wei which seller wants to sell his customERC20 token associated with "vaultAddr"
    /// @param tokenCount indicating how much tokens seller wants to sell
    /// @param currencyId indicating id of "Currency" in  which seller wants to be paid for customERC20 tokens
    ///
    function createSellOrder(
        address vaultAddr,
        uint256 pricePerToken,
        uint256 tokenCount,
        uint256 currencyId
    )
        public
        virtual
        vaultActive(vaultAddr)
        validateCurrency(currencyId)
        validatePrice(pricePerToken, tokenCount, currencyId)
    {
        //
        tokenCount = (tokenCount / 10**18) * 10**18;
        Currency memory currency = _idCurrency[currencyId];
        _createOrder(
            vaultAddr,
            pricePerToken,
            tokenCount,
            OrderType.Sell,
            currency
        );

        // Here already approved by seller to exchange, tokens
        Token token = Token(Vault(vaultAddr).getToken());
        token.transferFrom({
            from: msg.sender,
            to: address(this),
            amount: tokenCount
        });
    }

    ///
    /// @notice Executes buy order for specific "Vault"
    /// @dev Method transfers tokens with amount "tokenCount" from seller to buyer
    /// also transfers funds from "Exchange" to seller proportional to "tokenCount" and
    /// finally transfers fee amount from "Exchange" to fee destination proportional to "tokenCount".
    /// Token count is enforced integer.
    /// NOTE: order does not need to be fully fullfilled and some funds cound be left
    /// on "Exchange" for future "Order" execution
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param tokenCount indicating how much tokens sellers wants to sell
    /// @param orderId references id of "Order" which is being executed
    ///
    function executeBuyOrder(
        address vaultAddr,
        uint256 tokenCount,
        uint256 orderId
    ) public virtual validateVault(vaultAddr) vaultActive(vaultAddr) {
        Order memory order = _idOrder[orderId];
        require(order.exists == true, "No order");
        require(order.orderType == OrderType.Buy, "Wrong order type");
        require(vaultAddr == order.vault, "No order in entity");

        require(tokenCount >= 10**18, "Token number below minimum");
        tokenCount = (tokenCount / 10**18) * 10**18;

        uint256 totalPrice = order.pricePerToken * (tokenCount / (10**18));
        uint256 fee = _calculateFee(totalPrice, order.feePerc);

        require(fee > 0, "Total price lower then minimum");

        _executeOrder(vaultAddr, tokenCount, order, fee);

        Vault v = Vault(vaultAddr);
        Token token = Token(v.getToken());
        // Here already approved by seller to exchange
        token.transferFrom({
            from: msg.sender,
            to: order.creator,
            amount: tokenCount
        });

        // seller should get funds from exchange
        IERC20Upgradeable coin = IERC20Upgradeable(order.currency.contractAddr);
        coin.safeTransfer({to: msg.sender, value: totalPrice});

        // fee destination should get fees from exchange
        coin.safeTransfer({to: _feeDest, value: fee});
    }

    ///
    /// @notice Executes sell order for specific "Vault"
    /// @dev Method transfers funds from buyer to seller proportional to "tokenCount"
    /// also transfers fee funds from buyer to fee destination proportional to "tokenCount" and
    /// finally transfers token amount "tokenCount" from "Exchange" to buyer.
    /// Token count is enforced integer.
    /// NOTE: order does not need to be fully fullfilled and some funds cound be left
    /// on "Exchange" for future "Order" execution
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param tokenCount indicating how much tokens buyer wants to buy
    /// @param orderId references id of "Order" which is being executed
    ///
    function executeSellOrder(
        address vaultAddr,
        uint256 tokenCount,
        uint256 orderId
    ) public virtual validateVault(vaultAddr) vaultActive(vaultAddr) {
        Order memory order = _idOrder[orderId];
        require(order.exists == true, "No order");
        require(order.orderType == OrderType.Sell, "Wrong order type");
        require(vaultAddr == order.vault, "No order in entity");

        require(tokenCount >= 10**18, "Token number below minimum");
        tokenCount = (tokenCount / 10**18) * 10**18;

        uint256 totalPrice = order.pricePerToken * (tokenCount / (10**18));
        uint256 fee = _calculateFee(totalPrice, order.feePerc);

        require(fee > 0, "Total price lower then minimum");

        _executeOrder(vaultAddr, tokenCount, order, fee);

        Token token = Token(Vault(vaultAddr).getToken());

        // here already approved buyer to exchange, funds + fee
        // transfer fee from buyer to fee destination
        IERC20Upgradeable coin = IERC20Upgradeable(order.currency.contractAddr);
        coin.safeTransferFrom({from: msg.sender, to: _feeDest, value: fee});

        // transfer funds from  buyer to seller
        coin.safeTransferFrom({
            from: msg.sender,
            to: order.creator,
            value: totalPrice
        });
        // send tokens to buyer
        token.transfer({to: msg.sender, amount: tokenCount});
    }

    ///
    /// @notice Cancels buy order for specific "Vault"
    /// @dev Method transfers funds + fee from "Exchange" to buyer
    /// NOTE: transfer back is proportional to "order.remainingTokens" in "Order"
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param orderId references id of "Order" which is being cancelled
    ///
    function cancelBuyOrder(address vaultAddr, uint256 orderId)
        public
        virtual
        validateVault(vaultAddr)
    {
        Order memory order = _idOrder[orderId];
        _cancelBuyOrder(vaultAddr, order);
    }

    ///
    /// @notice Cancels sell order for specific "Vault"
    /// @dev Method transfers "order.remainingTokens" token amount from "Exchange" to seller
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param orderId references id of "Order" which is being cancelled
    ///
    function cancelSellOrder(address vaultAddr, uint256 orderId)
        public
        virtual
        validateVault(vaultAddr)
    {
        Order memory order = _idOrder[orderId];
        _cancelSellOrder(vaultAddr, order);
    }

    ///
    /// @notice Cancels buy or sell "Order" in "Vault"
    /// @dev Method is used by "Destroyer" for removing/redeeming "Vault"
    /// Only "Destroyer" is allowed
    /// @param vaultAddr vaultAddr is address of "Vault" which "Order" associates to
    /// @param orderId references id of "Order" which is being cancelled
    ///
    function cancelAnyOrder(address vaultAddr, uint256 orderId) public virtual {
        require(_destroyer == msg.sender, "Not authorized");

        Order memory order = _idOrder[orderId];

        if (order.orderType == OrderType.Buy) {
            _destroyBuyOrder(vaultAddr, order);
        } else if (order.orderType == OrderType.Sell) {
            _destroySellOrder(vaultAddr, order);
        } else {
            require(false, "Order type invalid");
        }
    }

    ///
    /// @dev Gets "Order" by Order Id. Available only through proxy
