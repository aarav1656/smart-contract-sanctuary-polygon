    {
        uint256 diff = datetime - _refTime;
        uint256 bin = (diff / 60 / 60 / 24) % 7;

        uint256 currentWeek = diff / (7 * 24 * 60 * 60);

        uint256 sum = 0;
        uint256 highestPrice = 0;
        for (uint256 i = bin; i >= 0; i--) {
