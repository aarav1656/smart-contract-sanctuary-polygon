contract ProxyAdminPin is  ERC2771Context, Ownable {
    constructor(address trustedForwarder) ERC2771Context(trustedForwarder) {}

    function _msgSender() internal view virtual override(Context, ERC2771Context) returns (address sender) {
        return ERC2771Context._msgSender();
    }

    function _msgData() internal view virtual override(Context, ERC2771Context) returns (bytes calldata) {
        return ERC2771Context._msgData();
    }

    function pin(ITokenProxy proxy) public virtual onlyOwner {
        proxy.pin();
    }

    function upgrade(ITokenProxy proxy) public virtual onlyOwner {
        proxy.upgradeTo();
    }

    function unpin(ITokenProxy proxy) public virtual onlyOwner {
        proxy.unpin();
    }

    function getProxyImplementation(ITokenProxy proxy) public view virtual returns (address) {
        // We need to manually run the static call since the getter cannot be flagged as view
        // bytes4(keccak256("implementation()")) == 0x5c60da1b
        (bool success, bytes memory returndata) = address(proxy).staticcall(hex"5c60da1b");
        require(success);
        return abi.decode(returndata, (address));
    }

    function getProxyAdmin(ITokenProxy proxy) public view virtual returns (address) {
        // We need to manually run the static call since the getter cannot be flagged as view
        // bytes4(keccak256("admin()")) == 0xf851a440
        (bool success, bytes memory returndata) = address(proxy).staticcall(hex"f851a440");
        require(success);
        return abi.decode(returndata, (address));
    }

    function changeProxyAdmin(ITokenProxy proxy, address newAdmin) public virtual onlyOwner {
        proxy.changeAdmin(newAdmin);
    }
}

contract ERC1155ImplementationV1 is Initializable,
OwnableUpgradeable,
PausableUpgradeable,
ERC2981,
UsableErc1155,
ERC2771ContextUpgradeable,
ERC1155Upgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter private _tokenIdCounter;

    string private _name;
    string private _symbol;
    mapping(uint256 => mapping(address => mapping(address => bool))) public _tokenApprovals;

    event Approval(address indexed owner, address indexed approved, uint256 indexed tokenId);
    event RevokeApproval(address indexed owner, address indexed approved, uint256 indexed tokenId);

    modifier onlyFactoryOwner {
        require(msg.sender == factory.owner());
        _;
    }

    modifier onlyTrustedSender {
        require(
            isTrustedForwarder(msg.sender) ||
            msg.sender == address(factory),
            "sender must be trusted"
        );
        _;
    }

    uint256 private capacity;
    uint256 private copyCapacity;
    IFactory private factory;

    constructor(address trustedForwarder) ERC2771ContextUpgradeable(trustedForwarder) {
    }

    function initialize(
        string calldata name,
        string calldata symbol,
        address _factory,
        uint256 _capacity,
        uint256 _copyCapacity,
        uint96 _royaltyFee,
        address _royaltyReceiver
    ) initializer public {
        __ERC1155_init("");
        __Ownable_init();
        __Pausable_init();

        _name = name;
        _symbol = symbol;
        factory = IFactory(_factory);
        capacity = _capacity;
        copyCapacity = _copyCapacity;
        _setDefaultRoyalty(_royaltyReceiver, _royaltyFee);
    }

    function mint(
        address receiver,
        uint256 tokenID,
        uint256 amount,
        bytes memory data
    ) public onlyTrustedSender onlyOwner returns (uint256)  {
        _tokenIdCounter.increment();
        require(_tokenIdCounter.current() <= capacity, "capacity exceeded");
        require(amount <= copyCapacity, "copy capacity exceeded");
        require(!_minted[tokenID], "already minted");

        _mint(receiver, tokenID, amount, data);
        _minted[tokenID] = true;

        return tokenID;
    }

    function batchMint(
        address receiver,
        uint256[] calldata tokenIDs,
        uint256[] calldata amount,
        bytes memory data
    )
    public onlyTrustedSender onlyOwner {
        require(tokenIDs.length == amount.length, "token amount and copies amount is not eq");
        uint256 _now = _tokenIdCounter.current();
        uint256 _after = _now + tokenIDs.length;
        require(_after <= capacity, "capacity exceeded");
        for (uint256 i = 0; i < tokenIDs.length; i++) {
            uint256 id = tokenIDs[i];

            require(!_minted[id], "already minted");
            require(amount[i] <= copyCapacity, "copy capacity is exceeded");
            _minted[id] = true;
        }
        _mintBatch(receiver, tokenIDs, amount, data);
    }

    function use(uint256 tokenId, uint256 amount) public {
        uint256 balance = balanceOf(_msgSender(), tokenId);
        require(balance >= amount, "low balance for this token id");
        require(inUse(tokenId, _msgSender()) + amount <= balance, "locked amount can't be more then balance");
        _use(tokenId, amount, _msgSender());
    }

    function unuse(uint256 tokenId, uint256 amount) public {
        uint256 balance = balanceOf(_msgSender(), tokenId);
        require(balance > 0, "low balance for unuse this token id");
        _unuse(tokenId, amount, _msgSender());
    }

    function pause() public onlyFactoryOwner {
        _pause();
    }

    function unpause() public onlyFactoryOwner {
        _unpause();
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
    internal
    whenNotPaused
    whenNotInUse(ids, amounts, _msgSender())
    override
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function _msgSender() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (address) {
        return super._msgSender();
    }

    function _msgData() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (bytes calldata) {
        return super._msgData();
    }

    function uri(uint256 tokenId) public view override(ERC1155Upgradeable) returns (string memory)
    {
        return string(
            abi.encodePacked(
                factory.baseURI(),
                "/",
                Strings.toHexString(uint256(uint160(address(this))), 20),
                "/",
                Strings.toString(tokenId)
            )
        );
    }

    function contractURI() public view returns (string memory)
    {
        return string(
            abi.encodePacked(
                factory.baseURI(),
                "/",
                Strings.toHexString(uint256(uint160(address(this))), 20)
            )
        );
    }

    function burn(address from, uint256 id, uint256 amount) public onlyOwner {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        _burn(from, id, amount);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override {
        require(
            from == _msgSender() ||
            isApprovedForAll(from, _msgSender()) ||
            this.isApproved(from, _msgSender(), id),
            "ERC1155: caller is not owner nor approved"
        );
        _safeTransferFrom(from, to, id, amount, data);
    }

    function batchApprove(address from, address to, uint256[] calldata tokenIds) external {
        require(to != address(0), "spender can't be zero address");
        require(tokenIds.length > 0, "zero len array illegal");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            approve(from, to, tokenIds[i]);
        }
    }

    function batchRevoke(address from, address to, uint256[] calldata tokenIds) external {
        require(tokenIds.length > 0, "zero len array illegal");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            revoke(from, to, tokenIds[i]);
        }
    }

    function _approve(address owner, address spender, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId][owner][spender] = true;
        emit Approval(owner, spender, tokenId);
    }

    function _revoke(address owner, address spender, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId][owner][spender] = false;
        emit RevokeApproval(owner, spender, tokenId);
    }

    function revoke(address from, address to, uint256 tokenId) public {
        require(ERC1155Upgradeable.balanceOf(_msgSender(), tokenId) > 0, "balance should be non zero");

        _revoke(from, to, tokenId);
    }

    function approve(address from, address to, uint256 tokenId) public {
        require(ERC1155Upgradeable.balanceOf(_msgSender(), tokenId) > 0, "balance should be non zero");
        require(_msgSender() == from, "invalid sender");

        _approve(from, to, tokenId);
    }

    function isApproved(address from, address to, uint256 tokenId) public view returns (bool) {
        return _tokenApprovals[tokenId][from][to];
    }

    function totalSupply() public view returns (uint256) {
        return capacity;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC2981, ERC1155Upgradeable) returns (bool)
    {
        return
        interfaceId == type(IERC2981).interfaceId ||
        super.supportsInterface(interfaceId);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    mapping(uint256 => bool) private _minted;
}

contract FactoryERC1155 is IFactory, Ownable, EIP712 {
    using StringStorage for StringStorage.Set;
    using ECDSA for bytes32;

    string uri;
    address _implementation;

    mapping(address => address) public _proxyAdmins;

    event Deployed(address collection, address owner);

    mapping(address => StringStorage.Set) private  _onetimestrs;
    mapping(address => bool) private _managers;

    bytes32 private constant _TYPECREATEERC1155 = keccak256(abi.encodePacked("createERC1155"));
    bytes32 private constant _TYPEHASH =
    keccak256("DeployRequest(string Tx_Type,string Description,address Owner,string Name,string Symbol,uint256 Max_Items_per_Collection,uint256 Max_Copies_per_NFT,uint96 Percent_Royalty_in_Hundredths,address Royalties_to_this_Address,uint256[] Token_ID,uint256[] Amount,string Onetimestr,bytes32 Salt)");

    struct DeployRequest {
        string description;
        address owner;
        string name;
        string symbol;
        uint256 capacity;
        uint256 maximumCopyCapacity;
        uint96 royaltyP;
        address royaltyWallet;
        uint256[] tokenID;
        uint256[] amount;
        string onetimestr;
        bytes32 salt;
    }

    function verify(DeployRequest calldata req, bytes calldata signature) public view returns (bool) {
        if (_hashTypedDataV4(
            keccak256(
                bytes.concat(
                    abi.encode(
                        _TYPEHASH,
                        _TYPECREATEERC1155,
                        keccak256(abi.encodePacked(req.description)),
                        req.owner,
                        keccak256(abi.encodePacked(req.name)),
                        keccak256(abi.encodePacked(req.symbol)),
                        req.capacity
                    ),
                    abi.encode(
                        req.maximumCopyCapacity,
                        req.royaltyP,
                        req.royaltyWallet,
                        keccak256(abi.encodePacked(req.tokenID)),
                        keccak256(abi.encodePacked(req.amount)),
                        keccak256(abi.encodePacked(req.onetimestr)),
                        req.salt
                    ))
            )).recover(signature) != req.owner) {
            return false;
        }
        return !_onetimestrs[req.owner].contains(req.onetimestr);
    }

    constructor (string memory _uri, address implementation) EIP712("Pixchange 1155 Minting", "0.0.1") {
        uri = _uri;
        _implementation = implementation;
    }

    function owner() public override(Ownable, IFactory) view returns (address) {
        return Ownable.owner();
    }

    function baseURI() external override view returns (string memory) {
        return uri;
    }

    function setBaseURI(string memory _uri) external onlyOwner {
        uri = _uri;
    }

    function setImplementation(address implementation) external onlyOwner {
        _implementation = implementation;
    }

    function implementation() external view returns (address){
        return _implementation;
    }

    function addManager(address _manager) external onlyOwner {
        _managers[_manager] = true;
    }

    function deleteManager(address _manager) external onlyOwner {
        _managers[_manager] = false;
    }

    function isManager(address _manager) public view returns (bool){
        return _managers[_manager];
    }

    modifier onlyManagerOrOwner() {
        require(isManager(msg.sender) || msg.sender == owner(), "must be manager");
        _;
    }

    function deploy(
        DeployRequest calldata req,
        bytes calldata signature
    ) external onlyManagerOrOwner {
        require(verify(req, signature), "failed to verify signature");
        _onetimestrs[req.owner].mustAdd(req.onetimestr);
        address tokenProxy = deployProxyForAddr(req);
        ERC1155ImplementationV1(tokenProxy).transferOwnership(req.owner);
    }

    function deployWithMint(
        DeployRequest calldata req,
        bytes calldata signature
    ) external onlyManagerOrOwner {
        require(verify(req, signature), "failed to verify signature");
        _onetimestrs[req.owner].mustAdd(req.onetimestr);
        address tokenProxy = deployProxyForAddr(req);
        ERC1155ImplementationV1 _impl = ERC1155ImplementationV1(tokenProxy);
        _impl.batchMint(req.owner, req.tokenID, req.amount, "");
        _impl.transferOwnership(req.owner);
    }

    function deployProxyForAddr(
        DeployRequest calldata req
    ) internal returns (address){
        address _admin = getAdmin(req.owner, req.salt);
        address _tokenProxy = deployProxy(
            _admin,
            req.name,
            req.symbol,
            req.capacity,
            req.maximumCopyCapacity,
            req.royaltyP,
            req.royaltyWallet,
            req.salt
        );
        return _tokenProxy;
    }

    function getAdmin(address _owner, bytes32 _salt) internal returns (address){
        address _admin = _proxyAdmins[_owner];
        if (_admin == address(0)) {
            _admin = address(new ProxyAdminPin{salt : _salt}(_owner));
            _proxyAdmins[_owner] = _admin;
        }
        return _admin;
    }

    function getProxyAdminPin(address owner) external view returns (address) {
        return _proxyAdmins[owner];
    }

    function deployProxy(
        address admin,
        string calldata name,
        string calldata symbol,
        uint256 capacity,
        uint256 maximumCopyCapacity,
        uint96 royaltyP,
        address royaltyWallet,
        bytes32 _salt
    ) internal returns (address) {
        require(capacity <= 1000, "high capacity");
        require(maximumCopyCapacity <= 1000, "high copies capacity");

        TokenProxy tokenProxy = new TokenProxy{salt : _salt}(
            admin,
            _implementation,
            abi.encodeWithSelector(
                bytes4(keccak256("initialize(string,string,address,uint256,uint256,uint96,address)")),
                name,
                symbol,
                address(this),
                capacity,
                maximumCopyCapacity,
                royaltyP,
                royaltyWallet
            )
        );
        emit Deployed(address(tokenProxy), admin);
        return address(tokenProxy);
    }

    function predictAddress(bytes32 salt, address owner, string calldata name, string calldata symbol, uint256 capacity, uint256 maximumCopyCapacity, uint96 royaltyP, address royaltyWallet) external view returns (address) {
        address _admin = _proxyAdmins[owner];
        if (_admin == address(0)) {
            bytes32 hash = keccak256(
                abi.encodePacked(
                    bytes1(0xff), address(this), salt, keccak256(getProxyAdminBytecode(owner))
                )
            );

            _admin = address(uint160(uint(hash)));
        }

        bytes32 hash = keccak256(
            abi.encodePacked(
                bytes1(0xff), address(this), salt, keccak256(getERC1155Bytecode(_admin, name, symbol, capacity, maximumCopyCapacity, royaltyP, royaltyWallet))
            )
        );
        return address(uint160(uint(hash)));
    }

    function getERC1155Bytecode(address _admin, string calldata name, string calldata symbol, uint256 capacity, uint256 maximumCopyCapacity, uint96 royaltyP, address royaltyWallet) internal view returns (bytes memory) {
        bytes memory bytecode = type(TokenProxy).creationCode;
        return abi.encodePacked(bytecode, abi.encode(_admin, _implementation,
            abi.encodeWithSelector(
                bytes4(keccak256("initialize(string,string,address,uint256,uint256,uint96,address)")),
                name,
                symbol,
                address(this),
                capacity,
                maximumCopyCapacity,
                royaltyP,
                royaltyWallet
            )));
    }

    function getProxyAdminBytecode(address _owner) internal view returns (bytes memory) {
        bytes memory bytecode = type(ProxyAdminPin).creationCode;
        return abi.encodePacked(bytecode, abi.encode(_owner));
    }

}