pragma solidity ^0.8.17;





contract VIPSaleContract is AccessControl, NativeMetaTransaction{
    bytes32 public constant METATNX_EXECUTOR_ROLL = keccak256("METATNX_EXECUTOR_ROLL");
    address internal constant NFT_BURN_ADDRESS = 0xd65a2b2266F300a734ADA120d7cF580DBE73b5b6;
    IDCLNFT vipPass_contract = IDCLNFT(0x8cefC42959A23Fe092613344347bAbcCaA9c221D);
    event AddItem(address indexed _address, uint256 indexed _item, uint256 _price, uint256 _saleStartTime);
    event RemoveItem(address indexed _address, uint256 indexed _item);
    event PriceUpdate(address indexed _address, uint256 indexed _item, uint256 _price);
    event AddUpgradableItem(address indexed _address, uint256 indexed _item, uint256 _price, uint256 _saleStartTime,address upgrade_from_address, uint256 upgrade_from_item);
    event RemoveContract(address indexed _address, uint256[] _items);

    constructor() {
        issueDetails.reciver_add.push(_msgSender());
        issueDetails.reciver_item.push(0);
        _initializeEIP712("WLDYVIPSale", "1");
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(METATNX_EXECUTOR_ROLL, _msgSender());
        
    }
    struct TokenContract{
        address token_address;
        IERC20 token_contract;
        bool isInitilized;
    }

    struct IssueDetails{
        address[]  reciver_add;
        uint256[]  reciver_item;
    }

    TokenContract token;
    IssueDetails issueDetails;

    struct NFTContract {
        address nft_address;
        uint256[] items;
        uint256[] tokenIds;
        mapping(uint256 => uint256) item_indexMap;
        mapping(uint256 => uint256) price;
        mapping(uint256 => uint256) saleStartTime;
        mapping(uint256 => uint256[]) vipTokenIdItemMap;
        mapping(uint256 => address) upgrade_from_address;
        mapping(uint256 => uint256) upgrade_from_item;
        IDCLNFT nft_contract;
        bool isInitilized;
    }

    mapping(address => NFTContract) public contracts;

    function verifyVIPSale(address _address, uint256 _item, uint256 tokenId) internal{
        require(vipPass_contract.ownerOf(tokenId) == _address, "User dont have provided token");
        require(contracts[_address].saleStartTime[_item] + 86400 > block.timestamp, "Sale is already over");
        uint256 len = contracts[_address].vipTokenIdItemMap[tokenId].length;
        if(_item < len){
            require(contracts[_address].vipTokenIdItemMap[tokenId][_item] == 0, "This vip pass is already used");
        }else{
            for(uint256 i =  len; i<=_item; i++){
                contracts[_address].vipTokenIdItemMap[tokenId].push(0);
            }
        }
        contracts[_address].vipTokenIdItemMap[tokenId][_item] = 1;
        contracts[_address].tokenIds.push(tokenId);
    }

    function executeMetaTransaction(
        address userAddress,
        bytes memory functionSignature,
        bytes32 sigR,
        bytes32 sigS,
        uint8 sigV
    ) external onlyRole(METATNX_EXECUTOR_ROLL) returns (bytes memory) {
        return _executeMetaTransaction(userAddress, functionSignature, sigR, sigS,sigV);
    }

    function setTokenContract(address _address) external onlyRole(DEFAULT_ADMIN_ROLE){
        token.token_contract = IERC20(_address);
        token.token_address = _address;
        token.isInitilized = true;
    }

    function addContract(address _address) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(!contracts[_address].isInitilized, "contract already exists");
        IDCLNFT nft_contract = IDCLNFT(_address);
        contracts[_address].nft_address = _address;
        contracts[_address].nft_contract = nft_contract;
        contracts[_address].isInitilized = true;
    }

    function removeContract(address _address) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(contracts[_address].isInitilized, "contract dose not exits");
        //delete all the item price map
        uint256[] memory _items = contracts[_address].items;
        uint256[] memory _tokenIds = contracts[_address].tokenIds;
        
        for(uint i=0; i<_items.length; i++){
            delete contracts[_address].price[_items[i]];
            delete contracts[_address].saleStartTime[_items[i]];
            delete contracts[_address].item_indexMap[_items[i]];
            delete contracts[_address].upgrade_from_address[_items[i]];
            delete contracts[_address].upgrade_from_item[_items[i]];
        }
        for(uint i=0; i<_tokenIds.length; i++){
            delete contracts[_address].vipTokenIdItemMap[_tokenIds[i]];
        }
        delete contracts[_address];
        emit RemoveContract(_address, _items);
    }

    function addItem(address _address, uint256 _item, uint256 _price, uint256 _saleStartTime) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(contracts[_address].isInitilized, "contract dose not exits");
        require(contracts[_address].price[_item] == 0, "item already added");
        contracts[_address].item_indexMap[_item] = contracts[_address].items.length;
        contracts[_address].items.push(_item);
        contracts[_address].price[_item] = _price;
        contracts[_address].saleStartTime[_item] = _saleStartTime;
        emit AddItem(_address, _item, _price, _saleStartTime);
    }

    function addUpgradableItem(address _address, uint256 _item, uint256 _price, uint256 _saleStartTime,address upgrade_from_address, uint256 upgrade_from_item) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(contracts[_address].isInitilized, "contract dose not exits");
        require(contracts[_address].price[_item] == 0, "item already added");
        contracts[_address].item_indexMap[_item] = contracts[_address].items.length;
        contracts[_address].items.push(_item);
        contracts[_address].price[_item] = _price;
        contracts[_address].saleStartTime[_item] = _saleStartTime;
        contracts[_address].upgrade_from_address[_item] = upgrade_from_address;
        contracts[_address].upgrade_from_item[_item] = upgrade_from_item; 
        emit AddUpgradableItem(_address, _item, _price, _saleStartTime,upgrade_from_address, upgrade_from_item);
    }

    function removeItem(address _address, uint256 _item) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(contracts[_address].isInitilized, "contract dose not exits");
        require(contracts[_address].price[_item] != 0, "item dose not exist");
        if(contracts[_address].items[contracts[_address].items.length -1] != _item ){
            contracts[_address].items[contracts[_address].item_indexMap[_item]] = contracts[_address].items[contracts[_address].items.length -1];
            contracts[_address].items.pop();
            contracts[_address].item_indexMap[contracts[_address].items[contracts[_address].item_indexMap[_item]]] = contracts[_address].item_indexMap[_item];
        }else{
             contracts[_address].items.pop();
        }

        for(uint i = 0; i<contracts[_address].tokenIds.length; i++){
            if(_item < contracts[_address].vipTokenIdItemMap[contracts[_address].tokenIds[i]].length){
                contracts[_address].vipTokenIdItemMap[contracts[_address].tokenIds[i]][_item] = 0;
            }
        }
        
        delete contracts[_address].item_indexMap[_item];
        delete contracts[_address].price[_item];
        delete contracts[_address].saleStartTime[_item];
        delete contracts[_address].upgrade_from_address[_item];
        delete contracts[_address].upgrade_from_item[_item];
        emit RemoveItem(_address, _item);
    }

    function setItemPrice(address _address, uint256 _item, uint256 _price) external onlyRole(DEFAULT_ADMIN_ROLE){
        require(contracts[_address].isInitilized, "contract dose not exits");
        require(contracts[_address].price[_item] != 0, "item dose not exist");
        contracts[_address].price[_item] = _price;
        emit PriceUpdate(_address, _item, _price);
    }

    function buyItem(address _address, uint256 _item, uint256 tokenId) external {
        require(contracts[_address].isInitilized, "This NFT collection dose not exist");
        require(contracts[_address].price[_item] != 0, "This item dose not exist");
        require(contracts[_address].upgrade_from_address[_item] != address(0), "This item can only be upgraded");
        require(token.isInitilized, "Token Contract is not initilized");
        require(token.token_contract.allowance(_msgSender(), address(this)) >= contracts[_address].price[_item], "failed to get sufficent token for this transaction");
        verifyVIPSale(_address, _item, tokenId);
        token.token_contract.burnFrom(_msgSender(), contracts[_address].price[_item]);
        issueDetails.reciver_add[0] = _msgSender();
        issueDetails.reciver_item[0] = _item;
        contracts[_address].nft_contract.issueTokens( issueDetails.reciver_add, issueDetails.reciver_item);   
    }


    function upgradeItem(address _address, uint256 _item, uint256 tokenId,address upgrade_from_address, uint256 upgrade_from_item, uint256 upgrade_tokenID) external {
        require(contracts[_address].isInitilized, "This NFT collection dose not exist");
        require(contracts[_address].price[_item] != 0, "This item dose not exist");
        require((contracts[_address].upgrade_from_address[_item] == upgrade_from_address) && (contracts[_address].upgrade_from_item[_item] == upgrade_from_item), "upgrade info is provided is not correct");
        require(token.isInitilized, "Token Contract is not initilized");
        require(token.token_contract.allowance(_msgSender(), address(this)) >= contracts[_address].price[_item], "failed to get sufficent token for this transaction");
        verifyVIPSale(_address, _item, tokenId);
        contracts[upgrade_from_address].nft_contract.transferFrom(_msgSender(), address(0), upgrade_tokenID);
        token.token_contract.burnFrom(_msgSender(), contracts[_address].price[_item]);
        issueDetails.reciver_add[0] = _msgSender();
        issueDetails.reciver_item[0] = _item;
        contracts[_address].nft_contract.issueTokens( issueDetails.reciver_add, issueDetails.reciver_item);
    }

    function metaBuyItem(address _address, uint256 _item, uint256 tokenId,address buyerAddress, bytes32 sigR, bytes32 sigS, uint8 sigV) external {
        require(contracts[_address].isInitilized, "This NFT collection dose not exist");
        require(contracts[_address].price[_item] != 0, "This item dose not exist");
        require(contracts[_address].upgrade_from_address[_item] == address(0), "This item can only be upgraded");
        require(token.isInitilized, "Token Contract is not initilized");
        // require(token.token_contract.allowance(_msgSender(), address(this)) >= contracts[_address].price[_item], "failed to get sufficent token for this transaction");
        //require();
        bytes memory brunFunctionalSignature = abi.encodePacked(
            bytes4(keccak256("burn(uint256)")), // function signature
            abi.encode(
                contracts[_address].price[_item]
            )
        );
        verifyVIPSale(_address, _item, tokenId);
        token.token_contract.executeMetaTransaction(buyerAddress, brunFunctionalSignature, sigR, sigS, sigV);
        issueDetails.reciver_add[0] = buyerAddress;
        issueDetails.reciver_item[0] = _item;
        contracts[_address].nft_contract.issueTokens( issueDetails.reciver_add, issueDetails.reciver_item);
    }

    function verifyMetaUpgradeItem (address _address, uint256 _item, uint256 tokenId, uint256 upgrade_tokenID) internal {
        require(contracts[_address].isInitilized && contracts[contracts[_address].upgrade_from_address[_item]].isInitilized, "This NFT collection dose not exist");
        require(contracts[_address].price[_item] != 0, "This item dose not exist");
        //require((contracts[_address].upgrade_from_address[_item] == upgrade_from_address) && (contracts[_address].upgrade_from_item[_item] == upgrade_from_item), "upgrade info is provided is not correct");
        require(token.isInitilized, "Token Contract is not initilized");
        //require(token.token_contract.allowance(_msgSender(), address(this)) >= contracts[_address].price[_item], "failed to get sufficent token for this transaction");
        require(contracts[_address].upgrade_from_address[_item] != address(0), "This NFT is not upgradable");

        (uint256 itemId, ) = contracts[contracts[_address].upgrade_from_address[_item]].nft_contract.decodeTokenId(upgrade_tokenID);
        require(contracts[_address].upgrade_from_item[_item] == itemId, "Invalid Item id to upgrade from");
        verifyVIPSale(_address, _item, tokenId);
    }

    function metaUpgradeItem(address _address, uint256 _item, uint256 tokenId, uint256 upgrade_tokenID, address userAddress ,bytes32 nft_SigR, bytes32 nft_SigS, uint8 nft_SigV, bytes32 coinSigR, bytes32 coinSigS, uint8 coinSigV) external {        
        verifyMetaUpgradeItem(_address, _item, tokenId, upgrade_tokenID);
        bytes memory transferFromFunctionalSignature = abi.encodePacked(
            bytes4(keccak256("transferFrom(address,address,uint256)")), // function signature
            abi.encode(
                userAddress,
                NFT_BURN_ADDRESS,
                upgrade_tokenID
            )
        );
        contracts[contracts[_address].upgrade_from_address[_item]].nft_contract.executeMetaTransaction(userAddress, transferFromFunctionalSignature, nft_SigR, nft_SigS, nft_SigV);
        bytes memory brunFunctionalSignature = abi.encodePacked(
            bytes4(keccak256("burn(uint256)")), // function signature
            abi.encode(
                contracts[_address].price[_item]
            )
        );
        token.token_contract.executeMetaTransaction(userAddress, brunFunctionalSignature, coinSigR, coinSigS, coinSigV);
        issueDetails.reciver_add[0] = userAddress;
        issueDetails.reciver_item[0] = _item;
        contracts[_address].nft_contract.issueTokens(issueDetails.reciver_add, issueDetails.reciver_item);
       
    }

    function getVIPTokenInfo(address _address, uint256 tokenId) external view returns(uint256[] memory){
        return contracts[_address].vipTokenIdItemMap[tokenId];
    }

    function getIteamList(address _address) external view returns(uint256[] memory){
        return contracts[_address].items;
    }

    function getUpgradeFromAddress(address _address, uint256 _item) external view returns(address){
        return contracts[_address].upgrade_from_address[_item];
    }

    function getUpgradeFromItem(address _address, uint256 _item) external view returns(uint256){
        return contracts[_address].upgrade_from_item[_item];
    }

    function getIteamPrize(address _address, uint256 _item) view external returns(uint256){
        return contracts[_address].price[_item];
    }
    function getIteamIndexMap(address _address, uint256 _item) view external returns(uint256){
        return contracts[_address].item_indexMap[_item];
    }
    function getContractAddress() external view returns(address){
        return address(this);
    }
}