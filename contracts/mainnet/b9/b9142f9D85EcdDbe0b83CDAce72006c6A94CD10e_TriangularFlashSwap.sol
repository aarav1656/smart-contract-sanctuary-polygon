pragma solidity ^0.8.4;

import "hardhat/console.sol";
import "./libraries/SafeERC20.sol";
import "./interfaces/IUniswapV2Router01.sol";
import "./interfaces/IUniswapV2Router02.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/IERC20.sol";

contract TriangularFlashSwap {
    using SafeERC20 for IERC20;

    address private owner;

    struct Dex {
        address FACTORY;
        address ROUTER;
        uint8 protocol; // 2 UniswapV2, 3 UniswapV3, 1 Balancer
    }

    Dex[] private Dexes;

    constructor() {
        owner = msg.sender;
        Dexes.push(
            Dex(
                0x5757371414417b8C6CAad45bAeF941aBc7d3Ab32,
                0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff,
                2
            )
        ); // QUICKSWAP
        Dexes.push(
            Dex(
                0xc35DADB65012eC5796536bD9864eD8773aBc74C4,
                0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506,
                2
            )
        ); // SUSHISWAP

          Dexes.push(
            Dex(
                0xCf083Be4164828f00cAE704EC15a36D711491284,
                0xC0788A3aD43d79aa53B09c2EaCc313A787d1d607,
                2
            )
        ); // APESWAP

            Dexes.push(
            Dex(
                0xE7Fb3e833eFE5F9c441105EB65Ef8b261266423B,
                0xA102072A4C07F06EC3B4900FDC4C7B80b6c57429,
                2
            )
        ); // DFYN
    }

    address private constant WMATIC =
        0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270; //MATIC

    uint256 MAX_INT =
        0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff;
    event Arbitrage(uint256 loanIn, uint256 loanOut, address token);

    function startArbitrage(
        uint256 amountInMATIC,
        address[3] memory tokenOrder,
        uint8[3] memory dexOrder
    ) external {
        // RESET safeApprove to 0: SafeERC20: approve from non-zero to non-zero allowance
        address A = tokenOrder[0];
        address B = tokenOrder[1];
        address C = tokenOrder[2];
        approveABC(A,B,C,dexOrder);
      
        uint256 loanAmount = getAmountsInMATIC(A, amountInMATIC, dexOrder[0]);
        console.log("loanAmount", loanAmount);

        // TRANSFER HERE A from WALLET
        IERC20(A).transferFrom(msg.sender, address(this), loanAmount);

        //////////////////
        // DO ARBITRAGE
        uint256 amountOut;
        amountOut = placeTrade(A, B, loanAmount, address(this), dexOrder[0]); // (A-B)
        console.log("FlashSwap.sol A to B  = ", amountOut);

        amountOut = placeTrade(B, C, amountOut, address(this), dexOrder[1]); // (B-C)
        console.log("FlashSwap.sol B to C  = ", amountOut);

        amountOut = placeTrade(C, A, amountOut, address(this), dexOrder[2]); // (C-A)
        console.log("FlashSwap.sol C to A = ", amountOut);


        // Check Profitability
        bool profCheck = checkProfitability(loanAmount, amountOut);
        require(profCheck, "Arbitrage not profitable"); //////////////////////////////////////////////////////////////////////////

        // PAY YOUSELF
        IERC20(A).transfer(msg.sender, amountOut);
       

        emit Arbitrage(loanAmount, amountOut, A);
    }

    // PLACE A TRADE
    // Executed placing a trade
    function placeTrade(
        address _fromToken,
        address _toToken,
        uint256 _amountIn,
        address recipient,
        uint8 dexId
    ) private returns (uint256) {
        address pair = IUniswapV2Factory(Dexes[dexId].FACTORY).getPair(
            _fromToken,
            _toToken
        );
        require(pair != address(0), "Pool does not exist");

        // Calculate Amount Out from One token to Another
        address[] memory path = new address[](2);
        path[0] = _fromToken;
        path[1] = _toToken;

        uint256 amountRequired = IUniswapV2Router01(Dexes[dexId].ROUTER)
            .getAmountsOut(_amountIn, path)[1];

        // console.log("amountRequired", amountRequired);

        // Perform Arbitrage - Swap for another token
        uint256 amountReceived = IUniswapV2Router01(Dexes[dexId].ROUTER)
            .swapExactTokensForTokens(
                _amountIn, // amountIn
                amountRequired, // amountOutMin
                path, // path
                recipient, // address to
                 block.timestamp // deadline
            )[1];

        require(amountReceived > 0, "Aborted Tx: Trade returned zero");

        return amountReceived;
    }

    // ////////////////////
    function getAmountsInMATIC(
        address X,
        uint256 amountOut,
        uint8 dexId
    ) private view returns (uint256) {
        uint256 AmountsIn;
        if (X != WMATIC) {
            address[] memory path = new address[](2);
            path[0] = WMATIC;
            path[1] = X;

            AmountsIn = IUniswapV2Router02(Dexes[dexId].ROUTER).getAmountsOut(
                amountOut,
                path
            )[1];
        } else {
            AmountsIn = amountOut;
        }

        return AmountsIn;
    }

    function simulate_swap_tokens(
        address X,
        address Y,
        uint256 amountIn
    ) private view returns (uint256, uint8) {
        address[] memory path = new address[](2);
        path[0] = X;
        path[1] = Y;

        uint256 AmountsOut;
        uint256 MaxAmountsOut = 0;
        uint8 dexId;
        for (uint8 i = 0; i < Dexes.length; i++) {
            address pair = IUniswapV2Factory(Dexes[i].FACTORY).getPair(X,Y);
            if(pair != address(0)) {
        console.log("Amounts pair",pair);
            AmountsOut = IUniswapV2Router02(Dexes[i].ROUTER).getAmountsOut(
                amountIn,
                path
            )[1];

            if (AmountsOut > MaxAmountsOut) {
                MaxAmountsOut = AmountsOut;
                dexId = i;
            }}
        }

        return (MaxAmountsOut, dexId);
    }

    function try_arbitrage(
        address J,
        address K,
        address L,
        uint256 amountInMATIC
    ) private view returns (uint256, uint8[3] memory) {
        uint256 loanAmount = getAmountsInMATIC(J, amountInMATIC, 0);
        uint256 J_Balance = IERC20(J).balanceOf(msg.sender);
        console.log("J_Balance", J_Balance);

        uint8[3] memory dexOrder;
        if (J_Balance > 0) {
            console.log(
                "FlashSwap (loanAmount,J_Balance, J)=",
                loanAmount,
                J_Balance,
                J
            );
            uint256 amountOut;
            (amountOut, dexOrder[0]) = simulate_swap_tokens(J, K, loanAmount);
            console.log("FlashSwap amountOut1", amountOut);

            (amountOut, dexOrder[1]) = simulate_swap_tokens(K, L, amountOut);
            console.log("FlashSwap amountOut2", amountOut);

            (amountOut, dexOrder[2]) = simulate_swap_tokens(L, J, amountOut);
            console.log("FlashSwap amountOut3", amountOut);

            return ((amountOut * 100000) / loanAmount, dexOrder);
        } else {
            return (0, dexOrder);
        }
    }

    function Calculate_Arbitrage(
        address J,
        address K,
        address L,
        uint256 amountInMATIC
    ) external view returns (uint256, address[3] memory, uint8[3] memory) {
        uint256 ratio;
        uint256 best_ratio;
        address[3] memory best_tokenOrder;
        uint8[3] memory dexOrder;
        uint8[3] memory best_dexOrder;

        console.log("FlashSwap amountInMATIC", amountInMATIC);
        // J K L
        (ratio, dexOrder) = try_arbitrage(J, K, L, amountInMATIC);
        best_ratio = ratio;
        best_tokenOrder = [J, K, L];
        best_dexOrder = dexOrder;

        // J L K
        (ratio, dexOrder) = try_arbitrage(J, L, K, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [J, L, K];
            best_dexOrder = dexOrder;
        }
        //  L K J
        (ratio, dexOrder) = try_arbitrage(L, K, J, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [L, K, J];
            best_dexOrder = dexOrder;
        }
        //  K J L
        (ratio, dexOrder) = try_arbitrage(K, J, L, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [K, J, L];
            best_dexOrder = dexOrder;
        }
        // L J K
        (ratio, dexOrder) = try_arbitrage(L, J, K, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [L, J, K];
            best_dexOrder = dexOrder;
        }
        // K L J
        (ratio, dexOrder) = try_arbitrage(K, L, J, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [K, L, J];
            best_dexOrder = dexOrder;
        }

        console.log(
            "FlashSwap best_tokenOrder",
            best_tokenOrder[0],
            best_tokenOrder[1],
            best_tokenOrder[2]
        );
        console.log(
            "FlashSwap best_dexOrder",
            best_dexOrder[0],
            best_dexOrder[1],
            best_dexOrder[2]
        );
        console.log("FlashSwap ratio", best_ratio);

        return (best_ratio, best_tokenOrder, best_dexOrder);
    }

    // GET CONTRACT BALANCE
    // Allows public view of balance for contract
    function getBalanceOfToken(
        address _addressToken
    ) public view returns (uint256) {
        return IERC20(_addressToken).balanceOf(address(this));
    }

    // CHECK PROFITABILITY
    // Checks whether > output > input
    function checkProfitability(
        uint256 _input,
        uint256 _output
    ) private pure returns (bool) {
        return _output > _input;
    }

    function testFunction() public returns (address) {
        emit Arbitrage(1, 1, WMATIC);
        console.log("FlashSwap.sol testFunction");
        return WMATIC;
    }

    function approveABC(address _A,address _B,address _C,uint8[3] memory _dexOrder) private{
        
        uint256 allowance = IERC20(_A).allowance(address(this),address(Dexes[_dexOrder[0]].ROUTER));
        if(allowance == 0){
        IERC20(_A).safeApprove(address(Dexes[_dexOrder[0]].ROUTER), MAX_INT);
        console.log('allowance A');
        }
        
        allowance = IERC20(_B).allowance(address(this),address(Dexes[_dexOrder[0]].ROUTER));
        if(allowance == 0){
        IERC20(_B).safeApprove(address(Dexes[_dexOrder[1]].ROUTER), MAX_INT);
        console.log('allowance B');
        }

        allowance = IERC20(_C).allowance(address(this),address(Dexes[_dexOrder[0]].ROUTER));
        if(allowance == 0){
        IERC20(_C).safeApprove(address(Dexes[_dexOrder[2]].ROUTER), MAX_INT);
        console.log('allowance C');
    }}
}

