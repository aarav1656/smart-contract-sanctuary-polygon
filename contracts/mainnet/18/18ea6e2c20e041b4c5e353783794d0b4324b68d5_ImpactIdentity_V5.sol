pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";

import "./IR_NFT.sol";
import "./IR_SBT.sol";
import "./IReturnPool.sol";
import "./IReturnPoolFactory.sol";

contract ReturnPoolManager is Initializable, AccessControlUpgradeable, UUPSUpgradeable {
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap;

    // Goleri USDT 0xC2C527C0CACF457746Bd31B2a698Fe89de2b6d49, aUSDT 0x73258E6fb96ecAc8a979826d503B45803a382d68
    // Goleri USDC 0xA2025B15a1757311bfD68cb14eaeFCc237AF5b43, aUSDC 0x1Ee669290939f8a8864497Af3BC83728715265FF
    // Goleri DAI 0xDF1742fE5b0bFc12331D8EAec6b478DfDbD31464, aDAI   0x310839bE20Fc6a8A89f33A59C7D5fC651365068f
    // AAVE Pool 0x368EedF3f56ad10b9bC57eed4Dac65B26Bb667f6
    // Goleri SUSHI ROUTER 0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
    // Mainnet sushi router 0x1b02dA8Cb0d097eB8D57A175b88c7D8b47997506
    // Mainnet USDC 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174
    // Mainnet aUSDC = 0x625E7708f30cA75bfd92586e17077590C60eb4cD
    // Mainnet NCT = 0xD838290e877E0188a4A44700463419ED96c16107
    // Offset Token (Testing): Goerli Link: 0x326C977E6efc84E512bB9C30f76E30c160eD06FB
    // New offset token (Testing): Goerli Fake Dai: 0xdc31Ee1784292379Fbb2964b3B9C4124D8F89C60
    // Goerli WETH: 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6

    // Roles Info
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    struct PoolInfo {
        address poolAddress;
        address stakedToken;
        address offsetToken;
    }

    // Data Mappings
    // User --> SBT
    // Contains length, allows iteration over all indexs, get key,value by index, get value by key
    EnumerableMapUpgradeable.AddressToUintMap private userSBT; // map for iterating over all SBT holders

    // Return Pools
    EnumerableMapUpgradeable.AddressToUintMap private returnPools;
    mapping(address => mapping(address => address)) public activePools; // (stakeToken ==> offsetToken ==> ReturnPool) (if it exists);
    uint public poolIndex;


    mapping(uint => address) public sbtToUser; // will allow for user lookup via sbt tokenID

    // SBT --> NFT(s)
    mapping(uint => uint[]) public sbtToNFT;
    mapping(uint => uint) public nftToSBT;

    // NFT(s) --> Return Pools
    mapping(uint => address) public nftToReturnPool;

    // User --> Return Pool --> NFT ID
    mapping(address => mapping(address => uint)) public userToReturnPool;

    IR_NFT public RETURN_NFT;
    IR_SBT public RETURN_SBT;

    address public aavePoolManager;
    address public returnPoolFactory;

    event FundNFT(uint amount, uint nftID, address stakedToken, address tco2, address returnPool);
    event WithdrawNFTFunds(uint amount, uint nftID, address stakedToken, address returnPool, address user);
    event MintNFT(uint nftID, uint sbtID, uint amount, address stakedToken, address tco2, address returnPool, address user);
    event MintSBT(uint sbtID, address user);
    event PoolAdded(address stakedToken, address tco2, address returnPool);
    event HistoricalOffset(uint nftID, uint amount, address pool, address tco2);

    modifier nonZeroPurchase(uint _amount) {
        require(_amount > 0);
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(address _NFT, address _SBT, address _factory) initializer public {
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MANAGER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);

        RETURN_NFT = IR_NFT(_NFT);
        RETURN_SBT = IR_SBT(_SBT);
        aavePoolManager = 0x794a61358D6845594F94dc1DB02A252b5b4814aD; // Testnet 0x368EedF3f56ad10b9bC57eed4Dac65B26Bb667f6; Mainnet 0x794a61358D6845594F94dc1DB02A252b5b4814aD
        returnPoolFactory = _factory;
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    onlyRole(UPGRADER_ROLE)
    override
    {}

    function supportsInterface(bytes4 interfaceId)
    public
    view
    override(AccessControlUpgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    // @dev: Allows the admin to add a new return pool
    // @param stakedToken: the token that will be staked in the return pool
    // @param offsetToken: offset token address, (0 address for community pool)
    // @param aaveToken: address of received aToken
    // @param tco2: address of the tco2 token which will be redeemed
    // @return returnPool: address of the return pool
    // TODO: Enable creation of same pool if emergency mode has been toggled
    function addReturnPool(address stakedToken, address aaveToken, address offsetToken, address tco2, uint _decimals) external onlyRole(MANAGER_ROLE) returns(address) {
        require(activePools[stakedToken][tco2] == address(0));
        address newPool = IReturnPoolFactory(returnPoolFactory).createNewReturnPool(stakedToken, aaveToken, offsetToken, tco2, aavePoolManager, _decimals, address(RETURN_NFT));
        ReturnPool(newPool).setRole(DEFAULT_ADMIN_ROLE, msg.sender);
        ReturnPool(newPool).setRole(MANAGER_ROLE, msg.sender);
        setReturnPool(newPool, poolIndex);
        poolIndex += 1;
        activePools[stakedToken][tco2] = newPool;
        emit PoolAdded(stakedToken, tco2, newPool);
        return(newPool);
    }

    // @dev: Account deposit funds into a pool for the first time
    //       If the user has no SBT, a new SBT is minted
    //       NFT is minted and funds deposited into the pool
    // @param uint amount: amount of stakedToken that will be deposited
    // @param address stakedToken: address of the token deposited in the pool
    // @param address offsetToken: address of the offset token earned
    // TODO: Stake Amount Cap
    function buyNFT(uint amount, address stakedToken, address tco2) external nonZeroPurchase(amount) {
        ReturnPool pool = ReturnPool(activePools[stakedToken][tco2]);
        require(address(pool) != address(0));
        uint nftID = userToReturnPool[address(pool)][msg.sender];
        if (nftID == 0) { // user does not have an nft minted for the pool
            require(IERC20(stakedToken).transferFrom(msg.sender, address(pool), amount));

            (bool exists, uint id) = tryGetUserSBT(msg.sender);
            if (!exists) { // user has no sbt
                id = _mintSBT(msg.sender);
            }

            uint tokenID = _mintNFT(msg.sender, tco2);

            pool.stakeNFT(tokenID, amount);
            userToReturnPool[address(pool)][msg.sender] = tokenID;
            nftToReturnPool[tokenID] = address(pool);

            emit MintNFT(tokenID, id, amount, stakedToken, tco2, address(pool), msg.sender);

        } else { // user has an nft minted for the pool
            fundNFT(nftID, amount);
        }
    }

    // @dev: Account add additional funds to a pool
    // @param: uint nftID: ID of the NFT whose stake will be updated
    // @param: uint amount: amount of stakedToken that will be deposited
    function fundNFT(uint nftID, uint amount) public nonZeroPurchase(amount){
        require(RETURN_NFT.ownerOf(nftID) == msg.sender);
        ReturnPool pool = ReturnPool(nftToReturnPool[nftID]);
        require(IERC20(pool.STAKED_TOKEN()).transferFrom(msg.sender, address(pool), amount));
        pool.addFunds(nftID, amount);

        emit FundNFT(amount, nftID, pool.STAKED_TOKEN(), pool.OFFSET_TOKEN(), address(pool));
    }

    // @dev: Account withdraw funds from a pool
    // @param: uint nftID: ID of the NFT whose stake will be updated
    // @param: uint amount: amount of stakedToken that will be withdrawn
    function withdrawFunds(uint nftID, uint amount) external nonZeroPurchase(amount) {
        ReturnPool pool = ReturnPool(nftToReturnPool[nftID]);
        address user = RETURN_NFT.ownerOf(nftID);
        pool.withdrawStake(nftID, amount, user);
        emit WithdrawNFTFunds(amount, nftID, pool.STAKED_TOKEN(), address(pool), msg.sender);
    }

    // @dev: Account perform one time offset on an nft, no NFT param allows users to immediately OTO
    // @param: address paymentToken: token that will be used to swap for offset token
    // @param: address TCO2: specific TCO2 to be redeemed and retired
    // @param: uint amount: amount of payment token to swap for TCO2
    function historicalOffset(address paymentToken, address tco2, uint amount) external nonZeroPurchase(amount) {
        address poolAddress = activePools[paymentToken][tco2];
        (bool exists, uint id) = tryGetUserSBT(msg.sender);
        if (!exists) { // user has no sbt
            id = _mintSBT(msg.sender);
        }
        uint nftID = userToReturnPool[poolAddress][msg.sender];
        if (nftID == 0) { // user does not have an nft minted for the pool
            nftID = _mintNFT(msg.sender, tco2);
            nftToReturnPool[nftID] = poolAddress;
            userToReturnPool[poolAddress][msg.sender] = nftID;
        }
        _historicalOffset(nftID, amount, poolAddress);
    }

    // @dev: Account perform one time offset on an nft
    // @param: uint nftID: ID of the NFT which will be associated with the offset
    // @param: uint amount: amount of stakedToken that will be used to purchase the offset
    // @param: address poolAddress: address of the pool that will be used to purchase the offset
    function _historicalOffset(uint nftID, uint amount, address poolAddress) internal {
        require(RETURN_NFT.ownerOf(nftID) == msg.sender);
        ReturnPool pool = ReturnPool(poolAddress);
        require(IERC20(pool.STAKED_TOKEN()).transferFrom(msg.sender, address(pool), amount));
        pool.offsetWalletHistory(nftID, amount, msg.sender);
        emit HistoricalOffset(nftID, amount, address(pool), pool.OFFSET_TOKEN());
    }

    // @dev: mint new nft to user
    // @param: address user: address of the user
    function _mintNFT(address user, address tco2) internal returns (uint) {
        uint sbtID = getUserSBT(user); // normal get, should revert if user doesnt have sbt
        uint nftID = RETURN_NFT.safeMint(user, tco2);
        nftToSBT[nftID] = sbtID;
        sbtToNFT[sbtID].push(nftID);
        return nftID;
    }

    // @dev: mint new sbt to user
    // @param: address user: address of the user
    function _mintSBT(address user) internal returns (uint) {
        uint sbtID = RETURN_SBT.safeMint(user);
        _setUserSBT(user, sbtID);
        sbtToUser[sbtID] = user;
        emit MintSBT(sbtID, user);
        return sbtID;
    }


    // Enumerable Map Getters/Setters
    // Set key, value pair
    function _setUserSBT(address user, uint tokenId) internal {
        userSBT.set(user, tokenId);
    }

    function setUserSBT(address user, uint tokenId) external onlyRole(MANAGER_ROLE) {
        _setUserSBT(user, tokenId);
    }

    // Remove key, value pair
    function _removeUserSBT(address user) internal {
        userSBT.remove(user);
    }

    function removeUserSBT(address user) external onlyRole(MANAGER_ROLE) {
        _removeUserSBT(user);
    }

    function setSBTToUser(uint tokenId, address user) external onlyRole(MANAGER_ROLE) {
        sbtToUser[tokenId] = user;
    }

    function setSBTToNFT(uint tokenId, uint[] calldata nftIds) external onlyRole(MANAGER_ROLE) {
        sbtToNFT[tokenId] = nftIds;
    }

    function getSBTToNFT(uint tokenId) external view returns (uint[] memory) {
        return sbtToNFT[tokenId];
    }

    function setNFTToSBT(uint tokenId, uint sbtId) external onlyRole(MANAGER_ROLE) {
        nftToSBT[tokenId] = sbtId;
    }

    function setNFTToReturnPool(uint tokenId, address pool) external onlyRole(MANAGER_ROLE) {
        nftToReturnPool[tokenId] = pool;
    }

    function setUserToReturnPool(address pool, address user, uint nftId) external onlyRole(MANAGER_ROLE) {
        userToReturnPool[user][pool] = nftId;
    }

    // key, value pair at index
    function getUserSBTAtIndex(uint index) external view returns(address, uint) {
        return userSBT.at(index);
    }

    // Get value at key, Does not revert is user does not exist
    function tryGetUserSBT(address user) public view returns(bool, uint) {
        return userSBT.tryGet(user);
    }

    // Get value at key, reverts of key does not exist
    function getUserSBT(address user) public view returns(uint) {
        return userSBT.get(user);
    }

    // Return Pools
    // Enumerable Map Getters/Setters
    // Set key, value pair
    function setReturnPool(address pool, uint poolId) internal {
        returnPools.set(pool, poolId);
    }

    // key, value pair at index
    function getReturnPoolAtIndex(uint index) external view returns(address, uint) {
        return returnPools.at(index);
    }

    // Get value at key, Does not revert is user does not exist
    function tryGetReturnPool(address pool) external view returns(bool, uint) {
        return returnPools.tryGet(pool);
    }

    // Get value at key, reverts of key does not exist
    function getReturnPool(address pool) external view returns(uint) {
        return returnPools.get(pool);
    }

    // Pool Getters
    // @dev: Get all staked NFTs for a user
    function getAllUserNFTs(address user) external view returns (uint[] memory) {
        (bool exists, uint sbtID) = tryGetUserSBT(user);
        if (!exists) {
            return new uint[](0);
        }
        return sbtToNFT[sbtID];
    }

    // @dev: Get all available return pools
    function getAllReturnPools() external view returns (PoolInfo[] memory) {
        PoolInfo[] memory pools = new PoolInfo[](returnPools.length());
        uint len = returnPools.length();
        for (uint i = 0; i < len; i++) {
            (address pool, ) = returnPools.at(i);
            PoolInfo memory info;
            info.poolAddress = pool;
            info.stakedToken = ReturnPool(pool).STAKED_TOKEN();
            info.offsetToken = ReturnPool(pool).TCO2();
            pools[i] = info;
        }
        return pools;
    }

    // @dev: Get total offset for an nft
    function getNFTOffsetAmount(uint nftID) public view returns(uint total, uint staking, uint oneTime) {
        address pool = nftToReturnPool[nftID];
        if (pool != address(0)) {
            (total, staking, oneTime) = ReturnPool(pool).getTotalOffset(nftID);
        }
        return (total, staking, oneTime);
    }

    // @dev: Get total offset for an sbt
    function getTotalSBTOffset(uint sbtID) public view returns (uint total, uint staking, uint oneTime) {
        uint[] memory nfts = sbtToNFT[sbtID];
        uint len = nfts.length;
        for (uint i = 0; i < len; i++) {
            (uint t, uint s, uint o) = getNFTOffsetAmount(nfts[i]);
            total += t;
            staking += s;
            oneTime += o;
        }
        return (total, staking, oneTime);
    }

    // @dev: Get total offset for a user
    function getTotalUserOffset(address user) external view returns (uint, uint, uint) {
        (bool exists, uint sbtID) = tryGetUserSBT(user);
        if (!exists) {
            return (0,0,0);
        }
        return getTotalSBTOffset(sbtID);
    }

    // @dev: Get total deposit and yield for an nft
    function getNFTAmounts(uint nftID) external view returns(uint, uint) {
        address pool = nftToReturnPool[nftID];
        if (pool != address(0)) {
            return ReturnPool(pool).getTotals(nftID);
        }
        else {
            return (0,0);
        }
    }

    function getTotalUserAmounts(address user) external view returns (uint,uint,uint) {
        (bool exists, uint sbtID) = tryGetUserSBT(user);
        if (!exists) {
            return (0,0,0);
        }
        return getTotalSBTOffset(sbtID);
    }

    function getTotalStakedInPool(address pool) external view returns (uint) {
        return ReturnPool(pool).totalStakedAmount();
    }

    function getTotalOffsetInPool(address pool) external view returns (uint) {
        return ReturnPool(pool).totalOffset();
    }

    function getPoolNFTInfo(uint nftId) external view returns (uint amount, uint timestamp, uint offsetEarn, uint oneTimeOffset, uint offsetDecimals, uint stakedDecimals, address offsetToken, address stakedToken) {
        address poolAddress = nftToReturnPool[nftId];
        if (poolAddress != address(0)) {
            ReturnPool pool = ReturnPool(poolAddress);
            (amount, timestamp, , , , offsetEarn, oneTimeOffset, offsetToken, stakedToken) = pool.nftStakeInfo(nftId);
            offsetDecimals = 18;
            stakedDecimals = pool.TOKEN_MULTIPLIER();
        }
        else {
            return (0, 0, 0, 0, 0, 0, address(0), address(0));
        }
    }

    // Beta Function
    function userEmergencyUnstake(uint nftID) external {
        address poolAddress = nftToReturnPool[nftID];
        require(poolAddress != address(0), "NFT not staked");
        ReturnPool pool = ReturnPool(poolAddress);
        pool.userEmergencyUnstake(nftID, msg.sender);
    }
}

