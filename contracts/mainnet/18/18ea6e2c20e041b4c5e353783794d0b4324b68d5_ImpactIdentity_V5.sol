pragma solidity ^0.8.4;

import './R_SBT_V4.sol';
import './IPolygonReturnSkin.sol';


contract ImpactIdentity_V5 is ImpactIdentity_V4 {

    uint[] public partnershipIDs;

    function getTotalSBTOffsetPartnership(uint sbtID) public view returns (uint total, uint staking, uint oneTime, uint partnership) {
        ReturnPoolManager poolManager = ReturnPoolManager(PoolManagerAddress);
        (total, staking, oneTime) = poolManager.getTotalSBTOffset(sbtID);

        for (uint i = 0; i < partnershipIDs.length; i++) {
            if (ERC721Upgradeable(skins[partnershipIDs[i]]).balanceOf(ownerOf(sbtID)) > 0) {
                uint curPartnership = IPolygonReturnSkin(skins[partnershipIDs[i]]).getCalculatedOffset(sbtID);
                total += curPartnership;
                partnership += curPartnership;
            }
        }
        return (total, staking, oneTime, partnership);
    }

    function calculateTierPartnership(uint sbtID) public view returns (uint) {
        (uint totalOffset, , ,) = getTotalSBTOffsetPartnership(sbtID);

        for (uint i = 0; i < tiers.length; i++) {
            if (totalOffset < tiers[i]) {
                return i;
            }
        }
        return tiers[tiers.length - 1];
    }

    function setPartnershipID(uint id) external onlyRole(MINTER_ROLE) {
        partnershipIDs.push(id);
    }
}

