pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";

import "./ReturnPoolManager.sol";


contract ImpactIdentity is Initializable, ERC721Upgradeable, AccessControlUpgradeable, UUPSUpgradeable {
    using CountersUpgradeable for CountersUpgradeable.Counter;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    CountersUpgradeable.Counter private _tokenIdCounter;
    address public PoolManagerAddress;

    mapping(uint => mapping(uint => string)) public tierURI; // skin ID --> tier --> URI
    mapping(uint => uint) public sbtToSkin; // sbtID => skinID (0 for default)
    mapping(uint => address) public skins; // skinID => skinNFTAddress (0 for default)
    uint[] public tiers;


    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __ERC721_init("Return Climate Passport", "RSBT");
        __AccessControl_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        tiers.push(5 * (10**18));
        tiers.push(25 * (10**18));
        tiers.push(50 * (10**18));
        tiers.push(100 * (10**18));
        tiers.push(250 * (10**18));
        tiers.push(500 * (10**18));
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://ipfs.io/ipfs/QmYrV99fVyyw9qYcxsUg9uhtBSn6hFsEh3ZaTbqxKqbr4j/Tier0.json";
    }

    function setPoolManagerAddress(address _PoolManagerAddress) public onlyRole(MINTER_ROLE) {
        PoolManagerAddress = _PoolManagerAddress;
    }

    function getTotalSBTOffset(uint sbtID) external view returns(uint, uint, uint) {
        ReturnPoolManager poolManager = ReturnPoolManager(PoolManagerAddress);
        return poolManager.getTotalSBTOffset(sbtID);
    }

    function setSkin(uint skinID, address skinAddress) public onlyRole(MINTER_ROLE) {
        skins[skinID] = skinAddress;
    }

    function setAllTiers(string[] memory _tierURI, uint _skinID) public onlyRole(MINTER_ROLE) {
        for (uint i = 0; i < _tierURI.length; i++) {
            tierURI[_skinID][i] = _tierURI[i];
        }
    }

    function setTierURI(uint tier, string memory uri, uint _skinID) public onlyRole(MINTER_ROLE) {
        tierURI[_skinID][tier] = uri;
    }

    function setTiers(uint[] memory _tiers) public onlyRole(MINTER_ROLE) {
        tiers = _tiers;
    }

    function calculateTier(uint sbtID) public view returns (uint) {
        ReturnPoolManager poolManager = ReturnPoolManager(PoolManagerAddress);
        (uint totalOffset, ,) = poolManager.getTotalSBTOffset(sbtID);

        for (uint i = 0; i < tiers.length; i++) {
            if (totalOffset < tiers[i]) {
                return i;
            }
        }
        return tiers[tiers.length - 1];
    }

    function tokenURI(uint256 tokenId) public view override(ERC721Upgradeable) returns (string memory){
        uint tier = calculateTier(tokenId);
        uint skinID = sbtToSkin[tokenId];
        return tierURI[skinID][tier];
    }

    function mutate(uint sbtID, uint skinTokenID, uint skinID) external {
        require(msg.sender == ownerOf(sbtID)); // sbt owner
        require(skins[skinID] != address(0)); // skin exists
        require(msg.sender == ERC721Upgradeable(skins[skinID]).ownerOf(skinTokenID)); // skin owner
        sbtToSkin[sbtID] = skinID;
    }

    function revertMutate(uint sbtID) external {
        require(msg.sender == ownerOf(sbtID)); // sbt owner
        sbtToSkin[sbtID] = 0;
    }

    function safeMint(address to) public onlyRole(MINTER_ROLE) returns (uint256) {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        //_setTokenURI(tokenId, tokenURIs[tco2]);
        return tokenId;
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    onlyRole(UPGRADER_ROLE)
    override
    {}

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId) internal override(ERC721Upgradeable) {
        super._burn(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721Upgradeable, AccessControlUpgradeable) returns (bool){
        return super.supportsInterface(interfaceId);
    }

    function noxfr() internal pure {
        require(false, "ImpactIdentity cannot be transfered, try burning this token + minting a new one.");
    }

    function transferFrom(address from, address to, uint256 tokenId) public override {
        noxfr();
    }

    function safeTransferFrom(address from, address to, uint256 tokenId) public override {
        noxfr();
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory _data) public override {
        noxfr();
    }


}

