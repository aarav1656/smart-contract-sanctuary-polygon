abstract contract ERC20_ML_withTax_Timeline is
    ERC20_ML_withTax
{
    //========================
    // STRUCTS
    //========================

    struct TaxTimeline
    {
        uint256 beforeTimestamp;
        uint256 tier;
    }

    //========================
    // ATTRIBUTES
    //========================

    TaxTimeline[] public taxTimeline;

    //========================
    // CONFIG FUNCTIONS
    //======================== 

    function setTaxTimeline(TaxTimeline[] memory _timeline) internal
    {
        //check
        requireAdmin(true);
        require(taxTimeline.length == 0, "Timeline already set");

        //check order & push
        uint256 lastTimestamp = 0;
        for (uint256 n = 0; n < _timeline.length; n++)
        {
            require(lastTimestamp < _timeline[n].beforeTimestamp, "Invalid order");
            taxTimeline.push(_timeline[n]);
        }
    }

    //========================
    // HELPER FUNCTIONS
    //========================  

    function getTaxTier(address, address) internal virtual override view returns (uint256)
    {
        //get timeline
        for (uint256 n = 0; n < taxTimeline.length; n++)
        {
            if (block.timestamp <= taxTimeline[n].beforeTimestamp)
            {
                return taxTimeline[n].tier;
            }
        }

        return 0;
    }
}
