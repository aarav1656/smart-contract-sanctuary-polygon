contract ML_RoleManager is
    AccessControlEnumerable
{
    //========================
    // CONSTANTS
    //========================
    
    bytes32 public constant ROLE_SUPER_ADMIN = keccak256("ROLE_SUPER_ADMIN");
    bytes32 public constant ROLE_ADMIN = keccak256("ROLE_ADMIN");  

    //========================
    // ATTRIBUTES
    //========================

    address private superAdmin; //super admin

    //========================
    // EVENTS
    //========================

    event SuperAdminTransferred(address _old, address _new);

    //========================
    // CREATE
    //========================

    constructor()
    {
        //init access control        
        _setRoleAdmin(ROLE_ADMIN, ROLE_SUPER_ADMIN);

        //setup roles
        _setupRole(ROLE_SUPER_ADMIN, msg.sender);
        superAdmin = msg.sender;
    }

    //========================
    // INFO FUNCTIONS
    //======================== 

    function getSuperAdmin() public view returns (address)
    {
        return superAdmin;
    }

    //========================
    // CONFIG FUNCTIONS
    //========================  

    function transferSuperAdmin(address _newSuperAdmin) public
    {
        require(hasRole(ROLE_SUPER_ADMIN, msg.sender), "User is not SuperAdmin");
        require(_newSuperAdmin != superAdmin, "User already is SuperAdmin");
        _setupRole(ROLE_SUPER_ADMIN, _newSuperAdmin);
        renounceRole(ROLE_SUPER_ADMIN, superAdmin);
        superAdmin = _newSuperAdmin; 
        emit SuperAdminTransferred(msg.sender, superAdmin);  
    }  

    //========================
    // SECURITY FUNCTIONS
    //========================

    function requireRole(bytes32 _role, bool _allowTxOrigin) internal view returns (bool)
    {
        return (hasRole(_role, msg.sender)
            || (_allowTxOrigin
                && hasRole(_role, tx.origin))
        );
    }

    function isAdmin(bool _allowTxOrigin) internal view returns (bool)
    {
        return requireRole(ROLE_SUPER_ADMIN, _allowTxOrigin)
            || requireRole(ROLE_ADMIN, _allowTxOrigin);
    }

    function requireAdmin(bool _allowTxOrigin) internal view
    {
        require(
            isAdmin(_allowTxOrigin),
            "User is not Admin");
    }
}
