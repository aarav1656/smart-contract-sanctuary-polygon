    // Executed placing a trade
    function placeTrade(
        address _fromToken,
        address _toToken,
        uint256 _amountIn,
        address recipient,
        address _Factory,
        address _Router,
        uint8 _protocol
    ) internal returns (uint256) {
        Dex memory _dex = Dex(_Factory, _Router, _protocol);
        uint256 amountReceived;
        if (_dex.protocol == 2) {
            amountReceived = placeTradeV2(
                _fromToken,
                _toToken,
                _amountIn,
                recipient,
                _dex
            );
        } else if (_dex.protocol == 3) {
            amountReceived = placeTradeV3(
                _fromToken,
                _toToken,
                _amountIn,
                recipient,
                _dex
            );
        } else if (_dex.protocol == 1) {
            // CURVE CRV SWAP
            amountReceived = 0;
        }

        return amountReceived;
    }

    // PLACE A TRADE
    // Executed placing a trade
    function placeTradeV2(
        address _fromToken,
        address _toToken,
        uint256 _amountIn,
        address recipient,
        Dex memory _dex
    ) private returns (uint256) {
        address pair = IUniswapV2Factory(_dex.FACTORY).getPair(
            _fromToken,
            _toToken
        );
        require(pair != address(0), "Pool does not exist");

        // Calculate Amount Out from One token to Another
        address[] memory path = new address[](2);
        path[0] = _fromToken;
        path[1] = _toToken;

        uint256 amountRequired = IUniswapV2Router01(_dex.ROUTER).getAmountsOut(
            _amountIn,
            path
        )[1];

        uint256 amountReceived = IUniswapV2Router01(_dex.ROUTER)
            .swapExactTokensForTokens(
                _amountIn, // amountIn
                amountRequired, // amountOutMin
                path, // path
                recipient, // address to
                block.timestamp // deadline
            )[1];
        console.log("IUniswapV2Router01 amounts.length");

        console.log(amountReceived);

        require(amountReceived > 0, "Aborted Tx: Trade returned zero");

        return amountReceived;
    }

    function placeTradeV3(
        address _fromToken,
        address _toToken,
        uint256 _amountIn,
        address recipient,
        Dex memory _dex
    ) private returns (uint256) {
        uint256 amountOut = 0;
        (address pool, uint24 fee) = findBestPoolByFee(
            _fromToken,
            _toToken,
            _dex.FACTORY
        );
        require(pool != address(0), "Pool does not exist placeTradeV3");

        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams(
                _fromToken,
                _toToken,
                fee,
                recipient,
                block.timestamp,
                _amountIn,
                0,
                0
            );
        amountOut = ISwapRouter(_dex.ROUTER).exactInputSingle(params);
        console.log("  ISwapRouter exactInputSingle amountOut ", amountOut);
        return amountOut;
    }

    function findBestPoolByFee(
        address X,
        address Y,
        address FACTORY
    ) internal view returns (address, uint24) {
        uint24[3] memory fees = [uint24(500), uint24(3000), uint24(10000)];
        address bestPool;
        uint128 bestLiquidity = 0;
        uint24 bestFee;
        for (uint i = 0; i < fees.length; i++) {
            address pool = IUniswapV3Factory(FACTORY).getPool(X, Y, fees[i]);
            console.log("findBestPoolByFee pool", pool);

            if (pool != address(0)) {
                uint128 liquidity = IUniswapV3Pool(pool).liquidity();
                if (liquidity > bestLiquidity) {
                    bestPool = pool;
                    bestLiquidity = liquidity;
                    bestFee = fees[i];
                }
            }
        }
        console.log("bestPool, bestFee", bestPool, bestFee);
        return (bestPool, bestFee);
    }
}

