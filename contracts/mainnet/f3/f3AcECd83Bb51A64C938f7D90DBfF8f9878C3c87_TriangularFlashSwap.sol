pragma solidity ^0.8.4;

import "hardhat/console.sol";
import "./libraries/SafeERC20.sol";
import "./libraries/DexUtils.sol";
import "./libraries/DexesPolygon.sol";
import "./interfaces/IERC20.sol";
import "./interfaces/ICRV.sol";

contract TriangularFlashSwap is DexesPolygon {
    using SafeERC20 for IERC20;

    address private owner;

    address private constant WMATIC =
        0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270; //MATIC

    uint256 private constant MAX_INT =
        0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff;
    event Arbitrage(
        uint256 loanIn,
        uint256 loanOut,
        uint256 profit,
        uint8 decimals,
        address token
    );

    function startArbitrage(
        uint256 amountInMATIC,
        address[3] memory tokenOrder,
        uint8[3] memory dexOrder
    ) external {
        // RESET safeApprove to 0: SafeERC20: approve from non-zero to non-zero allowance
        address A = tokenOrder[0];
        address B = tokenOrder[1];
        address C = tokenOrder[2];
        approveABC(A, B, C, dexOrder);

        uint256 loanAmount = getAmountsInMATIC(A, amountInMATIC, dexOrder[0]);
        console.log("loanAmount", loanAmount);

        // TRANSFER HERE A from WALLET
        IERC20(A).transferFrom(msg.sender, address(this), loanAmount); //ERC20: transfer amount exceeds balance

        uint256 loanAmount_balance = IERC20(A).balanceOf(address(this)); //4.8
        require(loanAmount_balance > 0, "A transferFrom is Zero");

        require(
            loanAmount_balance == loanAmount,
            "loanAmount_balance is different than loanAmount"
        );

        //////////////////
        // DO ARBITRAGE
        uint256 amountOut;

        amountOut = DexUtils.placeTrade(
            A,
            B,
            loanAmount,
            address(this),
            Dexes[dexOrder[0]].FACTORY,
            Dexes[dexOrder[0]].ROUTER,
            Dexes[dexOrder[0]].Protocol
        ); // (A-B)
        console.log("FlashSwap.sol A to B  = ", amountOut);
        require(amountOut > 0, "A to B amountOut is Zero");

        amountOut = DexUtils.placeTrade(
            B,
            C,
            amountOut,
            address(this),
            Dexes[dexOrder[1]].FACTORY,
            Dexes[dexOrder[1]].ROUTER,
            Dexes[dexOrder[1]].Protocol
        ); // (B-C)
        console.log("FlashSwap.sol B to C  = ", amountOut);
        require(amountOut > 0, "B to C amountOut is Zero");

        amountOut = DexUtils.placeTrade(
            C,
            A,
            amountOut,
            address(this),
            Dexes[dexOrder[2]].FACTORY,
            Dexes[dexOrder[2]].ROUTER,
            Dexes[dexOrder[2]].Protocol
        ); // (C-A)
        console.log("FlashSwap.sol C to A = ", amountOut);
        require(amountOut > 0, "C to A amountOut is Zero"); //4.9

        // Check Profitability

        bool profCheck = checkProfitability(loanAmount, amountOut);
        require(profCheck, "Arbitrage not profitable"); //////////////////////////////////////////////////////////////////////////

        // PAY YOUSELF
        IERC20(A).transfer(msg.sender, amountOut);

        emit Arbitrage(
            loanAmount,
            amountOut,
            amountOut,
            IERC20(A).decimals(),
            A
        );
    }

    // ////////////////////
    function getAmountsInMATIC(
        address X,
        uint256 _amountsIn,
        uint8 _dexId
    ) private view returns (uint256) {
        uint256 AmountsOut;
        if (X != WMATIC) {
            AmountsOut = DexUtils.getAmountsOut(
                _amountsIn,
                WMATIC,
                X,
                Dexes[_dexId].FACTORY,
                Dexes[_dexId].ROUTER,
                Dexes[_dexId].Protocol // 2 uniswap v2; 3 uniswap v3; 1 CRV
            );
        } else {
            AmountsOut = _amountsIn;
        }

        return AmountsOut;
    }

    function simulateSwapTokens(
        address X,
        address Y,
        uint256 _amountsIn
    ) private view returns (uint256, uint8) {
        uint256 AmountsOut;
        uint256 maxAmountsOut = 0;
        uint8 dexId;
        for (uint8 i = 0; i < Dexes.length; i++) {
            AmountsOut = DexUtils.getAmountsOut(
                _amountsIn,
                X,
                Y,
                Dexes[i].FACTORY,
                Dexes[i].ROUTER,
                Dexes[i].Protocol
            );

            // console.log("Dexes i", i, AmountsOut);
            if (AmountsOut > maxAmountsOut) {
                maxAmountsOut = AmountsOut;
                dexId = i;
            }
        }

        return (maxAmountsOut, dexId);
    }

    function tryArbitrage(
        address J,
        address K,
        address L,
        uint256 amountInMATIC
    ) private view returns (uint256, uint8[3] memory) {
        uint256 loanAmount = getAmountsInMATIC(J, amountInMATIC, 0);
        uint256 J_Balance = IERC20(J).balanceOf(msg.sender);
        console.log("J_Balance", J_Balance);

        uint8[3] memory dexOrder;
        if (J_Balance > 0) {
            // MAyor loanAmount
            console.log(
                "FlashSwap (loanAmount,J_Balance, J)=",
                loanAmount,
                J_Balance,
                J
            );
            uint256 amountOut;
            (amountOut, dexOrder[0]) = simulateSwapTokens(J, K, loanAmount);
            console.log("FlashSwap amountOut1", amountOut);

            (amountOut, dexOrder[1]) = simulateSwapTokens(K, L, amountOut);
            console.log("FlashSwap amountOut2", amountOut);

            (amountOut, dexOrder[2]) = simulateSwapTokens(L, J, amountOut);
            console.log("FlashSwap amountOut3", amountOut);

            return ((amountOut * 100000) / loanAmount, dexOrder);
        } else {
            return (0, dexOrder);
        }
    }

    function calculateArbitrage(
        address J,
        address K,
        address L,
        uint256 amountInMATIC
    ) external view returns (uint256, address[3] memory, uint8[3] memory) {
        uint256 ratio;
        uint256 best_ratio;
        address[3] memory best_tokenOrder;
        uint8[3] memory dexOrder;
        uint8[3] memory best_dexOrder = [10, 10, 10];

        console.log("FlashSwap amountInMATIC", amountInMATIC);
        // J K L
        (ratio, dexOrder) = tryArbitrage(J, K, L, amountInMATIC);
        best_ratio = ratio;
        best_tokenOrder = [J, K, L];
        best_dexOrder = dexOrder;

        // J L K
        (ratio, dexOrder) = tryArbitrage(J, L, K, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [J, L, K];
            best_dexOrder = dexOrder;
        }
        //  L K J
        (ratio, dexOrder) = tryArbitrage(L, K, J, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [L, K, J];
            best_dexOrder = dexOrder;
        }
        //  K J L
        (ratio, dexOrder) = tryArbitrage(K, J, L, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [K, J, L];
            best_dexOrder = dexOrder;
        }
        // L J K
        (ratio, dexOrder) = tryArbitrage(L, J, K, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [L, J, K];
            best_dexOrder = dexOrder;
        }
        // K L J
        (ratio, dexOrder) = tryArbitrage(K, L, J, amountInMATIC);
        if (ratio > best_ratio) {
            best_ratio = ratio;
            best_tokenOrder = [K, L, J];
            best_dexOrder = dexOrder;
        }

        console.log(
            "FlashSwap best_tokenOrder",
            best_tokenOrder[0],
            best_tokenOrder[1],
            best_tokenOrder[2]
        );
        console.log(
            "FlashSwap best_dexOrder",
            best_dexOrder[0],
            best_dexOrder[1],
            best_dexOrder[2]
        );
        console.log("FlashSwap ratio", best_ratio);

        return (best_ratio, best_tokenOrder, best_dexOrder);
    }

    // GET CONTRACT BALANCE
    // Allows public view of balance for contract
    function getBalanceOfToken(
        address _addressToken
    ) public view returns (uint256) {
        return IERC20(_addressToken).balanceOf(address(this));
    }

    // CHECK PROFITABILITY
    // Checks whether > output > input
    function checkProfitability(
        uint256 _input,
        uint256 _output
    ) private pure returns (bool) {
        return _output > _input;
    }

    function testFunction() public returns (address) {
        address CRV_FACTORY = 0x722272D36ef0Da72FF51c5A65Db7b870E2e8D4ee;
        address A = 0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174; //USDC
        address B = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270; // WMATIC
        address pool = ICRV(CRV_FACTORY).find_pool_for_coins(A, B, 0);
        console.log("FlashSwap.sol testFunction", pool);
        return pool;
    }

    function approveABC(
        address _A,
        address _B,
        address _C,
        uint8[3] memory _dexOrder
    ) private {
        uint256 allowance = IERC20(_A).allowance(
            address(this),
            address(Dexes[_dexOrder[0]].ROUTER)
        );
        if (allowance == 0) {
            IERC20(_A).safeApprove(
                address(Dexes[_dexOrder[0]].ROUTER),
                MAX_INT
            );
            console.log("allowance A");
        }

        allowance = IERC20(_B).allowance(
            address(this),
            address(Dexes[_dexOrder[1]].ROUTER)
        );
        if (allowance == 0) {
            IERC20(_B).safeApprove(
                address(Dexes[_dexOrder[1]].ROUTER),
                MAX_INT
            );
            console.log("allowance B");
        }

        allowance = IERC20(_C).allowance(
            address(this),
            address(Dexes[_dexOrder[2]].ROUTER)
        );
        if (allowance == 0) {
            IERC20(_C).safeApprove(
                address(Dexes[_dexOrder[2]].ROUTER),
                MAX_INT
            );
            console.log("allowance C");
        }
    }

    function getV3Pool(
        uint256 _amountsIn,
        address X,
        address Y,
        uint8 _dexId
    ) external view returns (uint256) {
        uint256 AmountsOut = DexUtils.getAmountsOut(
            _amountsIn,
            X,
            Y,
            Dexes[_dexId].FACTORY,
            Dexes[_dexId].ROUTER,
            Dexes[_dexId].Protocol
        );
        // console.log("AmountsOut", AmountsOut);

        return AmountsOut;
    }
}

