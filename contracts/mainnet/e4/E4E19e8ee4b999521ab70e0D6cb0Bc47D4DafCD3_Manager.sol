    /**
     * @notice convert amount to stable token decimal size
     * @param _amount amount to convert
     */
    function toStableDecimal(uint256 _amount) internal pure returns (uint256) {
        return _amount / (10**12);
    }

    function toWei(uint256 _amount) internal pure returns (uint256) {
        return _amount * (10**12);
    }

    function getCollateralRatio(IAmm _amm, address _trader)
        public
        view
        returns (int256)
    {
        IAmm.Position memory position = _amm.getTraderPosition(_trader);
        requirePositionSize(position.size);

        (uint256 spotPositionNotional, int256 spotPricePnl) = (
            getPositionNotionalAndUnrealizedPnl(
                _amm,
                _trader,
                PnlCalcOption.SPOT_PRICE
            )
        );
        (uint256 twapPositionNotional, int256 twapPricePnl) = (
            getPositionNotionalAndUnrealizedPnl(
                _amm,
                _trader,
                PnlCalcOption.TWAP
            )
        );

        // console.log(
        //     "getCollateralRatio spotPricePnl",
        //     unsignedInt(spotPricePnl),
        //     spotPricePnl > 0 ? "pos" : "neg",
        //     spotPositionNotional
        // );
        // console.log(
        //     "getCollateralRatio twapPricePnl",
        //     unsignedInt(twapPricePnl),
        //     twapPricePnl > 0 ? "pos" : "neg",
        //     twapPositionNotional
        // );

        (int256 unrealizedPnl, uint256 positionNotional) = spotPricePnl >
            twapPricePnl
            ? (spotPricePnl, spotPositionNotional)
            : (twapPricePnl, twapPositionNotional);

        // remainCollateral / positionalNotional
        return
            _getCollateralRatio(
                _amm,
                position,
                unrealizedPnl,
                positionNotional
            );
    }

    function _getCollateralRatio(
        IAmm _amm,
        IAmm.Position memory _position,
        int256 _unrealizedPnl,
        uint256 _positionNotional
    ) internal view returns (int256) {
        (uint256 collateral, , uint256 badDebt) = _amm
            .getCollateralWithFundingPayment(_position, _unrealizedPnl);

        // console.log(
        //     "_unrealizedPnl",
        //     unsignedInt(_unrealizedPnl),
        //     _unrealizedPnl > 0 ? "pos" : "neg",
        //     _position.collateral
        // );
        // console.log("collateral", collateral, badDebt, _positionNotional);
        // console.log("ratio", (badDebt * 10**18) / _positionNotional);

        return
            ((int256(collateral) - int256(badDebt)) * (10**18)) /
            int256(_positionNotional);
    }

    function _getCollateralRatioBasedOnOracle(IAmm _amm, address _trader)
        internal
        view
        returns (int256)
    {
        IAmm.Position memory position = _amm.getTraderPosition(_trader);
        (uint256 oraclePositionNotional, int256 oraclePricePnl) = (
            getPositionNotionalAndUnrealizedPnl(
                _amm,
                _trader,
                PnlCalcOption.ORACLE
            )
        );
        // console.log(
        //     "_getCollateralRatioBasedOnOracle",
        //     unsignedInt(oraclePricePnl),
        //     oraclePricePnl > 0 ? "pos" : "neg",
        //     oraclePositionNotional
        // );
        return
            _getCollateralRatio(
                _amm,
                position,
                oraclePricePnl,
                oraclePositionNotional
            );
    }

    /**
     * @notice get position notional and unrealized Pnl without fee expense and funding payment
     * @dev Explain to a developer any extra details
     */
    function getPositionNotionalAndUnrealizedPnl(
        IAmm _amm,
        address _trader,
        PnlCalcOption _pnlCalcOption
    ) public view returns (uint256 positionNotional, int256 unrealizedPnl) {
        IAmm.Position memory position = _amm.getTraderPosition(_trader);
        uint256 positionSizeAbs = unsignedInt(position.size);
        uint256 baseAssetReserveAfter;
        uint256 quoteAssetReserveAfter;

        if (positionSizeAbs != 0) {
            bool isShortPosition = position.size < 0;
            IAmm.Dir dir = isShortPosition
                ? IAmm.Dir.REMOVE_FROM_AMM
                : IAmm.Dir.ADD_TO_AMM;
            if (_pnlCalcOption == PnlCalcOption.TWAP) {
                positionNotional = _amm.getOutputTwap(dir, positionSizeAbs);
            } else if (_pnlCalcOption == PnlCalcOption.SPOT_PRICE) {
                (
                    positionNotional,
                    baseAssetReserveAfter,
                    quoteAssetReserveAfter
                ) = _amm.getOutputPrice(dir, positionSizeAbs);
            } else {
                uint256 oraclePrice = _amm.getUnderlyingPrice();
                positionNotional = (positionSizeAbs * oraclePrice) / 10**18;
            }
            // unrealizedPnlForLongPosition = positionNotional - openNotional
            // unrealizedPnlForShortPosition = positionNotionalWhenBorrowed - positionNotionalWhenReturned =
            // openNotional - positionNotional = unrealizedPnlForLongPosition * -1

            unrealizedPnl = isShortPosition
                ? int256(position.openNotional) - int256(positionNotional)
                : int256(positionNotional) - int256(position.openNotional);
        }
    }

