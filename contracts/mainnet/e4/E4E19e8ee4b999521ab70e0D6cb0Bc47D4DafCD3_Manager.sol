pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

interface IAmm {
    /**
     * @notice stable token action
     * @param ADD_TO_AMM Long position, add asset to Amm
     * @param REMOVE_FROM_AMM Short position, remove asset from Amm
     */
    enum Dir {
        ADD_TO_AMM,
        REMOVE_FROM_AMM
    }

    // internal usage
    enum QuoteAssetDir {
        QUOTE_IN,
        QUOTE_OUT
    }

    enum TwapCalcOption {
        RESERVE_ASSET,
        INPUT_ASSET
    }

    struct ReserveSnapshot {
        uint256 quoteAssetReserve;
        uint256 baseAssetReserve;
        uint256 timestamp;
        uint256 blockNumber;
    }

    // To record current base/quote asset to calculate TWAP
    struct TwapInputAsset {
        Dir dir;
        int256 assetAmount;
        QuoteAssetDir inOrOut;
    }

    /// @notice Struct to record position of user
    /// @param size synth token amount
    /// @param collateral stable token amount, cost of position
    /// @param blockNumber the block number of the last position
    struct Position {
        int256 size;
        uint256 collateral;
        uint256 openNotional;
        int256 lastRecordedFundingFraction;
        int256 lastRecordedFundingFractionLong;
        int256 lastRecordedFundingFractionShort;
        uint256 blockNumber;
    }

    struct PositionData {
        Position position;
        // the quote asset amount trader will send if open position, will receive if close
        int256 exchangedQuoteAssetAmount;
        // if realizedPnl + realizedFundingPayment + collatoral is negative, it's the abs value of it
        uint256 badDebt;
        // the base asset amount trader will receive if open position, will send if close
        int256 exchangedPositionSize;
        // funding payment incurred during this position response
        int256 fundingPayment;
        // realizedPnl = unrealizedPnl * closedRatio
        int256 realizedPnl;
        // positive = trader transfer collatoral to vault, negative = trader receive collatoral from vault
        // it's 0 when internalReducePosition, its addedMargin when internalIncreasePosition
        // it's min(0, oldPosition + realizedFundingPayment + realizedPnl) when internalClosePosition
        int256 collateralToVault;
        // unrealized pnl after open position
        int256 unrealizedPnlAfter;
        uint256 feeToInsuranceFund;
        uint256 feeToLiquidator;
        uint256 liquidationBadDebt;
        bool isNew;
    }

    struct TwapPriceCalcParams {
        TwapCalcOption opt;
        uint256 snapshotIndex;
        TwapInputAsset asset;
    }

    function openPosition(
        address _trader,
        Dir _dirOfPosition,
        uint256 stableTokenAmount,
        uint256 _leverage,
        uint256 _baseAssetLimit,
        int256 unrealizedPnl,
        uint256 _oldPositionNotional
    ) external returns (PositionData memory posData);

    function closePosition(
        address _trader,
        int256 unrealizedPnl,
        uint256 _stableTokenLimit,
        uint256 partialLiquidationRatio,
        uint256 _oldPositionNotional
    ) external returns (PositionData memory posData);

    //VIEW
    function open() external view returns (bool);

    function lastRestrictionBlock() external view returns (uint256);

    function getSpotPrice() external view returns (uint256);

    function getAssetReserves() external view returns (uint256, uint256);

    function stableAsset() external view returns (IERC20);

    function getTraderPosition(address _trader)
        external
        view
        returns (Position memory);

    function getInputPrice(Dir _dirOfQuote, uint256 _quoteAssetAmount)
        external
        view
        returns (
            int256,
            uint256,
            uint256
        );

    function getOutputPrice(Dir _dirOfQuote, uint256 _baseAssetAmount)
        external
        view
        returns (
            uint256,
            uint256,
            uint256
        );

    function calculateFundingPayment() external returns (int256);

    function addCollateral(address _trader, uint256 _addedcollateral) external;

    function removeCollateral(address _trader, uint256 _removedCollateral)
        external
        returns (uint256 remainCollateral, int256 fundingPayment);

    function getUnderlyingPrice() external view returns (uint256);

    function getInputTwap(Dir _dirOfQuote, uint256 _quoteAssetAmount)
        external
        view
        returns (uint256);

    function getOutputTwap(Dir _dirOfQuote, uint256 _baseAssetAmount)
        external
        view
        returns (uint256);

    function calculateOpenPosition(
        address _trader,
        Dir _dirOfPosition,
        uint256 stableTokenAmount,
        uint256 _leverage,
        int256 unrealizedPnl,
        uint256 _baseAssetLimit,
        bool _isOverFluctuationLimit,
        uint256 _oldPositionNotional
    ) external returns (PositionData memory posData);

    function calculateClosePosition(
        address _trader,
        uint256 stableTokenAmount,
        int256 unrealizedPnl,
        uint256 _stableTokenLimit,
        uint256 _oldPositionNotional
    ) external returns (PositionData memory posData);

    function getLatestCumulativePremiumFraction()
        external
        view
        returns (int256);

    function getCollateralWithFundingPayment(
        Position memory _oldPosition,
        int256 _collateralChange
    )
        external
        view
        returns (
            uint256 adjustedCollateral,
            int256 fundingPayment,
            uint256 badDebt
        );

    function setPositionByManager(address _trader, Position memory _position)
        external;

    function setRestrictionMode() external;

    function settleTraderPosition(address _trader)
        external
        returns (uint256 settledValue);

    function getSettlementPrice() external view returns (uint256);

    function isOverSpreadLimit() external view returns (bool);

    function isOverPriceBandLimit(Dir _dirOfPosition)
        external
        view
        returns (bool);

    function getMaxBaseAssetLimit() external view returns (uint256);
}

