    function initialize(
        IInsuranceFund _insuranceFund,
        uint256 _liquidationFeeRatio,
        uint256 _initCollateralRatio
    ) public initializer {
        require(address(_insuranceFund) != address(0), "invalid params");

        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();

        insuranceFund = _insuranceFund;
        liquidationFeeRatio = _liquidationFeeRatio;
        initCollateralRatio = _initCollateralRatio;
    }

    /**
     * @notice open a position
     * @param _amm amm address
     */
    function openPosition(
        IAmm _amm,
        IAmm.Dir _dirOfPosition,
        uint256 _stableTokenAmount,
        uint256 _leverage,
        uint256 _baseAssetLimit
    ) external override {
        address trader = _msgSender();
        // require(isWhitelist(trader), "restricted mode");
        requireAmm(_amm, true);
        requireNotRestrictionMode(_amm);
        requireNonZeroInput(_stableTokenAmount);
        requireMoreCollateralRatio(
            int256((1 * (10**18)) / _leverage),
            initCollateralRatio,
            true
        );

        IAmm.PositionData memory posData;
        uint256 feeTransferred;
        {
            (uint256 oldPositionNotional, int256 unrealizedPnl) = (
                getPositionNotionalAndUnrealizedPnl(
                    _amm,
                    trader,
                    PnlCalcOption.SPOT_PRICE
                )
            );
            posData = _amm.openPosition(
                trader,
                _dirOfPosition,
                _stableTokenAmount,
                _leverage,
                _baseAssetLimit,
                unrealizedPnl,
                oldPositionNotional
            );

            require(
                !_amm.isOverPriceBandLimit(_dirOfPosition),
                "Price is beyond price band limit"
            );

            uint256 maxBaseAsset = _amm.getMaxBaseAssetLimit();
            if (!isWhitelist(trader) && maxBaseAsset != 0) {
                require(
                    unsignedInt(posData.position.size) < maxBaseAsset,
                    "over max base limit"
                );
            }

            if (!posData.isNew && posData.position.size != 0) {
                requireMoreCollateralRatio(
                    getCollateralRatio(_amm, trader),
                    maintenanceCollateralRatio,
                    true
                );
            }

            if (posData.badDebt > 0) {
                _amm.setRestrictionMode();
            }

            IERC20 stableAsset = _amm.stableAsset();

            if (posData.collateralToVault > 0) {
                // console.log(
                //     "collateralToVault +",
                //     uint256(posData.collateralToVault)
                // );
                stableAsset.transferFrom(
                    trader,
                    address(this),
                    toStableDecimal(unsignedInt(posData.collateralToVault))
                );
            } else if (posData.collateralToVault < 0) {
                // console.log(
                //     "collateralToVault -",
                //     uint256(0 - posData.collateralToVault)
                // );
                safeTransferToTrader(
                    stableAsset,
                    unsignedInt(posData.collateralToVault)
                );
            }

            feeTransferred = transferFee(
                stableAsset,
                trader,
                _stableTokenAmount * _leverage
            );
        }

        emit PositionChanged(
            trader,
            address(_amm),
            posData.position.collateral,
            _stableTokenAmount * _leverage,
            posData.exchangedPositionSize,
            posData.position.size,
            _amm.getSpotPrice(),
            posData.realizedPnl,
            posData.unrealizedPnlAfter,
            posData.badDebt,
            posData.liquidationBadDebt,
            posData.fundingPayment,
            feeTransferred,
            posData.position.openNotional
        );
    }

    /**
     * @notice close a position
     * @param _amm amm address
     */
    function closePosition(IAmm _amm, uint256 _stableTokenLimit)
        external
        override
    {
        requireAmm(_amm, true);
        requireNotRestrictionMode(_amm);

        address trader = _msgSender();

        (uint256 oldPositionNotional, int256 unrealizedPnl) = (
            getPositionNotionalAndUnrealizedPnl(
                _amm,
                trader,
                PnlCalcOption.SPOT_PRICE
            )
        );

        IAmm.PositionData memory posData = _amm.closePosition(
            trader,
            unrealizedPnl,
            _stableTokenLimit,
            partialLiquidationRatio,
            oldPositionNotional
        );

        uint256 feeTransferred;
        {
            IERC20 stableAsset = _amm.stableAsset();

            if (posData.badDebt > 0) {
                // console.log("realise bad debt", posData.badDebt);
                _amm.setRestrictionMode();
                realizeBadDebt(stableAsset, posData.badDebt);
            }

            safeTransferToTrader(
                stableAsset,
                unsignedInt(posData.collateralToVault)
            );

            feeTransferred = transferFee(
                stableAsset,
                trader,
                unsignedInt(posData.exchangedQuoteAssetAmount)
            );
        }

        emit PositionChanged(
            trader,
            address(_amm),
            posData.position.collateral,
            unsignedInt(posData.exchangedQuoteAssetAmount),
            posData.exchangedPositionSize,
            posData.position.size,
            _amm.getSpotPrice(),
            posData.realizedPnl,
            posData.unrealizedPnlAfter,
            posData.badDebt,
            posData.liquidationBadDebt,
            posData.fundingPayment,
            feeTransferred,
            posData.position.openNotional
        );
    }

    /**
     * @notice settle all the positions when amm is shutdown. The settlement price is according to IAmm.settlementPrice
     * @param _amm IAmm address
     */
    function settlePosition(IAmm _amm) external nonReentrant {
        // check condition
        requireAmm(_amm, false);
        address trader = _msgSender();

        uint256 settledValue = _amm.settleTraderPosition(trader);

        IERC20 stableAsset = _amm.stableAsset();
        // transfer token based on settledValue. no insurance fund support
        if (settledValue > 0) {
            // _transfer(_amm.quoteAsset(), trader, settledValue);
            stableAsset.transfer(trader, toStableDecimal(settledValue));
        }
        // emit event
        emit PositionSettled(
            address(_amm),
            trader,
            toStableDecimal(settledValue)
        );
    }

    function fundingJob(IAmm _amm) external override {
        requireAmm(_amm, true);

        int256 ammFundingProfit = _amm.calculateFundingPayment();
        IERC20 stableAsset = _amm.stableAsset();

        if (ammFundingProfit > 0) {
            // Transfer amount to insurance fund
            uint256 managerBalance = stableAsset.balanceOf(address(this));
            stableAsset.transfer(
                address(insuranceFund),
                managerBalance < toStableDecimal(uint256(ammFundingProfit))
                    ? managerBalance
                    : toStableDecimal(uint256(ammFundingProfit))
            );
        } else if (ammFundingProfit < 0) {
            // Withdraw amount from insurance fund
            insuranceFund.withdraw(stableAsset, uint256(0 - ammFundingProfit));
        }
    }

    function addCollateral(IAmm _amm, uint256 _addedcollateral)
        external
        whenNotPaused
        nonReentrant
    {
        // check condition
        requireAmm(_amm, true);
        requireNonZeroInput(_addedcollateral);

        address trader = _msgSender();

        _amm.addCollateral(trader, _addedcollateral);

        // transfer token from trader
        IERC20 stableAsset = _amm.stableAsset();
        stableAsset.transferFrom(
            trader,
            address(this),
            toStableDecimal(_addedcollateral)
        );

        emit CollateralChanged(
            trader,
            address(_amm),
            int256(_addedcollateral),
            0
        );
    }

    function removeCollateral(IAmm _amm, uint256 _removedCollateral)
        external
        whenNotPaused
        nonReentrant
    {
        // check condition
        requireAmm(_amm, true);
        requireNonZeroInput(_removedCollateral);
        address trader = _msgSender();

        int256 collateralDelta = int256(_removedCollateral) * -1;

        (uint256 remainCollateral, int256 fundingPayment) = _amm
            .removeCollateral(trader, _removedCollateral);

        requireMoreCollateralRatio(
            getCollateralRatio(_amm, trader),
            initCollateralRatio,
            true
        );

        // withdraw token to trader
        IERC20 stableAsset = _amm.stableAsset();

        // stableAsset.transfer(trader, toStableDecimal(remainCollateral));
        safeTransferToTrader(stableAsset, _removedCollateral);

        emit CollateralChanged(
            trader,
            address(_amm),
            int256(collateralDelta),
            fundingPayment
        );
    }

    /**
     * @notice close a position
     * @param _amm amm address
     */
    function liquidate(IAmm _amm, address _trader) public nonReentrant {
        requireAmm(_amm, true);
        int256 collateralRatio = getCollateralRatio(_amm, _trader);
        // // including oracle-based Collateral ratio as reference price when amm is over spread limit
        if (_amm.isOverSpreadLimit()) {
            // console.log("inside over spread limit");
            int256 collateralRatioBasedOnOracle = _getCollateralRatioBasedOnOracle(
                    _amm,
                    _trader
                );
            // console.log(
            //     "inside over spread limit collateralRatio",
            //     unsignedInt(collateralRatio),
            //     collateralRatio > 0 ? "pos" : "neg"
            // );
            // console.log(
            //     "inside over spread limit",
            //     unsignedInt(collateralRatioBasedOnOracle),
            //     collateralRatioBasedOnOracle > 0 ? "pos" : "neg"
            // );
            if (collateralRatioBasedOnOracle - collateralRatio > 0) {
                collateralRatio = collateralRatioBasedOnOracle;
            }
        }

        requireMoreCollateralRatio(
            collateralRatio,
            maintenanceCollateralRatio,
            false
        );

        IAmm.PositionData memory posData;
        uint256 liquidationPenalty;
        {
            uint256 feeToInsuranceFund;
            uint256 totalBadDebt;
            uint256 feeToLiquidator;
            IERC20 stableAsset = _amm.stableAsset();

            (uint256 oldPositionNotional, int256 unrealizedPnl) = (
                getPositionNotionalAndUnrealizedPnl(
                    _amm,
                    _trader,
                    PnlCalcOption.SPOT_PRICE
                )
            );

            if (
                collateralRatio > int256(liquidationFeeRatio) &&
                cmp(partialLiquidationRatio, 10**18) < 0 &&
                partialLiquidationRatio != 0
            ) {
                IAmm.Position memory currPosition = _amm.getTraderPosition(
                    _trader
                );
                IAmm.Dir _dirOfPosition = currPosition.size > 0
                    ? IAmm.Dir.ADD_TO_AMM
                    : IAmm.Dir.REMOVE_FROM_AMM;

                (uint256 partiallyLiquidatedPositionNotional, , ) = _amm
                    .getOutputPrice(
                        _dirOfPosition,
                        mulD(
                            unsignedInt(currPosition.size),
                            partialLiquidationRatio
                        )
                    );

                posData = _amm.calculateOpenPosition(
                    _trader,
                    currPosition.size > 0
                        ? IAmm.Dir.REMOVE_FROM_AMM
                        : IAmm.Dir.ADD_TO_AMM,
                    partiallyLiquidatedPositionNotional,
                    1,
                    unrealizedPnl,
                    0,
                    true,
                    oldPositionNotional
                );

                //calculate remaining openNotional
                posData.unrealizedPnlAfter =
                    unrealizedPnl -
                    posData.realizedPnl;
                if (currPosition.size > 0) {
                    posData.position.openNotional = uint256(
                        int256(oldPositionNotional) -
                            posData.exchangedQuoteAssetAmount -
                            posData.unrealizedPnlAfter
                    );
                } else {
                    posData.position.openNotional = uint256(
                        posData.unrealizedPnlAfter +
                            int256(oldPositionNotional) -
                            posData.exchangedQuoteAssetAmount
                    );
                }

                liquidationPenalty = mulD(
                    unsignedInt(posData.exchangedQuoteAssetAmount),
                    liquidationFeeRatio
                );
                feeToLiquidator = liquidationPenalty / 2;
                feeToInsuranceFund = liquidationPenalty - feeToLiquidator;
                posData.position.collateral =
                    posData.position.collateral -
                    liquidationPenalty;
            } else {
                liquidationPenalty = _amm.getTraderPosition(_trader).collateral;
                posData = _amm.calculateClosePosition(
                    _trader,
                    0,
                    unrealizedPnl,
                    0,
                    oldPositionNotional
                );

                uint256 remainCollateral = unsignedInt(
                    posData.collateralToVault
                );
                feeToLiquidator =
                    mulD(
                        unsignedInt(posData.exchangedQuoteAssetAmount),
                        liquidationFeeRatio
                    ) /
                    2;

                totalBadDebt = posData.badDebt;
                if (feeToLiquidator > remainCollateral) {
                    posData.liquidationBadDebt =
                        feeToLiquidator -
                        remainCollateral;
                    totalBadDebt = totalBadDebt + posData.liquidationBadDebt;
                } else {
                    remainCollateral = remainCollateral - feeToLiquidator;
                }

                if (totalBadDebt > 0) {
                    realizeBadDebt(stableAsset, totalBadDebt);
                }

                if (remainCollateral > 0) {
                    feeToInsuranceFund = remainCollateral;
                }
            }

            _amm.setPositionByManager(_trader, posData.position);

            if (feeToInsuranceFund > 0) {
                stableAsset.transfer(
                    address(insuranceFund),
                    toStableDecimal(feeToInsuranceFund)
                );
            }

            if (feeToLiquidator > 0) {
                safeTransferToTrader(stableAsset, feeToLiquidator);
            }

            _amm.setRestrictionMode();

            emit PositionLiquidated(
                _trader,
                address(_amm),
                unsignedInt(posData.exchangedQuoteAssetAmount),
                unsignedInt(posData.exchangedPositionSize),
                feeToLiquidator,
                _msgSender(),
                posData.liquidationBadDebt
            );
        }

        emit PositionChanged(
            _trader,
            address(_amm),
            posData.position.collateral, //stableTokenAmount,
            unsignedInt(posData.exchangedQuoteAssetAmount), //stableTokenAmount * _leverage,
            posData.exchangedPositionSize, //synthTokenPurchased,
            posData.position.size,
            _amm.getSpotPrice(),
            posData.realizedPnl,
            posData.unrealizedPnlAfter,
            posData.badDebt,
            liquidationPenalty,
            posData.fundingPayment,
            0,
            posData.position.openNotional
        );
    }

    function safeTransferToTrader(IERC20 _stableToken, uint256 _amount)
        private
    {
        uint256 managerBalance = _stableToken.balanceOf(address(this));
        uint256 amountToTransfer = toStableDecimal(_amount);
        // console.log("amountToTransfer", amountToTransfer, managerBalance);
        if (managerBalance < amountToTransfer) {
            uint256 balanceAmount = amountToTransfer - managerBalance;
            prepaidBadDebt[address(_stableToken)] =
                prepaidBadDebt[address(_stableToken)] +
                balanceAmount;
            insuranceFund.withdraw(_stableToken, toWei(balanceAmount));
        }

        _stableToken.transfer(_msgSender(), amountToTransfer);
    }

    function addToWhitelist(address _trader) public onlyOwner {
        whitelistAddresses[_trader] = true;
    }

    function remnoveFromWhitelist(address _trader) public onlyOwner {
        whitelistAddresses[_trader] = false;
    }

    function isWhitelist(address _trader) internal view returns (bool) {
        return whitelistAddresses[_trader];
    }

    function realizeBadDebt(IERC20 _token, uint256 _badDebt) internal {
        uint256 badDebtBalance = prepaidBadDebt[address(_token)];
        uint256 badDebtStable = toStableDecimal(_badDebt);

        if (badDebtBalance > badDebtStable) {
            // no need to move extra tokens because vault already prepay bad debt, only need to update the numbers
            prepaidBadDebt[address(_token)] = badDebtBalance - badDebtStable;
        } else {
            // in order to realize all the bad debt vault need extra tokens from insuranceFund
            insuranceFund.withdraw(
                _token,
                toWei(badDebtStable - badDebtBalance)
            );
            prepaidBadDebt[address(_token)] = 0;
        }
    }

    function transferFee(
        IERC20 _token,
        address _trader,
        uint256 notional
    ) internal returns (uint256 feeToTransfer) {
        feeToTransfer = (notional * feeRatio) / 10**18;

        if (feePool != address(0) && feeToTransfer > 0) {
            _token.transferFrom(
                _trader,
                address(feePool),
                toStableDecimal(feeToTransfer)
            );
        }
    }

