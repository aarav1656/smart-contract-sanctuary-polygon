        address indexed trader,
        address indexed amm,
        uint256 collateral,
        uint256 positionNotional,
        int256 synthTokenPurchased,
        int256 positionSizeAfter,
        uint256 spotPrice,
        int256 realizedPnl,
        int256 unrealizedPnlAfter,
        uint256 badDebt,
        uint256 liquidationPenalty,
        int256 fundingPayment,
        uint256 fee,
