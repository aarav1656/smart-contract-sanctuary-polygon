    function requireMoreCollateralRatio(
        int256 _collateralRatio,
        uint256 _baseCollateralRatio,
        bool _largerThanOrEqualTo
    ) private view {
        // console.log(
        //     "requireMoreCollateralRatio",
        //     _baseCollateralRatio,
        //     unsignedInt(_collateralRatio),
        //     _collateralRatio > 0 ? "+" : "-"
        // );
        int256 remainingCollateralRatio = _collateralRatio -
            int256(_baseCollateralRatio);
        // console.log(
        //     "remainingCollateralRatio",
        //     remainingCollateralRatio > 0 ? "+" : "-",
        //     unsignedInt(remainingCollateralRatio)
        // );
        require(
            _largerThanOrEqualTo
                ? remainingCollateralRatio >= 0
                : remainingCollateralRatio < 0,
            "Collateral ratio not meet criteria"
        );
    }

    function requirePositionSize(int256 _size) private pure {
        require(_size != 0, "positionSize is 0");
    }

    function requireNotRestrictionMode(IAmm _amm) private view {
        uint256 currentBlock = _blockNumber();
        if (currentBlock == _amm.lastRestrictionBlock()) {
            require(
                _amm.getTraderPosition(_msgSender()).blockNumber !=
                    currentBlock,
                "only one action allowed"
            );
        }
    }

    //SET FUNCTIONS
    /**
     * @notice set liquidation fee ratio
     * @dev only owner can call
     * @param _liquidationFeeRatio new liquidation fee ratio in 18 digits
     */
    function setLiquidationFeeRatio(uint256 _liquidationFeeRatio)
        external
        onlyOwner
    {
        liquidationFeeRatio = _liquidationFeeRatio;
        emit LiquidationFeeRatioChanged(liquidationFeeRatio);
    }

    /**
     * @notice set maintenance collateral ratio
     * @dev only owner can call
     * @param _maintenanceCollateralRatio new maintenance Collateral ratio in 18 digits
     */
    function setMaintenanceCollateralRatio(uint256 _maintenanceCollateralRatio)
        external
        onlyOwner
    {
        maintenanceCollateralRatio = _maintenanceCollateralRatio;
        emit CollateralRatioChanged(maintenanceCollateralRatio);
    }

    /**
     * @notice set the Collateral ratio after deleveraging
     * @dev only owner can call
     */
    function setPartialLiquidationRatio(uint256 _ratio) external onlyOwner {
        require(cmp(_ratio, 10**18) <= 0, "invalid partial liquidation ratio");
        partialLiquidationRatio = _ratio;
    }

    function setFeeRatio(uint256 _feeRatio) external onlyOwner {
        feeRatio = _feeRatio;
    }

    function setFeePool(address _feePool) external onlyOwner {
        feePool = _feePool;
    }

    function unsignedInt(int256 _num) internal pure returns (uint256) {
        if (_num < 0) {
            return uint256(0 - _num);
        }
        return uint256(_num);
    }

    function cmp(uint256 x, uint256 y) internal pure returns (int8) {
        if (x > y) {
            return 1;
        } else if (x < y) {
            return -1;
        }
        return 0;
    }

    function mulD(uint256 x, uint256 y) internal pure returns (uint256) {
        return (x * y) / 10**18;
    }
}

