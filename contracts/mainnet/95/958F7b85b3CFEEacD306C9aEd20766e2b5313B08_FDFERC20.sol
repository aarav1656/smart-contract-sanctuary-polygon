pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "../lib/ABDKMath64x64.sol";
import "./IFDFERC20.sol";
import "./IFSetting.sol";
import "./IRouter.sol";
import "./IReceiveUSDT.sol";
import "./IBentoBoxMinimal.sol";
import "./IPool.sol";

contract FDFERC20 is Ownable, IFDFERC20, IERC20Metadata  {
    using SafeMath for uint256;

    struct UserInfo {
        uint256 lastRewardBlock;
        uint256 activeBlockNumber;
    }

    mapping(address => UserInfo) private _users;

    mapping(address => uint256) private _balances;

    mapping(address => mapping(address => uint256)) private _allowances;

    uint256 private _totalSupply;

    string private _name;
    string private _symbol;

    uint256 constant  public baseProfit = 208;
    uint256 constant public base = 10000;
    uint256 constant  public epoch = 90;
    uint256 public constant timeStep = 1 days;

    uint256 public constant dayBlocks = 28800;
    uint256 public constant epochBlocks = epoch * dayBlocks;


    struct FeeTier {
        uint256 ecoSystemFee; //10%
        uint256 burnFee; //20%
        uint256 nftPoolFee; //20%
        uint256 stakingPoolFee; //50%
    }

    //10%
    struct FeeValues {
        uint256 ecoSystemAmount; //10%
        uint256 burnAmount; //20%
        uint256 nftPoolAmount; //20%
        uint256 stakingPoolAmount; //50%
        uint256 transferAmount;
    }

    uint256 public taxPercent = 10;

    FeeTier public defaultFees;

    event UpdateReward(address account,uint256 amount);

    address private setting;

    constructor(string memory name_, string memory symbol_, address setting_, uint256 init_) {
        _name = name_;
        _symbol = symbol_;
        require(setting_ != address(0),"setting is err");
        setting = setting_;
        address ownerAdmin = IFSetting(setting).safeAdmin();
        _transferOwnership(ownerAdmin);
        _mint(ownerAdmin,init_);
        initDefaultFees();
        IFSetting(setting).setInit(5,address(this));
    }

    function setSetting(address setting_) external onlyOwner {
        setting = setting_;
    }

    function initDefaultFees() private {
        defaultFees = FeeTier(10,20,20,50);
    }

    /**
* @dev Moves `amount` of tokens from `from` to `to`.
     *
     * This internal function is equivalent to {transfer}, and can be used to
     * e.g. implement automatic token fees, slashing mechanisms, etc.
     *
     * Emits a {Transfer} event.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `from` must have a balance of at least `amount`.
     */
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount >0, "ERC20: transfer amount zero");

        IFSetting fSetting = IFSetting(setting);

        _updateReward(to);
        _updateReward(from);

        uint256 fromBalance = _balances[from];
        require(fromBalance >= amount, "ERC20: transfer amount exceeds balance");
        _balances[from] = fromBalance - amount;

        if (_users[to].activeBlockNumber == 0) {
            _users[to].activeBlockNumber = block.number;
            _users[to].lastRewardBlock = block.number;
        }

        if (IFSetting(setting).isTaxExcluded(from) || IFSetting(setting).isTaxExcluded(to)
            || from == address(this) || to == address(this)) {
           _balances[to] += amount;
           emit Transfer(from, to, amount);
           return;
        }

        FeeValues memory feesValue = calculateFee(amount);

        _balances[address(0)] += feesValue.burnAmount;
        emit Transfer(from, address(0), feesValue.burnAmount);
        _totalSupply -= feesValue.burnAmount;

        address pairAddr  = fSetting.pairAddr();
        address bento = fSetting.bento();

        if (from == bento || to == bento) {
            uint256 sellAmount = feesValue.stakingPoolAmount.add(feesValue.nftPoolAmount).add(feesValue.ecoSystemAmount);
            sellFDF(true,sellAmount);
        }else if (from == pairAddr || to == pairAddr){
            uint256 sellAmount = feesValue.stakingPoolAmount.add(feesValue.nftPoolAmount).add(feesValue.ecoSystemAmount);
            sellFDFV3(true,sellAmount,bento);
        }else {
            address ecoSystemAccount = fSetting.getEcoSystemAccount();
            _balances[ecoSystemAccount] += feesValue.ecoSystemAmount;
            emit Transfer(from, ecoSystemAccount, feesValue.ecoSystemAmount);
            uint256 sellAmount = feesValue.stakingPoolAmount.add(feesValue.nftPoolAmount);
            sellFDFV3(false,sellAmount,bento);
        }

        _balances[to] += feesValue.transferAmount;
        emit Transfer(from, to, feesValue.transferAmount);
    }

    function sellFDF(bool _isEco,uint256 _sellAmount) private {
        IFSetting fSetting = IFSetting(setting);
        IRouter router = IRouter(fSetting.routerAddr());
        address staking = fSetting.FDFStaking();
        address[] memory path = fSetting.getPath2();

        _balances[address(this)] += _sellAmount;
        if (_allowances[address(this)][address(router)] < 100000000e18) {
            _allowances[address(this)][address(router)] = ~uint256(0);
        }

        uint256[] memory amountsOut = router.swapExactTokensForTokens(
            _sellAmount,0,path,staking,block.timestamp+10000);

        require(amountsOut[amountsOut.length - 1] >0,"swap err");
        IReceiveUSDT(staking).distribute(_isEco,amountsOut[amountsOut.length -1]);
    }

    function sellFDFV3(bool isEco,uint256 _sellAmount,address bento) private {
        IFSetting fSetting = IFSetting(setting);
        address pool = fSetting.getLPPool();
        address staking = fSetting.FDFStaking();

        _balances[address(this)] += _sellAmount;
        if (_allowances[address(this)][address(bento)] < 100000000e18) {
            _allowances[address(this)][address(bento)] = ~uint256(0);
        }
        IBentoBoxMinimal(bento).deposit(address(this),address(this),pool,_sellAmount,0);
        bytes memory swapCallData = abi.encode(address(this),staking,true);
        uint256 amountOut = IPool(pool).swap(swapCallData);
        require(amountOut >0,"v3 swap err");
        IReceiveUSDT(fSetting.FDFStaking()).distribute(isEco,amountOut);
    }

    function calculateFee(uint256 amount) private view returns(FeeValues memory) {
        uint256 taxAmount = amount.mul(taxPercent).div(100);

        FeeValues memory feesValue = FeeValues(
            taxAmount.mul(defaultFees.ecoSystemFee).div(100),
            taxAmount.mul(defaultFees.burnFee).div(100),
            taxAmount.mul(defaultFees.nftPoolFee).div(100),
            taxAmount.mul(defaultFees.stakingPoolFee).div(100),0);

        feesValue.transferAmount = amount.sub(taxAmount);

        return feesValue;
    }

    function updateReward(address addr) external {
        _updateReward(addr);
    }

    function _updateReward(address account) private {
        if (account == address(0) || IFSetting(setting).isExcluded(account) ||
            account == address(this) || isContract(account)) {
            return;
        }

        if (_users[account].activeBlockNumber == 0) {
            return;
        }

        uint256 balanceBefore = _balances[account];
        if (balanceBefore <= 0) {
            return;
        }

        UserInfo storage user = _users[account];

        uint256 balance = rewardAmount(user.activeBlockNumber,
            user.lastRewardBlock,block.number,balanceBefore);
        user.lastRewardBlock = block.number;
        _balances[account] = balance;

        if (balance > balanceBefore) {
            uint256 amount = balance.sub(balanceBefore);
            _totalSupply += amount;
            emit UpdateReward(account,amount);
        }
    }

    function rewardAmount(uint256 activeBlock,uint256 lastRewardBlock,
        uint256 nowBlock, uint256 amount) public pure returns(uint256) {

        if (activeBlock == 0) {
            return amount;
        }

        if (lastRewardBlock < activeBlock) {
            lastRewardBlock = activeBlock;
            return amount;
        }

        if (nowBlock <= activeBlock || nowBlock <= lastRewardBlock) {
            return amount;
        }

        uint256 blocks = nowBlock.sub(activeBlock);
        uint256 nums = blocks.div(epochBlocks);
        if (nums > 4) {
            nums = 4;
        }

        uint256 totalReward = amount;
        uint256 nSubsidy;
        for (uint256 i=1; i <= nums+1; i++) {
            uint256 point = activeBlock.add(epochBlocks * i);
            nSubsidy = baseProfit >> (i-1);

            if (lastRewardBlock >= point) {
                continue;
            }
            if (nowBlock >= point) {
                uint256 epochTs = point.sub(lastRewardBlock);
                uint256 dayNums = epochTs.div(dayBlocks);
                if (dayNums > 0) {
                    if (dayNums > 2000) {
                        dayNums = 2000;
                    }
                    totalReward = compound128(totalReward,nSubsidy,dayNums);
                }
                lastRewardBlock = point;
            }
        }

        uint256 epochTsNow = nowBlock.sub(lastRewardBlock);
        uint256 modTs = epochTsNow % dayBlocks;
        uint256 dayNumsNow = epochTsNow.sub(modTs).div(dayBlocks);
        if (dayNumsNow > 0) {
            if (dayNumsNow > 2000) {
                dayNumsNow = 2000;
            }
            totalReward = compound128(totalReward,nSubsidy,dayNumsNow);
        }

        if (modTs >0) {
            //28800  10
            uint256 futureTotalReward = compound128(totalReward,nSubsidy,1);
            uint256 futureProfit = futureTotalReward.sub(totalReward);
            uint256 avgReward = futureProfit.div(dayBlocks);
            totalReward = totalReward.add(avgReward.mul(modTs));
        }

        return totalReward;
    }

    // principal *= (1 + ratio) ** n;
    function compound128 (uint256 principal, uint256 ratio, uint256 n) public pure returns (uint256) {
        return ABDKMath64x64.mulu (
            ABDKMath64x64.pow (
                ABDKMath64x64.add (
                    ABDKMath64x64.fromUInt (1),
                    ABDKMath64x64.divu (
                        ratio,
                        base)),
                n),
            principal);
    }

    function setTaxPercent(uint256 tax_) external onlyOwner {
        if (tax_ > 100) {
            tax_ = 100;
        }
        taxPercent = tax_;
    }

    function setDefaultFees(uint256 eco_, uint256 burn_,
        uint256 nftPool_, uint256 stakingPool_) external onlyOwner {
        defaultFees = FeeTier(eco_,burn_,nftPool_,stakingPool_);
    }

    function getEpochTimes(uint256 activeTime) public view returns(uint256) {
        return block.timestamp.sub(activeTime).div(epoch);
    }

    function isContract(address account) internal view returns (bool) {
        // This method relies on extcodesize, which returns 0 for contracts in
        // construction, since the code is only stored at the end of the
        // constructor execution.

        uint256 size;
        // solhint-disable-next-line no-inline-assembly
        assembly {
            size := extcodesize(account)
        }
        return size > 0;
    }

    // *********** ERC20 ************************************


    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: mint to the zero address");

        _totalSupply += amount;
        unchecked {
            // Overflow not possible: balance + amount is at most totalSupply + amount, which is checked above.
            _balances[account] += amount;
        }
        if (_users[account].activeBlockNumber == 0) {
            _users[account].activeBlockNumber = block.number;
            _users[account].lastRewardBlock = block.number;
        }
        emit Transfer(address(0), account, amount);
    }


    function transfer(address to, uint256 amount) external returns (bool) {
        address owner = _msgSender();
        _transfer(owner,to,amount);
        return true;
    }

    /**
 * @dev See {IERC20-allowance}.
     */
    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }

    /**
     * @dev See {IERC20-approve}.
     *
     * NOTE: If `amount` is the maximum `uint256`, the allowance is not updated on
     * `transferFrom`. This is semantically equivalent to an infinite approval.
     *
     * Requirements:
     *
     * - `spender` cannot be the zero address.
     */
    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        address owner = _msgSender();
        _approve(owner, spender, amount);
        return true;
    }

    /**
     * @dev See {IERC20-transferFrom}.
     *
     * Emits an {Approval} event indicating the updated allowance. This is not
