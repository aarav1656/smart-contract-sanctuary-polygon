import "hardhat/console.sol";

import "./interfaces/IKUtils.sol";
import "./interfaces/IUserProfiles.sol";
import "./interfaces/IMessageFormat.sol";

contract Comments {

    // Admins
    mapping (address => bool) admins;

    // Trusted Contracts
    mapping (address => bool) trustedContracts;

    // Maximum amount of records to return on query
    uint16 maxItemsReturn = 0;

    // Max number of items to store in a bucket in a mapping
    uint256 public maxItemsPerBucket;

    // Message Stats
    struct MsgStats {
        int likes;
        int comments;
        int reposts;
        int tipsReceived;
        uint postByContract;
        uint256 time;
        uint256 block;
    }

    // The Message data struct
    struct MsgData {
        uint msgID;
        address poster;
        string message;
        uint256 tip;
        uint256 paid;
        string[] hashtags;
        address[] taggedAccounts;
        string asGroup;
        uint256 isRepostOf;
        uint256 isCommentOf;
        string uri;
        uint256 commentLevel;
        MsgStats msgStats;
    }

    // Map the message address to a list of comment IDs and the address bucket of the poster
    // Address is a string for msg address "buckets" 0x123, 0x123-1, 0x123-2 ...
    mapping (string => uint256[]) public commentsMap;
    mapping (string => mapping (uint256 => bool)) public commentsMapMap;

    // Bucket Key => Msg ID => Msg Data
    mapping (string => mapping (uint256 => MsgData)) public commentsMsgData;
    
    // Link the KUtils
    IKUtils public KUtils;

    // Link the User Profiles contract
    IUserProfiles public userProfiles;

    // Link to the Message Formatter
    IMessageFormat private MessageFormat;

    constructor(address _kutils) {
        // Setup the default Admin
        admins[msg.sender] = true;
        trustedContracts[msg.sender] = true;

        maxItemsReturn = 4;
        maxItemsPerBucket = 3;

        require((maxItemsPerBucket + 1) >= maxItemsReturn, "Invalid Setup");

        // Setup link to KUtils
        KUtils = IKUtils(_kutils);
    }


    /*

    EVENTS

    */

    event logNewComment(uint256 indexed msgID, address indexed poster, MsgData newMsg, string indexed asGroup);
    event logRemoveComment(uint256 indexed msgID, uint256 indexed isCommentOf);
    event logUpdateCommentStats(uint256 indexed msgID, uint256 indexed statType, int256 statValue, uint256 indexed isCommentOf);



    /*

    MODIFIERS

    */

    modifier onlyAdmins() {
        require(admins[msg.sender], "Only admins can call this function.");
        _;
    }

    modifier onlyTrustedContracts() {
        require(trustedContracts[msg.sender], "Only trusted contracts can call this function.");
        _;
    }


    /*

    ADMIN FUNCTIONS

    */

    function addAdmin(address newAdmin) public onlyAdmins{
        admins[newAdmin] = true;
    }

    function removeAdmin(address oldAdmin) public onlyAdmins{
        admins[oldAdmin] = false;
    }

    function addContract(address newContract) public onlyAdmins{
        trustedContracts[newContract] = true;
    }

    function removeContract(address oldContract) public onlyAdmins{
        trustedContracts[oldContract] = false;
    }



    function updateContracts(address _kutils, address _userProfiles, address _messageFormat) public onlyAdmins {
        // Update the User KUtils address
        KUtils = IKUtils(_kutils);

        // Update the User Profiles contract address
        userProfiles = IUserProfiles(_userProfiles);

        // Update the Message Formatting
        MessageFormat = IMessageFormat(_messageFormat);
    }


    /*
    * Returns a list of messages that are posted by a given user
    * @param usrAddress : the address to retrieve the details for
    * @param startFrom : the number to start getting records from
    */
    function getCommentIDs(uint256 msgID, uint256 startFrom) public view returns(uint256[] memory) {
        // Lowercase the address
        string memory msgStr = KUtils.toString(msgID);

        // We may need to change this
        uint256 _maxRecords = maxItemsReturn;

        // Get the latest bucket
        uint256 bucketKeyID = getBucketKey(msgStr, false);

        // If they pass a 0, then return newest set
        if (startFrom == 0){
            startFrom = ((commentsMap[KUtils.append(msgStr,'-',KUtils.toString(bucketKeyID),'','')].length) + (bucketKeyID * maxItemsPerBucket));
            if (startFrom != 0){
                startFrom -= 1;
            } else {
                // It's empty, so end
                uint256[] memory empty = new uint256[](0);
                return empty;
            }
        }

        // Figure out where the list should be pulled from
        for (uint i=0; i <= bucketKeyID; i++) {
            string memory _bucketKey = KUtils.append(msgStr,'-',KUtils.toString(bucketKeyID),'','');

            // if the starting point is greater than the beginning item and less than the max in this bucket, this is the correct bucket
            if (startFrom >= (i * maxItemsPerBucket) && startFrom <= ((commentsMap[_bucketKey].length) + (i * maxItemsPerBucket))) {
                bucketKeyID = i;

                // Adjust the startFrom to work with this bucket
                if (i != 0){
                    startFrom = startFrom - (i * maxItemsPerBucket);
                }

            }
        }

        // Get the bucket key from where we will pull from
        string memory bucketKey = KUtils.append(msgStr,'-',KUtils.toString(bucketKeyID),'','');

        // Initialize the remainder bucket
        string memory remainderBucketKey = bucketKey;

        // Check if there's less than max records in this bucket and only go to the end
        if (startFrom < _maxRecords){
            _maxRecords = startFrom + 1;
        }

        // Initialize the count as empty;
        uint256 postCount = 0;

        // Loop through all items in the first bucket up to max return amount
        for (uint i=(startFrom + 1); i > (startFrom + 1 - _maxRecords); i--) {

            // Check that the item is still enabled
            if (commentsMapMap[bucketKey][commentsMap[bucketKey][i - 1]]){
                postCount += 1;
            }

        }

        // Figure out the amount left to get from remainder bucket
        uint amountLeft = maxItemsReturn - postCount;

        // Add the remainder from the next bucket if there are any
        if (bucketKeyID != 0 && amountLeft > 0){

            // Get the new bucket key from where we will pull the remainder from (the previous one)
            remainderBucketKey = KUtils.append(msgStr,'-',KUtils.toString((bucketKeyID - 1)),'','');

            for (uint i=commentsMap[remainderBucketKey].length; i > commentsMap[remainderBucketKey].length - amountLeft; i--) {
                // Add it if the item is still enabled
                if (commentsMapMap[remainderBucketKey][commentsMap[remainderBucketKey][i - 1]]){
                    postCount += 1;
                }
            }
        }

        // Start the array
        uint256[] memory commentIDs = new uint256[](postCount);

        // Counter to keep track of iterations since we're listing in reverse
        uint counter = 0;

        // Loop through all items in the first bucket up to max return amount
        for (uint i=(startFrom + 1); i > (startFrom + 1 - _maxRecords); i--) {
            if(commentsMapMap[bucketKey][commentsMap[bucketKey][i - 1]]){
                commentIDs[counter] = commentsMap[bucketKey][i - 1];
                counter += 1;
            }
        }

        // If there are more records to get from the next contract, check how many and KUtils.append them
        if (bucketKeyID != 0 && amountLeft > 0){
            // Loop through all items in the next bucket
            for (uint i=commentsMap[remainderBucketKey].length; i > commentsMap[remainderBucketKey].length - amountLeft; i--) {
                // Add it if the item is still enabled (have to do i-1 because if we go to zero it'll try to process -1 and fail next)
                if (commentsMapMap[remainderBucketKey][commentsMap[remainderBucketKey][i - 1]]){
                    commentIDs[counter] = commentsMap[remainderBucketKey][i - 1];
                    counter += 1;
                }
            }
        }


        delete msgStr;
        delete postCount;
        delete _maxRecords;
        delete bucketKeyID;
        delete bucketKey;
        delete remainderBucketKey;
        delete amountLeft;

        return commentIDs;
    }

    function getCommentsByIDs(uint256[] memory commentIDs, uint256 isCommentOf) public view returns (string memory){
        string memory allData;

        for (uint i=0; i < commentIDs.length; i++) {

            string memory thisBucketKey = getBucketKeyByID(commentIDs[i], false, isCommentOf);

            // Check to make sure the comment exists
            require(bytes(thisBucketKey).length > 0, append("Invalid Comment ID: ", KUtils.toString(commentIDs[i]), '', '', ''));

            // Get the message data from the bucket
            MsgData memory thisCommentMsgData = commentsMsgData[thisBucketKey][commentIDs[i]];

            uint256[] memory thisData = new uint256[](9);

            thisData[0] = thisCommentMsgData.msgID;
            thisData[1] = thisCommentMsgData.msgStats.time;
            thisData[2] = thisCommentMsgData.msgStats.block;
            thisData[3] = thisCommentMsgData.tip;
            thisData[4] = thisCommentMsgData.paid;
            thisData[5] = thisCommentMsgData.msgStats.postByContract;
            thisData[6] = uint(thisCommentMsgData.msgStats.likes);
            thisData[7] = uint(thisCommentMsgData.msgStats.reposts);
            thisData[8] = uint(thisCommentMsgData.msgStats.comments);

            // Get the formatted message
            string memory thisMsg = MessageFormat.buildMsg(thisData, thisCommentMsgData.message, thisCommentMsgData.poster, thisCommentMsgData.hashtags, thisCommentMsgData.taggedAccounts, thisCommentMsgData.asGroup);

            if (i == 0){
                allData = append('{"Messages": [', thisMsg, '','','');
            } else {
                allData = append(allData, ',', thisMsg,'','');
            }

            delete thisMsg;
            delete thisData;
        }

        if (bytes(allData).length > 0){
            allData = append(allData, ']}', '','','');
        }

        return allData;
    }



    function removeComment(uint256 commentID, address addressPoster, uint256 isCommentOf) public onlyAdmins {
        // Stringify the address
        string memory msgIDstr = KUtils.toString(isCommentOf);

        // Get the latest bucket on that user
        uint256 bucketKeyID = getBucketKey(msgIDstr, false);

        // Loop through each bucket and set the comment to false
        for (uint i=0; i <= bucketKeyID; i++) {
            string memory postBucketKey = KUtils.append(msgIDstr,'-',KUtils.toString(i),'','');
            // Update the map map for quick removal
            commentsMapMap[postBucketKey][commentID] = false;
        }

        // Update their total post count
        userProfiles.updatePostCount(addressPoster, 1, false);

        // Emit to the logs for external reference
        emit logRemoveComment(commentID, isCommentOf);

        delete msgIDstr;
        delete bucketKeyID;
    }


    function saveComment(MsgData memory newComment) public onlyAdmins {
        // Check if this is a repost, and if so, update that messages updateStats
        require(newComment.isRepostOf == 0, "You can't repost as a comment");

        // Check if this is a comment, and if so, update that messages updateStats
        require(newComment.isCommentOf != 0, "Need to know what msgID this is a comment of");

        // Get the bucket to store the message
        string memory commentOf = KUtils.toString(newComment.isCommentOf);
        uint256 bucketKeyID = getBucketKey(commentOf, true);
        string memory thisBucketKey = KUtils.append(commentOf, '-',KUtils.toString(bucketKeyID),'','');

        // Update the Comments Mapping with the new message ID
        commentsMap[thisBucketKey].push(newComment.msgID);

        // Save the Comments MessageData
        commentsMsgData[thisBucketKey][newComment.msgID] = newComment;

        // Update the Comments Map Map for quick removal
        commentsMapMap[thisBucketKey][newComment.msgID] = true;

        // Record the post to the poster
        // TODO: Need to keep track of users' comments
//        Posts.addPost(newComment.msgID, newComment.poster);

        // Emit to the logs for external reference
        emit logNewComment(newComment.msgID, newComment.poster, newComment, newComment.asGroup);

        delete bucketKeyID;
        delete thisBucketKey;
        delete newComment;

    }

    // statType
    // 1 = like
    // 2 = comment
    // 3 = repost
    // 4 = tip
    function addStat(uint8 statType, uint256 commentID, int amount, uint256 isCommentOf) public onlyAdmins {
        // Find the bucket where this message is
        string memory thisBucketKey = getBucketKeyByID(commentID, false, isCommentOf);

        MsgData memory thisCommentMsgData = commentsMsgData[thisBucketKey][commentID];

        if (statType == 1){
            thisCommentMsgData.msgStats.likes += amount;
        } else if (statType == 2){
            thisCommentMsgData.msgStats.comments += amount;
        } else if (statType == 3){
            thisCommentMsgData.msgStats.reposts += amount;
        } else if (statType == 4){
            thisCommentMsgData.msgStats.tipsReceived += amount;
        }

        commentsMsgData[thisBucketKey][commentID] = thisCommentMsgData;

        // Emit to the logs for external reference
        emit logUpdateCommentStats(commentID, statType, amount, isCommentOf);
    }

    function getBucketKeyByID(uint256 msgID, bool forInsert, uint256 isCommentOf) internal view returns (string memory){
        // Initialize the bucket key
        string memory thisBucketKey;

        // Initialize the bucket prefix
        string memory bucketKeyPrefix = KUtils.toString(isCommentOf);
        
        uint256 topBucketKey = getBucketKey(bucketKeyPrefix, forInsert);

        // Go through each bucket to see if it's there in reverse
        for (uint b=topBucketKey; b >= 0; b--) {
            // Get the next bucket key
            thisBucketKey = KUtils.append(bucketKeyPrefix, '-',KUtils.toString(b),'','');

            // Get the poster address from the message for a post
            address poster = commentsMsgData[thisBucketKey][msgID].poster;

            if (poster != address(0)) {
                // We found the bucket with the message in it
                break;
            } else if (b == 0){
                thisBucketKey = '';
                break;
            }
        }

        return thisBucketKey;
    }

    function getBucketKey(string memory mapKey, bool toInsert) private view returns (uint256){
        uint256 prevBucketLen = 0;
        uint256 b = 0;
        uint256 mapID = 99999999999999999999;

        while (mapID == 99999999999999999999){
            // Get the bucket key to check
            string memory bucketToCheck = KUtils.append(mapKey,'-',KUtils.toString(b),'','');
            if (commentsMap[bucketToCheck].length > 0){
                // exists
                b++;
                prevBucketLen = commentsMap[bucketToCheck].length;
            } else if (b == 0) {
                // Doesn't exist at all, so set it to 0
                mapID = b;
            } else {
                // It's the previous one
                mapID = b - 1;

                // If we're inserting, check to see if the previous bucket is full and we should insert to a new one
                if (prevBucketLen >= maxItemsPerBucket && toInsert == true){
                    // We've reached the max items per bucket so return the next key (which is this one)
                    mapID = b;
                }
            }
        }

        delete b;
        delete prevBucketLen;
        return mapID;
    }

    function append(string memory a, string memory b, string memory c, string memory d, string memory e) internal pure returns (string memory) {
        return string(abi.encodePacked(a, b, c, d, e));
    }
}

