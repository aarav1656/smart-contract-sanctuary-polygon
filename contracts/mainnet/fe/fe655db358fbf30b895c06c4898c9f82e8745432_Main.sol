abstract contract Config is IConfig, Pausable, Ownable, ERC2771Recipient {
    address private immutable _recovery;
    address private immutable sysAdmin;
    error Disabled();

    constructor(address _recoveryIn, address _projectAdminIn, address _trustedForwarderIn, address _sysAdminIn) {
        require(_recoveryIn != address(0), "Recovery address should not be Zero!");
        _recovery = _recoveryIn;
        _transferOwnership(_projectAdminIn);
        _setTrustedForwarder(_trustedForwarderIn);
        sysAdmin = _sysAdminIn;
    }

    function changeTrustedForwarder(address _newAddress) external {
        require(sysAdmin == _msgSender(), "unAuthorized!");
        require(_newAddress != address(0), "Config:Trusted Forwarder address should not be Zero!");

        _setTrustedForwarder(_newAddress);
    }

    modifier onlyRecoveryWallet() {
        require(_recovery == _msgSender(), "You are not authorized!");
        _;
    }

    function _msgSender() internal view override(Context, ERC2771Recipient) returns (address sender) {
        sender = ERC2771Recipient._msgSender();
    }

    function _msgData() internal view override(Context, ERC2771Recipient) returns (bytes calldata) {
        return ERC2771Recipient._msgData();
    }

    function pause() external override onlyOwner {
        _pause();
    }

    function unpause() external override onlyOwner {
        _unpause();
    }

    function changeAdmin(address _newAdmin) external virtual override onlyRecoveryWallet {
        require(_newAdmin != address(0), "Config: New admin address can not be Zero!");
        require(_newAdmin != owner(), "Config: New admin is the same!");
        _transferOwnership(_newAdmin);
        emit AdminChanged(_newAdmin);
    }

    // to limit ownership transfer to {changeAdmin} function only.
