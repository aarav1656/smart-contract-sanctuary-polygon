pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract Katjes721 is ERC721, ERC721Burnable, ERC721Enumerable, AccessControl {
    // Enable counters for token ids and set max supply
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;
    uint256 public maxSupply;

    // The price for minting 3 tokens
    uint256 public mintPrice;

    // Create roles
    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant MEMBERR_ROLE = keccak256("MEMBERR_ROLE");
    bytes32 public constant UNICORN_MINT_ROLE = keccak256("UNICORN_MINT_ROLE");
    bytes32 public constant AIRDROP_ROLE = keccak256("AIRDROP_ROLE");

    // The base URI for all tokens
    string private baseUri;

    // The address that receives the funds
    address payable private receiver;

    // The minting enabled status
    bool public mintingEnabled = true;

    // Password recovery enabled
    bool public passwordRecoveryEnabled = true;

    // The contract constructor function sets the name, symbol, base URI, max supply, mint price and receiver address
    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseUri,
        uint256 _maxSupply,
        uint256 _mintPrice,
        address payable _receiver
    ) ERC721(_name, _symbol) {
        // set the admin role and the member role
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MEMBERR_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        baseUri = _baseUri;
        receiver = _receiver;
        maxSupply = _maxSupply;

        // start the token id counter at 1
        _tokenIdCounter.increment();

        // set the mint price
        setMintPrice(_mintPrice);
    }

    /* Set the minting enabled status.
     * @param _mintingEnabled bool of the new minting enabled status
     */
    function setMintingEnabled(
        bool _mintingEnabled
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        mintingEnabled = _mintingEnabled;
    }

    /* Set the mint price.
     * @param _mintPrice uint of the new mint price
     */
    function setMintPrice(
        uint256 _mintPrice
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        mintPrice = _mintPrice;
    }

    /* Set the receiver address.
     * @param _receiver address of the new receiver
     */
    function setReceiver(
        address payable _receiver
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        receiver = _receiver;
    }

    /* Set the base token URI.
     * @param _baseUri string of the base URI
     */
    function setBaseTokenURI(
        string memory _baseUri
    ) public onlyRole(URI_SETTER_ROLE) {
        baseUri = _baseUri;
    }

    /* Disable the password recovery.
     */
    function disablePasswordRecovery() public onlyRole(DEFAULT_ADMIN_ROLE) {
        passwordRecoveryEnabled = false;
    }

    /* @dev Let anyone mint X tokens for a fee.
     * @param to address of the future owner of the token
     */
    function safeMint(uint amount) public payable {
        // check if eth value is sufficient
        require(
            msg.value == mintPrice * amount,
            "Eth value is not sufficient."
        );

        // send funds to contract payout address
        receiver.transfer(msg.value);

        _mintXClouds(msg.sender, amount);
    }

    /* @dev Let wallets with the member role mint X tokens for free.
     * @param to address of the future owner of the token
     */
    function memberrMint(
        address to,
        uint amount
    ) public onlyRole(MEMBERR_ROLE) {
        _mintXClouds(to, amount);
    }

    /* Lets wallets with the airdrop role mint 1 token for free.
     * @param to address of the future owner of the token
     */
    function airdrop(address to) public onlyRole(AIRDROP_ROLE) {
        _mintSingleCloud(to);
    }

    /* Airdrops many tokens to many addresses.
     * @param tos array of addresses of the future owners of the token
     */
    function airdropMany(
        address[] memory holder
    ) public onlyRole(AIRDROP_ROLE) {
        for (uint256 i = 0; i < holder.length; i++) {
            _mintSingleCloud(holder[i]);
        }
    }

    /* @dev Mint 1 token to the given address.
     * @param to address of the future owner of the token
     */
    function _mintSingleCloud(address to) private {
        require(_checkMaxSupply(), "Max supply reached.");
        require(_checkMintingEnabled(), "Minting phase is over.");
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
    }

    /* @dev Mints 3 tokens to the given address.
     * @param to address of the future owner of the token
     */
    function _mintXClouds(address to, uint amount) private {
        // a for loop that mints 3 tokens
        for (uint256 i = 0; i < amount; i++) {
            _mintSingleCloud(to);
        }
    }

    /* @dev Checks if the minting is enabled.
     * @return bool
     */
    function _checkMintingEnabled() private view returns (bool) {
        return mintingEnabled;
    }

    /* @dev Checks if the max supply has been reached.
     * @return bool
     */
    function _checkMaxSupply() private view returns (bool) {
        // check if there are less than 1000 tokens minted
        return _tokenIdCounter.current() < maxSupply;
    }

    /* @dev Checks if the password recovery is enabled.
     * @return bool
     */
    function _checkPasswordRecoveryEnabled() private view returns (bool) {
        return passwordRecoveryEnabled;
    }

    /* Allow MEMBERR_ROLE to recover tokens from account if password recovery is enabled.
     * @dev This function is used to recover tokens from an account if the user forgot his password.
     * @dev First we collect all tokens the user owns and then transfer them to the recovery account.
     * @param oldAccount address of the old account
     * @param recoveryAccount address of the recovery account
     */
    function recoverTokens(
        address oldAccount,
        address recoveryAccount
    ) public onlyRole(MEMBERR_ROLE) {
        require(
            _checkPasswordRecoveryEnabled(),
            "Password recovery is disabled."
        );

        // transfer all katjes721 tokens from old account to recovery account
        // using tokenOfOwnerByIndex to get all tokens
        uint256 tokenCount = balanceOf(oldAccount);
        uint256[] memory tokens = new uint256[](tokenCount);
        for (uint256 i = 0; i < tokenCount; i++) {
            tokens[i] = tokenOfOwnerByIndex(oldAccount, i);
        }

        // Splitting this operation in to loops because of index out of bounds error
        for (uint256 i = 0; i < tokenCount; i++) {
            _transfer(oldAccount, recoveryAccount, tokens[i]);
        }
    }

    /* returns the token URI.
     * @param _tokenId uint256 ID of the token to query
     * @return string URI of _tokenId
     */
    function tokenURI(
        uint256 _tokenId
    ) public view override returns (string memory) {
        return
            string(
                abi.encodePacked(baseUri, Strings.toString(_tokenId), ".json")
            );
    }

    /* overwrites the burn function to check if the sender is the owner of the token or has the unicorn mint role.
     * @param tokenId uint256 ID of the token to be burned
     */
    function burn(uint256 tokenId) public override {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId) ||
                hasRole(UNICORN_MINT_ROLE, _msgSender()),
            "ERC721Burnable: caller is not owner nor approved"
        );
        _burn(tokenId);
    }

    // The following functions are overrides required by Solidity.
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId,
        uint256 batchSize
    ) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        override(ERC721, ERC721Enumerable, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}