// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "contracts/Katjes721.sol";

contract Katjes1155 is ERC1155, AccessControl, ERC1155Burnable, ERC1155Supply {
    // The following variables are optional metadata.
    string private _name;
    string private _symbol;

    // The following roles are used by different functionalities of the contract.
    bytes32 public constant URI_SETTER_ROLE = keccak256("URI_SETTER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    // The address of the Katjes721 contract
    address public Katjes721Address;

    // Token assignment
    uint256 public constant UNICORN_1 = 1;
    uint256 public constant UNICORN_2 = 2;
    uint256 public constant UNICORN_3 = 3;
    uint256 public constant FAIRY_1 = 4;
    uint256 public constant FAIRY_2 = 5;
    uint256 public constant FAIRY_3 = 6;
    uint256 public constant FAIRY_4 = 7;

    uint public staticOffset;
    uint public staticSupply;

    // Define the unicorn assignment to the Katjes721 token ids
    mapping(uint256 => uint256) public unicornAssignment;

    // Struct that is used to input the unicorn mapping data
    struct UnicornMapping {
        uint256 tokenId;
        uint256 unicornId;
    }

    // Struct that is used to display a benefit that can be claimed
    struct Benefit {
        string name;
        uint256 fairyId;
        uint256 amountLeft;
        uint256 pricePool;
    }

    // Array of benefits that can be claimed
    Benefit[] public benefits;

    // Mapping of Array of benefits that are claimed by the users
    mapping(address => uint256[]) public claimedBenefits;

    // The minting enabled status
    bool public claimingEnabled = false;

    constructor(
        string memory _uri,
        string memory _setName,
        string memory _setSymbol,
        address _Katjes721Address
    ) ERC1155(_uri) {
        _name = _setName;
        _symbol = _setSymbol;
        Katjes721Address = _Katjes721Address;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(URI_SETTER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
    }

    // The following functions are getters for the optional metadata.
    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    /*
     * The setURI function is used to set the base URI for all tokens.
     * It can only be called by an account with the URI_SETTER_ROLE.
     */
    function setURI(string memory newuri) public onlyRole(URI_SETTER_ROLE) {
        _setURI(newuri);
    }

    /* Set the claiming enabled status.
     * @param _claimingEnabled bool of the new claiming enabled status
     */
    function setclaimingEnabled(
        bool _claimingEnabled
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        claimingEnabled = _claimingEnabled;
    }

    /* Set the unicorn assignment
     * This function is used to set the unicorn assignment.
     * This function converst a list of ids to a mapping.
     * It can only be called by an account with the DEFAULT_ADMIN_ROLE.
     * @param _unicornMapping array of UnicornMapping structs
     */
    function setUnicornAssignment(
        UnicornMapping[] memory _unicornMapping
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < _unicornMapping.length; i++) {
            unicornAssignment[_unicornMapping[i].tokenId] = _unicornMapping[i]
                .unicornId;
        }
    }

    /* Set unicorn assignment directly
     * This function is used to set the unicorn assignment directly.
     * It can only be called by an account with the DEFAULT_ADMIN_ROLE.
     * @param _unicornMapping []uint256 array of the token ids
     */
    function setUnicornAssignmentDirectly(
        uint256[] memory _unicornMapping
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < _unicornMapping.length; i++) {
            unicornAssignment[i + 1] = _unicornMapping[i];
        }
    }

    function calculateOffset() public virtual {
        // Set the Katjes721 contract
        Katjes721 Katjes721Contract = Katjes721(Katjes721Address);
        staticSupply = Katjes721Contract.totalSupply();

        staticOffset = _calculateOffsetDeterministic(
            block.timestamp,
            block.prevrandao
        );
    }

    /* Calculate offset deterministic
     * This helper function is used to make the actual calulation for the offset with deterministic parameters.
     */

    function _calculateOffsetDeterministic(
        uint timestamp,
        uint difficulty
    ) internal view returns (uint256) {
        // calculate random offset
        uint offset = uint256(
            keccak256(abi.encodePacked(timestamp, difficulty, msg.sender))
        ) % staticSupply;

        return offset;
    }

    /* Reveal unicorns
     * For every user that has a token in the Katjes721 contract a unicorn will be minted.
     * This function will reveal the unicorns for all tokens
     * The contract checks which user has a token in the Katjes721 contract.
     * If the user has a token, the contract checks the unicornAssignment mapping.
     * If the user has a token, the contract mints a unicorn token.
     * The contract burns the Katjes721 token.
     */
    function revealUnicorns(
        uint start,
        uint stop
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        // Set the Katjes721 contract
        Katjes721 Katjes721Contract = Katjes721(Katjes721Address);

        // calculate random offset
        // Offset cant be zero
        require(staticOffset != 0, "Offset is zero");
        uint offset = staticOffset;

        // For eac token in the Katjes721 contract
        for (uint256 i = start; i <= stop; i++) {
            // Get the unicornAssignment for the token with offset and modulo
            uint256 unicornAssignmentId = unicornAssignment[
                ((i + offset - 1) % staticSupply) + 1
            ];
            // Get the onwer of the token in the Katjes721 contract
            address owner = Katjes721Contract.ownerOf(i);
            // Require that the owner has a token in the Katjes721 contract and the unicornAssignment is not 0
            require(unicornAssignmentId != 0, "No unicorn assigned");
            require(owner != address(0), "No owner");
            // Mint the unicorn token
            _mint(owner, unicornAssignmentId, 1, "");
            // Burn the Katjes721 token
            Katjes721Contract.burn(i);
        }
    }

    /* Add a benefit
     * This function is used to add a benefit to the benefits array.
     * It can only be called by an account with the DEFAULT_ADMIN_ROLE.
     * @param _benefitName name of the benefit
     * @param _fairyId id of the fairy
     * @param _amountLeft amount of benefits that can be claimed
     * @param _pricePool price pool of the benefit
     */
    function addBenefit(
        string memory _benefitName,
        uint256 _fairyId,
        uint256 _amountLeft,
        uint256 _pricePool
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        benefits.push(Benefit(_benefitName, _fairyId, _amountLeft, _pricePool));
    }

    /* Add a list of benefits
     * This function is used to add a list of benefits to the benefits array.
     * It can only be called by an account with the DEFAULT_ADMIN_ROLE.
     * Benefits can never be removed because of the way the benefits are claimed.
     * Removing a benefit would mean that the benefits that are claimed by users would be incorrect.
     * @param _benefits array of Benefit structs
     */
    function addBenefits(
        Benefit[] memory _benefits
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint256 i = 0; i < _benefits.length; i++) {
            benefits.push(_benefits[i]);
        }
    }

    /* set the amount of benefits that can be claimed
     * This function is used to set the amount of benefits that can be claimed.
     * It can only be called by an account with the DEFAULT_ADMIN_ROLE.
     * @param _benefitId id of the benefit
     * @param _amount amount to set
     */
    function setAmountLeft(
        uint256 _benefitId,
        uint256 _amount
    ) public onlyRole(DEFAULT_ADMIN_ROLE) {
        benefits[_benefitId].amountLeft = _amount;
    }

    /* determine price pool
     * This function is used to determine the price pool.
     * It checks the amount of unicorn tokens that are burned.
     * Depending on the amount of unicorn tokens, the price pool is determined.
     * Depending on the type of unicorn tokens the user burns, the contract will mint a benefit token.
     * The UNICORN_1 token is the most valuable token.
     * If you have three UNICORN_1 tokens, you will be in the PRICE_POOL_1.
     * If you have two UNICORN_1 tokens and one UNICORN_2 token, you will be in the PRICE_POOL_2.
     * If you have two UNICORN_1 tokens and one UNICORN_3 token, you will be in the PRICE_POOL_3.
     * If you have one UNICORN_1 token, and two UNICORN_2 tokens, you will be in the PRICE_POOL_4.
     * This can be encoded like this:
     * 1, 1, 1 = 1
     * 1, 1, 2 = 2
     * 1, 1, 3 = 3
     * 1, 2, 2 = 4
     * 1, 2, 3 = 5
     * 1, 3, 3 = 6
     * 2, 2, 2 = 7
     * 2, 2, 3 = 8
     * 2, 3, 3 = 9
     * 3, 3, 3 = 10
     * The contract checks if the user has enough tokens to claim a benefit.
     * The contract burns the unicorn tokens.
     * The user can claim a benefit of the PRICE_POOL that the user is in or lower.
     * Which means that if you are in the PRICE_POOL_1, you can claim every benefit.
     * If you are in the PRICE_POOL_2, you can claim every benefit with a PRICE_POOL_2 or lower.
     * If you are in the PRICE_POOL_10, you can only claim the benefit with a PRICE_POOL_10.
     * @param _amountOfUnicorn1 amount of UNICORN_1 tokens
     * @param _amountOfUnicorn2 amount of UNICORN_2 tokens
     * @param _amountOfUnicorn3 amount of UNICORN_3 tokens
     * @return price pool
     */
    function determinePricePool(
        uint256 _amountOfUnicorn1,
        uint256 _amountOfUnicorn2,
        uint256 _amountOfUnicorn3
    ) public pure returns (uint256) {
        // determine the price pool

        require(
            _amountOfUnicorn1 + _amountOfUnicorn2 + _amountOfUnicorn3 == 3,
            "You must burn exactly three tokens"
        );
        uint256 pricePool = 11;
        if (_amountOfUnicorn1 == 3) {
            pricePool = 1;
        } else if (_amountOfUnicorn1 == 2 && _amountOfUnicorn2 == 1) {
            pricePool = 2;
        } else if (_amountOfUnicorn1 == 2 && _amountOfUnicorn3 == 1) {
            pricePool = 3;
        } else if (_amountOfUnicorn1 == 1 && _amountOfUnicorn2 == 2) {
            pricePool = 4;
        } else if (
            _amountOfUnicorn1 == 1 &&
            _amountOfUnicorn2 == 1 &&
            _amountOfUnicorn3 == 1
        ) {
            pricePool = 5;
        } else if (_amountOfUnicorn1 == 1 && _amountOfUnicorn3 == 2) {
            pricePool = 6;
        } else if (_amountOfUnicorn2 == 3) {
            pricePool = 7;
        } else if (_amountOfUnicorn2 == 2 && _amountOfUnicorn3 == 1) {
            pricePool = 8;
        } else if (_amountOfUnicorn2 == 1 && _amountOfUnicorn3 == 2) {
            pricePool = 9;
        } else if (_amountOfUnicorn3 == 3) {
            pricePool = 10;
        }
        return pricePool;
    }

    /* claim benefit
     * This function is used to claim a benefit.
     * To claim a benefit you have to combine three unicorn tokens and burn them.
     * The contract checks if the user has three unicorn tokens.
     * Then it determines the best price pool the user could claim.
     * The contract checks if the benefit is in the price pool or lower and if there are enough benefits left.
     * The contract burns the unicorn tokens.
     * The contract stores the benefit id in the claimedBenefits mapping.
     * The contract mints a fairy token and returns the fairy id.
     * @param _amountOfUnicorn1 amount of UNICORN_1 tokens
     * @param _amountOfUnicorn2 amount of UNICORN_2 tokens
     * @param _amountOfUnicorn3 amount of UNICORN_3 tokens
     * @param _benefitId id of the benefit
     * @return fairy id
     */
    function claimBenefit(
        address _beneficiary,
        uint256 _amountOfUnicorn1,
        uint256 _amountOfUnicorn2,
        uint256 _amountOfUnicorn3,
        uint256 _benefitId
    ) public onlyRole(DEFAULT_ADMIN_ROLE) returns (uint256) {
        // require claiming is enabled
        require(claimingEnabled, "Claiming is disabled");

        // Check if the user has enough tokens to claim a benefit
        require(
            _amountOfUnicorn1 + _amountOfUnicorn2 + _amountOfUnicorn3 == 3,
            "You must burn exactly three tokens"
        );
        // Check if the user has enough UNICORN_1 tokens
        require(
            balanceOf(_beneficiary, UNICORN_1) >= _amountOfUnicorn1,
            "Not enough UNICORN_1 tokens"
        );
        // Check if the user has enough UNICORN_2 tokens
        require(
            balanceOf(_beneficiary, UNICORN_2) >= _amountOfUnicorn2,
            "Not enough UNICORN_2 tokens"
        );
        // Check if the user has enough UNICORN_3 tokens
        require(
            balanceOf(_beneficiary, UNICORN_3) >= _amountOfUnicorn3,
            "Not enough UNICORN_3 tokens"
        );

        // Determine the price pool
        uint256 possiblePricePool = determinePricePool(
            _amountOfUnicorn1,
            _amountOfUnicorn2,
            _amountOfUnicorn3
        );

        // Check if claimed benefit is in the price pool
        // Requires that the possible price pool is equal or SMALLER than the price pool of the benefit
        // (small numbers == good price pool)
        require(
            possiblePricePool <= benefits[_benefitId].pricePool,
            "You are not in the right price pool"
        );

        // Check if there are benefits left
        require(
            benefits[_benefitId].amountLeft > 0,
            "No benefits left in this category"
        );

        // Burn the UNICORN_1 tokens
        _burn(_beneficiary, UNICORN_1, _amountOfUnicorn1);
        // Burn the UNICORN_2 tokens
        _burn(_beneficiary, UNICORN_2, _amountOfUnicorn2);
        // Burn the UNICORN_3 tokens
        _burn(_beneficiary, UNICORN_3, _amountOfUnicorn3);
        // Decrease the amount of benefits left
        benefits[_benefitId].amountLeft--;

        // Store the benefit id in the claimedBenefits mapping
        claimedBenefits[_beneficiary].push(_benefitId);

        // Get the fairy id
        uint256 fairyId = benefits[_benefitId].fairyId;

        // Mint the fairy token
        _mint(_beneficiary, fairyId, 1, "");

        // Return the fairy id
        return fairyId;
    }

    /* get claimed benefits
     * This function is used to get all benefits that a user has claimed.
     * Which means that when you have an array with [1, 0, 2] it
     * means that you have claimed benefits with the ids 1, 0 and 2.
     * @param _user address of the user
     * @return array of benefit ids
     */
    function getClaimedBenefits(
        address _user
    ) public view returns (uint256[] memory) {
        return claimedBenefits[_user];
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155, ERC1155Supply) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view override(ERC1155, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

