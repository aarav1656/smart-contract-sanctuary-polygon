contract AlantixBusiness is Ownable {
    struct UpgradeAddress {
        bool canUpgradeFlag;
        string upgradeAlert;
        uint256 feeAmount;
        uint256 parentAmount;
        address feeAccountAddress;
        address payableParentAddress;
        address[] missAddress;
        uint256 missAddressCount;
    }

    address private userDataContractAddress;
    address private alantixHistoryContractAddress;
    address private feeAccountAddress;
    address private tixTokenContractAddress;
    address private usdtTokenContractAddress;

    uint256 private airdropOneMax = 500000;
    uint256 private airdropOneUnit = 100;
    uint256 private airdropStartLevel = 6;
    uint256 private airdropTwoMax = 500000;
    uint256 private airdropTwoUnit = 200;
    uint256 private airdropTwoChildrenCount = 5;

    constructor(
        address userDataAddress,
        address alantixHistoryAddress,
        address feeAddress,
        address tixAddress,
        address usdtAddress
    ) {
        userDataContractAddress = userDataAddress;
        alantixHistoryContractAddress = alantixHistoryAddress;
        feeAccountAddress = feeAddress;
        tixTokenContractAddress = tixAddress;
        usdtTokenContractAddress = usdtAddress;
    }

    // Contract Core Functions
    function rewardProcess() external onlyOwner {
        uint256 totalSpeed = 0;
        UserDataInterface.Member[] memory members;
        members = UserDataInterface(userDataContractAddress).getMemberList();
        bool isAirdropFinished = UserDataInterface(userDataContractAddress)
            .getAirdropState();
        totalSpeed = getTotalSpeed();
        uint256 rewardCount = UserDataInterface(userDataContractAddress)
            .getRewardCount();

        if (isAirdropFinished && totalSpeed != 0) {
            uint256 miningSpeed = 7000 ether / totalSpeed;
            for (uint256 i = 0; i < members.length; i++) {
                uint256 speed = calculateSpeedByLevel(members[i].level);
                uint256 yesterdayAmount = members[i].todayClaimableAmount;
                UserDataInterface(userDataContractAddress).setMemberAttribute(
                    members[i].untilYesterdayClaimableAmount + yesterdayAmount,
                    members[i].addr,
                    "UNTIL_YESTERDAY_CLAIMABLE_AMOUNT"
                );
                UserDataInterface(userDataContractAddress).setMemberAttribute(
                    speed * miningSpeed,
                    members[i].addr,
                    "TODAY_CLAIMABLE_AMOUNT"
                );
                if (speed > 0) {
                    AlantixHistoryInterface(alantixHistoryContractAddress)
                        .emitMiningEvent(members[i].addr, speed * miningSpeed);
                }
            }
            rewardCount++;
            UserDataInterface(userDataContractAddress).setRewardCount(
                rewardCount
            );
        }
    }

    function transferTixToken(uint256 amount, address addr) external onlyOwner {
        require(
            IERC20(tixTokenContractAddress).balanceOf(address(this)) >= amount,
            "Tix token amount is not enough!"
        );
        IERC20(tixTokenContractAddress).transfer(addr, amount);
    }

    function claim() external returns (bool) {
        bool canClaimFlag;
        string memory claimAlert;
        uint256 claimableAmount;

        (canClaimFlag, claimAlert, claimableAmount) = canClaim(msg.sender);

        if (canClaimFlag) {
            IERC20(tixTokenContractAddress).transfer(
                msg.sender,
                claimableAmount
            );

            bool isRegistered;
            UserDataInterface.Member memory myData;
            (isRegistered, myData) = UserDataInterface(userDataContractAddress)
                .getMemberByAddress(msg.sender);

            uint256 newRewardEarning = myData.totalRewardEarning +
                claimableAmount;
            UserDataInterface(userDataContractAddress).setMemberAttribute(
                newRewardEarning,
                msg.sender,
                "TOTAL_REWARD_EARNING"
            );
            UserDataInterface(userDataContractAddress).setMemberAttribute(
                0,
                msg.sender,
                "TODAY_CLAIMABLE_AMOUNT"
            );
            UserDataInterface(userDataContractAddress).setMemberAttribute(
                0,
                msg.sender,
                "UNTIL_YESTERDAY_CLAIMABLE_AMOUNT"
            );

            AlantixHistoryInterface(alantixHistoryContractAddress)
                .emitClaimEvent(msg.sender, claimableAmount);

            return true;
        }

        return false;
    }

    function upgradeLevel() external returns (bool, string memory) {
        bool canUpgradeFlag;
        string memory upgradeAlert;
        uint256 feeAmount;
        uint256 parentAmount;
        address payableParentAddress;
        address feeAddress;
        address[] memory missAddress;
        uint256 missAddressCount = 0;
        (
            canUpgradeFlag,
            upgradeAlert,
            feeAmount,
            parentAmount,
            feeAddress,
            payableParentAddress,
            missAddress,
            missAddressCount
        ) = canUpgrade(msg.sender);

        if (!canUpgradeFlag) return (false, upgradeAlert);

        bool isRegistered;
        UserDataInterface.Member memory myData;
        (isRegistered, myData) = UserDataInterface(userDataContractAddress)
            .getMemberByAddress(msg.sender);
        myData.level++;
        UserDataInterface(userDataContractAddress).setMemberAttribute(
            myData.level,
            msg.sender,
            "LEVEL"
        );

        IERC20(usdtTokenContractAddress).transferFrom(
            msg.sender,
            feeAddress,
            feeAmount
        );
        IERC20(usdtTokenContractAddress).transferFrom(
            msg.sender,
            payableParentAddress,
            parentAmount
        );

        AlantixHistoryInterface(alantixHistoryContractAddress).emitUpgradeEvent(
            msg.sender,
            myData.level,
            payableParentAddress,
            parentAmount,
            feeAddress,
            feeAmount
        );

        // Miss usdt earning calculation
        UserDataInterface.Member memory feeAccountMemberData;
        (isRegistered, feeAccountMemberData) = UserDataInterface(
            userDataContractAddress
        ).getMemberByAddress(feeAddress);
        feeAccountMemberData.earnUSDTAmount += feeAmount;
        UserDataInterface(userDataContractAddress).setMemberAttribute(
            feeAccountMemberData.earnUSDTAmount,
            feeAddress,
            "EARN_USDT_AMOUNT"
        );

        AlantixHistoryInterface(alantixHistoryContractAddress)
            .emitUSDTEarningEvent(
                "earned",
                msg.sender,
                feeAddress,
                feeAmount,
                myData.level
            );

        UserDataInterface.Member memory payableParentMemberData;
        (isRegistered, payableParentMemberData) = UserDataInterface(
            userDataContractAddress
        ).getMemberByAddress(payableParentAddress);
        payableParentMemberData.earnUSDTAmount += parentAmount;
        UserDataInterface(userDataContractAddress).setMemberAttribute(
            payableParentMemberData.earnUSDTAmount,
            payableParentAddress,
            "EARN_USDT_AMOUNT"
        );

        AlantixHistoryInterface(alantixHistoryContractAddress)
            .emitUSDTEarningEvent(
                "earned",
                msg.sender,
                payableParentAddress,
                parentAmount,
                myData.level
            );

        for (uint256 i = 0; i < missAddressCount; i++) {
            UserDataInterface.Member memory missedMemberData;
            (isRegistered, missedMemberData) = UserDataInterface(
                userDataContractAddress
            ).getMemberByAddress(missAddress[i]);
            missedMemberData.missUSDTAmount += parentAmount;
            UserDataInterface(userDataContractAddress).setMemberAttribute(
                missedMemberData.missUSDTAmount,
                missAddress[i],
                "MISS_USDT_AMOUNT"
            );

            AlantixHistoryInterface(alantixHistoryContractAddress)
                .emitUSDTEarningEvent(
                    "missed",
                    msg.sender,
                    missAddress[i],
                    parentAmount,
                    myData.level
                );
        }
        bool isAirdropFinished = UserDataInterface(userDataContractAddress)
            .getAirdropState();
        if (!isAirdropFinished) {
            // 1 step airdrop
            uint256 airdropOneTotalAmount = UserDataInterface(
                userDataContractAddress
            ).getAirdropOneTotalAmount();
            if (
                airdropOneTotalAmount < airdropOneMax * 1 ether &&
                myData.level == airdropStartLevel
            ) {
                myData.todayClaimableAmount += airdropOneUnit * 1 ether;
                UserDataInterface(userDataContractAddress).setMemberAttribute(
                    myData.todayClaimableAmount,
                    msg.sender,
                    "TODAY_CLAIMABLE_AMOUNT"
                );
                myData.totalAirdropEarning += airdropOneUnit * 1 ether;
                UserDataInterface(userDataContractAddress).setMemberAttribute(
                    myData.totalAirdropEarning,
                    msg.sender,
                    "TOTAL_AIRDROP_EARNING"
                );

                airdropOneTotalAmount += airdropOneUnit * 1 ether;
                UserDataInterface(userDataContractAddress)
                    .setAirdropOneTotalAmount(airdropOneTotalAmount);

                AlantixHistoryInterface(alantixHistoryContractAddress)
                    .emitAirdropEvent(
                        "one",
                        myData.addr,
                        airdropOneUnit * 1 ether,
                        myData.level
                    );
            }
            // 2 step airdrop
            uint256 airdropTwoTotalAmount;
            airdropTwoTotalAmount = UserDataInterface(userDataContractAddress)
                .getAirdropTwoTotalAmount();

            if (myData.level == airdropStartLevel) {
                UserDataInterface.Member memory parentData;
                (isRegistered, parentData) = UserDataInterface(
                    userDataContractAddress
                ).getMemberByAddress(myData.parentAddr);

                parentData.airdropOnePassedChildrenCount++;
                UserDataInterface(userDataContractAddress).setMemberAttribute(
                    parentData.airdropOnePassedChildrenCount,
                    parentData.addr,
                    "AIRDROP_ONE_PASSED_CHILDREN_COUNT"
                );

                if (
                    airdropTwoTotalAmount < airdropTwoMax * 1 ether &&
                    parentData.airdropOnePassedChildrenCount %
                        airdropTwoChildrenCount ==
                    0 &&
                    parentData.level >= airdropStartLevel
                ) {
                    parentData.todayClaimableAmount += airdropTwoUnit * 1 ether;
                    UserDataInterface(userDataContractAddress)
                        .setMemberAttribute(
                            parentData.todayClaimableAmount,
                            parentData.addr,
                            "TODAY_CLAIMABLE_AMOUNT"
                        );

                    parentData.totalAirdropEarning += airdropTwoUnit * 1 ether;
                    UserDataInterface(userDataContractAddress)
                        .setMemberAttribute(
                            parentData.totalAirdropEarning,
                            parentData.addr,
                            "TOTAL_AIRDROP_EARNING"
                        );

                    airdropTwoTotalAmount += airdropTwoUnit * 1 ether;
                    UserDataInterface(userDataContractAddress)
                        .setAirdropTwoTotalAmount(airdropTwoTotalAmount);

                    AlantixHistoryInterface(alantixHistoryContractAddress)
                        .emitAirdropEvent(
                            "two",
                            parentData.addr,
                            airdropTwoUnit * 1 ether,
                            parentData.airdropOnePassedChildrenCount
                        );
                }
            }
            // Check airdrop end state
            if (
                airdropOneTotalAmount >= airdropOneMax * 1 ether &&
                airdropTwoTotalAmount >= airdropTwoMax * 1 ether
            ) {
                isAirdropFinished = true;
                UserDataInterface(userDataContractAddress).setAirdropState(
                    isAirdropFinished
                );
            }
        }

        return (true, "Successfully upgraded!");
    }

    // Contract Get Functions
    function canClaim(
        address addr
    ) public view returns (bool, string memory, uint256 amount) {
        bool isRegistered;
        UserDataInterface.Member memory myData;

        (isRegistered, myData) = UserDataInterface(userDataContractAddress)
            .getMemberByAddress(addr);
        uint256 claimableAmount = myData.untilYesterdayClaimableAmount +
            myData.todayClaimableAmount;

        if (!isRegistered) return (false, "The address is not registered!", 0);

        if (claimableAmount == 0)
            return (false, "Claimable amount is zero!", 0);

        if (
            IERC20(tixTokenContractAddress).balanceOf(address(this)) <
            claimableAmount
        ) return (false, "Claimable wallet balance is not enough!", 0);

        return (true, "", claimableAmount);
    }

    function canUpgrade(
        address addr
    )
        public
        view
        returns (
            bool,
            string memory,
            uint256,
            uint256,
            address,
            address,
            address[] memory,
            uint256
        )
    {
        UpgradeAddress memory upgradeData;

        upgradeData = getUpgradeData(addr);

        if (!upgradeData.canUpgradeFlag)
            return (
                false,
                upgradeData.upgradeAlert,
                upgradeData.feeAmount,
                upgradeData.parentAmount,
                upgradeData.feeAccountAddress,
                upgradeData.payableParentAddress,
                upgradeData.missAddress,
                upgradeData.missAddressCount
            );

        if (
            IERC20(usdtTokenContractAddress).balanceOf(addr) <
            (upgradeData.feeAmount + upgradeData.parentAmount)
        ) {
            return (
                false,
                "USDT amount is not enough!",
                upgradeData.feeAmount,
                upgradeData.parentAmount,
                upgradeData.feeAccountAddress,
                upgradeData.payableParentAddress,
                upgradeData.missAddress,
                upgradeData.missAddressCount
            );
        }

        return (
            true,
            "",
            upgradeData.feeAmount,
            upgradeData.parentAmount,
            upgradeData.feeAccountAddress,
            upgradeData.payableParentAddress,
            upgradeData.missAddress,
            upgradeData.missAddressCount
        );
    }

    function getUpgradeData(
        address addr
    ) public view returns (UpgradeAddress memory) {
        address payableParentAddress;
        address[] memory missAddress;
        uint256 missAddressCount = 0;

        UserDataInterface.Member memory myData;
        bool isRegistered;
        (isRegistered, myData) = UserDataInterface(userDataContractAddress)
            .getMemberByAddress(addr);

        if (!isRegistered)
            return
                UpgradeAddress(
                    false,
                    "This account is not registered!",
                    0,
                    0,
                    feeAccountAddress,
                    payableParentAddress,
                    missAddress,
                    missAddressCount
                );

        uint256 upgradeAmount = calculateUpgradeAmount(myData.level + 1);
        uint256 feeAmount = (upgradeAmount * 1 ether) / 6;
        uint256 parentAmount = (upgradeAmount * 1 ether * 5) / 6;
        bool canUpgradeFlag;
        string memory upgradeAlert;
        (
            canUpgradeFlag,
            upgradeAlert,
            payableParentAddress,
            missAddress,
            missAddressCount
        ) = getPayableAndMissAddressByUpgrade(addr);

        if (!canUpgradeFlag)
            return
                UpgradeAddress(
                    false,
                    upgradeAlert,
                    feeAmount,
                    parentAmount,
                    feeAccountAddress,
                    payableParentAddress,
                    missAddress,
                    missAddressCount
                );

        return
            UpgradeAddress(
                true,
                upgradeAlert,
                feeAmount,
                parentAmount,
                feeAccountAddress,
                payableParentAddress,
                missAddress,
                missAddressCount
            );
    }

    function getTotalSpeed() public view returns (uint256) {
        uint256 totalSpeed = 0;
        UserDataInterface.Member[] memory members;
        members = UserDataInterface(userDataContractAddress).getMemberList();

        for (uint256 i = 0; i < members.length; i++) {
            totalSpeed += calculateSpeedByLevel(members[i].level);
        }
        return totalSpeed;
    }

    // Contract private Functions
    function calculateSpeedByLevel(
        uint256 level
    ) private pure returns (uint256) {
        uint256 speed = 0;
        if (level <= 20) {
            speed += level * 10;
        } else if (level >= 21 && level <= 40) {
            speed += (level - 20) * 100 + 200;
        } else if (level >= 41 && level <= 60) {
            speed += (level - 40) * 1000 + 200 + 2000;
        } else if (level >= 61 && level <= 80) {
            speed += (level - 60) * 10000 + 200 + 2000 + 20000;
        } else if (level >= 81 && level <= 100) {
            speed += (level - 80) * 100000 + 200 + 2000 + 20000 + 200000;
        } else if (level >= 101) {
            speed +=
                (level - 100) *
                1000000 +
                200 +
                2000 +
                20000 +
                200000 +
                2000000;
        }
        return speed;
    }

    function calculateUpgradeAmount(
        uint256 level
    ) private pure returns (uint256) {
        uint256 step;

        if (level == 0) step = 0;
        else if (level > 100) step = 5;
        else {
            step = uint256((level - 1) / 20);
        }
        return 30 * (2 ** step);
    }

    function getParents(
        address addr
    ) private view returns (UserDataInterface.Member[] memory, uint256) {
        uint256 totalMembers;
        totalMembers = UserDataInterface(userDataContractAddress)
            .getTotalMembers();

        UserDataInterface.Member[]
            memory parents = new UserDataInterface.Member[](totalMembers);
        uint256 parentCount = 0;

        bool isRegistered;
        UserDataInterface.Member memory myData;

        (isRegistered, myData) = UserDataInterface(userDataContractAddress)
            .getMemberByAddress(addr);

        while (isRegistered && myData.parentAddr != address(0)) {
            (isRegistered, myData) = UserDataInterface(userDataContractAddress)
                .getMemberByAddress(myData.parentAddr);
            parents[parentCount] = myData;
            parentCount++;
        }

        return (parents, parentCount);
    }

    function getPayableAndMissAddressByUpgrade(
        address addr
    )
        private
        view
        returns (bool, string memory, address, address[] memory, uint256)
    {
        UserDataInterface.Member[] memory parents;
        uint256 parentCount = 0;

        (parents, parentCount) = getParents(addr);
        address[] memory missAddressList = new address[](parentCount);
        uint256 missAddressLength = 0;

        UserDataInterface.Member memory myData;
        bool isRegistered;

        (isRegistered, myData) = UserDataInterface(userDataContractAddress)
            .getMemberByAddress(addr);
        address payableParentAddress = feeAccountAddress;

        if (!isRegistered)
            return (
                false,
                "This account is not registered!",
                payableParentAddress,
                missAddressList,
                missAddressLength
            );

        if (parentCount > 0) {
            if (myData.level < parentCount - 1) {
                for (uint256 i = myData.level; i < parentCount - 1; i++) {
                    if (parents[i].level > myData.level) {
                        payableParentAddress = parents[i].addr;
                        break;
                    } else {
                        missAddressList[missAddressLength] = parents[i].addr;
                        missAddressLength++;
                    }
                }
            }

            return (
                true,
                "",
                payableParentAddress,
                missAddressList,
                missAddressLength
            );
        } else {
            return (
                false,
                "Parent accounts are not existed!",
                payableParentAddress,
                missAddressList,
                missAddressLength
            );
        }
    }
}

