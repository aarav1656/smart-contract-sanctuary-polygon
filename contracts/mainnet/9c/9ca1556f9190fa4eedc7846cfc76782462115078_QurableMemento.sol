pragma solidity 0.8.17;

import {ERC721AUpgradeable} from "../libs/ERC721A/ERC721AUpgradeable.sol";
import {UUPSUpgradeable} from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import {QurableMementoManager} from "./QurableMementoManager.sol";
import {IERC721Minter} from "../IERC721Minter.sol";
import {IMementoMinter} from "./IMementoMinter.sol";
import {IHasVersion} from "../IHasVersion.sol";
import {MarketLibV4} from "../libs/MarketLibV4.sol";

import "hardhat/console.sol";

contract QurableMemento is
    ERC721AUpgradeable,
    OwnableUpgradeable,
    UUPSUpgradeable,
    IMementoMinter
{
    error InvalidQurableMementoManagerAddress();
    error NotOperatorOrOwner();
    error InvalidOperator();

    mapping(uint256 => string) private _tokenURIs;
    QurableMementoManager private _qurableMementoManager;

    event QurableMementoTransfer(
        address from,
        address indexed to,
        uint256 indexed fromTokenId,
        uint256 indexed quantity
    );

    event QurableMementoMinted(
        address indexed to,
        uint256 indexed fromTokenId,
        uint256 indexed quantity,
        bytes data
    );

    function initialize(
        address mementoManager_,
        string calldata name_,
        string calldata symbol_
    ) public virtual initializer {
        if (mementoManager_ == address(0)) {
            revert InvalidQurableMementoManagerAddress();
        }
        _ERC721A_init(name_, symbol_);
        __Ownable_init();

        _qurableMementoManager = QurableMementoManager(mementoManager_);

        transferOwnership(_qurableMementoManager.owner());
    }

    function _authorizeUpgrade(address) internal virtual override onlyOwner {}

    function version() external pure virtual returns (string memory) {
        return "1";
    }

    modifier onlyOperatorOrOwner() {
        if (
            owner() != msg.sender &&
            !_qurableMementoManager.operators(msg.sender)
        ) {
            revert NotOperatorOrOwner();
        }
        _;
    }

    function mintMultiple(
        address to,
        uint256 quantity,
        string calldata absoluteTokenURI,
        bytes calldata data
    ) external override onlyOperatorOrOwner {
        for (uint256 index = 0; index < quantity; ) {
            unchecked {
                _tokenURIs[_currentIndex + index] = absoluteTokenURI;
                index++;
            }
        }

        emit QurableMementoMinted(to, _currentIndex, quantity, data);

        _safeMint(to, quantity);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721AUpgradeable)
        returns (string memory)
    {
        return _tokenURIs[tokenId];
    }

    function _afterTokenTransfers(
        address from,
        address to,
        uint256 startTokenId,
        uint256 quantity
    ) internal override {
        emit QurableMementoTransfer(from, to, startTokenId, quantity);
    }

    function isApprovedForAll(address owner, address operator)
        public
        view
        virtual
        override
        returns (bool)
    {
        if (_qurableMementoManager.operators(msg.sender)) {
            return true;
        }
        return super.isApprovedForAll(owner, operator);
    }
}

