pragma solidity 0.8.17;
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import {ECDSA} from "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import {AddressUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import {IERC20WithExecuteMetaTransaction} from "../tokenInterfaces/IERC20WithExecuteMetaTransaction.sol";
import {DecimalLib} from "./DecimalLib.sol";

library MementoLib {
    using SafeMath for uint256;
    using ECDSA for bytes32;

    error NotEnoughBalanceToPay();
    error NotEnoughAllowance();
    error ErrorTransferToCreator();
    error ErrorTransferToQurable();
    error ErrorTransferToSeller();
    error NotEnoughEthersToPay();

    struct SplittedShellShares {
        uint256 creator;
        uint256 qurable;
        uint256 seller;
    }

    struct MementoPaymentInfo {
        uint256 amount;
        address tokenAddress;
        address userAddress;
        bool executeTokenApprovalWithMetaTransaction;
        bytes functionSignature;
        bytes32 sigR;
        bytes32 sigS;
        uint8 sigV;
    }

    struct MementoSellShares {
        DecimalLib.D256 creator;
        DecimalLib.D256 qurable;
        DecimalLib.D256 seller;
    }

    struct MementoTransferOrder {
        address mementoContractAddress;
        address from;
        address to;
        uint256 quantity;
        string tokenURI;
        address creatorRoyaltiesAddress;
        MementoPaymentInfo paymentInfo;
        MementoSellShares sellShares;
        bytes data;
    }

    function isValidSellShares(
        MementoSellShares memory sellShares,
        uint256 sellValue
    ) internal pure returns (bool) {
        return
            sellValue == 0 ||
            (sellValue > 0 &&
                sellShares.creator.value +
                    sellShares.qurable.value +
                    sellShares.seller.value ==
                100 * DecimalLib.BASE);
    }

    function transferSellSharesWithPaymentInfo(
        address spender,
        MementoPaymentInfo memory paymentInfo,
        MementoSellShares memory sellShares,
        address payable qurableAddress,
        address payable creatorAddress,
        address payable sellerAddress
    ) internal {
        if (paymentInfo.amount == 0) {
            return;
        }

        SplittedShellShares
            memory splittedShellShares = splitSellSharesFromAmount(
                sellShares,
                paymentInfo.amount
            );

        // address(0) == Ether
        if (paymentInfo.tokenAddress != address(0)) {
            IERC20WithExecuteMetaTransaction token = IERC20WithExecuteMetaTransaction(
                    paymentInfo.tokenAddress
                );

            if (token.balanceOf(paymentInfo.userAddress) < paymentInfo.amount) {
                revert NotEnoughBalanceToPay();
            }

            // Execute approve of allowance (allowances should be at least enough to sell value + fees)
            if (
                token.allowance(paymentInfo.userAddress, spender) <
                paymentInfo.amount &&
                paymentInfo.executeTokenApprovalWithMetaTransaction
            ) {
                token.executeMetaTransaction(
                    paymentInfo.userAddress,
                    paymentInfo.functionSignature,
                    paymentInfo.sigR,
                    paymentInfo.sigS,
                    paymentInfo.sigV
                );
            }

            if (
                token.allowance(paymentInfo.userAddress, spender) <
                paymentInfo.amount
            ) {
                revert NotEnoughAllowance();
            }

            if (
                creatorAddress != address(0) &&
                splittedShellShares.creator > 0 &&
                !token.transferFrom(
                    paymentInfo.userAddress,
                    creatorAddress,
                    splittedShellShares.creator
                )
            ) {
                revert ErrorTransferToCreator();
            }
            if (
                qurableAddress != address(0) &&
                splittedShellShares.qurable > 0 &&
                !token.transferFrom(
                    paymentInfo.userAddress,
                    qurableAddress,
                    splittedShellShares.qurable
                )
            ) {
                revert ErrorTransferToQurable();
            }
            if (
                sellerAddress != address(0) &&
                splittedShellShares.seller > 0 &&
                !token.transferFrom(
                    paymentInfo.userAddress,
                    sellerAddress,
                    splittedShellShares.seller
                )
            ) {
                revert ErrorTransferToSeller();
            }
        } else {
            if (msg.value < paymentInfo.amount) {
                revert NotEnoughEthersToPay();
            }

            if (splittedShellShares.creator > 0) {
                AddressUpgradeable.sendValue(
                    creatorAddress,
                    splittedShellShares.creator
                );
            }
            if (splittedShellShares.qurable > 0) {
                AddressUpgradeable.sendValue(
                    qurableAddress,
                    splittedShellShares.qurable
                );
            }
            if (splittedShellShares.seller > 0) {
                AddressUpgradeable.sendValue(
                    sellerAddress,
                    splittedShellShares.seller
                );
            }
        }
    }

    /**
     * @notice return a % of the specified amount. This function is used to split a bid into shares
     * for a media's shareholders.
     */
    function splitShare(DecimalLib.D256 memory sharePercentage, uint256 amount)
        internal
        pure
        returns (uint256)
    {
        return DecimalLib.mul(amount, sharePercentage).div(100);
    }

    function splitSellSharesFromAmount(
        MementoSellShares memory sellShares,
        uint256 amount
    ) internal pure returns (SplittedShellShares memory) {
        uint256 artistShare = splitShare(sellShares.creator, amount);
        uint256 qurableShare = splitShare(sellShares.qurable, amount);
        uint256 sellerShare = splitShare(sellShares.seller, amount);

        return SplittedShellShares(artistShare, qurableShare, sellerShare);
    }
}

