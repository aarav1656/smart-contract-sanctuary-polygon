        if (isPaused) {
            revert ContractPaused();
        }
        _;
    }

    modifier onlyOperatorOrOwner() {
        if (msg.sender != owner() && !operators[msg.sender]) {
            revert NotOperatorOrOwner();
        }
        _;
    }

    function pause() external onlyOwner {
        isPaused = true;
    }

    function unpause() external onlyOwner {
        isPaused = false;
    }

    function setVault(address vault_) external onlyOwner {
        if (vault_ == address(0)) {
            revert InvalidVaultAddress();
        }

        vault = vault_;
    }

    function addOperator(address operator) external onlyOwner {
        if (operator == address(0)) {
            revert InvalidOperatorAddress();
        }
        operators[operator] = true;
    }

    function removeOperator(address operator) external onlyOwner {
        operators[operator] = false;
    }

    function mint(MementoLib.MementoTransferOrder calldata order)
        external
        payable
        whenNotPaused
        onlyOperatorOrOwner
    {
        if (order.mementoContractAddress == address(0)) {
            revert InvalidMementoContractAddress();
        }
        if (order.to == address(0)) {
            revert InvalidToAddress();
        }
        if (order.quantity == 0) {
            revert InvalidQuantity();
        }
        if (bytes(order.tokenURI).length == 0) {
            revert InvalidTokenURI();
        }
        if (order.paymentInfo.amount > 0) {
            if (
                !MementoLib.isValidSellShares(
                    order.sellShares,
                    order.paymentInfo.amount
                )
            ) {
                revert InvalidSellShares();
            }

            MementoLib.transferSellSharesWithPaymentInfo(
                address(this),
                order.paymentInfo,
                order.sellShares,
                payable(vault),
                payable(order.creatorRoyaltiesAddress),
                payable(vault)
            );
        }
        IMementoMinter mementoContract = IMementoMinter(
            order.mementoContractAddress
        );
        mementoContract.mintMultiple(
            order.to,
            order.quantity,
            order.tokenURI,
            order.data
        );

        emit QurableMementoMinted(
            order.to,
            order.quantity,
            order.tokenURI,
            order.data
        );
    }
}

