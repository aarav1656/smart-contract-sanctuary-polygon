    event TokenMint(address indexed player, uint256 indexed token1, uint256 indexed tokenTag);

    //contract admin
    address public _nftAdmin;

    //token tags
    mapping(uint256 => uint256)    internal _tokenTagMap;

    uint256 public _tokenIdIndex;
    


    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
        _tokenIdIndex = 0;
    }


