abstract contract  DarwinStore is ReentrancyGuarded, ERC721, Ownable{

    struct Player {
        string  name;       
        address refferier;
        bool    isInit;
        uint256 tokenId;
        uint256 DPoint;  //darwinpoint
        bool    isActive;
        uint256 landLevel;
    }

    /* An ECDSA signature. */ 
