pragma experimental ABIEncoderV2;

pragma solidity ^0.7.1;

import {DarwinSBT,Proxy, ERC721, ReentrancyGuarded, Ownable, Strings, Address, SafeMath, Context} from "./Darwin721.sol";
import {Armory} from "./Armory.sol";
import {Darwin1155,IERC1155} from "./Darwin1155.sol";
import {IERC20} from "./IERC20.sol";
import {ArrayUtils} from "./ArrayUtils.sol";
import {BPExchange} from "./BPExchange.sol";
import {DarwinStore} from "./DarwinStore.sol";

contract Darwin is  DarwinStore{

    using SafeMath for uint256;

    event PlayerInitialized(address indexed player, uint256 indexed tokenId, address indexed refer, string metaHash);
    event PlayerRename(address indexed player, string name);
    event CraftItem(address indexed player, uint256 indexed orderId, uint256 indexed itemTag, uint256 amount);
    event BluePrint(address indexed player, uint256 indexed itemTag, uint256 indexed count);
    event DPointAdd(address indexed player, uint256 indexed dPoint, uint256 indexed amount);
    event DPointSub(address indexed player, uint256 indexed dPoint, uint256 indexed amount);
    event LandUpgrade(address indexed player, uint256 indexed orderId, uint256 indexed landId, uint256 landLevel);
    event ClaimDReward(address indexed player, uint256 indexed orderId);
    event ClaimMine(address indexed player, uint256 indexed orderId);
    event PlayerActived(address indexed player, bool indexed actived);

    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
        _activePrice = 10 * baseEther();
        _dPointLootPair = 100;
    }

    
     //set base uri
    function setBaseURI(string memory baseURI) public onlyOwner {
        _setBaseURI(baseURI);
    }
    
    /*
    * Pause sale if active, make active if paused
    */
    function flipContractState() public onlyOwner {
        contractIsOpen = !contractIsOpen;
    }

    function playerBluePrint(address addr, uint32 itemTag) public view returns (uint256){
        return _bluePrintMap[itemTag][addr];
    }

    function armoryNFT() public view returns (address){
        return _armoryNFT;
    }

    function nft1155() public view returns (address){
        return _NFT1155;
    }

    function setArmoryNFT(address addr) public virtual onlyOwner {
        _armoryNFT = addr;
    }

    function setNFT1155(address addr) public virtual onlyOwner {
        _NFT1155 = addr;
    }

    function playerDPoint(address addr) public view returns (uint256){
        return _players[addr].DPoint;
    } 

    function setBPExchange(address addr) public virtual onlyOwner {
        _BPExchange = addr;
    }

    function bpExchange() public view returns (BPExchange){
        return BPExchange(_BPExchange);
    } 

    function setActivePrice(uint256 price)  public onlyOwner {
        _activePrice = price;
    }

    function activePrice()public view returns (uint256){
        return _activePrice;
    } 

    function dPointLootPair() public view returns(uint256){
        return _dPointLootPair;
    }

    function setDPointLootPair(uint256 price)  public onlyOwner {
        _dPointLootPair = price;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override virtual {
        require(from == address(0), "Err: token transfer is BLOCKED");   
        super._beforeTokenTransfer(from, to, tokenId);  
    }

    function baseEther() internal pure returns (uint256){
        return 1000000000000000000;
    }

    function loot() public view returns (IERC20){
        return IERC20(_lootContract);
    }

    function setLoot(address addr)  public onlyOwner {
        _lootContract = addr;
    }



    function version() public pure returns (uint){
        return 1;
    }

    function playerExisit(address addr) public view returns (bool){
        return _players[addr].isInit;
    }

    function orderIdExist(uint256 orderId) public view  returns (bool) {
        return _orderIdMap[orderId];
    }

    function metaExist(bytes32 metaHash)  public view returns (bool) {
        return _metaHashMap[metaHash];
    }

    function isBluePrintId(uint id) public pure returns(bool){
        return id >=6000 && id <7000;
    }


    function isDPointId(uint id) public pure returns(bool){
        return id == 1;
    }


    function isGiftNFT(uint id) public pure returns(bool){
        return id >=60000 && id <70000;
    }

    function isResourceId(uint id) public pure returns(bool){
        uint[5] memory nft1155Ids = [uint(100303),uint(100304),uint(100305),uint(100306),uint(100045)];
        for(uint i=0; i<nft1155Ids.length; ++i){
            if(id == nft1155Ids[i]){
                return true;
            }
        }
        return false;
    }

    function consume1155(uint[] memory consumeIds, uint[] memory consumeAmounts) internal {
        for(uint i=0; i<consumeIds.length; ++i){
            if(consumeIds[i] == 0){
                continue;
            }
            if(!isResourceId(consumeIds[i])){   //blueprint
                
                uint bluePrint = consumeIds[i];

                require(_bluePrintMap[consumeIds[i]][msg.sender] >= 1, "no blue print");

                _bluePrintMap[consumeIds[i]][msg.sender] = SafeMath.sub(_bluePrintMap[consumeIds[i]][msg.sender], consumeAmounts[i]);

                emit BluePrint(msg.sender, bluePrint, _bluePrintMap[consumeIds[i]][msg.sender]);

                consumeIds[i] = 0;
                consumeAmounts[i] = 0;
            }
        }

        Darwin1155(_NFT1155).burnBatch(msg.sender, consumeIds, consumeAmounts);
    }

    function initPlayer(address refferier, string memory name, string memory metaHash) public reentrancyGuard{
        require(contractIsOpen, "Contract must active");

        require(!playerExisit(msg.sender), "player had initialized");

        bytes32 metaBytes32 = keccak256(abi.encodePacked(metaHash));

        require(!metaExist(metaBytes32), "meta hash exist");

        _metaHashMap[metaBytes32] = true;

        uint256 tokenId = totalSupply() + 1;

        _players[msg.sender].refferier = refferier;
        _players[msg.sender].isInit = true;
        _players[msg.sender].name = name;
        _players[msg.sender].tokenId = tokenId;
        _players[msg.sender].DPoint = 0;   
        _players[msg.sender].landLevel = 1;   

        _safeMint(msg.sender, tokenId);

        emit PlayerInitialized(msg.sender, tokenId, refferier, metaHash);
        
	    emit PlayerRename(msg.sender, name);
    }

    function activePlayer() public payable reentrancyGuard{
        require(contractIsOpen, "Contract must active");

        require(playerExisit(msg.sender), "player have not initialized");

        require(!_players[msg.sender].isActive, "player had actived");

        require(msg.value == activePrice(), "active price unmatch");

        _players[msg.sender].isActive = true;

        emit PlayerActived(msg.sender, true);
    }



    function reName(string memory name) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");

        require(playerExisit(msg.sender), "player have not initialized");

        _players[msg.sender].name = name;

        emit PlayerRename(msg.sender, name);
    }

    function craftItem(uint256 orderId, uint256 itemTag, uint256 amount, uint[] memory consumeIds, uint[] memory consumeAmounts, uint8 v, bytes32 r, bytes32 s) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");
        
        require(verifyOrder(orderId, itemTag, amount, consumeIds, consumeAmounts, v, r, s),"verify Order error");

        require(itemTag < 1000000, "item tag check error");

        consume1155(consumeIds, consumeAmounts);

        emit CraftItem(msg.sender, orderId, itemTag, amount);
    }

    function upgradeLand(uint256 orderId, uint256 landId, uint256 landLevel, uint[] memory consumeIds, uint[] memory consumeAmounts, uint8 v, bytes32 r, bytes32 s) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");
        
        require(verifyOrder(orderId, landId, landLevel, consumeIds, consumeAmounts, v, r, s),"verify Order error");

        require(landLevel <= 8, "land level error");

        require(landLevel == _players[msg.sender].landLevel + 1, "land level check error");

        consume1155(consumeIds, consumeAmounts);

        _players[msg.sender].landLevel = landLevel;

        emit LandUpgrade(msg.sender, orderId, landId, landLevel);
    }

    function mintArmory(uint256 orderId, uint256 tokenId, uint256 tokenTag, uint[] memory consumeIds, uint[] memory consumeAmounts, uint8 v, bytes32 r, bytes32 s) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");
        

        require(verifyOrder(orderId, tokenId, tokenTag, consumeIds, consumeAmounts, v, r, s),"verify Order error");

        consume1155(consumeIds, consumeAmounts);

        Armory(armoryNFT())._mintByAdmin(tokenTag, msg.sender);
    }

    // only for mining
    //
    function claimMine(uint256 orderId, uint256 markTag, uint32 actionTag, uint[] memory rewardIds, uint[] memory rewardAmounts, uint8 v, bytes32 r, bytes32 s) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");
        
        require(verifyOrder(orderId, markTag, actionTag, rewardIds, rewardAmounts, v, r, s),"verify Order error");

        for(uint i=0; i<rewardIds.length; ++i){
            if(rewardIds[i] == 0){
                continue;
            }
            require(isResourceId(rewardIds[i]), "can't claim other nft id");

            //1 tx limited 1k
            require(rewardAmounts[i] < 1000, "amount to much");
        }

        Darwin1155(_NFT1155).claim(msg.sender, rewardIds, rewardAmounts, "claim mine");

        emit ClaimMine(msg.sender, orderId);
    }


    // Maybe DPoint/Blueprint/operation nft
    //
    function claimDReward(uint256 orderId, uint256 markTag, uint32 actionTag, uint[] memory rewardIds, uint[] memory rewardAmounts, uint8 v, bytes32 r, bytes32 s) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");
        
        require(verifyOrder(orderId, markTag, actionTag, rewardIds, rewardAmounts, v, r, s),"verify Order error");

        for(uint i=0; i<rewardIds.length; ++i){
            if(rewardIds[i] == 0){
                continue;
            }

            if(isDPointId(rewardIds[i])){
                _players[msg.sender].DPoint = _players[msg.sender].DPoint.add(rewardAmounts[i]);
                emit DPointAdd(msg.sender, _players[msg.sender].DPoint, rewardAmounts[i]);
            }else if(isBluePrintId(rewardIds[i])){
                _bluePrintMap[rewardIds[i]][msg.sender] = SafeMath.add(_bluePrintMap[rewardIds[i]][msg.sender], rewardAmounts[i]);
                emit BluePrint(msg.sender, rewardIds[i], _bluePrintMap[rewardIds[i]][msg.sender]);
            }else {
                //mint armory to msg.sender
                Armory(armoryNFT())._mintByAdmin(rewardIds[i], msg.sender);
            }
        }

        emit ClaimDReward(msg.sender, orderId);
    }



    
    function exchangeDPoint(uint256 amount) public reentrancyGuard {
        require(contractIsOpen, "Contract must active");

        require(playerDPoint(msg.sender) >= amount, "You DPoint not enought");
        
        _players[msg.sender].DPoint = SafeMath.sub(_players[msg.sender].DPoint, amount);
        emit DPointSub(msg.sender, _players[msg.sender].DPoint, amount);

        uint256 lootAmount = SafeMath.mul(amount, baseEther()).div(dPointLootPair()); 
        loot().transfer(msg.sender, lootAmount);
    }

    function exchangeBluePrint(uint32 itemTag, uint256 count) public payable reentrancyGuard {
        require(contractIsOpen, "Contract must active");

        require(isBluePrintId(itemTag), "item tag not blue print");
        
        require(bpExchange().itemPrice(itemTag) > 0, "Blue print not define");

        uint256 totalPrice = SafeMath.mul(bpExchange().itemPrice(itemTag), count);

        require(totalPrice == msg.value, "price unmatch");

        _bluePrintMap[itemTag][msg.sender] = SafeMath.add(_bluePrintMap[itemTag][msg.sender], count);

        emit BluePrint(msg.sender, itemTag, _bluePrintMap[itemTag][msg.sender]);
    }


    function sizeOf()
        internal
        pure
        returns (uint)
    {   
        //address   = 0x14
        //uint      = 0x20
        //uint8     = 1
        //uint256   = 0x100
        //return (0x100 * 3 + 0x20 * 8 + 0x14 * 1);
        return (0x100 * 3 + 0x20 * 8);
    }
     // 签名账户
    function signAddress() private pure returns(address){
        return address(0x34C533Bdd04d02a71d836463Aae0503854734eF1);
    }

    function hashOrder(Order memory order) internal pure returns(bytes32 hash) {
        uint size = sizeOf();
        bytes memory array = new bytes(size);
        uint index;
        assembly {
            index := add(array, 0x20)
        }

        index = ArrayUtils.unsafeWriteUint(index, order.orderId);

        index = ArrayUtils.unsafeWriteUint(index, order.tokenId);

        index = ArrayUtils.unsafeWriteUint(index, order.tokenTag);
        
        for(uint i = 0; i< 4; i++){
            index = ArrayUtils.unsafeWriteUint(index, order.consumeId[i]);
        }

        for(uint i = 0; i< 4; i++){
            index = ArrayUtils.unsafeWriteUint(index, order.consumeAmount[i]);
        }
        
        assembly {
            hash := keccak256(add(array, 0x20), size)
        }
        return hash;
    }

    function hashOrder_(uint256 orderId, uint256 tokenId, uint256 tokenTag, uint[] memory consumeIds, uint[] memory consumeAmounts) public pure returns (bytes32){
        return hashOrder(
            Order(orderId, tokenId, tokenTag, consumeIds, consumeAmounts)
        );
    }

     function hashToSign_(uint256 orderId, uint256 tokenId, uint256 tokenTag, uint[] memory consumeIds, uint[] memory consumeAmounts) public pure returns (bytes32){
        return hashToSign(
            Order(orderId, tokenId, tokenTag, consumeIds, consumeAmounts)
        );
    }
  
     function hashToSign(Order memory order)
        internal
        pure
        returns (bytes32)
    {
        return keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", hashOrder(order)));
    }

    
    function cancelOrder(uint256 orderId) public reentrancyGuard {
        require(!orderIdExist(orderId), "Order id check error");

        _orderIdMap[orderId] = true;
    }

    function verifyOrder(uint256 orderId, uint256 tokenId, uint256 tokenTag, uint[] memory consumeIds, uint[] memory consumeAmounts, uint8 v, bytes32 r, bytes32 s) internal returns(bool)  {            

        require(validateOrder_(orderId, tokenId, tokenTag, consumeIds, consumeAmounts, v, r, s), "Order validate error");

        _orderIdMap[orderId] = true;

        return true;
    }

    /**
     * @dev Validate a provided previously approved / signed order, hash, and signature.
     * @param hash Order hash (already calculated, passed to avoid recalculation)
     * @param order Order to validate
     * @param sig ECDSA signature
     */
    function validateOrder(bytes32 hash, Order memory order, Sig memory sig) 
        internal
        view
        returns (bool)
    {
        /* Order must have not been canceled or already filled. */
        if (orderIdExist(order.orderId)) {
            return false;
        }
        
        /* or (b) ECDSA-signed by maker. */
        if (ecrecover(hash, sig.v, sig.r, sig.s) == signAddress()) {
            return true;
        }

        return false;
    }

    
    /**
     * @dev Call validateOrder - Solidity ABI encoding limitation workaround, hopefully temporary.
     */
    function validateOrder_ (
        uint256 orderId, uint256 tokenId, uint256 tokenTag, uint[] memory consumeIds, uint[] memory consumeAmounts, uint8 v, bytes32 r, bytes32 s) 
        view public returns (bool)
    {
        Order memory order = Order(orderId, tokenId, tokenTag, consumeIds, consumeAmounts);
        return validateOrder(
          hashToSign(order),
          order,
          Sig(v, r, s)
        );
    }
}

