        _nftAdmin = addr;
    }
    
    function tokenTag(uint256 tokenId) public view returns (uint256){
        require(_exists(tokenId), "ERC721: token not found");

        return _tokenTagMap[tokenId];
    }


    //set base uri
    function setBaseURI(string memory baseURI) public onlyOwner {
        _setBaseURI(baseURI);
    }

    function mint(address tokenOwner, uint256 tokenId, uint256 tag) internal {
        
        _tokenTagMap[tokenId] = tag;

        _safeMint(tokenOwner, tokenId);

        emit TokenMint(msg.sender, tokenId, tag);
