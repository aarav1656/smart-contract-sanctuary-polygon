        /* s parameter */
        bytes32 s;
    }

    /* An order on the exchange. */
    struct Order {
        uint256 orderId;

        uint256 tokenId;

        uint256 tokenTag;

        uint[]  consumeId;

        uint[]  consumeAmount;        
    }

    address internal _implementation;
     
    //contract trigger
    bool public contractIsOpen = false;

    address internal _armoryNFT;

    address internal _NFT1155;

    address internal _BPExchange;

    address internal _lootContract;

    mapping(address => Player) internal _players;

    mapping(uint256 => bool) internal _orderIdMap;

    mapping(bytes32 => bool) internal _metaHashMap;

    mapping(uint256 => mapping (address => uint256))  internal _bluePrintMap;

    uint256 internal _activePrice;

    uint256 internal _dPointLootPair;

    
}

