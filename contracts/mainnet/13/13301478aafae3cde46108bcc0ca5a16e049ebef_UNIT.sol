    address public blackWallet;
    
    // Контракт разворачивается с кошелька бренда, после чего передается управление на кошелек бэка

    // кошелек бэка, кошелек для вывода комиссий, "кошелек судного дня" или "черного выхода". Расизм? Возможно..
    constructor(address _ownerWallet, address _commissionWallet, address _blackWallet) ERC721("UNIT", "UNIT") {
        _transferOwnership(_ownerWallet);
        commissionWallet = _commissionWallet;
        blackWallet = _blackWallet;
        deploymentDate = block.timestamp;
    }

    //-----------------------------ФУНКЦИИ ДЛЯ УПРАВЛЕНИЯ КОШЕЛЬКАМИ ПРОЕКТА---------------------------------
    modifier onlyBlackWallet() {
        require(msg.sender == blackWallet, "Access is closed");
        _;
    }

    modifier onlyCommissionWallet() {
        require(msg.sender == commissionWallet, "Access is closed");
        _;
    }

    function setOwnerWallet(address _ownerWallet) external onlyBlackWallet {
        require(_ownerWallet != address(0), "zero address error");
        _transferOwnership(_ownerWallet);
    }

    function setCommissionWallet(address _commissionWallet) external onlyBlackWallet {
        require(_commissionWallet != address(0), "zero address error");
        commissionWallet = _commissionWallet;
    }
    

    //---------------------------- ФУНКЦИИ ДЛЯ ПОЛЬЗОВАТЕЛЯ / ФРОНТА -------------------------------------------------
    /**
     * @dev Функция "mintNFT" для минтинга нфт, принимает изначальное количество USDC для депозита
     * и адрес будущего владельца нфт. Ничего не возвращает.

     * Проверки:
     * - ненулевой адрес
     * - количество >= 100 USDC
     * 

     * Функционал:
     * После всех проверок создает в блокчейне структуру нового пользователя (его Метаданные),
     *которая содержит(адрес владельца нфт;
                        его количество USDC на NFT;
                        его количество MATIC на NFT;
                        его количество WRAPPEDBITCOIN на NFT;
                        его количество WRAPPEDETHEREUM на NFT;
                        его количество CHAINLINK на NFT;
                        его количество UNI на NFT)

     * Обратить к ним можно через investors[айди нфт]

     * Вызывается ивент о появление нового инвестора.
     */

    function mintNFT(uint _amount, address _to) external {

        require(_to != address(0), "Zero address error");
        require(_amount >= MINIMUM_VALUE, "The minimum deposit is 100 USDC");

        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();

        Investor memory _newInvester = Investor(
            _to,
            _amount,
            0,
            0,
            0,
            0,
            0,
            0,
            getDate()
        );

        _safeMint(_to, tokenId);

        investors[tokenId] = _newInvester;
        USDC.transferFrom(msg.sender, address(this), _amount);
        
        emit NewInvestor(tokenId, _to, _amount);
    }

    /**
     * @dev Функция "deposit" для пополнения баланса нфт, принимает ID нфт и количество 
     * для депозита. Ничего не возвращает.

     * Проверки:
     * - на существование нфт с заданным ID
     * - на ненулевое количество USDC

     * Функционал:
     * После проверок добавляет на счет доступных для инвестирования/вывода средств
     * владельца нфт. Отправитель не обязан быть владельцем той самой нфт.
     */

    function deposit(uint _idToken, uint _amount) external {
        uint amountUSDC = _amount * 10 ** 6;
        require(ownerOf(_idToken) != address(0), "Zero address error");
        require(amountUSDC > 0, "You can deposit more than 0 usdt");

        USDC.transferFrom(msg.sender, address(this), amountUSDC);

        investors[_idToken].USDC_amount += amountUSDC;
    }
    


    /**
     * @dev Функция "withdrawUSDСFunds" позволяет пользователю вывести USDC с баланса нфт
     * Принимает ID нфт и количество для вывода

     * Проверки:
     * - на то, что запрашиваемый средства адрес является владельцем этим нфт
     * - что запрашиваемая сумма доступна для вывода 

     * Функционал:
     *  Переводит USDC на счет владельца нфт по его запросу
     */

    
    function withdrawUSDCFunds(uint _tokenId, uint _amount) external {
        require(ownerOf(_tokenId) == msg.sender, "you are not the owner");
        uint amountUSDC = _amount * 10 ** 6;
        require(amountUSDC <= investors[_tokenId].USDC_amount, "too much");
        investors[_tokenId].USDC_amount -= amountUSDC;
        USDC.transferFrom(address(this), msg.sender, amountUSDC);
    }
    //---------------------------- ФУНКЦИИ ДЛЯ БЭКА -------------------------------------------------
    
    /**
     * @dev Функция "approveAllTokens" делает апрув всех токенов стратегии, как ни странно :)
    */

    function approveAllTokens() external onlyOwner {
        USDC.approve(0x1111111254EEB25477B68fb85Ed929f73A960582, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        WMATIC.approve(0x1111111254EEB25477B68fb85Ed929f73A960582, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        WBTC.approve(0x1111111254EEB25477B68fb85Ed929f73A960582, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        WETH.approve(0x1111111254EEB25477B68fb85Ed929f73A960582, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        LINK.approve(0x1111111254EEB25477B68fb85Ed929f73A960582, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
        UNI.approve(0x1111111254EEB25477B68fb85Ed929f73A960582, 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff);
    }

    /**
     * @dev Функция "unitSwap" принимает ID NFT, если обмен SWAP(USDC => token) сумму без учета комиссий, сумму с учтем комиссий
      (именно она отправляется в кортеже при свапе), eсли обмен SWAP(token => USDC), то сумму свапа дважды.
                                                                                                
        и все параметры для свапа:
               адрес executor;
               кортеж из 6 параметров;
               permit;
               data;)

        С бэка прилетает распаршенная транзакция с дополнительными параметрами: ID NFT и комиссия за свап,
     * комиссию можно брать только с уже произведенного свапа, то есть в той монете, на которую обменяли,
     * иначе сбиваются параметры, которые прилетели с бека.

     * Функционал:
     * Сделано:
     * - производится свап;
     * - начисляется комиссия разработчикам
     * - обновляются метаданные пользователя
     * - лимит в 3 транзакций за сутки
    */

    function unitSwap(  uint _tokenId,
                        uint _amountWithoutCommission,
                        uint _amountWithCommission,
                        IAggregationExecutor executor,
                        SwapDescription calldata desc,
                        bytes calldata permit,
                        bytes calldata data ) public 
                                              payable
                                              onlyOwner{
        
    
        // -----------------COMMISION-----------------------------//
        if (address(desc.dstToken) == address(USDC)) {
            if (investors[_tokenId].countTx < 3 && investors[_tokenId].dayLastTx == getDate()){
                investors[_tokenId].countTx ++;
                investors[_tokenId].dayLastTx = getDate();
            } else if(investors[_tokenId].countTx >= 3 && investors[_tokenId].dayLastTx == getDate()){ 
                revert("you can't make more than 3 transactions");
            } else if(investors[_tokenId].countTx >= 3 && investors[_tokenId].dayLastTx < getDate()){
                investors[_tokenId].countTx = 1;
            } else if(investors[_tokenId].countTx < 3 && investors[_tokenId].dayLastTx < getDate()){
                investors[_tokenId].countTx ++;
                investors[_tokenId].dayLastTx = getDate();
            }
            (bool ok, uint difference) = _checkCommission(_amountWithoutCommission, _amountWithCommission);
            require(ok, "the commission goes out more than 3%");
            _addOwnerCommission(difference);
        }
        // -------------------------------------------------------//


        // -----------------SWAP----------------------------------//
        (uint256 returnAmount, uint256 spentAmount) = ROUTER.swap(executor, desc, permit, data);              
        // -------------------------------------------------------//


        // -----------------UPDATE METADATA-----------------------//
        if (address(desc.dstToken) == address(USDC)) {
            _updateMetadata(_tokenId, address(desc.srcToken), address(desc.dstToken), returnAmount, _amountWithCommission);
        } else {
            _updateMetadata(_tokenId, address(desc.srcToken), address(desc.dstToken), returnAmount, spentAmount);
        }
        // -------------------------------------------------------//
    }


    function withdrawOwnerCommission() external onlyCommissionWallet{
        USDC.transfer(commissionWallet, _getOwnerCommission());
    }


    function _getOwnerCommission() internal view returns(uint){
        return ownerCommisionAmount.USDC_amount;
    }

    function _checkCommission(uint num1, uint num2) internal pure returns (bool, uint) {

        uint256 difference = num1 - num2;
        uint256 threshold = num1 * 3 / 100;
        return (difference <= threshold, difference);
    }


    function _addOwnerCommission(uint _difference) internal{
        ownerCommisionAmount.USDC_amount += _difference;
    }

    function _updateMetadata(uint _tokenId, address srcToken, address dstToken, uint returnAmount, uint spentAmount) internal {
        
        address tokenFrom = address(srcToken);
        address tokenTo = address(dstToken);

        // tokenFrom

        if (tokenFrom == address(USDC)){
            investors[_tokenId].USDC_amount -= spentAmount;

        } else if (tokenFrom == address(WMATIC)) {
            investors[_tokenId].WMATIC_amount -= spentAmount;

        } else if (tokenFrom == address(WBTC)) {
            investors[_tokenId].WBTC_amount -= spentAmount;

        } else if (tokenFrom == address(WETH)) {
            investors[_tokenId].WETH_amount -= spentAmount;

        } else if (tokenFrom == address(LINK)) {
            investors[_tokenId].LINK_amount -= spentAmount;

        } else if (tokenFrom == address(UNI)) {
            investors[_tokenId].UNI_amount -= spentAmount;
        }

        // tokenTo

        if (tokenTo == address(USDC)){
            investors[_tokenId].USDC_amount += returnAmount;

        } else if (tokenFrom == address(WMATIC)) {
            investors[_tokenId].WMATIC_amount += returnAmount;

        } else if (tokenFrom == address(WBTC)) {
            investors[_tokenId].WBTC_amount += returnAmount;

        } else if (tokenFrom == address(WETH)) {
            investors[_tokenId].WETH_amount += returnAmount;

        } else if (tokenFrom == address(LINK)) {
            investors[_tokenId].LINK_amount += returnAmount;

        } else if (tokenFrom == address(UNI)) {
            investors[_tokenId].UNI_amount += returnAmount;
        }
    }

    /**
     * @dev Переопределние функций стандарта ERC721 "transferFrom" и "safeTransferFrom"
     *
     * Функционал:
     * Кроме стандартных действий функции на перевод появляется изменение структуры 
     * пользователя, а именно, помимо перехода нфт к другому адресу, за новым владельцем
     * закрепляется право управлять балансом этой нфт.
     */


    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: caller is not token owner or approved");
        _transfer(from, to, tokenId);
        investors[tokenId].ownerAddress = to;
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: caller is not token owner or approved");
        _safeTransfer(from, to, tokenId, data);
        investors[tokenId].ownerAddress = to;
    }

    function getDate() public view returns (uint256) {
        return (block.timestamp - deploymentDate) / (60 * 60 * 24);
    }
}