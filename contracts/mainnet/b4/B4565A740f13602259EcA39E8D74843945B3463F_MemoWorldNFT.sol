    function mint(
        address account,
        uint256 id,
        uint256 amount,
        string memory data
    ) public payable whenNotPaused {
        emit mintInfoEvent(_msgSender(), id, account);

        require(_tokenSetting[id]._tokenLimit > 0, "limit not set");
        require(!isBlacklisted(_msgSender()), "in blacklist");
        require(amount > 0, "amount error");
        require(
            _tokenSoldAmount[id] + amount <= _tokenSetting[id]._tokenLimit,
            "Not enough token"
        );

        MemoWorldNFTSetting memory checkSetting = _tokenSetting[id];

        if (
            owner() != _msgSender() &&
            checkSetting._tokenCreator != _msgSender()
        ) {
            if (checkSetting._tokenLimitPerUser > 0) {
                require(
                    balanceOf(account, id) + amount <=
                        checkSetting._tokenLimitPerUser,
                    "You exceed limit per user"
                );
            } else {
                require(balanceOf(account, id) == 0, "You have owned it");
            }

            //check condition
            if (_tokenMintCondition[id].isInited) {
                if (_tokenMintCondition[id].onlyTokenId) {
                    IERC1155 conditionContract = IERC1155(
                        _tokenMintCondition[id].conditionAddress
                    );
                    require(
                        conditionContract.balanceOf(
                            account,
                            _tokenMintCondition[id].conditionTokenId
                        ) > 0,
                        "not meet mint condition tokenid"
                    );
                } else {
                    IERC721 conditionContract = IERC721(
                        _tokenMintCondition[id].conditionAddress
                    );
                    require(
                        conditionContract.balanceOf(account) > 0,
                        "not meet mint condition"
                    );
                }
            }

            uint256 mintlimit = 0;

            if (_whitelistAddress[id][account] > 0) {
                mintlimit = _whitelistAddress[id][account];
            }

            bool freeMint = false;
            if (mintlimit > 0 && balanceOf(account, id) + amount < mintlimit) {
                freeMint = true;
            }

            if (!freeMint) {
                if (
                    !isTrustedForwarder(msg.sender) &&
                    keccak256(
                        abi.encodePacked(
                            id.mod(_transKey).toString(),
                            StringsUpgradeable
                                .toHexString(uint256(uint160(account)), 20)
                                .substring(7, 17),
                            id.mod(_transKey.add(9121283736199)).toString()
                        )
                    ) !=
                    keccak256(abi.encodePacked(data))
                ) {
                    (, int256 price, , , ) = priceFeed.latestRoundData();

                    if (price == 0) {
                        require(
                            msg.value >=
                                _tokenSetting[id]._tokenMintPrice.mul(amount),
                            "insuficient payment (1)"
                        );
                    } else {
                        require(
                            msg.value >=
                                _tokenSetting[id]
                                    ._tokenMintPrice
                                    .mul(amount)
                                    .div(uint256(price))
                                    .mul(1e8),
                            "insuficient payment (2)"
                        );
                    }
                }
            }
        }

        _mint(account, id, amount, bytes(data));
        _tokenSoldAmount[id] += amount;

        if (msg.value > 0) {
            uint256 recieverPayment = msg.value;

            if (_tokenSetting[id]._tokenCreatorRate > 0) {
                uint256 transferCreatorAmount = msg
                    .value
                    .mul(_tokenSetting[id]._tokenCreatorRate)
                    .div(10000);

                _accountBalance[
                    _tokenSetting[id]._tokenCreator
                ] += transferCreatorAmount;

                transferPayment(
                    _tokenSetting[id]._tokenCreator,
                    transferCreatorAmount
                );

                recieverPayment = recieverPayment.sub(transferCreatorAmount);
            }
            /*
            if (_tokenSetting[id]._tokenDesignerRate > 0) {
                uint256 transferDesignerAmount = _tokenSetting[id]
                    ._tokenMintPrice
                    .mul(amount)
                    .mul(_tokenSetting[id]._tokenDesignerRate)
                    .div(10000);

                _accountBalance[
                    _tokenSetting[id]._tokenDesigner
                ] += transferDesignerAmount;

                transferPayment(
                    _tokenSetting[id]._tokenDesigner,
                    transferDesignerAmount
                );

                recieverPayment = recieverPayment.sub(transferDesignerAmount);
            }
*/
            if (recieverPayment > 0) {
                _accountBalance[_receiver] += recieverPayment;
                transferPayment(_receiver, recieverPayment);
            }
        }
    }

