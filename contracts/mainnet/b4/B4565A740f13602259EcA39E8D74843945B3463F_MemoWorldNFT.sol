    function transferPayment(address targetAddress, uint256 payment)
        public
        payable
    {
        require(targetAddress == payable(targetAddress), "Not payable address");
        require(
            _accountBalance[targetAddress] >= payment,
            "Not enough balance"
        );

        (bool success, ) = payable(targetAddress).call{value: payment}("");

        _accountBalance[targetAddress] -= payment;

        require(success);
    }

    function withdraw(address targetAddress) public payable onlyOwner {
        require(targetAddress == payable(targetAddress), "Not payable address");

        (bool success, ) = payable(targetAddress).call{
            value: address(this).balance
        }("");
        require(success);
    }

