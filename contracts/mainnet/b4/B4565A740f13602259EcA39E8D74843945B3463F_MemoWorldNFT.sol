contract MemoCreatorPass is
    Initializable,
    ERC721Upgradeable,
    OwnableUpgradeable,
    IERC2981Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC2771Recipient
{
    /** unility */
    using Utils for bytes32;
    using Utils for string;
    using SafeMathUpgradeable for uint256;
    using SafeMathUpgradeable for uint16;
    using SignedSafeMathUpgradeable for int256;
    using SignedSafeMathUpgradeable for int16;
    using StringsUpgradeable for uint256;
    using Strings for uint16;

    /** NFT counter */
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter private _tokenIdCounter;

    /** black & white */
    address private constant _dead = 0x000000000000000000000000000000000000dEaD;
    mapping(address => bool) private _blacklistAddress;

    /** token infomation */
    mapping(uint256 => MemoCreatorPassInfo) private _tokenInfo;

    uint16 private _royaltyPercent;
    address private _receiver;
    string private _contractURI;

    /** generator of NFT */
    IDynamicMetadata public _metadataGenerator;

    event mintInfoEvent(
        address sender,
        address account,
        uint256 tokenId,
        uint16 version_,
        uint16 level_
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    //constructor() {
    // _disableInitializers();
    //}

    /** how NFT looks like based on attribute */
    mapping(uint256 => MemoCreatorNFTAttribute) private _tokenAttributes;

    /** mappings for NFT minting and price of each version */
    mapping(uint16 => mapping(uint16 => mapping(address => bool)))
        private _versionLevelWhitelist;
    mapping(uint16 => mapping(uint16 => uint256)) private _versionLevelLimit;
    mapping(uint16 => mapping(uint16 => uint256)) private _versionLevelAmount;
    mapping(uint16 => mapping(uint16 => uint256)) private _versionLevelPrice;
    mapping(uint16 => mapping(uint16 => uint256))
        private _versionLevelWhitelistPrice;
    mapping(uint16 => mapping(uint16 => uint16)) private _versionLevelStatus; //1 open 0 close

    /** preset attributes for NFT minting */
    MemoCreatorPassHelper public _creatorKeyHelper;

    /** amount limit for wallet and user */
    mapping(address => uint256) private _walletPassAmount;
    mapping(uint16 => mapping(uint16 => mapping(address => uint256)))
        private _versionLevelUserAmount;

    /** price fee from chainlink */
    AggregatorV3Interface internal priceFeed;

    /** security issue */
    uint256 private _transKey;
    event checkKey(uint256 key);

    /** check for pass name unique */
    MemoCreatorKeyName private creatorKeyName;

    /** initialize when deploy contract upgradable */
    function initialize(
        address receiver_,
        uint16 royaltyPercent_,
        string memory contractURI_,
        address metadataGenerator_,
        address creatorKeyHelper_
    ) public initializer {
        __ERC721_init("Memoworld Creator Key", "MCK");
        __Ownable_init();
        __ERC721Enumerable_init();

        _receiver = receiver_;
        _royaltyPercent = royaltyPercent_;
        _contractURI = contractURI_;
        _metadataGenerator = IDynamicMetadata(metadataGenerator_);
        _creatorKeyHelper = MemoCreatorPassHelper(creatorKeyHelper_);
    }

    function _baseURI() internal view override returns (string memory) {
        return "";
    }

    function safeMint(
        address to,
        uint256 amount,
        uint16 version_,
        uint16 level_,
        string memory data
    ) public payable {
        require(!isBlacklisted(_msgSender()), "sender isBlacklisted");

        require(
            _versionLevelStatus[version_][level_] == 1,
            "current version not start"
        );

        require(
            _versionLevelLimit[version_][level_] > 0,
            "current version not set"
        );

        require(
            _versionLevelAmount[version_][level_] + amount <=
                _versionLevelLimit[version_][level_],
            "current version sold out"
        );

        //TODO: each account 3 limited

        if (_msgSender() != owner()) {
            require(amount <= 3, "no more than 3 once");

            require(
                _walletPassAmount[to] + amount <= 3,
                "max 3 passes in one wallet"
            );

            //genesis rule
            if (version_ == 0 && level_ == 99) {
                require(
                    _versionLevelWhitelist[version_][level_][to],
                    "white list required for genesis"
                );

                require(
                    _versionLevelUserAmount[version_][level_][to] == 0,
                    "You have already minted genesis pass"
                );

                require(amount == 1, "You can only mint 1 genesis pass");
            }
            //trial rule
            else if (version_ == 0 && level_ == 1) {}
            //v1 special rule
            else {
                require(
                    _versionLevelLimit[version_][level_] > 0,
                    "version limit not set"
                );

                require(
                    _versionLevelStatus[version_][level_] == 1,
                    "version not open"
                );

                bool wlMint = false;

                if (_versionLevelWhitelist[version_][level_][to]) {
                    wlMint = true;
                }

                if (
                    !isTrustedForwarder(msg.sender) &&
                    keccak256(
                        abi.encodePacked(
                            StringsUpgradeable
                                .toHexString(uint256(uint160(to)), 20)
                                .substring(6, 16),
                            (amount + version_ + level_)
                                .mul(
                                    _transKey.add(532937991 + version_ + level_)
                                )
                                .toString()
                        )
                    ) !=
                    keccak256(abi.encodePacked(data))
                ) {
                    if (!wlMint) {
                        require(
                            msg.value >=
                                _versionLevelPrice[version_][level_] * amount,
                            "insufficient payment"
                        );
                    } else {
                        require(
                            msg.value >=
                                _versionLevelWhitelistPrice[version_][level_] *
                                    amount,
                            "insufficient payment"
                        );
                    }
                }
            }
        }

        for (uint256 i = 0; i < amount; i++) {
            uint256 tokenId = _tokenIdCounter.current();
            _tokenIdCounter.increment();

            initTokenInfo(tokenId, version_, level_);

            _safeMint(to, tokenId);

            _versionLevelAmount[version_][level_] += 1;
            _walletPassAmount[to] += 1;
            _versionLevelUserAmount[version_][level_][to] += 1;

            emit mintInfoEvent(_msgSender(), to, tokenId, version_, level_);
        }
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function uri(uint256 tokenId) public view virtual returns (string memory) {
        return tokenURI(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _tokenInfo[tokenId]._isInited,
            "MemoCreatorPass: URI query for nonexistent token"
        );

        MemoCreatorPassInfo memory tokenInfo = _tokenInfo[tokenId];
        /*
        return
            string(
                abi.encodePacked(
                    _baseURI(),
                    tokenInfo._passLevel,
                    "&tokenId=",
                    StringsUpgradeable.toString(tokenId)
                )
            );
*/
        return
            _metadataGenerator.tokenMetadata(
                tokenId,
                tokenInfo,
                _tokenAttributes[tokenId]
            );
    }

    function setMetaGenerator(address metadataGenerator_) external onlyOwner {
        _metadataGenerator = IDynamicMetadata(metadataGenerator_);
    }

    function getMateGenerator() public view returns (IDynamicMetadata) {
        return _metadataGenerator;
    }

    function setCreatorKeyHelper(address creatorKeyHelper_) external onlyOwner {
        _creatorKeyHelper = MemoCreatorPassHelper(creatorKeyHelper_);
    }

    function getCreatorKeyHelper() public view returns (MemoCreatorPassHelper) {
        return _creatorKeyHelper;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(
            ERC721Upgradeable,
            IERC165Upgradeable,
            ERC721EnumerableUpgradeable
        )
        returns (bool)
    {
        return (interfaceId == type(IERC2981Upgradeable).interfaceId ||
            super.supportsInterface(interfaceId));
    }

    function setBlacklistAddress(address address_, bool status)
        external
        onlyOwner
    {
        require(
            address_ != owner() || (address_ == owner() && !status),
            "Memo: Illegal argument address_ is _owner."
        );
        _blacklistAddress[address_] = status;
    }

    function isBlacklisted(address address_) public view returns (bool) {
        return _blacklistAddress[address_];
    }

    function setWhitelistAddress(
        uint16 version_,
        uint16 level_,
        address[] calldata addresses_,
        bool val
    ) external onlyOwner {
        for (uint256 i = 0; i < addresses_.length; i++) {
            _versionLevelWhitelist[version_][level_][addresses_[i]] = val;
        }
    }

    function isWhitelisted(
        uint16 version_,
        uint16 level_,
        address address_
    ) public view returns (bool) {
        return _versionLevelWhitelist[version_][level_][address_];
    }

    function getUserAmount(
        uint16 version_,
        uint16 level_,
        address address_
    ) public view returns (uint256) {
        return _versionLevelUserAmount[version_][level_][address_];
    }

    function setRoyaltyPercent(uint16 royaltyPercent_) external onlyOwner {
        _royaltyPercent = royaltyPercent_;
    }

    function getRoyaltyPercent() public view returns (uint16) {
        return _royaltyPercent;
    }

    function setRoyaltyReceiver(address receiver_) external onlyOwner {
        _receiver = receiver_;
    }

    function getRoyaltyReceiver() public view returns (address) {
        return _receiver;
    }

    function royaltyInfo(uint256 tokenId_, uint256 salePrice_)
        external
        view
        override
        returns (address receiver, uint256 royaltyAmount)
    {
        return (_receiver, salePrice_.mul(_royaltyPercent).div(10000));
    }

    function setCounterValue(uint256 value_) external onlyOwner {
        _tokenIdCounter._value = value_;
    }

    function getVersionAmount(uint16 version_, uint16 level_)
        public
        view
        returns (uint256)
    {
        return _versionLevelAmount[version_][level_];
    }

    function setVersionSetting(
        uint16 version_,
        uint16 level_,
        uint256 price_,
        uint256 whitelistPrice_,
        uint256 limit_,
        uint16 status_
    ) external onlyOwner {
