            _creatorName: string(
                abi.encodePacked("MemoCreatorKey #", Strings.toString(tokenId_))
            ),
            _passStatus: 0,
            _passLevel: level_,
            _creatorAddr: _dead,
            _isInited: true,
            _version: version_
        });

        _tokenInfo[tokenId_] = newTokenInfo;

        uint256 index = _versionLevelAmount[version_][level_];
        if (_creatorKeyHelper.isInited(version_, level_, index)) {
            MemoCreatorNFTAttribute memory attrs = _creatorKeyHelper
                .getTokenAttributes(version_, level_, index);

            _tokenAttributes[tokenId_] = attrs;
        }

