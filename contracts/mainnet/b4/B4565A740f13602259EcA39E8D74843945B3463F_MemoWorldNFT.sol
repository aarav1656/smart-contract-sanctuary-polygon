import "Strings.sol";
import "OwnableUpgradeable.sol";
import "Initializable.sol";

contract MemoCreatorKeyName is Initializable, OwnableUpgradeable {
    /** check for pass name unique */
    mapping(string => bool) private _creatorNameExists;
    mapping(address => bool) private _trusedCallers;

    function initialize() public initializer {
        __Ownable_init();
    }

    function setTrustedCaller(address trustedCaller_, bool truseted_)
        public
        onlyOwner
    {
        _trusedCallers[trustedCaller_] = truseted_;
    }

    function setCreatorName(string calldata creatorName_, bool exists_)
        external
    {
        require(
            _trusedCallers[msg.sender] || msg.sender == owner(),
            "not trust call"
        );
        _creatorNameExists[creatorName_] = exists_;
    }

    function checkCreatorNameExists(string calldata creatorName_)
        external
        view
        returns (bool)
    {
        return _creatorNameExists[creatorName_];
    }
}

