                    msg.value >=
                        _versionLevelPrice[targetVersion_][
                            tokenInfo._passLevel
                        ],
                    "insufficient upgrade payment"
                );
            }

            tokenInfo._version = targetVersion_;

            _tokenInfo[tokenId_] = tokenInfo;
            //_tokenAttributes[tokenId_]._attrValues = attrValues_;

            _versionLevelAmount[targetVersion_][tokenInfo._passLevel] += 1;
            _versionLevelUserAmount[targetVersion_][tokenInfo._passLevel][
                _msgSender()
            ] += 1;
        }
    }

    function checkCreatorNameExists(string calldata creatorName_)
        external
        view
        returns (bool)
    {
        return creatorKeyName.checkCreatorNameExists(creatorName_);
    }

    function setContractURI(string memory contractURI_) external onlyOwner {
        _contractURI = contractURI_;
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function setTrustForwarder(address _trustedForwarder) public onlyOwner {
        _setTrustedForwarder(_trustedForwarder);
    }

    function setPriceFeed(address aggregatorAddress_) public onlyOwner {
        priceFeed = AggregatorV3Interface(aggregatorAddress_);
    }

    function setTransKey(uint256 transkey_) public onlyOwner {
        uint256 randKey = (uint256(
            keccak256(abi.encodePacked(block.timestamp, msg.sender, transkey_))
        ) % transkey_) + transkey_;

        emit checkKey(randKey);

        _transKey = randKey;
    }

