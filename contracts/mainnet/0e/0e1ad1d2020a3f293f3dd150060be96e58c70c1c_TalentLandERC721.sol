import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "../distribution/vault/IDV.sol";

contract Staking is
    Initializable
{   

    mapping(address => uint256) public balanceOf;
    mapping(address => uint256) public rewardDebt;
    mapping(address => uint256) public notClaimed;
    mapping(address => address) public onBehalfOf;

    address public vault;
    address public token;

    uint256 internal _released;

    bool public autoreward;

    uint256 public balance;
    uint256 public accRewardsPerShare;
    uint public lastUpdate;

    /* EVENTS */

    /// @notice Event emitted when the staking account claims rewards
    event Claim (
        address account
    );

    /// @notice Event emitted when the staking account deposits funds
    event Deposit (
        address account,
        uint256 amount
    );

    /// @notice Event emitted when the staking account withdraws funds
    event Withdraw (
        address account,
        uint256 amount
    );

    /// @notice Event emitted when the staking account receives rewards
    event Rewards (
        address account,
        uint256 amount
    );

    /// @notice Event emitted when real balance and virtual balance are synced
    event SyncBalance (
        address indexed recipient,
        uint256 amount
    );
    
    /// @notice Event emitted when autorewards are enabled or disabled
    event ToggleAutoreward (
        bool autoreward
    );

    /// @notice Event emitted when the staking contract is updated
    event Update (
        uint256 balance,
        uint256 accRewardsPerShare,
        uint lastUpdate
    );

    /// @notice Event emitted when the staking account data is updated
    event UpdateData (
        address account,
        uint256 amount,
        uint256 rewardDebt,
        uint256 notClaimed
    );

    /** PRIVATE / INTERNAL METHODS */

    function _update ()
        internal
    {
        IDV dvInterface = IDV(vault);
        uint256 released = dvInterface.releasedFor(address(this)) - _released;
        _released += released;
        if(balance > 0) {
            accRewardsPerShare += (released * 1 ether / balance);
        }
        lastUpdate = block.number;
        emit Update (
            balance,
            accRewardsPerShare,
            lastUpdate
        );
    }

    function _updateData (
        address account
    )
        internal
    {
        uint256 diff = accRewardsPerShare - rewardDebt[account];
        notClaimed[account] += diff * balanceOf[account] / 1 ether;
        rewardDebt[account] = accRewardsPerShare;
        emit UpdateData (
            account,
            balanceOf[account],
            rewardDebt[account],
            notClaimed[account]
        );
    }

    function _reward (
        address account
    )
        internal
    {   
        uint256 pending = notClaimed[account];
        notClaimed[account] = 0;
        IDV(vault).distribute(account, pending);
        emit Rewards(account, pending);
    }

    /** PUBLIC METHODS */

    // solhint-disable-next-line
    function __Staking_init (
        address vault_,
        address token_
    )
        internal
        onlyInitializing
    {
        __Staking_init_unchained(
            vault_,
            token_
        );
    }

    // solhint-disable-next-line
    function __Staking_init_unchained (
        address vault_,
        address token_
    )
        internal
        onlyInitializing
    {
        autoreward = true;
        lastUpdate = block.number;
        vault = vault_;
        token = token_;
        balance = 0;
        accRewardsPerShare = 0;
    }

    function deposit (
        address account,
        uint256 amount
    )
        public
        virtual
    {   
        require(account == msg.sender || msg.sender == onBehalfOf[account], "Staking: unauthorized account");
        require(amount > 0, "Staking: amount must be over zero");

        _update();
        _updateData(account);

        balanceOf[account] += amount;
        balance += amount;

        if(autoreward) {
            _reward(account);
        }

        IERC20Upgradeable(token).transferFrom(account, address(this), amount);

        emit Deposit(
            account,
            amount
        );
    }


    function withdraw (
        address account,
        uint256 amount
    )
        public
        virtual
    {   
        require(account == msg.sender || msg.sender == onBehalfOf[account], "Staking: unauthorized account");
        require(amount > 0, "Staking: amount must be over zero");
        require(amount <= balanceOf[account], "Staking: user has not enough staking balance");

        _update();
        _updateData(account);

        rewardDebt[account] = accRewardsPerShare;
        balanceOf[account] -= amount;
        balance -= amount;

        if(autoreward) {
            _reward(account);
        }

        IERC20Upgradeable(token).transfer(account, amount);

        emit Withdraw(
            account,
            amount
        );
    }

    function claim (
        address account
    )
        public
        virtual
    {
        _update();
        _updateData(account);

        uint256 pending = notClaimed[account];
        require(pending > 0, "Staking: nothing to claim");

        _reward(account);

        emit Claim(account);
    }

    function authorize (
        address account
    )
        public
        virtual
    {
        onBehalfOf[msg.sender] = account;
    }

    function unauthorize (
    )
        public
        virtual
    {
        onBehalfOf[msg.sender] = address(0);
    }

    function _toggleAutoreward (  )
        internal
    {
        autoreward = !autoreward;
        emit ToggleAutoreward(autoreward);
    }

    function pendingRewards (
        address account
    )
        public
        view
        returns ( uint256 )
    {
        uint256 rewards = notClaimed[account];
        if(balance > 0){
            IDV dvInterface = IDV(vault);
            uint256 released = dvInterface.releasedFor(address(this)) - _released;
            uint256 total = (released * 1 ether / balance);
            rewards += (accRewardsPerShare - rewardDebt[account] + total)
            * balanceOf[account] / 1 ether;
        }
        return rewards;
    }

    /**
     * This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     */
    uint256[45] private __gap;
}

