import "./Staking.sol";

interface IERC20BurnableUpgradeable is
    IERC20Upgradeable
{
    function burn(uint256 amount) external;
}

contract StakingWithBurning is
    Staking
{
    mapping(address => uint) public endInterval;
    mapping(address => uint256) public minFeeOf;
    mapping(address => uint256) public maxFeeOf;
    mapping(address => uint256) public feeIntervalOf;

    uint256 public minFee;
    uint256 public maxFee;
    uint public feeInterval;

    /* EVENTS */

    /// @notice Event emitted when the staking fees are updated
    event SetFees (
        uint256 minFee,
        uint256 maxFee,
        uint feeInterval
    );

    /** PUBLIC METHODS */

    // solhint-disable-next-line
    function __StakingWithBurning_init (
        address vault_,
        address token_,
        uint256 minFee_,
        uint256 maxFee_,
        uint feeInterval_
    )
        internal
        onlyInitializing
    {   
        __Staking_init(vault_, token_);
        __StakingWithBurning_init_unchained(
            minFee_,
            maxFee_,
            feeInterval_
        );
    }

    // solhint-disable-next-line
    function __StakingWithBurning_init_unchained (
        uint256 minFee_,
        uint256 maxFee_,
        uint feeInterval_
    )
        internal
        onlyInitializing
    {
        minFee = minFee_;
        maxFee = maxFee_;
        feeInterval = feeInterval_;
    }

    function _setFees (
        uint256 minFee_,
        uint256 maxFee_,
        uint feeInterval_
    )
        internal
    {
        require(minFee_ <= maxFee_, "StakingWithBurning: mininum fee must be greater or equal than maximum fee");
        require(maxFee_ <= 100 ether, "StakingWithBurning: maxFee cannot exceed 100 ether");
        minFee = minFee_;
        maxFee = maxFee_;
        feeInterval = feeInterval_;
        emit SetFees(minFee, maxFee, feeInterval);
    }

    function getFee (
        address account
    )
        public
        view
        returns ( uint256 )
    {   
        uint blocks = getBlocksLeft(account);
        return feeIntervalOf[account] > 0
        ? minFeeOf[account]
            + (maxFeeOf[account] - minFeeOf[account])
            * blocks / feeIntervalOf[account]
        : minFeeOf[account];
    }

    function getBlocksLeft (
        address account
    )
        public
        view
        returns ( uint )
    {   
        return block.number > endInterval[account]
        ? 0
        : endInterval[account] - block.number;
    }

    function deposit (
        address account,
        uint256 amount
    )
        public
        virtual
        override
    {   
        super.deposit(account, amount);
        endInterval[account] = block.number + feeInterval;
        minFeeOf[account] = minFee;
        maxFeeOf[account] = maxFee;
        feeIntervalOf[account] = feeInterval;
    }


    function withdraw (
        address account,
        uint256 amount
    )
        public
        virtual
        override
    {
        require(account == msg.sender || msg.sender == onBehalfOf[account], "StakingWithBurning: unauthorized account");
        require(amount > 0, "StakingWithBurning: amount must be over zero");
        require(amount <= balanceOf[account], "StakingWithBurning: user has not enough staking balance");

        _update();
        _updateData(account);

        rewardDebt[account] = accRewardsPerShare;
        balanceOf[account] -= amount;
        balance -= amount;

        if(autoreward) {
            _reward(account);
        }

        IERC20BurnableUpgradeable tokenInterface = IERC20BurnableUpgradeable(token);

        uint256 burned = amount * getFee(account) / 100 ether;

        if(burned > 0){
            amount -= burned;
            tokenInterface.burn(burned);
        }

        tokenInterface.transfer(account, amount);

        emit Withdraw(
            account,
            amount
        );
    }

    /**
     * This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     */
    uint256[45] private __gap;
}

