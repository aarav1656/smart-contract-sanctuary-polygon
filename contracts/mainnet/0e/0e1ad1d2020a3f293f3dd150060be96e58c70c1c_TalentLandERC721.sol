import "src/lib/jointheredpill/security/upgrade/UUPSUpgradeablePausableByRole.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/SignatureCheckerUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "./interfaces/ITalentLandERC1155.sol";

/**
 * @notice Implementation of TalentLandERC1155
 */
contract TalentLandERC1155 is
    ITalentLandERC1155,
    UUPSUpgradeablePausableByRole,
    ERC1155Upgradeable,
    ERC1155URIStorageUpgradeable,
    EIP712Upgradeable
{

    bytes32 public constant MINT_TYPEHASH = keccak256("Mint(bytes32 code,uint256 limit,address account,uint256 id,uint256 amount,uint256 deadline)");
    bytes32 public constant ERC1155_ADMIN_ROLE = keccak256("ERC1155_ADMIN_ROLE");
    bytes32 public constant ERC1155_AUTH_ROLE = keccak256("ERC1155_AUTH_ROLE");
    bytes32 public constant ERC1155_MINTER_ROLE = keccak256("ERC1155_MINTER_ROLE");

    mapping(bytes32=>uint256) public codeUsed;

    modifier onTime (uint256 deadline) {
        require(deadline >= block.timestamp, "TalentLandERC1155: expired deadline");
        _;
    }

    function initialize (
        string memory uri_
    )
        public
        initializer
    {   
        __ERC1155_init(uri_);
        __ERC1155URIStorage_init();
        __ACC_init(msg.sender);
        __EIP712_init("TalentLandERC1155", "1");
        emit Init(uri_);
    }

    function mintByMinter (
        address account,
        uint256 id,
        uint256 amount
    )
        public
        onlyRole(ERC1155_MINTER_ROLE)
    {
        _mint(account, id, amount, "");
    }

    function mint (
        bytes32 code,
        uint256 limit,
        uint256 id,
        uint256 amount,
        uint256 deadline,
        address signer,
        bytes memory signature
    )
        public
        onTime(deadline)
    {
        require(codeUsed[code] < limit, "TalentLandERC1155: code limit reached");
        require(hasRole(ERC1155_AUTH_ROLE, signer), "TalentLandERC1155: invalid signer");
        bytes32 structHash = keccak256(abi.encode(
            MINT_TYPEHASH,
            code,
            limit,
            _msgSender(),
            id,
            amount,
            deadline
        ));
        bytes32 hash = _hashTypedDataV4(structHash);
        bool validation = SignatureCheckerUpgradeable.isValidSignatureNow(signer, hash, signature);
        require(validation, "TalentLandERC1155: invalid signature");
        codeUsed[code] = codeUsed[code] + 1;
        _mint(_msgSender(), id, amount, "");
        emit MintByCode(
            code,
            limit,
            _msgSender(),
            id,
            amount,
            deadline,
            signer,
            signature
        );
    }

    function burn (
        uint256 id,
        uint256 amount
    )
        public
    {
        _burn(_msgSender(), id, amount);
    }

    function setURI (
        string memory uri_
    )
        public
        onlyRole(ERC1155_ADMIN_ROLE)
    {
        _setURI(uri_);
    }

    function setTokenURI (
        uint256 id,
        string memory uri_
    )
        public
        onlyRole(ERC1155_ADMIN_ROLE)
    {
        _setURI(id, uri_);
    }

    function setBaseURI (
        string memory baseURI
    )
        public
        onlyRole(ERC1155_ADMIN_ROLE)
    {
        _setBaseURI(baseURI);
    }

    // Overrides

    function uri (
        uint256 id
    )
        public
        view
        override(ERC1155Upgradeable, ERC1155URIStorageUpgradeable)
        returns (string memory)
    {
        return ERC1155URIStorageUpgradeable.uri(id);
    }

    function _msgData ()
        internal
        view
        override(ContextUpgradeable, Context)
        returns (bytes calldata)
    {
        return ContextUpgradeable._msgData();
    }

    function _msgSender ()
        internal
        view
        override(ContextUpgradeable, Context)
        returns (address)
    {
        return ContextUpgradeable._msgSender();
    }
}

