import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "./IDV.sol";

/// @title The contract of DV (stands for Distribution Vault)
/// @notice Manages distribution of tokens
abstract contract DV is
  IDV,
  Initializable
{

  /** STRUCTS */

  /// @notice Structure for storing data about a reward account
  struct Info {
    uint256 allocation;
    uint256 released;
    uint256 distributed;
  }

  /** EVENTS */

  /// @notice Event emitted when the contract is initialized
  event Init (
  );

  /// @notice Event emitted when an account is added
  event Add (
    address account
  );

  /// @notice Event emitted when the allocation is updated
  event UpdateAllocation (
    uint256[] allocation
  );

  /// @notice Event emitted when the distribution is made
  event Distribution (
    address token,
    address account,
    uint256 amount
  );

  /** STORAGE */
  
  mapping( address => Info ) public info;

  mapping( uint256 => address ) public id;

  uint256 public addresses;

  uint256 public lastUpdate;

  /** PUBLIC / EXTERNAL METHODS */

  function _add (
    address account,
    uint256[] memory allocation
  )
    internal
  {
    id[addresses] = account;
    info[account] = Info(0,0,0);
    ++addresses;
    _updateAllocation(allocation);
    emit Add(account);
  }


  function _updateAllocation (
    uint256[] memory allocation
  )
    internal
  {
    uint256 sum = 0;
    uint256 length = allocation.length;
    require(length == addresses, "DV: allocation array must have same length as number of addresses");
    for (uint256 i=0; i < length; ++i) {
      info[id[i]].released = releasedFor(id[i]);
      info[id[i]].allocation = allocation[i];
      sum+=allocation[i];
    }
    lastUpdate = block.number;
    require(sum == 100 ether , "DV: total allocation must be 100 ether");
    emit UpdateAllocation(allocation);
  }

  function releasedRange ( 
    uint from,
    uint to
   )
    public
    virtual
    view
    returns ( uint256 )
  { 
    (from, to);
    return 0;
  }

  function availableFor (
    address account
  )
    public
    view
    returns ( uint256 )
  {
    return releasedFor(account) - info[account].distributed;
  }

  function releasedFor (
    address account
  )
    public
    virtual
    view
    returns ( uint256 )
  {
    return info[account].released
    + releasedRange(lastUpdate, block.number)
    * info[account].allocation / 100 ether;
  }

  /// @notice Sender distributes rewards if available
  function _distribute (
    address token,
    address account,
    uint256 amount
  )
    internal
  {
    require(amount <= availableFor(msg.sender), "DV: amount exceeds available");
    info[msg.sender].distributed += amount;

    IERC20Upgradeable tokenInterface = IERC20Upgradeable(token);
    tokenInterface.transfer(account, amount);

    emit Distribution (
      token,
      account,
      amount
    );
  }

  // solhint-disable-next-line func-name-mixedcase
  function __DV_init ()
    internal
    onlyInitializing
  {
    __DV_init_unchained();
  }

  // solhint-disable-next-line func-name-mixedcase
  function __DV_init_unchained ()
    internal
    onlyInitializing
  {
    lastUpdate = block.number;
  }
