import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "./MVUpgradeable.sol";

abstract contract MDERC20 is
  MVUpgradeable,
  Initializable
{

  address public token;
  mapping( address => uint256 ) public claimed;

  // solhint-disable-next-line
  function __MDERC20_init_unchained (
    address token_
  )
    internal
    onlyInitializing
  {
    token = token_;
  }

  // solhint-disable-next-line
  function __MDERC20_init (
    address token_
  )
    internal
    onlyInitializing
  {
    __MDERC20_init_unchained(token_);
  }

  function available (
    uint256 index,
    address account,
    uint256 amount,
    bytes32[] calldata merkleProof
  )
    public
    view
    returns( uint256 )
  {
    _verify(index, account, amount, merkleProof);
    uint256 claimedAmount = claimed[account];
    if (amount > claimedAmount) {
      return amount - claimedAmount;
    } else {
      revert("MDERC20: nothing to claim");
    }
  }

  function _transferFrom (
    address from,
    uint256 index,
    address account,
    uint256 amount,
    bytes32[] calldata merkleProof
  )
    internal
  {
    _verify(index, account, amount, merkleProof);
    IERC20Upgradeable tokenInterface = IERC20Upgradeable(token);
    uint256 transferAmount = available(index, account, amount, merkleProof);
    claimed[account] += transferAmount;
    tokenInterface.transferFrom(from, account, transferAmount);
  }

    function _transfer (
    uint256 index,
    address account,
    uint256 amount,
    bytes32[] calldata merkleProof
  )
    internal
  {
    _verify(index, account, amount, merkleProof);
    IERC20Upgradeable tokenInterface = IERC20Upgradeable(token);
    uint256 transferAmount = available(index, account, amount, merkleProof);
    claimed[account] += transferAmount;
    tokenInterface.transfer(account, transferAmount);
  }
}

