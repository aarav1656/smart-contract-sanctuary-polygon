{
    uint256 public cap;
    uint256 public burned;

    // solhint-disable-next-line func-name-mixedcase
    function __ERC20CappedBurnable_init(
        uint256 cap_
    )
        internal
        onlyInitializing
    {
        __ERC20CappedBurnable_init_unchained(cap_);
    }

    // solhint-disable-next-line func-name-mixedcase
    function __ERC20CappedBurnable_init_unchained(
        uint256 cap_
    )
        internal
        onlyInitializing
    {
        cap = cap_;
    }

    function _mint(
        address account,
        uint256 amount
    )
        internal
        virtual
        override
    {
        require(totalSupply + amount <= cap - burned, "ERC20CappedBurnable: cap exceeded");
        super._mint(account, amount);
    }

    function _burn(
        address account,
        uint256 amount
    )
        internal
        virtual
        override
    {
        burned += amount;
        super._burn(account, amount);
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[50] private __gap;
}

