pragma solidity ^0.8.9;

import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { PullPayment } from "@openzeppelin/contracts/security/PullPayment.sol";
import { ReentrancyGuard } from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import { EnumerableSet } from "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "hardhat/console.sol";

contract Tournament is Ownable, ReentrancyGuard, PullPayment {
    using EnumerableSet for EnumerableSet.AddressSet;

    enum PlayerState {
        Finished,
        Registered
    }

    struct PlayerData {
        address wallet;
        uint256 highscore;
    }

    struct Details {
        string title;
        uint256 participationFee;
        uint256 endTime;
        uint256[] prizes;
    }

    Details private details;
    EnumerableSet.AddressSet private players;
    PlayerData[] private leaderboard;
    bool public isClosed;

    mapping(address => PlayerData) private playerData;
    mapping(address => PlayerState) private playerState;

    event PlayerRegistered(address indexed wallet);
    event LeaderboardUpdated(address indexed wallet, uint256 indexed highscore);

    /// @dev Check if the tournament is ended
    modifier onlyEnded() {
        require(!isOngoing(), "Tournament: Not yet ended");
        _;
    }

    /// @dev Check if the tournament is ongoing
    modifier onlyOngoing() {
        require(isOngoing(), "Tournament: Has ended");
        _;
    }

    /// @dev Check if the tournament is closed
    modifier onlyClosed() {
        require(isClosed, "Tournament: Has not been closed");
        _;
    }

    /// @dev Check if the tournament is closed
    modifier onlyOpen() {
        require(!isClosed, "Tournament: Has been closed");
        _;
    }

    constructor(
        string memory title,
        uint256 fee,
        uint256 endTime,
        uint256[] memory prizes
    ) payable {
        uint256 sum = 0;
        for (uint256 i = 0; i < prizes.length; i++) sum += prizes[i];
        require(msg.value >= sum, "Tournament: Pool must match prizes sum");
        details = Details(title, fee, endTime, prizes);
    }

    /// @dev Sort players by highscore
    function quickSort(int256 left, int256 right) internal {
        if (left >= right) return;
        uint256 pivot = leaderboard[uint256(left + (right - left) / 2)].highscore;
        (int256 i, int256 j) = (left, right);
        while (i <= j) {
            while (leaderboard[uint256(i)].highscore > pivot) i++;
            while (pivot > leaderboard[uint256(j)].highscore) j--;
            if (i <= j) {
                PlayerData memory tmp = leaderboard[uint256(i)];
                leaderboard[uint256(i)] = leaderboard[uint256(j)];
                leaderboard[uint256(j)] = tmp;
                i++;
                j--;
            }
        }
        if (left < j) quickSort(left, j);
        if (i < right) quickSort(i, right);
    }

    function isRegistered() external view onlyOpen returns (bool) {
        return playerState[msg.sender] == PlayerState.Registered;
    }

    /// @dev Returns the tournament prizes
    function getDetails() external view returns (Details memory) {
        return details;
    }

    /// @dev Returns the sorted leaderboard after the tournament has ended
    function getLeaderboard() external view onlyClosed returns (PlayerData[] memory) {
        return leaderboard;
    }

    /// @dev Gets the detailed list of players in the tournament.
    function getPlayers() external view returns (PlayerData[] memory) {
        PlayerData[] memory leaderboardUnordered = new PlayerData[](players.length());
        for (uint256 i = 0; i < players.length(); i++)
            leaderboardUnordered[i] = playerData[players.at(i)];

        return leaderboardUnordered;
    }

    /// @dev Get details of a player in the tournament.
    /// @param wallet The wallet of the player.
    function getPlayer(address wallet) external view returns (PlayerData memory) {
        require(players.contains(wallet), "Tournament: Player has not participated");

        return playerData[wallet];
    }

    /// @dev Register a player to the tournament
    /// @param wallet The wallet of the player
    function register(address wallet) external onlyOwner onlyOpen onlyOngoing {
        if (players.add(wallet)) playerData[wallet].wallet = wallet;
        playerState[wallet] = PlayerState.Registered;
        emit PlayerRegistered(wallet);
    }

    /// @dev Update the highscore of a player (once the game is finished)
    /// @param wallet The wallet of the player
    /// @param score The new score
    function addScore(address wallet, uint256 score) external onlyOwner onlyOpen onlyOngoing {
        require(players.contains(wallet), "Tournament: Player has not participated");
        PlayerData storage player = playerData[wallet];
        require(
            playerState[wallet] == PlayerState.Registered,
            "Tournament: Player already submitted score"
        );

        playerState[wallet] = PlayerState.Finished;
        if (score > player.highscore) player.highscore = score;
        emit LeaderboardUpdated(wallet, player.highscore);
    }

    /// @dev Checks if the tournament is currently ongoing
    function isOngoing() public view onlyOpen returns (bool) {
        return block.timestamp < details.endTime;
    }

    /// @dev End the tournament, distribute prizes
    function end() public nonReentrant onlyOpen onlyEnded {
        uint256 playersCount = players.length();
        for (uint256 i = 0; i < playersCount; i++) leaderboard.push(playerData[players.at(i)]);
        quickSort(0, int256(playersCount - 1));

        for (uint256 i = 0; i < details.prizes.length; i++) {
            if (i < playersCount) _asyncTransfer(leaderboard[i].wallet, details.prizes[i]);
            else _asyncTransfer(owner(), details.prizes[i]);
        }

        isClosed = true;
    }
}

