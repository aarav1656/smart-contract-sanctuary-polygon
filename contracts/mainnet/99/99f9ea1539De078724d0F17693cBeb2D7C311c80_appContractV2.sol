pragma solidity ^0.8.9;
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./appContractStorageV1.sol";
import "./appContractInterfaceV1.sol";
import "@donugr/solidity-library/library/SafeMathUpgradeable.sol";
import "@donugr/solidity-library/library/DateTime.sol";
import "@donugr/solidity-library/library/StringUtils.sol";
import "./helpersdecea.sol";

contract appContractV1 is
  appContractStorageV1,
  Initializable,
  OwnableUpgradeable,
  UUPSUpgradeable,
  appContractInterfaceV1
{
  using StringUtils for *;
  using SafeMathUpgradeable for *;
  using DateTime for *;
  using DeceaHelpers for *;

  function _authorizeUpgrade(address) internal override onlyOwner {}

  function __DeceaLink_init() internal onlyInitializing {
    initialize();
  }

  function initialize() public initializer {
    addressOwner = _msgSender();
    _appAdministrator.push(
      appAdministratorStruct(_msgSender(), enumRoles.ADMIN, false)
    );
    _appAdministratorIndex[_msgSender()] = 0;
    _appAdministratorExists[_msgSender()] = true;
    __Ownable_init_unchained();
  }

  function isAdministrator(
    address senderAddress
  ) internal view virtual returns (bool, enumRoles) {
    if (_appAdministratorExists[senderAddress] == false)
      return (false, enumRoles.GUEST);
    return (
      true,
      _appAdministrator[_appAdministratorIndex[senderAddress]].Roles
    );
  }

  function addUpdateAdministrator(
    address AddressNew,
    uint256 idEnumRoles,
    bool isBlocked
  ) external virtual returns (bool) {
    (bool admExist, enumRoles admRoles) = isAdministrator(_msgSender());
    require(admExist, "Sender Must Administrator");
    require(admRoles == enumRoles.ADMIN, "Sender Roles Must ADMIN");
    require(_msgSender() != AddressNew, "Cant Add Own Address");
    require(
      _appAdministratorExists[AddressNew] == false,
      "Address Already Exist"
    );
    require(AddressNew != addressOwner, "Cant Change Owner");
    enumRoles rolesid;
    if (idEnumRoles == 2) {
      require(
        _msgSender() == addressOwner,
        "To Create Admin, Sender Must Owner"
      );
      rolesid = enumRoles.ADMIN;
    } else if (idEnumRoles == 1) {
      rolesid = enumRoles.MODERATOR;
    } else {
      rolesid = enumRoles.GUEST;
    }
    if (_appAdministratorExists[AddressNew]) {
      _appAdministrator[_appAdministratorIndex[AddressNew]].Roles = rolesid;
      _appAdministrator[_appAdministratorIndex[AddressNew]]
        .isBlocked = isBlocked;
      emit addUpdateAdministratorEvent(
        _msgSender(),
        AddressNew,
        "addUpdateAdministrator",
        "update target data"
      );
    } else {
      _appAdministrator.push(
        appAdministratorStruct(AddressNew, rolesid, isBlocked)
      );
      _appAdministratorIndex[AddressNew] = _appAdministrator.length - 1;
      _appAdministratorExists[AddressNew] = true;
      emit addUpdateAdministratorEvent(
        _msgSender(),
        AddressNew,
        "addUpdateAdministrator",
        "add target"
      );
    }

    return true;
  }

  function addApp(
    string memory appid,
    string memory copyAgreementfrom
  ) external virtual returns (bool) {
    require(_appExists[appid] == false, "App Already Exist");
    (bool admExist, enumRoles admRoles) = isAdministrator(_msgSender());
    require(admExist, "Sender Must Administrator");
    require(admRoles == enumRoles.ADMIN, "Sender Roles Must ADMIN");
    _appExists[appid] = true;
    _appBlocked[appid] = false;
    if (copyAgreementfrom.strNotEmpty()) {
      if (_appExists[copyAgreementfrom]) {
        if (_appAgreementExists[copyAgreementfrom][string("deriv")]) {
          uint256 derivagreeindex = _appAgreementIndex[copyAgreementfrom][
            string("deriv")
          ];
          string memory clonederivId = _appAgreement[copyAgreementfrom][
            derivagreeindex
          ].Id;
          string memory clonederivAgreement = _appAgreement[copyAgreementfrom][
            derivagreeindex
          ].Agreement;
          _appAgreement[appid].push(
            appAgreementStruct(clonederivId, clonederivAgreement)
          );
          _appAgreementIndex[appid][clonederivId] =
            _appAgreement[appid].length -
            1;
          _appAgreementExists[appid][clonederivId] = true;
        }
        if (_appAgreementExists[copyAgreementfrom][string("app")]) {
          uint256 appagreeindex = _appAgreementIndex[copyAgreementfrom][
            string("app")
          ];
          string memory cloneappId = _appAgreement[copyAgreementfrom][
            appagreeindex
          ].Id;
          string memory cloneappAgreement = _appAgreement[copyAgreementfrom][
            appagreeindex
          ].Agreement;
          _appAgreement[appid].push(
            appAgreementStruct(cloneappId, cloneappAgreement)
          );
          _appAgreementIndex[appid][cloneappId] =
            _appAgreement[appid].length -
            1;
          _appAgreementExists[appid][cloneappId] = true;
        }
      }
    }
    emit AppEvent(_msgSender(), "addApp", appid);
    return true;
  }

  function blockUnblockApp(
    string memory appid,
    bool isBlocked
  ) external virtual returns (bool) {
    require(_appExists[appid], "App Not Exist");
    (bool admExist, enumRoles admRoles) = isAdministrator(_msgSender());
    require(admExist, "Sender Must Administrator");
    require(admRoles == enumRoles.ADMIN, "Sender Roles Must ADMIN");
    _appBlocked[appid] = isBlocked;
    emit AppEvent(_msgSender(), "blockUnblockApp", appid);
    return true;
  }

  function UpdateAppAgreement(
    string memory appid,
    string memory idagreement,
    string memory agreement
  ) external virtual returns (bool) {
    require(_appExists[appid], "App Not Exist");
    (bool admExist, enumRoles admRoles) = isAdministrator(_msgSender());
    require(admExist, "Sender Must Administrator");
    require(admRoles == enumRoles.ADMIN, "Sender Roles Must ADMIN");
    require(idagreement.strNotEmpty(), "Id Agreement Cant Empty");
    require(agreement.strNotEmpty(), "Agreement Cant Empty");
    string memory idagreement1 = string("deriv");
    string memory idagreement2 = string("app");
    require(
      (idagreement.strEqual(idagreement1) ||
        idagreement.strEqual(idagreement2)),
      "Id Agreement Can Only deriv or app"
    );
    if (_appAgreementExists[appid][idagreement]) {
      _appAgreement[appid][_appAgreementIndex[appid][idagreement]]
        .Agreement = agreement;
    } else {
      _appAgreement[appid].push(appAgreementStruct(idagreement, agreement));
      _appAgreementIndex[appid][idagreement] = _appAgreement[appid].length - 1;
      _appAgreementExists[appid][idagreement] = true;
    }
    emit AppEvent(_msgSender(), "UpdateAppAgreement", appid);
    return true;
  }

  function addUpdateAppIdList(
    string memory appid,
    uint256 applistid,
    string memory applistdata,
    bool isBlocked
  ) external virtual returns (bool) {
    require(_appExists[appid], "App Not Exist");
    (bool admExist, enumRoles admRoles) = isAdministrator(_msgSender());
    require(admExist, "Sender Must Administrator");
    require(admRoles == enumRoles.ADMIN, "Sender Roles Must ADMIN");
    if (_appIdListExists[appid][applistid]) {
      _appIdList[appid][_appIdListIndex[appid][applistid]].Data = applistdata;
      _appIdList[appid][_appIdListIndex[appid][applistid]]
        .isBlocked = isBlocked;
      emit addUpdateAppIdListEvent(
        _msgSender(),
        "addUpdateAppIdList",
        appid,
        applistid,
        "update"
      );
    } else {
      _appIdList[appid].push(
        appIdListStruct(applistid, applistdata, isBlocked)
      );
      _appIdListIndex[appid][applistid] = _appIdList[appid].length - 1;
      _appIdListExists[appid][applistid] = true;
      emit addUpdateAppIdListEvent(
        _msgSender(),
        "addUpdateAppIdList",
        appid,
        applistid,
        "add"
      );
    }
    return true;
  }

  function getApp(
    string memory appid
  )
    external
    view
    virtual
    returns (
      bool AppBlocked,
      appAgreementStruct[] memory AppAgreement,
      appIdListStruct[] memory AppListId
    )
  {
    if (_appExists[appid]) {
      AppBlocked = _appBlocked[appid];
      AppAgreement = _appAgreement[appid];
      AppListId = _appIdList[appid];
    } else {
      AppBlocked = true;
      AppAgreement = _appAgreement[appid];
      AppListId = _appIdList[appid];
    }
  }

  function emergencyWithdrawAll() external virtual onlyOwner {
    uint256 bal = address(this).balance;
    (bool success, ) = payable(addressOwner).call{value: bal}("");
    require(success, "Error emergencyWithdrawAll");
    emit Transfer(address(this), addressOwner, bal);
  }

  function withdrawToken(address _tokenContract) public virtual onlyOwner {
    IERC20 tokenContract = IERC20(_tokenContract);
    uint256 _amount = tokenContract.balanceOf(address(this));
    bool resapp = tokenContract.approve(address(this), _amount);
    require(resapp, 19.errCode());
    bool restf = tokenContract.transferFrom(
      address(this),
      addressOwner,
      _amount
    );
    require(restf, "Error withdrawToken");

    emit TransferToken(_tokenContract, address(this), addressOwner, _amount);
  }
}

