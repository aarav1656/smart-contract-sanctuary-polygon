library DeceaHelpers {
  using StringUtils for *;
  using SafeMathUpgradeable for *;
  using DateTime for *;
  using DeceaHelpers for *;

  function _tryPercent(
    uint256 a,
    uint256 b
  ) internal pure returns (bool, uint256) {
    unchecked {
      (bool divvalid, uint256 divres) = a.tryDiv(uint256(100));
      if (divvalid == false) return (false, 0);
      return divres.tryMul(b);
    }
  }

  function tryPercent(
    uint256 a,
    uint256 b
  ) internal pure returns (uint256 res) {
    (bool c, uint256 d) = a._tryPercent(b);
    require(c);
    res = d;
  }

  function isContract(address a) internal view {
    uint32 size;
    assembly {
      size := extcodesize(a)
    }
    require(size > 0);
  }

  function isNotContract(address a) internal view {
    require(a.addressValid());
    uint32 size;
    assembly {
      size := extcodesize(a)
    }
    require(size <= 0);
  }

  function strEqual(
    string memory a,
    string memory b
  ) internal pure returns (bool) {
    return keccak256(bytes(a)) == keccak256(bytes(b));
  }

  function strNotEmpty(string memory str) internal pure returns (bool) {
    return (bytes(str).length > 0);
  }

  function strIsYes(string memory str) internal pure returns (bool) {
    string memory strLC = str.strToLower();
    string memory strLCmod = "yes";
    return strLC.strEqual(strLCmod);
  }

  function strIsNo(string memory str) internal pure returns (bool) {
    string memory strLC = str.strToLower();
    string memory strLCmod = "no";
    return strLC.strEqual(strLCmod);
  }

  function strIsNone(string memory str) internal pure returns (bool) {
    string memory strLC = str.strToLower();
    string memory strLCmod = "none";
    return strLC.strEqual(strLCmod);
  }

  function strIsNotEmptyAndNone(
    string memory str
  ) internal pure returns (bool) {
    if (str.strNotEmpty() == false) return false;
    if (str.strIsNone() == false) return false;
    return true;
  }

  function strToLower(string memory str) internal pure returns (string memory) {
    bytes memory bStr = bytes(str);
    bytes memory bLower = new bytes(bStr.length);
    for (uint i = 0; i < bStr.length; i++) {
      if ((uint8(bStr[i]) >= 65) && (uint8(bStr[i]) <= 90)) {
        bLower[i] = bytes1(uint8(bStr[i]) + 32);
      } else {
        bLower[i] = bStr[i];
      }
    }
    return string(bLower);
  }

  function addressNull() internal pure returns (address) {
    return address(0x0000000000000000000000000000000000000000);
  }

  function addressDead() internal pure returns (address) {
    return address(0x000000000000000000000000000000000000dEaD);
  }

  function addressValid(address addr) internal pure returns (bool) {
    if (addr == addressDead()) return false;
    if (addr == addressNull()) return false;
    return true;
  }

  function hasAccessBlockAndUnblockUser(
    address addr,
    address[2] memory addrOwn
  ) internal pure returns (bool) {
    if (addr == addrOwn[0]) return true;
    if (addr == addrOwn[1]) return true;
    return false;
  }

  function errCode(
    uint256 code
  ) internal pure returns (string memory errorres) {
    assembly {
      switch code
      case 0 {
        errorres := "not owner"
      }
      case 1 {
        errorres := "address invalid"
      }
      case 2 {
        errorres := "distributor not exist"
      }
      case 3 {
        errorres := "distributor exist"
      }
      case 4 {
        errorres := "distributor is blocked"
      }
      case 5 {
        errorres := "string is empty"
      }
      case 6 {
        errorres := "agent not exist"
      }
      case 7 {
        errorres := "agent exist"
      }
      case 8 {
        errorres := "agent is blocked"
      }
      case 9 {
        errorres := "value must greater"
      }
      case 10 {
        errorres := "bot not exist"
      }
      case 11 {
        errorres := "bot exist"
      }
      case 12 {
        errorres := "bot is blocked"
      }
      case 13 {
        errorres := "uuid not exist"
      }
      case 14 {
        errorres := "uuid exist"
      }
      case 15 {
        errorres := "uuid is blocked"
      }
      case 16 {
        errorres := "not equal"
      }
      case 17 {
        errorres := "point distributor insufficient"
      }
      case 18 {
        errorres := "transfer failed"
      }
      case 19 {
        errorres := "approve failed"
      }
      default {
        errorres := "undefined"
      }
    }
  }

  function calcCommission(
    uint256 __amount,
    uint256 __commissionDev,
    uint256 __commissionInvestor,
    uint256 __commissionFoundation,
    uint256 __commissionMarketing,
    uint256 __commissionTax,
    uint256 __commissionDistributor,
    uint256 __commissionAgent
  )
    internal
    pure
    returns (
      uint256 _commissionDev,
      uint256 _commissionInvestor,
      uint256 _commissionFoundation,
      uint256 _commissionMarketing,
      uint256 _commissionTax,
      uint256 _commissionDistributor,
      uint256 _commissionAgent
    )
  {
    if (__commissionDev > 0)
      _commissionDev = __amount.tryPercent(__commissionDev);
    if (__commissionInvestor > 0)
      _commissionInvestor = __amount.tryPercent(__commissionInvestor);
    if (__commissionFoundation > 0)
      _commissionFoundation = __amount.tryPercent(__commissionFoundation);
    if (__commissionMarketing > 0)
      _commissionMarketing = __amount.tryPercent(__commissionMarketing);
    if (__commissionTax > 0)
      _commissionTax = __amount.tryPercent(__commissionTax);
    if (__commissionDistributor > 0)
      _commissionDistributor = __amount.tryPercent(__commissionDistributor);
    if (__commissionAgent > 0)
      _commissionAgent = __amount.tryPercent(__commissionAgent);
  }

  function timestampToDateTimeReturnArray(
    uint256 timestamp
  ) internal pure returns (uint256[6] memory) {
    (
      uint256 year,
      uint256 month,
      uint256 day,
      uint256 hour,
      uint256 minute,
      uint256 second
    ) = timestamp.timestampToDateTime();
    return [year, month, day, hour, minute, second];
  }
