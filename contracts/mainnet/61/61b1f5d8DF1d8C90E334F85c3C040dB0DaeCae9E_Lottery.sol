        uint8[] calldata winningNumbers,
        uint256 _maxPlayers
    ) external {
        require(_nftContracts.length > 0, "Lottery: Invalid nftContracts");
        require(winningNumbers.length > 0, "Lottery: Invalid winningNumbers");
        require(_maxPlayers > 0, "Lottery: Invalid maxPlayers");
