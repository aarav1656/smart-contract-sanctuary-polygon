pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import {VerifySignature} from "./library/VerifySignature.sol";

contract Lottery is Ownable, ERC1155Holder, ERC721Holder {
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    using Strings for uint8;
    using Strings for uint256;

    struct Game {
        uint256 id;
        address creator;
        address[] allowedNFTs;
        uint8[] winningNumbers;
        uint256 maxPlayers;
        uint256 currentRound;
        bool isActive;
    }

    struct Player {
        uint256 gameId;
        uint256 roundNumber;
        address playerAddress;
        uint256 chosenNumber;
        address nftContract;
        uint256 nftTokenId;
    }

    struct Round {
        uint256 roundNumber;
        bool isRoundOver;
        uint256 winnerNumber;
    }

    struct NFTItem {
        uint8 nftType; //1: ERC721  2: ERC1155
        address nftContract;
        uint256 nftTokenId;
    }

    Counters.Counter private gameId;
    mapping(uint256 => Game) public games;
    mapping(address => uint256[]) public userGames;
    //mapping(address => mapping(uint256 => bool)) public isCreator;
    mapping(uint256 => mapping(uint256 => mapping(address => bool)))
        public isJoined;
    mapping(uint256 => mapping(uint256 => Round)) public gameRounds;
    mapping(uint256 => mapping(uint256 => Player[])) public gameRoundPlayers;
    mapping(uint256 => mapping(uint256 => NFTItem[])) public gameRoundNFTs;
    mapping(uint256 => mapping(uint256 => Player[])) public gameRoundWinners;

    address public joinSigner;

    event GameCreated(
        uint256 indexed gameId,
        address indexed creator,
        uint8[] winningNumbers,
        address[] allowedNFTs,
        uint256 maxPlayers
    );
    event GameEnded(
        uint256 indexed gameId,
        uint256[] nftBalances,
        uint256[] roundBalances
    );
    event GameRoundStarted(uint256 indexed gameId, uint256 indexed roundNumber);
    event GameRoundEnded(
        uint256 indexed gameId,
        uint256 indexed roundNumber,
        uint8 indexed winnderNumber
    );
    //event GameTerminated(uint256 indexed gameId);

    event PlayerJoined(
        uint256 indexed gameId,
        address indexed player,
        uint256 indexed roundNumber,
        address nftContract,
        uint256 nftId,
        uint8 chosenNumber
    );

