        uint256 id = gameId.current();
        /*
        Game storage newGame = Game({
            id: id,
            creator: msg.sender,
            allowedNFTs: _nftContracts,
            winningNumbers: winningNumbers,
            maxPlayers: _maxPlayers,
            currentRound: 0,
            isActive: true,
            rounds: new Round[](0)
        });
*/

