        games[id].id = id;
        games[id].creator = msg.sender;
        games[id].allowedNFTs = _nftContracts;
        games[id].winningNumbers = winningNumbers;
        games[id].maxPlayers = _maxPlayers;
        games[id].currentRound = 0;
        games[id].isActive = true;
