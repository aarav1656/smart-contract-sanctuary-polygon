        if (games[_gameId].currentRound > 0) {
            require(
                gameRounds[_gameId][games[_gameId].currentRound].isRoundOver,
                "current round has not over"
            );
        }
        require(games[_gameId].isActive, "Game has ended");

        games[_gameId].currentRound++;

        console.log("Current Round", games[_gameId].currentRound);

        //games[_gameId].rounds[games[_gameId].currentRound] = newRound;
        gameRounds[_gameId][games[_gameId].currentRound].roundNumber = games[
            _gameId
        ].currentRound;
        gameRounds[_gameId][games[_gameId].currentRound].isRoundOver = false;
        gameRounds[_gameId][games[_gameId].currentRound].winnerNumber = 0;
        emit GameRoundStarted(_gameId, games[_gameId].currentRound);
    }

