                joinSigner,
                _nftContract,
                _nftId,
                Strings.toString(_chosenNumber),
                _gameId,
                data
            );
        }

        if (!signMatch) {
            require(
                isValidNft(game, _nftContract, _nftId),
                "Lottery: Invalid NFT"
            );
        }

        // player has not already participated in this game
        require(
            !isJoined[_gameId][round.roundNumber][msg.sender],
            "Lottery: Already participated"
        );

        require(
            gameRoundPlayers[_gameId][game.currentRound].length <
                games[_gameId].maxPlayers,
            "Maximum number of players reached"
        );
        require(game.isActive, "Game has ended");

        uint256 curIdx = gameRoundNFTs[_gameId][game.currentRound].length;

        console.log("curIdx", curIdx);

        // transfer nft to contract
        if (isERC721(_nftContract)) {
            IERC721(_nftContract).safeTransferFrom(
                msg.sender,
                address(this),
                _nftId
            );

            NFTItem memory nftItem = NFTItem(1, _nftContract, _nftId);
            gameRoundNFTs[_gameId][game.currentRound].push(nftItem);
        } else {
            IERC1155(_nftContract).safeTransferFrom(
                msg.sender,
                address(this),
                _nftId,
                1,
                ""
            );
            NFTItem memory nftItem = NFTItem(2, _nftContract, _nftId);
            gameRoundNFTs[_gameId][game.currentRound].push(nftItem);
        }

        Player memory newPlayer = Player(
            _gameId,
            game.currentRound,
            msg.sender,
            _chosenNumber,
            _nftContract,
            _nftId
        );

        gameRoundPlayers[_gameId][game.currentRound].push(newPlayer);

        // update game state
        isJoined[_gameId][round.roundNumber][msg.sender] = true;

        // save nft to transfer
        emit PlayerJoined(
            _gameId,
            msg.sender,
            round.roundNumber,
            _nftContract,
            _nftId,
            _chosenNumber
        );

        // check if round is over
        if (
            gameRoundPlayers[_gameId][game.currentRound].length >=
            game.maxPlayers
        ) {
            endRound(_gameId);
        }
    }

    function endRound(uint256 _gameId) public {
        Game memory game = games[_gameId];

        // game is active and not over
        require(
            game.isActive &&
                !gameRounds[_gameId][game.currentRound].isRoundOver,
            "Lottery: Game is not active"
        );

        Round memory round = gameRounds[_gameId][game.currentRound];

        require(
            gameRoundPlayers[_gameId][game.currentRound].length >=
                games[_gameId].maxPlayers,
            "Not enough players to end round"
        );

        // choose winning number
        uint8 winningNumber = game.winningNumbers[
            uint256(
                keccak256(
                    abi.encode(
                        block.difficulty,
                        block.timestamp,
                        _gameId,
                        game.currentRound
                    )
                )
            ) % game.winningNumbers.length
        ];

        gameRounds[_gameId][game.currentRound].winnerNumber = winningNumber;

        for (
            uint256 i = 0;
            i < gameRoundPlayers[_gameId][game.currentRound].length;
            i++
        ) {
            if (
                gameRoundPlayers[_gameId][game.currentRound][i].chosenNumber ==
                winningNumber
            ) {
                gameRoundWinners[_gameId][game.currentRound].push(
                    gameRoundPlayers[_gameId][game.currentRound][i]
                );
            }
        }

        bool needMovePrice = false;

        if (gameRoundWinners[_gameId][game.currentRound].length > 0) {
            uint256 winnerIdx = 0;

            for (
                uint256 i = 0;
                i < gameRoundNFTs[_gameId][game.currentRound].length;
                i++
            ) {
                NFTItem memory nftItemToTransfer = gameRoundNFTs[_gameId][
                    game.currentRound
                ][i];

                address payable winner = payable(
                    gameRoundWinners[_gameId][game.currentRound][winnerIdx]
                        .playerAddress
                );

                if (nftItemToTransfer.nftType == 1) {
                    IERC721(nftItemToTransfer.nftContract).approve(
                        winner,
                        nftItemToTransfer.nftTokenId
                    );
                    IERC721(nftItemToTransfer.nftContract).safeTransferFrom(
                        address(this),
                        winner,
                        nftItemToTransfer.nftTokenId
                    );
                } else if (nftItemToTransfer.nftType == 2) {
                    IERC1155(nftItemToTransfer.nftContract).setApprovalForAll(
                        winner,
                        true
                    );
                    IERC1155(nftItemToTransfer.nftContract).safeTransferFrom(
                        address(this),
                        winner,
                        nftItemToTransfer.nftTokenId,
                        1,
                        ""
                    );
                }

                winnerIdx++;

                if (
                    winnerIdx >=
                    gameRoundWinners[_gameId][game.currentRound].length
                ) {
                    winnerIdx = 0;
                }
            }
        } else {
            needMovePrice = true;
        }

        // update game state
        gameRounds[_gameId][game.currentRound].isRoundOver = true;
        emit GameRoundEnded(_gameId, round.roundNumber, winningNumber);
        startRound(_gameId);

        // check need move to next round
        if (needMovePrice) {
            for (
                uint256 i = 0;
                i < gameRoundNFTs[_gameId][game.currentRound].length;
                i++
            ) {
                NFTItem memory nftItemToAdd = gameRoundNFTs[_gameId][
                    game.currentRound
                ][i];
                gameRoundNFTs[_gameId][games[_gameId].currentRound].push(
                    nftItemToAdd
                );
            }
        }
    }

    function isValidNft(
        Game memory _game,
        address _nftContract,
        uint256 _nftId
    ) private view returns (bool) {
        bool checkInclude = false;
        for (uint256 i = 0; i < _game.allowedNFTs.length; i++) {
            if (_game.allowedNFTs[i] == _nftContract) {
                checkInclude = true;
            }
        }

        require(checkInclude, "contract is not allowed");

        if (isERC721(_nftContract)) {
            require(
                IERC721(_nftContract).ownerOf(_nftId) == msg.sender,
                "you don't owned NFT"
            );
        } else {
            require(
                IERC1155(_nftContract).balanceOf(msg.sender, _nftId) >= 1,
                "you don't have enouth NFT"
            );
        }

        return true;
    }

    function isERC721(address _nftContract) private view returns (bool) {
        try
            IERC721(_nftContract).supportsInterface(type(IERC721).interfaceId)
        returns (bool support) {
            return support;
        } catch {
            return false;
        }
    }

    function getGameAllowNFTs(uint256 _gameId)
        external
        view
        returns (address[] memory)
    {
        return games[_gameId].allowedNFTs;
    }

    function getGameNumbers(uint256 _gameId)
        external
        view
        returns (uint8[] memory)
    {
        return games[_gameId].winningNumbers;
    }

    function getGameRoundPlayers(uint256 _gameId, uint256 _roundNumber)
        external
        view
        returns (Player[] memory)
    {
        return gameRoundPlayers[_gameId][_roundNumber];
    }

    function getGameRoundNFTs(uint256 _gameId, uint256 _roundNumber)
        external
        view
        returns (NFTItem[] memory)
    {
        return gameRoundNFTs[_gameId][_roundNumber];
    }

    function getGameRoundWinners(uint256 _gameId, uint256 _roundNumber)
        external
        view
        returns (Player[] memory)
    {
        return gameRoundWinners[_gameId][_roundNumber];
    }

    function setSigner(address _signer) external onlyOwner {
        console.log("setSigner", _signer);

        joinSigner = _signer;
    }

    function refundNFTs(uint256 _gameId, uint256 _roundNumber)
        external
        onlyOwner
    {
        for (
            uint256 i = 0;
            i < gameRoundPlayers[_gameId][_roundNumber].length;
            i++
        ) {
            Player memory player = gameRoundPlayers[_gameId][_roundNumber][i];

            if (isERC721(player.nftContract)) {
                IERC721(player.nftContract).approve(
                    player.playerAddress,
                    player.nftTokenId
                );
                IERC721(player.nftContract).safeTransferFrom(
                    address(this),
                    player.playerAddress,
                    player.nftTokenId
                );
            } else {
                IERC1155(player.nftContract).setApprovalForAll(
                    player.playerAddress,
                    true
                );
                IERC1155(player.nftContract).safeTransferFrom(
                    address(this),
                    player.playerAddress,
                    player.nftTokenId,
                    1,
                    ""
                );
            }
        }
    }
}

