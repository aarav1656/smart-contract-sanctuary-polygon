    uint256 public bBBackFee = 500;
    uint256 public buyTotalFees = 1500;

    uint256 public sellMATICRewardsFee = 500;
    uint256 public sellMarketingFee = 200;
    uint256 public sellLiquidityFee = 300;
    uint256 public sBBackFee = 500;
    uint256 public sellTotalFees = 1500;

    uint256 public maticRewardsTokens;
    uint256 public marketingTokens;
    uint256 public liquidityTokens;
    uint256 public buybackTokens;
    uint256 public residualTokens;

    address payable public marketingWallet;

    // use by default 300,000 gas to process auto-claiming dividends
    uint256 public gasForProcessing = 300000;

    // exclude from fees and max transaction amount
    mapping (address => bool) private _isExcludedFromFees;
    mapping (address => bool) private _isExcludedMaxSellAmount;

    event UpdateDividendTracker(address indexed newAddress, address indexed oldAddress);

    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);

    event ExcludeFromFees(address indexed account, bool isExcluded);

    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    event GasForProcessingUpdated(uint256 indexed newValue, uint256 indexed oldValue);

    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    event SendDividends(
    	uint256 indexed tokensSwapped,
    	uint256 indexed maticRewards,
    	uint256 indexed buyBack,
    	uint256 marketing
    );

    event SwapETHForTokens(
        uint256 amountIn,
        address[] path
    );

    event ProcessedDividendTracker(
    	uint256 iterations,
    	uint256 claims,
      uint256 lastProcessedIndex,
    	bool indexed automatic,
    	uint256 gas,
    	address indexed processor
    );

    constructor(address _marketingWallet) ERC20("TradeEngine", "TEN") {

      	dividendTracker = new TENDividendTracker();

        marketingWallet = payable(_marketingWallet);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff);
           // Create a uniswap pair for this new token
          address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
              .createPair(address(this), _uniswapV2Router.WETH());

          uniswapV2Router = _uniswapV2Router;
          uniswapV2Pair = _uniswapV2Pair;

          // exclude from receiving dividends
          dividendTracker.excludeFromDividends(address(dividendTracker));
          dividendTracker.excludeFromDividends(address(this));
          dividendTracker.excludeFromDividends(deadAddress);

          // exclude from paying fees or having max transaction amount
          _isExcludedFromFees[deadAddress] = true;
          _isExcludedFromFees[address(this)] = true;
          _isExcludedFromFees[owner()] = true;

          _isExcludedMaxSellAmount[deadAddress] = true;
          _isExcludedMaxSellAmount[address(this)] = true;
          _isExcludedMaxSellAmount[owner()] = true;
          _isExcludedMaxSellAmount[address(_uniswapV2Router)] = true;
          _isExcludedMaxSellAmount[address(uniswapV2Pair)] = true;

          _mint(owner(), 10**11 * 10**18);
    }

    receive() external payable {}

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        if (to == address(0) || from == address(0)) {
            revert InvalidTransfer();
        }

        if (!isTradingEnabled) {
            if (!_isExcludedMaxSellAmount[to] && !_isExcludedMaxSellAmount[from]) {
                revert TradingNotEnabled();
            }
            if (to == uniswapV2Pair){
                isTradingEnabled = true;
                feesPeriod = block.timestamp + 60;
            }
        }

        if(amount == 0) {
            return;
        }

        if (
            from != owner() &&
            to != owner() &&
            to != address(0xdead) &&
            !swapping
        ){
            if (to == uniswapV2Pair) {
                if (!_isExcludedMaxSellAmount[from] && amount > maxSellAmount) {
                    revert ExceedsMaxSellAmount();
                }
            } else if (!_isExcludedMaxSellAmount[to] && amount + balanceOf(to) > maxWallet) {
                revert ExceedsMaxWalletAmount();
            }
        }

        bool canSwap = balanceOf(address(this)) >= swapTokensAtAmount;

        if(
            canSwap &&
            !swapping &&
            swapEnabled &&
            from != uniswapV2Pair &&
            !_isExcludedFromFees[from] &&
            !_isExcludedFromFees[to]
          ) {
              swapping = true;

              uint256 total = maticRewardsTokens + marketingTokens + liquidityTokens + buybackTokens;
              uint256 rewToSwap = swapTokensAtAmount * maticRewardsTokens / total;
              uint256 marToSwap = swapTokensAtAmount * marketingTokens / total;
              uint256 liqToSwap = swapTokensAtAmount * liquidityTokens / total;
              uint256 bbToSwap = swapTokensAtAmount * buybackTokens / total;

              processFees(rewToSwap, marToSwap, liqToSwap, bbToSwap);

              maticRewardsTokens -= rewToSwap;
              marketingTokens -= marToSwap;
              liquidityTokens -= liqToSwap;
              buybackTokens -= bbToSwap;

              swapping = false;
          }

        if(!swapping && !_isExcludedFromFees[from] && !_isExcludedFromFees[to]) {

          uint256 fees;

          if(to == uniswapV2Pair && sellTotalFees != 0) { //sell
              // high fees for the first minute to avoid sniping
              if(block.timestamp <= feesPeriod) {
                  fees = amount * 10 / 11;
              } else {
                  fees = amount * sellTotalFees / 10000;
              }
              maticRewardsTokens += fees * sellMATICRewardsFee / sellTotalFees;
              marketingTokens += fees * sellMarketingFee / sellTotalFees;
              liquidityTokens += fees * sellLiquidityFee / sellTotalFees;
              buybackTokens += fees * sBBackFee / sellTotalFees;
          } else if(from == uniswapV2Pair && buyTotalFees != 0) { //buy
              // high fees for the first minute to avoid sniping
              if(block.timestamp <= feesPeriod) {
                  fees = amount * 10 / 11;
              } else {
                  fees = amount * buyTotalFees / 10000;
              }
              maticRewardsTokens += fees * buyMATICRewardsFee / buyTotalFees;
              marketingTokens += fees * buyMarketingFee / buyTotalFees;
              liquidityTokens += fees * buyLiquidityFee / buyTotalFees;
              buybackTokens += fees * bBBackFee / buyTotalFees;
          }

          // saves us some gas if a normal transfer
          if(fees > 0) {
              amount -= fees;
              super._transfer(from, address(this), fees);
              _moveDelegates(_delegates[from], _delegates[address(this)], fees);
          }
        }

        super._transfer(from, to, amount);
        _moveDelegates(_delegates[from], _delegates[to], amount);

        try dividendTracker.setBalance(payable(from), balanceOf(from)) {} catch {}
        try dividendTracker.setBalance(payable(to), balanceOf(to)) {} catch {}

        if(!swapping) {
    	    	uint256 gas = gasForProcessing;

    	    	try dividendTracker.process(gas) returns (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) {
    	    		emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, true, gas, tx.origin);
    	    	}
    	    	catch {

    	    	}
        }
    }

    function processFees(uint256 _maticRewardTokens, uint256 _marketingTokens, uint256 _liquidityTokens, uint256 _buybackTokens) private {

        _liquidityTokens += residualTokens;

        uint256 half = _liquidityTokens / 2;
        uint256 otherHalf = _liquidityTokens - half;

        uint256 total = _maticRewardTokens + _marketingTokens + half + _buybackTokens;

        _approve(address(this), address(uniswapV2Router), total + otherHalf);

        swapTokensForETH(total);

        uint256 ethBalance = address(this).balance;

        uint256 dividends = ethBalance * _maticRewardTokens / total;
        uint256 marketing = ethBalance * _marketingTokens / total;
        uint256 buyback = ethBalance * _buybackTokens / total;
        uint256 liquidity = ethBalance - dividends - marketing - buyback;

        uint256 amountUsed = addLiquidity(otherHalf, liquidity);

        residualTokens = otherHalf - amountUsed;
        emit SwapAndLiquify(half, ethBalance, otherHalf);

        buyBackAndBurn(buyback);
        address(marketingWallet).call{value: marketing}("");
        (bool success,) = address(dividendTracker).call{value: dividends}("");
        if(success) {
           emit SendDividends(total, dividends, buyback, marketing);
        }

    }

    function swapTokensForETH(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );

    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private returns (uint256) {
        (uint256 amountA,,) = uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            deadAddress,
            block.timestamp
        );
        return amountA;
    }

    function buyBackAndBurn(uint256 amount) private {
        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = address(this);

        // make the swap
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: amount}(
            0, // accept any amount of Tokens
            path,
            deadAddress, // Burn address
            block.timestamp
        );

    }

    function _delegate(address delegator, address delegatee)
        internal override
    {
        address currentDelegate = _delegates[delegator];
        uint256 delegatorBalance = balanceOf(delegator);
        _delegates[delegator] = delegatee;

        emit DelegateChanged(delegator, currentDelegate, delegatee);

        _moveDelegates(currentDelegate, delegatee, delegatorBalance);
    }


    function processDividendTracker(uint256 gas) external {
         (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) = dividendTracker.process(gas);

         emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, false, gas, tx.origin);
    }

    function claim() external {
        dividendTracker.processAccount(payable(msg.sender), false);
    }

    function setSwapAtAmount(uint256 amount) external onlyOwner {
        swapTokensAtAmount = amount;
    }

    function setMaxSellAmount(uint256 _maxSellAmt) external onlyOwner {
        maxSellAmount = _maxSellAmt;
    }

    function setMaxWalletSize(uint256 _maxWalletSize) external onlyOwner {
        maxWallet = _maxWalletSize;
    }

    function setSwapAndLiquifyEnabled(bool _enabled) public onlyOwner {
        swapEnabled = _enabled;
    }

    function updateMarketingWallet(address newWallet) external onlyOwner {
        marketingWallet = payable(newWallet);
    }

    function updateBuyFees(uint256 _rewardsFee, uint256 _marketingFee, uint256 _liquidityFee, uint256 _buyBack) external onlyOwner {
        uint256 total = _rewardsFee + _marketingFee + _liquidityFee + _buyBack;
        if (total > 2000) {         // total fees cannot exceed 20%
            revert InvalidConfiguration();
        }
        buyMATICRewardsFee = _rewardsFee;
        buyMarketingFee = _marketingFee;
        buyLiquidityFee = _liquidityFee;
        bBBackFee = _buyBack;
        buyTotalFees = total;
    }

    function updateSellFees(uint256 _rewardsFee, uint256 _marketingFee, uint256 _liquidityFee, uint256 _buyBack) external onlyOwner {
        uint256 total = _rewardsFee + _marketingFee + _liquidityFee + _buyBack;
        if (total > 2000) {         // total fees cannot exceed 20%
            revert InvalidConfiguration();
        }
        sellMATICRewardsFee = _rewardsFee;
        sellMarketingFee = _marketingFee;
        sellLiquidityFee = _liquidityFee;
        sBBackFee = _buyBack;
        sellTotalFees = total;
    }

    function updateDividendTracker(address newAddress) external onlyOwner {
        require(newAddress != address(dividendTracker), "TEN: Address must change.");

        TENDividendTracker newDividendTracker = TENDividendTracker(payable(newAddress));

        require(newDividendTracker.owner() == address(this), "TEN: Contract must be owner of dividend tracker.");

        newDividendTracker.excludeFromDividends(address(newDividendTracker));
        newDividendTracker.excludeFromDividends(address(this));
        newDividendTracker.excludeFromDividends(address(uniswapV2Router));

        dividendTracker = newDividendTracker;

        emit UpdateDividendTracker(newAddress, address(dividendTracker));
    }

    function setExcludedFromMaxTransaction(address account, bool value) public onlyOwner {
        _isExcludedMaxSellAmount[account] = value;
    }

    function excludeManyFromMaxTransaction(address[] memory accounts) public onlyOwner {
        uint256 len = accounts.length;
        for (uint256 i = 0; i < len; i++) {
            _isExcludedMaxSellAmount[accounts[i]] = true;
        }
    }

    function setExcludedFromFees(address account, bool excluded) external onlyOwner {
        _isExcludedFromFees[account] = excluded;
    }

    function excludeManyFromFees(address[] memory accounts) public onlyOwner {
        uint256 len = accounts.length;
        for (uint256 i = 0; i < len; i++) {
            _isExcludedFromFees[accounts[i]] = true;
        }
    }

    function updateGasForProcessing(uint256 newValue) external onlyOwner {
        require(newValue >= 200000 && newValue <= 500000, "TEN:: Gas amount outside range.");
        require(newValue != gasForProcessing, "TEN:: Must be changing the value.");
        emit GasForProcessingUpdated(newValue, gasForProcessing);
        gasForProcessing = newValue;
    }

    function updateClaimWait(uint256 claimWait) external onlyOwner {
        dividendTracker.updateClaimWait(claimWait);
    }

    function getClaimWait() external view returns(uint256) {
        return dividendTracker.claimWait();
    }

    function getTotalDividendsDistributed() external view returns (uint256) {
        return dividendTracker.totalDividendsDistributed();
    }

    function isExcludedFromFees(address account) external view returns(bool) {
        return _isExcludedFromFees[account];
    }

    function withdrawableDividendOf(address account) external view returns(uint256) {
      return dividendTracker.withdrawableDividendOf(account);
    }

    function dividendTokenBalanceOf(address account) external view returns (uint256) {
      return dividendTracker.balanceOf(account);
    }

    function getAccountDividendsInfo(address account)
        external view returns (
            address,
            int256,
            int256,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256) {
        return dividendTracker.getAccount(account);
    }

    function getAccountDividendsInfoAtIndex(uint256 index)
          external view returns (
              address,
              int256,
              int256,
              uint256,
              uint256,
              uint256,
              uint256,
              uint256) {
        return dividendTracker.getAccountAtIndex(index);
    }

    function getLastProcessedIndex() external view returns(uint256) {
        return dividendTracker.getLastProcessedIndex();
    }

    function getNumberOfDividendTokenHolders() external view returns(uint256) {
        return dividendTracker.getNumberOfTokenHolders();
    }

}