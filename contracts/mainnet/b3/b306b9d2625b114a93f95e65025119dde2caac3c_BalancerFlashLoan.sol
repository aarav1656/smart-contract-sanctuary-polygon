// File: contracts/Balancer/BalancerFlashLoan.sol



pragma solidity ^0.8.0;










contract BalancerFlashLoan is IFlashLoanRecipient{
    using SafeMath for uint256;

    address public immutable vault;
    ILendingPool public immutable lendingPool;
    IPriceOracleGetter public immutable aaveOracle;
    Swapper public immutable swapper;
    Swapper public immutable quoter;
    bool deleverage = false;

    constructor() {
        vault = 0xBA12222222228d8Ba445958a75a0704d566BF2C8;
        lendingPool = ILendingPool(0x8dFf5E27EA6b7AC08EbFdf9eB090F32ee9a30fcf);
        aaveOracle = IPriceOracleGetter(0x0229F777B0fAb107F9591a41d5F02E4e98dB6f2d);
        quoter = Swapper(0xb27308f9F90D607463bb33eA1BeBb41C27CE5AB6);
        swapper = Swapper(0xE592427A0AEce92De3Edee1F18E0157C05861564);
    }

    function leverage(bytes calldata userData) external {
        (
            uint256 totalCollateralETH,
            uint256 totalDebtETH,
            uint256 availableBorrowsETH,
            uint256 currentLiquidationThreshold,
            uint256 ltv,
        ) = lendingPool.getUserAccountData(msg.sender);
        uint256 currentLTV = 10000*totalDebtETH/totalCollateralETH;
        if(10*currentLTV < (15*ltv - 5*currentLiquidationThreshold)) {
            (,,address collateral, ) = abi.decode(userData, (bytes, bytes, address, address));
            IERC20[] memory tokens = new IERC20[](1);
            uint256[] memory amounts = new uint256[](1);
            tokens[0] = IERC20(collateral);
            amounts[0] = availableBorrowsETH * 10**IERC20Metadata(collateral).decimals()*20000/(10000-ltv) / aaveOracle.getAssetPrice(collateral);
            IBalancerVault(vault).flashLoan(
                IFlashLoanRecipient(address(this)),
                tokens,
                amounts,
                userData
            );
        } else if(2*currentLTV > (currentLiquidationThreshold + ltv)) {
            (,,,address token) = abi.decode(userData, (bytes, bytes, address, address));
            IERC20[] memory tokens = new IERC20[](1);
            uint256[] memory amounts = new uint256[](1);
            tokens[0] = IERC20(token);
            amounts[0] = (10000*totalDebtETH - ltv*totalCollateralETH)/(10000-ltv) * 10**IERC20Metadata(token).decimals() / aaveOracle.getAssetPrice(token);
            deleverage = true;
            IBalancerVault(vault).flashLoan(
                IFlashLoanRecipient(address(this)),
                tokens,
                amounts,
                userData
            );
            deleverage = false;
        }
    }

    function unwind(uint256 amount, bytes memory data) internal {
        (,bytes memory path, address collateral, address token) = abi.decode(data, (bytes, bytes, address, address));
        IERC20(token).approve(address(lendingPool), amount);
        lendingPool.repay(address(token), amount, 2, tx.origin);
        uint256 amountIn = quoter.quoteExactOutput(path, amount);
        DataTypes.ReserveData memory reserveData = lendingPool.getReserveData(collateral);
        IERC20(reserveData.aTokenAddress).transferFrom(tx.origin,address(this), amountIn);
        lendingPool.withdraw(collateral, amountIn, address(this));
        IERC20(collateral).approve(address(swapper), amountIn);
        swapper.exactOutput(Swapper.ExactOutputParams({
            path: path,
            recipient: address(this),
            amountOut: amount,
            amountInMaximum: amountIn,
            deadline: block.timestamp
        }));
        IERC20(token).transfer(vault, amount);
    }

    function wind(uint256 amount, bytes memory data) internal {
        (bytes memory path,, address collateral, address token) = abi.decode(data, (bytes, bytes, address, address));
        uint256 amountOut = amount*100/200;
        uint256 amountIn = quoter.quoteExactOutput(path, amountOut);
        IERC20(collateral).approve(address(lendingPool), amount);
        lendingPool.deposit(address(collateral), amount, tx.origin, 0);
        lendingPool.borrow(token, amountIn, 2, 0, tx.origin);
        IERC20(token).approve(address(swapper), amountIn);
        swapper.exactOutput(Swapper.ExactOutputParams({
            path: path,
            recipient: address(this),
            amountOut: amountOut,
            amountInMaximum: amountIn,
            deadline: block.timestamp
        }));
        DataTypes.ReserveData memory reserveData = lendingPool.getReserveData(collateral);
        IERC20(reserveData.aTokenAddress).transferFrom(tx.origin,address(this), amount-amountOut);
        lendingPool.withdraw(collateral, amount-amountOut, address(this));
        IERC20(collateral).transfer(vault, amount);
    }

    function receiveFlashLoan(
        IERC20[] memory tokens,
        uint256[] memory amounts,
        uint256[] memory feeAmounts,
        bytes memory data
    ) external override {
        require(msg.sender == vault, "nope");
        if(deleverage) {
            unwind(amounts[0], data);
        } else {
            wind(amounts[0], data);
        }
    }

    function flashLoan(
        IERC20[] memory tokens,
        uint256[] memory amounts,
        bytes memory userData
    ) external {
        IBalancerVault(vault).flashLoan(
            IFlashLoanRecipient(address(this)),
            tokens,
            amounts,
            userData
        );
    }
}