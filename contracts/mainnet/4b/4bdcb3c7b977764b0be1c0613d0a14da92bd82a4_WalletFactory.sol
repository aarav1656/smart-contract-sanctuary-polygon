import {SafeProxy4337} from "account-abstraction/gnosis/GnosisSafeProxy4337.sol";
import {EIP4337Manager} from "account-abstraction/gnosis/EIP4337Manager.sol";

contract WalletFactory {
  
  // Will allow deployment without having a user register
  function deployWallet(
    address singleton,
    EIP4337Manager aaModule,
    address owner,
    string memory social
  ) public returns (SafeProxy4337) {
    return new SafeProxy4337{salt: stringToBytes32(social)}(singleton, aaModule, owner);
  }

  function getDeploymentAddress(
    address singleton,
    EIP4337Manager aaModule,
    address owner,
    string memory social
  ) public view returns (address wallet) {
    wallet = address(
      uint160(
        uint256(
          keccak256(
            abi.encodePacked(
              bytes1(0xff),
              address(this),
              stringToBytes32(social),
              keccak256(
                abi.encodePacked(
                  type(SafeProxy4337).creationCode,
                  abi.encode(singleton, aaModule, owner)
                )
              )
            )
          )
        )
      )
    );
  }

  function stringToBytes32(string memory source) public pure returns (bytes32 result) {
    bytes memory tempEmptyStringTest = bytes(source);
    if (tempEmptyStringTest.length == 0) return 0x0;

    assembly {
        result := mload(add(source, 32))
    }
  }
}