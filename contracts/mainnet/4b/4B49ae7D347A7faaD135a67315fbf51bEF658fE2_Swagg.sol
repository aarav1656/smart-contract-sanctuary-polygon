error ERC20SwaggCapExceeded();
  address constant WETH9 = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270; // Polygon's Wrapped Ether (WETH) address
    address constant WMATIC = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270; // Polygon's Wrapped MATIC (WMATIC) address


contract Swagg is
    Context,
    AccessControlEnumerable,
    ERC20("SWAGG", "SWAGR"),
    ERC20Permit("SWAGG"),
    ERC20Burnable
   
{
    using SafeMath for uint256;
    bool init = false;

    uint256 public cap;
    string public description;
    uint256 public hardCap = 1e24 ether;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    constructor()  {
      
        _setupRole( DEFAULT_ADMIN_ROLE, _msgSender());
 
        _grantRole(MINTER_ROLE, _msgSender());
       

    }

    function _init(address owner) public  {
        require(init == false, "already init");
        _mintSupply( 300_000_000_000_000 ether,owner);
        _grantRole(DEFAULT_ADMIN_ROLE, owner);
        _grantRole(MINTER_ROLE, owner);
    }

    function _mintSupply(uint256 _supply, address _to) internal {
        cap = _supply;
        super._mint(_to, _supply);
    }

    function setDescription(string memory _desc) public {
        description = _desc;
    }

    function mint(uint256 amt, address _to) public {
        require(hasRole(MINTER_ROLE, _msgSender()), "ERC20 Swagg: cannot mint must have minter role");
        if (totalSupply().add(amt) > hardCap) {
            revert ERC20SwaggCapExceeded();
        }
        super._mint(_to, amt);
    }

    function _beforeTokenTransfer(address from, address to, uint256 amount) internal virtual override(ERC20) {
        super._beforeTokenTransfer(from, to, amount);
    }
}


