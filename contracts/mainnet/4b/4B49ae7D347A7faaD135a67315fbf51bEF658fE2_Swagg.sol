import "lib/v3-periphery/contracts/interfaces/INonfungiblePositionManager.sol";
import "lib/v3-periphery/contracts/libraries/TransferHelper.sol";
import "./uniswapAddresses.sol";

contract UniswapAKXEcosystemLockerV3 {
    address public immutable positionManagerAddress;
    mapping(uint256 => bool) public isLocked;
    mapping(uint256 => uint256) public lockExp;

    constructor() {
        positionManagerAddress = UniNFPositionManager;
    }

    function lock(uint256 tokenId, uint256 expiration) external returns (bool) {
        require(!isLocked[tokenId], "Token already locked");
        require(expiration > block.timestamp, "cannot expire in the past");

        isLocked[tokenId] = true;
        lockExp[tokenId] = expiration;

        INonfungiblePositionManager(positionManagerAddress).transferFrom(msg.sender, address(this), tokenId);
        return true;
    }

    function unlock(uint256 tokenId) external returns (bool) {
        require(isLocked[tokenId], "Token not locked");
        uint256 current = block.timestamp;
        if (lockExp[tokenId] > current) {
            revert("cannot unlock expiration date not met");
        }
        isLocked[tokenId] = false;
        TransferHelper.safeTransferFrom(positionManagerAddress, address(this), msg.sender, tokenId);
        return true;
    }

   
}

