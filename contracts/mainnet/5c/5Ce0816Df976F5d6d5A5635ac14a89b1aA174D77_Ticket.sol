// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

import "../node_modules/@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "../node_modules/@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
//import "../node_modules/@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "../node_modules/@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "../node_modules/@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "../node_modules/@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "./VerifySig.sol";
import "./Ticket1155.sol";
contract Ticket is VerifySig , AccessControl,Initializable {

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    
    function initialize() public initializer {
         //_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(ADMIN_ROLE, msg.sender);

    }
    
    Ticket1155 public ticket1155;
    IERC20Upgradeable public Token; // USDT
    IERC20Upgradeable public PlatformToken; // 平台币
    address public platformaddr;  //平台账户，用来签名
    address public platformAccount;  //平台账户，用来入账
    uint constant decimals = 1000000000000000000;
    
    struct Issuer {
        uint64 id;
        string attribute; //可以用jsonstring
    }
    
    mapping(address => Issuer) public Issuers; 
    // id -> Issuer
    mapping(uint => Issuer) public IssuerIds; 

    struct Activity {
         uint64 id;  //数据库id
         uint8 state;  //0:正常  1：停用
         uint64 startTime; //时间戳
         uint64 endTime;
         string name;
         string attribute; //可以用jsonstring
         uint16 commission; //佣金，百分比
         uint16 royalty; //版税，占总额的百分比
         uint16 royaltyRate; //与首次购买者分配比例,发行商占比
         
    }
    
    //创建成功  address,ActivityId  --> Activity
    mapping(address => mapping (uint64 => Activity) ) public ActivityMap; 
    
    // address -> activityid[]  获取用户的全部演唱会Id
    mapping(address => uint64[]) public activityArray; 
    
    //activityid -> address
    mapping(uint64 => address) public ActivityHost;
    
    //批次,区域
    struct Batch{
        uint64 batchId;
        uint8 state;  //0:正常  1：停用
        uint64 startTime;  //sales startTime
        uint64 endTime;
        string name;
        uint32 price;
        string area;
        uint64 total;  //总数
        uint64 sale;   //未使用
        uint64 currentCount; //已经销售
        string attribute; //可以用jsonstring
        
    }
    
    //batchId -> address
    mapping(uint => address ) public BatchHost;

    // address,activityId  ->Batch[]
    mapping(address => mapping (uint64 => Batch[]) ) public BatchArray;     
    
    //Batchid -> Batch
    mapping(uint64 => Batch) public Batchs;
    //Batchid -> activityId
    mapping(uint64 => uint64) public BatchToActivity;
    
    mapping(address => uint) public Incomes;
    
    //nft tokenDetail
    struct TokenDetail{
        uint64 batchId;
        address firstBuyer;
    }
        
    // tokenId -> TokenDetail
    mapping(uint => TokenDetail) public TokenDetails;
    
    struct Sales{
        address seller;
        uint price;
        uint8 state; //0:selling  1:selled
    }
    
    // tokenId -> Sales
    mapping(uint => Sales) public SalesMap;

    //sign
    mapping(bytes32 => uint8) signMap;

    struct Checkin{
        uint64 tokenId;
        uint64 activityId;
        uint64 timestamp;
    }
    //tokenid -> checkinlist
    mapping(uint => Checkin[]) public Checkins;

    mapping(address => uint[]) public UserTokens;
    mapping(uint => uint) public tokenIndex;
    
    event OnSell(address indexed seller,uint price,uint tokenId);
    event TransfertTicket(address indexed buyer,uint64 activityId,uint64 batchId,uint price,uint tokenId,uint commission,uint royaltyIncome,uint firstBuyerIncomet);    
    
    //createIssuer
    event CreateIssuer(address indexed from, uint64 id, string attribute);
    
    //创建活动
    event CreateActivity(address indexed from,Activity activity);
    
    //修改活动时间
    event SetActivityTime(address indexed from,uint64 activityId,uint64 startTime,uint64 endTime);

    //设置活动状态
    event SetActivityState(address indexed from,uint64 id,uint8 state);
    
    //创建区域
    event CreateBatch(address indexed from, uint64 activityId, Batch batch);
    
    //修改区域售票时间
    event SetBatchTime(address indexed from,uint64 activityId,uint64 batchId,uint64 startTime,uint64 endTime);
    
    //修改区域票价
    event SetBatchPrice(address indexed from,uint64 batchId,uint32 price);
    
    //修改佣金
    event SetActivityCommission(address indexed from,uint64 activityId,uint16 commission);
     
    //修改区域状态
    event SetBatchState(address indexed from,uint64 batchId,uint8 state);
    
    //买票
    event BuyTicket(address indexed from,uint64 activityId,uint64 batchId,uint price,uint issuerIncome, uint platformIncome,uint tokenId,uint64 currentCount);
    
    event SetRoyalty(address indexed from,uint64 activityId,uint16 royalty,uint16 royaltyRate);

    event TicketUrl(uint tokenId,string url);

    function createIssuer(uint64 id_,string memory attribute_) external {
        require(Issuers[msg.sender].id == 0,"Issuer already exists");        
        Issuer memory i = Issuer(id_,attribute_);
        Issuers[msg.sender] = i;        
        emit CreateIssuer(msg.sender,id_,attribute_);
    }
    
    
    function createActivity(Activity memory activity) public {
       require(Issuers[msg.sender].id != 0,"Issuer does not exist");
       require(ActivityHost[activity.id] == address(0),"Activity already exists");

       if(activity.commission <= 0){
            activity.commission = 5;
       }
       if(activity.royalty <= 0){
            activity.royalty = 10;
       }
       if(activity.royaltyRate <= 0){
            activity.royaltyRate = 50;
       }

       ActivityMap[msg.sender][activity.id] = activity;
       activityArray[msg.sender].push(activity.id);
       ActivityHost[activity.id] = msg.sender;
       emit CreateActivity(msg.sender,activity);
       
    }
    
    function getActivityById(uint64 id) public view  returns(Activity memory){
        return ActivityMap[ActivityHost[id]][id];
    }

    function getActivityId(uint64 batchId) public view  returns(uint64){
        return BatchToActivity[batchId];
    }
    
    
    function createBatch (uint64 activityId_,Batch memory batch_) public {
        //ActivityMap
        require(ActivityMap[msg.sender][activityId_].id != 0,"activity does not exist");
        
        BatchArray[msg.sender][activityId_].push(batch_);
        Batchs[batch_.batchId] = batch_;
        BatchHost[batch_.batchId] = msg.sender;
        BatchToActivity[batch_.batchId] = activityId_;
        emit CreateBatch(msg.sender, activityId_, batch_);
    }
    
    function createAll(Activity memory activity, Batch[] memory batchArray ) external {
         createActivity(activity);
         for (uint i=0;i<batchArray.length;i++){
             createBatch(activity.id,batchArray[i]);
        }
     }
    
    
    function setSalesTime(uint64 activityId ,uint64 batchId,uint64 startTime,uint64 endTime) external{
        //CHECK userAddress
        require(BatchHost[batchId] == msg.sender,"Mismatch Issuer");
        require(batchCheck(activityId,batchId),"Mismatch batch");
        Batch storage batch = Batchs[uint64(batchId)];
        
        batch.startTime = startTime;
        batch.endTime = endTime;
        emit SetBatchTime(msg.sender,activityId, batchId, startTime, endTime);
    }
    
    
    function setCommission(uint64 activityId,uint16 commission) external onlyRole(ADMIN_ROLE){
        Activity storage activity = ActivityMap[ActivityHost[activityId]][activityId];
        activity.commission = commission;
        emit SetActivityCommission(msg.sender,activityId,commission);
    }
    //SetActivityState
    function setActivityState(uint64 activityId,uint8 state) external  {
        Activity storage activity = ActivityMap[ActivityHost[activityId]][activityId];
        activity.state = state;
        emit SetActivityState(msg.sender,activityId,state);
    }

    function setActivityTime(uint64 activityId,uint64 startTime,uint64 endTime) external  {
        Activity storage activity = ActivityMap[ActivityHost[activityId]][activityId];
        activity.startTime = startTime;
        activity.endTime = endTime;
        emit SetActivityTime(msg.sender,activityId,startTime,endTime);
    }

    function setRoyalty(uint64 activityId,uint16 royalty_,uint16 royaltyRate_) external onlyRole(ADMIN_ROLE){
        Activity storage activity = ActivityMap[ActivityHost[activityId]][activityId];
        activity.royalty = royalty_;
        activity.royaltyRate = royaltyRate_;
        emit SetRoyalty(msg.sender,activityId,royalty_,royaltyRate_);
    }
    
    function setBatchState(uint64 activityId,uint64 batchId,uint8 state) external {
        require(BatchHost[batchId] == msg.sender,"Mismatch Issuer");
        require(batchCheck(activityId,batchId),"Mismatch batch");
        Batch storage batch = Batchs[uint64(batchId)];
        
        batch.state = state;
        emit SetBatchState(msg.sender,batchId,state);
    }
    function batchCheck(uint64 activityId ,uint64 batchId)internal view returns(bool){
        for (uint i=0;i<BatchArray[BatchHost[batchId]][activityId].length;i++){
             if(BatchArray[BatchHost[batchId]][activityId][i].batchId == batchId){
                 return true;
             }
        }
        return false;
    }
    
    //SetBatchPrice
    function setBatchPrice(uint64 activityId ,uint64 batchId,uint32 price) external {
        require(BatchHost[batchId] == msg.sender,"Mismatch Issuer");
        require(batchCheck(activityId,batchId),"Mismatch batch");
        Batch storage batch = Batchs[uint64(batchId)];
        batch.price = price;
        emit SetBatchPrice(msg.sender,batchId,price);
    }
    
    
    function buyTicket(uint64 activityId ,uint64 batchId,string memory url,bytes memory sign_) external {
        require(signMap[keccak256(abi.encodePacked(url))] == 0,"old checkVal");
        string memory checkVal = AppendString(Strings.toString(activityId) , Strings.toString(batchId)) ;
        checkVal = AppendString(checkVal,url);
        buyCheck(activityId,batchId,checkVal,sign_,1);
        Batch storage batch = Batchs[uint64(batchId)];
        batch.currentCount ++ ;
        signMap[keccak256(abi.encodePacked(url))] = 1;

        Activity memory activity = getActivityById(activityId);
        //get price ,Commission
        //uint price =  batch.price*decimals;
        uint price = batch.price;
        uint commission = price*activity.commission/100;
        
        Incomes[msg.sender] += (price - commission);
        
        //createNft
        uint tokenId = ticket1155.mintx(msg.sender,1,url);
        TokenDetail memory tokenDetail = TokenDetail(batchId,msg.sender);
        TokenDetails[tokenId] = tokenDetail;
        //add token to array
        UserTokens[msg.sender].push(tokenId);
        tokenIndex[tokenId] = UserTokens[msg.sender].length-1;
        
        //platformAccount
        Token.transferFrom(msg.sender,platformAccount,price);
        Token.transferFrom(platformAccount,BatchHost[batchId],price-commission);
        
        emit BuyTicket(msg.sender,activityId,batchId,price,price-commission, commission,tokenId,batch.currentCount);
        emit TicketUrl(tokenId,url);
    }
    
    function buyCheck(uint64 activityId ,uint64 batchId,string memory checkVal ,bytes memory sign_,uint num) internal view {
    //CHECK sign
        require(platformaddr != address(0),"platformaddr error");
        
        require(VerifySig.verify(platformaddr,checkVal,sign_),"check sign error");
        Batch memory batch = Batchs[uint64(batchId)];
        require(batch.state == 0, "This batch invalid");
        require((batch.currentCount+num <= batch.total), "This batch sell out");
        require(BatchHost[batchId] == ActivityHost[activityId],"Mismatch Issuer");
        
        require(batchCheck(activityId,batchId),"Mismatch batch");
        require(block.timestamp>batch.startTime && block.timestamp<batch.endTime, "out of time");
    }

    function buyTickets(uint64 activityId ,uint64 batchId,string[] memory urls,bytes memory sign_) external {
        string memory checkVal = AppendString(Strings.toString(activityId) , Strings.toString(batchId)) ;        
        for(uint i=0;i<urls.length;i++){
            checkVal = AppendString(checkVal,urls[i]);
            require(signMap[keccak256(abi.encodePacked(urls[i]))] == 0,"old checkVal");
        }
        buyCheck(activityId,batchId,checkVal,sign_,urls.length);
        signMap[keccak256(abi.encodePacked(checkVal))] = 1;
        uint[] memory _amounts;
        _amounts = new uint[](urls.length);
        Batch storage batch = Batchs[uint64(batchId)];
        for(uint i=0;i<urls.length;i++){
            _amounts[i] = 1;
            
            signMap[keccak256(abi.encodePacked(urls[i]))] = 1;
        }
        //createNft
        uint[] memory tokenIds = ticket1155.mintBatchx(msg.sender,_amounts,urls);

        Activity memory activity = getActivityById(activityId);
        //get price ,Commission
        //uint price =  batch.price*decimals;
        uint price = batch.price;
        uint commission = price*activity.commission/100;        
        Incomes[msg.sender] += (price - commission)*tokenIds.length;

        for(uint i=0;i<tokenIds.length;i++){
            //add token to array
            UserTokens[msg.sender].push(tokenIds[i]);
            tokenIndex[tokenIds[i]] = UserTokens[msg.sender].length-1;
            TokenDetail memory tokenDetail = TokenDetail(batchId,msg.sender);
            TokenDetails[tokenIds[i]] = tokenDetail;
            batch.currentCount ++ ;
            emit BuyTicket(msg.sender,activityId,batchId,price,price-commission, commission,tokenIds[i],batch.currentCount);
            emit TicketUrl(tokenIds[i],urls[i]);
        }

        //platformAccount
        Token.transferFrom(msg.sender,platformAccount,price*tokenIds.length);
        Token.transferFrom(platformAccount,BatchHost[batchId],(price-commission)*tokenIds.length);
    }


    //挂售,要先授权给合约！
    function sell(uint tokenId,uint price) external {
        //check ticket
        uint num = ticket1155.balanceOf(msg.sender,tokenId);
        require(num > 0 ,"you are not token owner");
        Sales memory sales = Sales(msg.sender,price,0);
        SalesMap[tokenId] = sales;
        emit OnSell(msg.sender,price,tokenId);
    }
    
    
    function transfertTicket(uint tokenId) external {
        require(SalesMap[tokenId].seller != address(0) && SalesMap[tokenId].state == 0,"ticket not on sale");
        
        TokenDetail memory tokenDetail = TokenDetails[tokenId];
        
        address issuer = BatchHost[tokenDetail.batchId];
        address firstBuyer = tokenDetail.firstBuyer;
        SalesMap[tokenId].state = 1;
        //版税
        uint64 activityId = BatchToActivity[tokenDetail.batchId];
        Activity memory activity = getActivityById(activityId);
        
        //uint price = SalesMap[tokenId].price*decimals;
        uint price = SalesMap[tokenId].price;
        uint royalty = price*activity.royalty/100;
        uint royaltyIncome = royalty*activity.royaltyRate/100; //商家收益直接转？
        
        uint commission = price*activity.commission/100;

        //remove form original user
        removeFromArray(SalesMap[tokenId].seller,tokenIndex[tokenId]);

        //add to user token array
        UserTokens[msg.sender].push(tokenId);
        tokenIndex[tokenId] = UserTokens[msg.sender].length-1;

        Token.transferFrom(msg.sender,platformAccount,price);
        //Token.transferFrom(platformAccount,issuer,commission);
        Token.transferFrom(platformAccount,issuer,royaltyIncome); //发行商收益
        Token.transferFrom(platformAccount,firstBuyer,royalty-royaltyIncome); //第一次购票人收益
        Token.transferFrom(platformAccount,SalesMap[tokenId].seller,price-royalty-commission);

        ticket1155.safeTransferFrom(SalesMap[tokenId].seller, msg.sender, tokenId, 1, '');
        
        emit TransfertTicket(issuer,activityId,tokenDetail.batchId,price,tokenId,commission,royaltyIncome,royalty-royaltyIncome);
    }
    
     function removeFromArray(address user,uint index) internal {
        // Move the last element into the place to delete
        UserTokens[user][index] = UserTokens[user][UserTokens[user].length - 1];
        // Remove the last element
        UserTokens[user].pop();
        tokenIndex[UserTokens[user][index]] = index;
    }

    function setToken(address _token) external onlyRole(DEFAULT_ADMIN_ROLE) {
        Token = IERC20Upgradeable(_token);
    }
    
    function setTicket1155(address _token) external onlyRole(DEFAULT_ADMIN_ROLE) {
        ticket1155 = Ticket1155(_token);
    }
    
    function setPlatformToken(address _token) external onlyRole(DEFAULT_ADMIN_ROLE) {
        PlatformToken = IERC20Upgradeable(_token);
    }
    //address public platformaddr;  //平台账户，用来签名
    function setPlatformAddr(address addr) external onlyRole(DEFAULT_ADMIN_ROLE) {
        platformaddr = addr;
    }
    //address public platformAccount;  //平台账户，用来入账
    function setplatformAccount(address addr) external onlyRole(DEFAULT_ADMIN_ROLE) {
        platformAccount = addr;
    }
    function AppendString(string memory a, string memory b) public pure returns (string memory) {
        return string(bytes.concat(bytes(a), "-", bytes(b)));
    }

    function checkin(Checkin[] memory list) external onlyRole(ADMIN_ROLE){
        for(uint i=0;i<list.length;i++){
            Checkins[list[i].tokenId].push(list[i]);
        }
    }

    function getTokenCount(address userAddress) external view returns (uint) {
        return  UserTokens[userAddress].length;
    }

    function setTokenToArray(address userAddress,uint tokenId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        UserTokens[userAddress].push(tokenId);
        tokenIndex[tokenId] = UserTokens[userAddress].length-1;
    }

}

