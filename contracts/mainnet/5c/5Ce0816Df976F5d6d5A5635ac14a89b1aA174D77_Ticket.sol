pragma solidity ^0.8.7;

import "../node_modules/@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "../node_modules/@openzeppelin/contracts/utils/Counters.sol";
import "../node_modules/@openzeppelin/contracts/access/AccessControl.sol";

contract Ticket1155 is ERC1155 ,AccessControl{

    using Counters for Counters.Counter;
    Counters.Counter private _tokenId;

    mapping(uint => string) private _tokenURIs;
    
    struct MetaData {
        uint32 seatNo;
        uint16 area;
        uint32 level;
    }
    mapping(uint256 => MetaData) private _metaData;
    mapping(uint256 => uint256) private _locked;

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    

    constructor() ERC1155("Tickit") {
         //_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(PAUSER_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
    }

    function mint(address _recipient,uint _amount, string memory _tokenUrl,MetaData memory metaData) public returns(uint _mintTokenId){
        require(hasRole(MINTER_ROLE, msg.sender), "Caller is not miner");
        require(bytes(_tokenUrl).length > 0,"The _tokenUrl is nil");
        require(_amount > 0,"The _amount is nil");
        _tokenId.increment();
        uint newTokenId = _tokenId.current();
        _mint(_recipient, newTokenId,_amount,"");
        _tokenURIs[newTokenId] = _tokenUrl;
        _metaData[newTokenId] = metaData;
        return newTokenId;
    }
    
    function mintx(address _recipient,uint _amount, string memory _tokenUrl) public returns(uint _mintTokenId){
        require(hasRole(MINTER_ROLE, msg.sender), "Caller is not miner");
        require(bytes(_tokenUrl).length > 0,"The _tokenUrl is nil");
        require(_amount > 0,"The _amount is nil");
        _tokenId.increment();
        uint newTokenId = _tokenId.current();
        _mint(_recipient, newTokenId,_amount,"");
        _tokenURIs[newTokenId] = _tokenUrl;
        //_metaData[newTokenId] = metaData;
        return newTokenId;
    }

    function mintBatch(address _recipient,uint[] memory _amounts,string[] memory _tokenUrls,MetaData[] memory metaData) public returns(uint[] memory _mintTokenIds){
        require(hasRole(MINTER_ROLE, msg.sender), "Caller is not miner");
        require(_amounts.length > 0,"The _amounts is nil");
        require(_tokenUrls.length > 0,"The _tokenUrls is nil");
        require(_amounts.length == _tokenUrls.length,"The _tokenUrl length must be Equal _amounts length");
        require(_amounts.length == metaData.length,"The metaData length must be Equal _amounts length");

        uint newTokenId;
        uint[] memory returnTokenIds = new uint[](_amounts.length);

        for(uint256 i = 0; i < _amounts.length; i++){
            _tokenId.increment();
            newTokenId = _tokenId.current();
            _tokenURIs[newTokenId] = _tokenUrls[i];
            _metaData[newTokenId] = metaData[i];
            returnTokenIds[i] = newTokenId;
        }

        _mintBatch(_recipient, returnTokenIds,_amounts,"");

        return returnTokenIds;
    }
    
    
    function mintBatchx(address _recipient,uint[] memory _amounts,string[] memory _tokenUrls) public returns(uint[] memory _mintTokenIds){
        require(hasRole(MINTER_ROLE, msg.sender), "Caller is not miner");
        require(_amounts.length > 0,"The _amounts is nil");
        require(_tokenUrls.length > 0,"The _tokenUrls is nil");
        require(_amounts.length == _tokenUrls.length,"The _tokenUrl length must be Equal _amounts length");

        uint newTokenId;
        uint[] memory returnTokenIds = new uint[](_amounts.length);

        for(uint256 i = 0; i < _amounts.length; i++){
            _tokenId.increment();
            newTokenId = _tokenId.current();
            _tokenURIs[newTokenId] = _tokenUrls[i];
            returnTokenIds[i] = newTokenId;
        }

        _mintBatch(_recipient, returnTokenIds,_amounts,"");

        return returnTokenIds;
    }

    function uri(uint256 _id) public view override returns(string memory _tokenUrl){
        return _tokenURIs[_id];
        
    }

    function uriBatch(uint[] memory _tokenIds) public view returns(string[] memory _tokenUrls){
        require(_tokenIds.length > 0,"The _tokenId must be have");

        string[] memory returnTokenUrl = new string[](_tokenIds.length);

        for(uint256 i=0;i<_tokenIds.length;i++){
            returnTokenUrl[i] = _tokenURIs[_tokenIds[i]];
        }

        return returnTokenUrl;
    }
    
     function tokenMetaData(uint256 tokenId) public view returns (MetaData memory)
    {
        return _metaData[tokenId];
    }
    
    function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl,ERC1155) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

