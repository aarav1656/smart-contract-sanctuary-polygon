    function Withdraw_Balance  ( address dest , uint256 amount  ) public  onlyAdmin nonReentrant returns ( bool )
    {
        (bool success, )= payable(dest).call{value: amount}("");
        return success ; 
    }


    function Withdraw_Balance_StableCoin  ( address dest , uint256 amount ,  address StableCoin  ) public  onlyAdmin nonReentrant returns ( bool )
    {
        require ( ApprovedContracts[StableCoin] == true , "Contract is not Approved" ) ;
        IERC20  token = IERC20(StableCoin) ;
        bool success = token.transfer(dest, amount);
        return success ; 
    }





