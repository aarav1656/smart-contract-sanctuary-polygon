            //require ( msg.value == itemdb.get_item1155(ItemID).price.mul(amount).add( itemdb.get_item1155(ItemID).price.mul(tax).div(100).mul(amount)   ) , "BuyItem - Provided Value is not Enough");

            //require (  itemdb.get_item1155(ItemID).reward == false , "Item is Reward not for sale " ) ;

            require ( itemdb.get_item1155(ItemID).mintedAmount.add(amount) <= itemdb.get_item1155(ItemID).limit , "BuyItem - Limit Exceeded");
            require (  amount > 0 );
            //require (  itemdb.get_item1155(ItemID).reward == false , "Cant Mint Reward Item"    ) ;
            require (  itemdb.get_item1155(ItemID).forSale == true , "Item is not for sale ") ;

            uint256 temp = itemdb.get_item1155(ItemID).stableCoinPrice ;
            uint256 price = ( temp.add(  temp.mul(tax).div(100)   )  ).mul(amount) ;  
            uint256 pay = ( token.allowance( msg.sender  ,  address(this) ).add( token.allowance( msg.sender  ,  address(this) ).mul(tax).div(100)  ) ).mul(amount) ; 

            //marketItem[ItemID].minted = true ; 
            require( price == pay , "BuyItem - Provided Value is not Enough") ;
            bool result = token.transferFrom( msg.sender ,  address(this) , price  );
            require ( result == true , "BuyItem - Transfer Failed" ) ;

            nft1155.mintRequestedToken(msg.sender , itemdb.get_item1155(ItemID).tokenID , amount ,  ItemID) ;  
            uint256 mintAmount = itemdb.get_item1155(ItemID).mintedAmount.add(amount) ; 
            itemdb.update_item1155_mintAmount(  ItemID ,  mintAmount );


            //marketItem[ItemID].minted = true ; 
            //itemdb.create_token1155(ItemID, address(this) , msg.sender , itemdb.get_item1155(ItemID).tokenID , amount);

            // TokenInfo memory tokenitem = TokenInfo ( {  itemID: ItemID ,  PrevOwner : address(this) ,  currentOwner : msg.sender , forSale : false ,  price : 0 ,  tokenID :  marketItem[ItemID].tokenID , nft721: marketItem[ItemID].nft721 , amount:amount }) ; 

            // marketToken[marketItem[ItemID].tokenID] = tokenitem ; 
            //tokenIDS.push(tokenitem) ; 

            emit TokenBuyMint( ItemID  , string(abi.encodePacked(StableCoin))  , msg.sender , 1155 ,  "Buy" , referral  ) ; 

            return itemdb.get_item1155(ItemID).tokenID  ; 

        }


  

