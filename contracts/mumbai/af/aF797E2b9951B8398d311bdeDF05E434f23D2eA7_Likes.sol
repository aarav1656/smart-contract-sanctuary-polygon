// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

interface IComments {

    // Message Stats
    struct MsgStats {
        int likes;
        int comments;
        int reposts;
        int tipsReceived;
        uint postByContract;
        uint256 time;
        uint256 block;
    }

    // The Message data struct
    struct MsgData {
        uint msgID;
        address poster;
        string message;
        uint256 tip;
        uint256 paid;
        string[] hashtags;
        address[] taggedAccounts;
        string asGroup;
        uint256 isRepostOf;
        uint256 isCommentOf;
        string uri;
        uint256 commentLevel;
        MsgStats msgStats;
    }

    // Get a list of a users posts
    function getCommentIDs(uint256 msgID, uint256 startFrom) external view returns(uint256[] memory);

    // Remove a comment from a message
    function removeComment(uint256 commentID, address addressPoster, uint256 isCommentOf) external;

    // Add a new comment to a message
    function saveComment(MsgData memory newComment) external;

    // Get the Comments from an array of IDs
    function getCommentsByIDs(uint256[] memory msgIDs, uint256 isCommentOf) external view returns (string memory);

    // Add stats to a comment
    // statType (1 = like, 2 = comment, 3 = repost, 4 = tip)
    function addStat(uint8 statType, uint256 commentID, int amount, uint256 isCommentOf) external;
}

