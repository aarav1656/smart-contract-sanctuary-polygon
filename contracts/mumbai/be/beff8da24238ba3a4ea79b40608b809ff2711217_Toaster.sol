pragma solidity ^0.8.17;

import "solmate-tokens/ERC721.sol";
import "solmate/auth/Owned.sol";
import "opz/utils/Strings.sol";
import "./Whitelist.sol";
import "forge-std/console.sol";

uint32 constant callbackGasLimit = 300000;
uint8 constant rarities = 3;
uint256 constant maxId = 399;

contract Toaster is ERC721, Owned {

    uint256 private immutable mintPrice;
    bytes32 private immutable PASSWORD_HASH;
    uint256 public lastId = 0;
    string private cid;
    uint256 public immutable publicMintTime;
    Whitelist private immutable whitelist;
    uint8 private missingSecret = 10;

    error RequestNotFound(uint256 reqId);
    error RequestAlreadyFulfilled(uint256 reqId);
    error NotWhitelisted();
    error InsufficientETH(uint256 given, uint256 required);
    error OutOfToasts();
    error OnlyOneToastPerWhitelist(address whitelisted);

    constructor(
        string memory _name, 
        string memory _symbol, 
        uint256 _mintPrice,
        string memory _cid,
        uint256 _publicMintTime,
        address _whitelist,
        bytes32 _password_hash
    ) ERC721(_name, _symbol) Owned(msg.sender) {
        mintPrice=_mintPrice;
        cid=_cid;
        publicMintTime=_publicMintTime;
        whitelist=Whitelist(_whitelist);
        PASSWORD_HASH=_password_hash;
    }
    
    function _whitelistMint(address _to, bytes32[] calldata _merkleProof) internal{
        if(!whitelist.verifyAddress(msg.sender, _merkleProof)) revert NotWhitelisted();
        if(balanceOf(msg.sender) > 0) revert OnlyOneToastPerWhitelist(msg.sender);
        _safeMint(_to, lastId-1);
    }

    function _secretMint(address _to) internal {
        if(missingSecret <= 0) revert OutOfToasts();
        missingSecret = missingSecret - 1;
        _safeMint(_to, maxId+missingSecret+1);
    }

    function mint(address _to, bytes32[] calldata _merkleProof, string memory password) external payable{
        if(block.timestamp > publicMintTime && keccak256(abi.encode(password)) == PASSWORD_HASH) return _secretMint(_to);
        if(lastId >= maxId) revert OutOfToasts();
        lastId = lastId+1;
        if(msg.value != mintPrice) revert InsufficientETH(msg.value, mintPrice);
        if(block.timestamp < publicMintTime) return _whitelistMint(_to, _merkleProof);
        _safeMint(_to, lastId-1);
    }

    function tokenURI(uint256 id) public view override returns (string memory) {
        return string(abi.encodePacked("ipfs://",cid,"/",Strings.toString(id),".json"));
    }

    function withdraw() onlyOwner external {
        payable(owner).transfer(address(this).balance);
    }

    function setTokenURI(string memory _cid) external onlyOwner {
        cid = _cid;
    }
}

