contract WalkyriesNFT is ERC721A, Ownable, ReentrancyGuard {

    using SafeCast for int256;
    using SafeMath for uint256;
    uint256 public constant MAX_SUPPLY = 5000;
    uint256 public constant WHITELIST_LIMIT = 10;
    uint256 public constant PUBLIC_LIMIT = 1;
    uint256 public constant MINT_FEES = 35 ether; //In USD
    uint256 public constant WHITELIST_MINT_FEES = 25 ether; //In USD
    uint256 public currentLimit;

    bool public WhitelistMint;
    bool public GuestlistMint;
    bool public PublicMint;
    
    string private baseURI_;

    Guestlisting public guestlist;
    Whitelisting public whitelist;

    mapping(address=>AggregatorV3Interface) public priceFeed;
    mapping(address=>uint256) private whitelistCounter;
    mapping(address=>uint256) private guestlistCounter;
    mapping(address=>uint256) private publicCounter;

    mapping(IERC20=>bool) public whitelisted;

    constructor() ERC721A("Walkyries NFT", "WLKYRI") {
        PublicMint = true; //
        GuestlistMint = true;
        WhitelistMint = true;
        currentLimit = 1250; //
        whitelisted[IERC20(0xc2132D05D31c914a87C6611C10748AEb04B58e8F)] = true; //USDT 
        whitelisted[IERC20(0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174)] = true; //USDC
        whitelisted[IERC20(0xdAb529f40E671A1D4bF91361c21bf9f0C9712ab7)] = true; //BUSD 

        priceFeed[address(0)] = AggregatorV3Interface(0xAB594600376Ec9fD91F8e885dADF0CE036862dE0); //MATIC 0x0000000000000000000000000000000000000000
        priceFeed[0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619] = AggregatorV3Interface(0xF9680D99D6C9589e2a93a78A04A279e509205945); //WETH 
        priceFeed[0x3BA4c387f786bFEE076A58914F5Bd38d668B42c3] = AggregatorV3Interface(0x82a6c4AF830caa6c97bb504425f6A66165C2c26e); //BNB 
    }
    



    function getWhitelistMinted(address wallet) public view returns(uint256){
        return whitelistCounter[wallet];
    }

    function getPublicMinted(address wallet) public view returns(uint256){
        return publicCounter[wallet];
    }

    function getGuestlistMinted(address wallet) public view returns(uint256){
        return guestlistCounter[wallet];
    }


    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI_;
    }

    function isAllowed(address token) public view returns(bool){
        if(priceFeed[token] != AggregatorV3Interface(address(0)) || whitelisted[IERC20(token)]) {
            return true;
        } else {
            return false;
        }
    }

    function usdToToken(uint256 usdAmount, address token) public view returns(uint256){
        require(whitelisted[IERC20(token)] == true || priceFeed[token] != AggregatorV3Interface(address(0)), "Query for unauthorized token");
        if(whitelisted[IERC20(token)] == true){
            if(token != address(0)){
                uint256 tokenDecimals = IERC20(token).decimals();
                if(tokenDecimals < 18){
                    uint256 usdAmt = usdAmount.div(10**(18 - tokenDecimals));
                    return usdAmt;
                }
            }
        }
        uint256 decimals = pfDecimals(token);
        uint256 livePrice = getTokenPrice(token);
        uint256 amt = (usdAmount.mul(10**decimals)).div(livePrice);
        if(token != address(0)){
            uint256 tokenDecimals = IERC20(token).decimals();
            if(tokenDecimals < 18){
                amt = amt.div(10**(18 - tokenDecimals));
            }
        }
        return amt;
    }

    function getTokenPrice(address token) public view returns (uint) {
        (
            , int price, , , 
        ) = priceFeed[token].latestRoundData();

        return price.toUint256();
    }

    function pfDecimals(address token) public view returns(uint){
        return priceFeed[token].decimals();
    }

    function calculateMintFees(uint256 usdValue, address token) public view returns(uint256){
        return usdToToken(usdValue, token);
    }


    function setPriceFeed(address token, address feed) public onlyOwner {
        priceFeed[token] = AggregatorV3Interface(feed);
    }

    function whitelistToken(IERC20 token, bool status) public onlyOwner {
        whitelisted[token] = status;
    }

    function setContracts(Guestlisting guestlist_, Whitelisting whitelist_) public onlyOwner {
        guestlist = guestlist_;
        whitelist = whitelist_;
    }


    function setBaseURI(string memory URI) public onlyOwner {
        baseURI_ = URI;
    }
    
    function setCurrentLimit(uint256 limit_) public onlyOwner {
        require(limit_ <= MAX_SUPPLY, "MAX SUPPLY EXCEEDED");
        currentLimit = limit_;
    }

    function turnWhitelistMint() public onlyOwner {
        WhitelistMint = !WhitelistMint;
    }

    function turnGuestlistMint() public onlyOwner {
        GuestlistMint = !GuestlistMint;
    }

    function turnPubliclistMint() public onlyOwner {
        PublicMint = !PublicMint;
    }

    function whitelistMint(uint256 quantity, address token) external payable nonReentrant {
        require(WhitelistMint, "WhitelistMint is off");
        require(isAllowed(token), "Token Not Allowed");
        require(whitelist.checkIfWhitelisted(msg.sender), "You are not whitelisted");
        whitelistCounter[msg.sender] += quantity;
        require(getWhitelistMinted(msg.sender) <= WHITELIST_LIMIT, "Whitelist Limit Exceeded");
        require(totalSupply().add(quantity) <= currentLimit && totalSupply().add(quantity) <= MAX_SUPPLY, "Mint would exceed max limit");
        if(token == address(0)){
            uint256 mintFees = calculateMintFees(quantity.mul(WHITELIST_MINT_FEES), token);
            require(msg.value == mintFees, "Send proper mint fees");
        } else if(whitelisted[IERC20(token)]){
            uint256 tokenDecimals = IERC20(token).decimals();
            uint256 amt = quantity.mul(WHITELIST_MINT_FEES);
            if(tokenDecimals < 18){
                amt = amt.div(10**(18 - tokenDecimals));
            }
            TransferHelper.safeTransferFrom(token, msg.sender, owner(), amt);
        } else {
            uint256 mintFees = calculateMintFees(quantity.mul(WHITELIST_MINT_FEES), token);
            TransferHelper.safeTransferFrom(token, msg.sender, owner(), mintFees);
        }
        payable(owner()).transfer(msg.value);
        _safeMint(msg.sender, quantity);
    }

    function guestlistMint(uint256 quantity) external nonReentrant {
        require(GuestlistMint, "GuestlistMint is off");
        require(guestlist.checkIfGuestlisted(msg.sender), "You are not guestlisted");
        guestlistCounter[msg.sender] += quantity;
        uint256 guestLimit = guestlist.checkLimit(msg.sender);
        require(getWhitelistMinted(msg.sender) <= guestLimit, "Your mint Limit exceeded");
        require(totalSupply().add(quantity) <= currentLimit && totalSupply().add(quantity) <= MAX_SUPPLY, "Mint would exceed max limit");
        _safeMint(msg.sender, quantity);
    }

    function publicMint(uint256 quantity, address token) external payable nonReentrant {
        require(PublicMint, "Public Mint is off");
        require(isAllowed(token), "Token Not Allowed");
        publicCounter[msg.sender] += quantity;
        require(totalSupply().add(quantity) <= currentLimit && totalSupply().add(quantity) <= MAX_SUPPLY, "Mint would exceed max limit");
        if(token == address(0)){
            uint256 mintFees = calculateMintFees(quantity.mul(MINT_FEES), token);
            require(msg.value == mintFees, "Send proper mint fees");
        } else if(whitelisted[IERC20(token)]){
            uint256 tokenDecimals = IERC20(token).decimals();
            uint256 amt = quantity.mul(MINT_FEES);
            if(tokenDecimals < 18){
                amt = amt.div(10**(18 - tokenDecimals));
            }
            TransferHelper.safeTransferFrom(token, msg.sender, owner(), amt);
        } else {
            uint256 mintFees = calculateMintFees(quantity.mul(MINT_FEES), token);
            TransferHelper.safeTransferFrom(token, msg.sender, owner(), mintFees);
        }
        payable(owner()).transfer(msg.value);
        _safeMint(msg.sender, quantity);
    }


}