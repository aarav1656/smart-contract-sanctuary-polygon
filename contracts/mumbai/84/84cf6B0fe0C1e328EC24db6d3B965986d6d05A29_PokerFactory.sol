pragma solidity ^0.8.1;

// Uncomment this line to use console.log
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

interface mockToken {
    function transfer(address to, uint256 amount) external returns (bool);

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);

    function approve(address spender, uint256 amount) external returns (bool);

    function balanceOf(address account) external returns (uint256);
}

interface mockTournament {
    function playTournament(uint amount, uint multiplier) external;
}

contract PokerFinance is Ownable {
    mockToken tokenPokerFinance;
    mockTournament tournament;

    enum offerState {
        OPEN,
        TOURNAMENT,
        CLAIM
    }
    offerState public currentState;

    uint public immutable amountTotal; // in absolute value and in ERC20 tokens
    uint public fee; // in 1/1000 so 100 => 10%
    uint public platformFee; // in 1/1000 so 15 => 1'5%
    uint public immutable datetimeLimit;

    uint public amountFinancedTotal; // in absolute value and in ERC20 token
    uint public amountTournamentTotal; // in absolute value and in ERC20 token

    bool public playerClaimed;
    bool public platformClaimed;

    mapping(address => uint) amountFinancedPerPatron;

    event OfferFinanced(
        address indexed addressOffer,
        address indexed addressPlayer,
        address indexed addressPatron,
        uint amountFinanced,
        uint amountTotal,
        uint amountFinancedTotal
    );

    constructor(
        uint _amountTotal,
        uint _fee,
        uint _platformFee,
        uint _datetimeLimit,
        address _tokenPokerFinance,
        address _tournament
    ) {
        require(_fee > _platformFee, "Fee must be higher than the platformFee");

        amountTotal = _amountTotal; // in ERC20
        fee = _fee; // in 1/1000
        platformFee = _platformFee; // in 1/1000
        datetimeLimit = _datetimeLimit; // time until the patrons can finance the player

        amountFinancedTotal = 0;
        currentState = offerState.OPEN;

        tokenPokerFinance = mockToken(_tokenPokerFinance);
        tournament = mockTournament(_tournament);

        playerClaimed = false;
        platformClaimed = false;
    }

    function finance(uint amount) external {
        require(currentState == offerState.OPEN, "offer closed");
        require(block.timestamp < datetimeLimit, "datetime limit exceeded");

        uint amountSent;
        if (amount >= amountTotal - amountFinancedTotal) {
            amountSent = amountTotal - amountFinancedTotal;
            currentState = offerState.TOURNAMENT;
        } else {
            amountSent = amount;
        }

        tokenPokerFinance.transferFrom(msg.sender, address(this), amountSent);
        amountFinancedTotal += amountSent;
        amountFinancedPerPatron[msg.sender] += amountSent;

        emit OfferFinanced(
            address(this),
            owner(),
            msg.sender,
            amountSent,
            amountTotal,
            amountFinancedTotal
        );
    }

    // this function returns the amount financed to the patrons in case the datetimeLimit is reached
    // but the offer was still open <=> the offer didnt receive enough financing before the datetimeLimt
    function retrieveFundsFromFailedOffer() external {
        require(block.timestamp > datetimeLimit, "datetimeLimit not surpassed");
        require(
            currentState == offerState.OPEN,
            "offer received all financing"
        );
        require(
            amountFinancedPerPatron[msg.sender] > 0,
            "you are not a patron"
        );
        uint amountToClaim = amountFinancedPerPatron[msg.sender];
        amountFinancedPerPatron[msg.sender] = 0; // important before sending the tokens back to the patron
        tokenPokerFinance.transfer(msg.sender, amountToClaim);
    }

    // this function simulates a tournament
    // would be more functions probebly in a real case (1 to send, 1 to receive)
    function playTournament(uint multiplier) external onlyOwner {
        require(currentState == offerState.TOURNAMENT, "offer still open");
        require(block.timestamp > datetimeLimit, "datetimeLimit not surpassed");

        // approve allowance
        tokenPokerFinance.approve(address(tournament), amountFinancedTotal);
        tournament.playTournament(amountFinancedTotal, multiplier);

        amountTournamentTotal = tokenPokerFinance.balanceOf(address(this));
        require(
            amountTournamentTotal == (amountFinancedTotal * multiplier) / 1000
        ); // check that we receive the correct amount
        currentState = offerState.CLAIM;
    }

    // function to skip the tournament if needed
    function skipTournament(bool _cancelFees) external onlyOwner {
        require(currentState == offerState.TOURNAMENT, "offer still open");
        require(block.timestamp > datetimeLimit, "datetimeLimit not surpassed");
        if (_cancelFees) {
            fee = 0;
            platformFee = 0;
        }
        amountTournamentTotal = amountFinancedTotal;
        currentState = offerState.CLAIM;
    }

    function claimPlayer() external onlyOwner {
        require(
            playerClaimed == false,
            "the player already claimed the reward"
        );
        require(
            currentState == offerState.CLAIM,
            "offer not ready to claim yet"
        );
        playerClaimed = true; // important before sending the tokens back to the player
        uint amountToClaim = (amountTournamentTotal * (fee - platformFee)) /
            1000;
        tokenPokerFinance.transfer(msg.sender, amountToClaim);
    }

    function claimPatron() external {
        require(
            currentState == offerState.CLAIM,
            "offer not ready to claim yet"
        );
        require(
            amountFinancedPerPatron[msg.sender] > 0,
            "you are not a patron"
        );
        uint amountToClaim = (amountFinancedPerPatron[msg.sender] *
            (amountTournamentTotal * (1000 - fee))) /
            amountFinancedTotal /
            1000;
        amountFinancedPerPatron[msg.sender] = 0; // important before sending the tokens back to the patron
        tokenPokerFinance.transfer(msg.sender, amountToClaim);
    }

    function withdraw() external onlyOwner {
        require(
            platformClaimed == false,
            "the platform already withdraw the reward"
        );
        require(
            currentState == offerState.CLAIM,
            "offer not ready to claim yet"
        );
        platformClaimed = true; // important before sending the tokens back to the platform
        uint amountToClaim = (amountTournamentTotal * platformFee) / 1000;
        tokenPokerFinance.transfer(msg.sender, amountToClaim);
    }

    // view functions
    function getAmountFinancedPerPatron(
        address _address
    ) public view returns (uint) {
        return amountFinancedPerPatron[_address];
    }
}

