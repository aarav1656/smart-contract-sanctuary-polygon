import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "./PokerFinance.sol";

contract PokerFactory is Ownable {
    PokerFinance[] public _pokerFinances;

    uint public offersCreated;

    uint private constant PLATFORM_FEE = 15;

    event PokerFinanceCreated(
        address indexed addressContract,
        address indexed addressPlayer,
        uint amountTotal,
        uint playerFee,
        uint datetimeLimit
    );

    function createPokerFinance(
        uint _amountTotal,
        uint _playerFee,
        uint _datetimeLimit,
        address _tokenPokerFinance,
        address _tournament
    ) public {
        PokerFinance pokerFinance = new PokerFinance(
            _amountTotal,
            _playerFee,
            PLATFORM_FEE,
            _datetimeLimit,
            _tokenPokerFinance,
            _tournament
        );
        _pokerFinances.push(pokerFinance);
        emit PokerFinanceCreated(
            address(pokerFinance),
            msg.sender,
            _amountTotal,
            _playerFee,
            _datetimeLimit
        );
        offersCreated++;
    }

    // view functions
    function getPlatformFee() public pure returns (uint) {
        return PLATFORM_FEE;
    }
}

