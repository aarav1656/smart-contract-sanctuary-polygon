pragma solidity 0.8.15;

import {
    ERC721Upgradeable,
    IERC721MetadataUpgradeable
} from "@openzeppelin-upgradeable/contracts/token/ERC721/ERC721Upgradeable.sol";
import {Counters} from "@openzeppelin/contracts/utils/Counters.sol";
import {Strings} from "@openzeppelin/contracts/utils/Strings.sol";
import {AccessControlUpgradeable} from
    "@openzeppelin-upgradeable/contracts/access/AccessControlUpgradeable.sol";
import {
    ERC2981, IERC165, IERC2981
} from "@openzeppelin/contracts/token/common/ERC2981.sol";
import {IERC721Metadata} from
    "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";
import {INFTFactory} from "./interfaces/INFTFactory.sol";
import {IApprover} from "./interfaces/IApprover.sol";
import {LicenseVersion, CantBeEvilUpgradeable} from "a16z-contracts/contracts/licenses/CantBeEvilUpgradeable.sol";
import {INFT} from "./interfaces/INFT.sol";
import {
    EIP712Upgradeable,
    ECDSAUpgradeable
} from "@openzeppelin-upgradeable/contracts/utils/cryptography/draft-EIP712Upgradeable.sol";
import "operator-filter-registry/upgradeable/DefaultOperatorFiltererUpgradeable.sol";

contract NFT is ERC2981, ERC721Upgradeable, AccessControlUpgradeable, CantBeEvilUpgradeable, EIP712Upgradeable, DefaultOperatorFiltererUpgradeable {
    using Strings for uint256;

    //------------------- Errors ---------------------//

    error TokenIdExceedsMaximumSupply();
    error InvalidNewBaseUri(string);
    error ZeroAddressGlobalRoyaltyReceiver();
    error InvalidTokenURI(string);
    error ReachedMaximumSupply();
    error NotTokenOwner();
    error UnsupportedMutagen();
    error MutationNotAllowed();
    error SignatureExpired();
    error InvalidSignature();
    error RoyaltyExceedsMaxPercentage();
    error NoMoreChangesInRoyaltyAllowed();

    /// TypeHash
    bytes32 private constant _VOUCHER_TYPE_HASH = keccak256(
        "Mutate(uint256 tokenId, address mutagen, uint256 expiry, string memory tokenUri, uint256 nonce)"
    );

    /// Role
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    /// Maximum supply of the nft collection.
    uint256 public maximumSupply;

    /// Base url of the nft collection.
    string private _baseUri;

    /// Keep the address of the factory.
    INFTFactory public factory;

    /// Boolean flag to switch the usage of global royalty info.
    bool public globalRoyaltyInEffect;

    /// Signature nonce count.
    uint256 public nonce;

    /// Maximum royalty percentage
    uint96 public maxRoyalty;

    /// owner of the contract
    address public owner;

    /// Boolean flag if set then can't change the default royalty.
    bool public isAuthorityRevokedToChangeRoyalty;

    /// Counter for the tokenIds.
    Counters.Counter private _nextTokenId;

    /// Optional mapping to kepp the token URI for the tokenIds
    mapping(uint256 => string) private _tokenURIs;

    constructor() {
        _disableInitializers();
    }

    /// Emitted when token is minted.
    event Mint(address indexed _minter, address indexed _to, uint256 _tokenId);

    /// Emitted when the base URI get set.
    event SetBaseURI(string baseUri);

    /// Emitted when the token URI get set.
    event SetTokenURI(uint256 indexed tokenId, string tokenURI);

    /// Emitted when mutation happened on a tokenId
    event Mutation(uint256 indexed tokenId, address _mutagen, string newTokenURI);

    /// @notice Initialize the NFT collection.
    /// @param _maxSupply maximum supply of a collection.
    /// @param baseUri base Url of the nft's metadata.
    /// @param _name name of the collection.
    /// @param _symbol symbol of the collection.
    /// @param _owner owner of the collection.
    /// @param _minter Address of the minter allowed to mint tokenIds.
    /// @param _royaltyReceiver Beneficary of the royalty.
    /// @param _feeNumerator Percentage of fee charged as royalty.
    /// @param _maxRoyaltyPercentage Percentage of maximum fee charged as royalty.
    function initialize(
        uint256 _maxSupply,
        string calldata baseUri,
        string calldata _name,
        string calldata _symbol,
        address _owner,
        address _minter,
        address _royaltyReceiver,
        uint96 _feeNumerator,
        uint96 _maxRoyaltyPercentage
    )
        external
        initializer
    {
        __ERC721_init(_name, _symbol);
        __EIP712_init(_name, "1");
        __AccessControl_init();
        __CantBeEvil_init(LicenseVersion.CBE_CC0);
        __DefaultOperatorFilterer_init();
        _setupRole(DEFAULT_ADMIN_ROLE, _owner);
        _setupRole(MINTER_ROLE, _minter);

        // ---- set royalty details ---- //

        if (_royaltyReceiver != address(0) && _feeNumerator != uint96(0)) {
            _setDefaultRoyalty(_royaltyReceiver, _feeNumerator);
        }

        // ------- sstore ---------//
        factory = INFTFactory(msg.sender);
        maximumSupply = _maxSupply;
        owner = _owner;
        maxRoyalty = _maxRoyaltyPercentage;
        _setBaseUri(baseUri);
    }

    /// @notice Grant role to an account.
    function grantRole(bytes32 role, address account) public virtual override onlyRole(getRoleAdmin(role)) {
        super.grantRole(role, account);
        if (role == DEFAULT_ADMIN_ROLE) {
            owner = account;
        } 
    }

    /// @notice Mint a token and assign it to an address.
    /// @param _to NFT transferred to the given address.
    /// @param _tokenURI URI for token metadata.
    function mint(address _to, string memory _tokenURI)
        external
        onlyRole(MINTER_ROLE)
    {
        // Increment the counter.
        Counters.increment(_nextTokenId);
        // Access the current tokenId.
        uint256 tokenIdToMint = Counters.current(_nextTokenId);
        // Validate whether the tokenId should be less than maximum supply.
        if (tokenIdToMint > maximumSupply) {
            revert TokenIdExceedsMaximumSupply();
        }
        // Validate whether the token URI is empty, revert otherwise add token URI.
        if (bytes(_tokenURI).length == 0) {
            revert InvalidTokenURI(_tokenURI);
        }
        _mint(_to, tokenIdToMint);
        _setTokenUri(tokenIdToMint, _tokenURI);
        emit Mint(msg.sender, _to, tokenIdToMint);
    }

    /// @notice Returns the next tokenId to mint.
    function nextTokenId() external view returns (uint256) {
        if (Counters.current(_nextTokenId) == maximumSupply) {
            revert ReachedMaximumSupply();
        }
        return Counters.current(_nextTokenId) + uint256(1);
    }

    //--------------------------------- ROYALTY APIS ----------------------------------------//

    /// @notice Called with the sale price to determine how much royalty
    //          is owed and to whom.
    /// @param _tokenId - the NFT asset queried for royalty information
    /// @param _salePrice - the sale price of the NFT asset specified by _tokenId
    /// @return receiver - address of who should be sent the royalty payment
    /// @return royaltyAmount - the royalty payment amount for _salePrice
    function royaltyInfo(uint256 _tokenId, uint256 _salePrice)
        public
        view
        virtual
        override (ERC2981)
        returns (address, uint256)
    {
        if (globalRoyaltyInEffect) {
            INFTFactory.GlobalRoyaltyInfo memory info =
                factory.getGlobalRoyaltyInfo();
            if (info.globalRoyaltyReceiver == address(0)) {
                revert ZeroAddressGlobalRoyaltyReceiver();
            }
            return (
                info.globalRoyaltyReceiver,
                info.feeNumerator * _salePrice / _feeDenominator()
            );
        }
        return super.royaltyInfo(_tokenId, _salePrice);
    }

    /// @notice Sets the royalty information that all ids in this contract will default to.
    /// Requirements:
    /// `receiver` cannot be the zero address.
    /// `feeNumerator` cannot be greater than the fee denominator.
    function setDefaultRoyalty(address receiver, uint96 feeNumerator)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {   
        if(isAuthorityRevokedToChangeRoyalty) {
            revert NoMoreChangesInRoyaltyAllowed();
        }
        if (feeNumerator > maxRoyalty) {
            revert RoyaltyExceedsMaxPercentage();
        }
        super._setDefaultRoyalty(receiver, feeNumerator);
    }

    /// @notice Sets the royalty information for a specific token id, overriding the global default.
    /// Requirements:
    /// `receiver` cannot be the zero address.
    /// `feeNumerator` cannot be greater than the fee denominator.
    /// @param tokenId Token identitifer whom royalty information gonna set.
    /// @param receiver Beneficiary of the royalty.
    /// @param feeNumerator Percentage of fee gonna charge as royalty.
    function setTokenRoyalty(
        uint256 tokenId,
        address receiver,
        uint96 feeNumerator
    )
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {   
        if(isAuthorityRevokedToChangeRoyalty) {
            revert NoMoreChangesInRoyaltyAllowed();
        }
        if (feeNumerator > maxRoyalty) {
            revert RoyaltyExceedsMaxPercentage();
        }
        super._setTokenRoyalty(tokenId, receiver, feeNumerator);
    }

    /// @notice Deletes the default royalty information.
    function deleteDefaultRoyalty() external onlyRole(DEFAULT_ADMIN_ROLE) {
        super._deleteDefaultRoyalty();
    }

    /// @notice Global royalty would not be in use after this.
    function closeGlobalRoyalty() external onlyRole(DEFAULT_ADMIN_ROLE) {
        globalRoyaltyInEffect = false;
    }

    /// @notice Global royalty would be in use after this.
    function openGlobalRoyalty() external onlyRole(DEFAULT_ADMIN_ROLE) {
        globalRoyaltyInEffect = true;
    }

    /// @notice Resets royalty information for the token id back to the global default.
    function resetTokenRoyalty(uint256 tokenId)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        super._resetTokenRoyalty(tokenId);
    }

    //--------------------------------- METADATA APIS ----------------------------------------//

    /// @notice Returns the base URI for the contract.
    function baseURI() public view virtual returns (string memory) {
        return _baseUri;
    }

    /// @notice Returns the Uniform Resource Identifier (URI) for `tokenId` token.
    /// @param tokenId Identifier for the token
    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        _requireMinted(tokenId);

        string memory _tokenURI = _tokenURIs[tokenId];
        string memory base = baseURI();

        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return _tokenURI;
        }
        // If both are set, return the tokenURI
        if (bytes(_tokenURI).length > 0) {
            return _tokenURI;
        }
        // If there is a baseURI but no tokenURI, concatenate the tokenID to the baseURI.
        return string(abi.encodePacked(base, tokenId.toString()));
    }

    /// @notice Set the base URI, Only ADMIN can call it.
    /// @param newBaseUri New base uri for the metadata.
    function setBaseUri(string memory newBaseUri)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        _setBaseUri(newBaseUri);
    }

    /// @notice Set the default royalty flag.
    // this flag can be set only once and admin can't change royalty if this flag is set.
    function revokeRoyaltyChangingAuthority()
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        if (isAuthorityRevokedToChangeRoyalty) {
            revert NoMoreChangesInRoyaltyAllowed();
        }
        isAuthorityRevokedToChangeRoyalty = true;
    }

    /// @notice Returns the URI that provides the details of royalty for OpenSea support.
    /// Ref - https://docs.opensea.io/v2.0/docs/contract-level-metadata
    function contractURI() public view returns (string memory) {
        return _baseUri;
    }

    function _setTokenUri(uint256 tokenId, string memory tokenUri) internal {
        _tokenURIs[tokenId] = tokenUri;
        emit SetTokenURI(tokenId, tokenUri);
    }

    function _setBaseUri(string memory uri) internal {
        if (keccak256(bytes(uri)) == keccak256(bytes(""))) {
            revert InvalidNewBaseUri(uri);
        }
        _baseUri = uri;
        emit SetBaseURI(uri);
    }

    //-------- Support for opensea onchain enforcement method to set creator fees ----------------//
    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(address operator, uint256 tokenId) public override onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(address from, address to, uint256 tokenId) public override onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory data)
        public
        override
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    //---------------------------------------------------------------------------------------------//

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override (ERC2981, ERC721Upgradeable, AccessControlUpgradeable, CantBeEvilUpgradeable)
        returns (bool)
    { 
        return
            interfaceId == type(INFT).interfaceId ||
            interfaceId == type(IERC2981).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function _onlyTokenIdOwner(uint256 tokenId) internal view {
        if (ownerOf(tokenId) != msg.sender) {
            revert NotTokenOwner();
        }
    }

    /// @notice Perform the mutation on a tokenID. Only tokenId owner allowed to perform mutation.
    /// @param tokenId Identifier for the token.
    /// @param mutagen Address of the mutagen.
    /// @param expiry Expiry of the signature.
    /// @param tokenUri URI for the mutated tokenId.
    /// @param signature Offchain signature of the authorised address.
    function mutate(
        uint256 tokenId, 
        address mutagen, 
        uint256 expiry, 
        string memory tokenUri, 
        bytes memory signature
    ) external 
    {
        // Check whether caller is tokenId owner.
        _onlyTokenIdOwner(tokenId);
        // Check whether mutagen interface is supported.
        if (!IERC165(mutagen).supportsInterface(factory.mutagenInterfaceId())) {
            revert UnsupportedMutagen();
        }
        // Check whether mutagen is compatible with NFT project.
        if (!IApprover(factory.approver()).isMutationAllowed(mutagen, address(this), tokenId)) {
            revert MutationNotAllowed();
        }
        // Chech whether signature get expired or not.
        if (expiry < block.timestamp) {
            revert SignatureExpired();
        }
        // Verify signature
        _verifySignature(tokenId, mutagen, expiry, tokenUri, signature);
        _setTokenUri(tokenId, tokenUri);
        // Emit event
        emit Mutation(tokenId, mutagen, tokenUri);
    }

    function _verifySignature(
        uint256 tokenId, 
        address mutagen, 
        uint256 expiry, 
        string memory tokenUri, 
        bytes memory signature
    ) internal 
    {   

        nonce = nonce + 1;
        bytes32 messageHash = keccak256(
            abi.encode(
                _VOUCHER_TYPE_HASH,
                tokenId,
                mutagen,
                expiry,
                tokenUri,
                nonce
            )
        );
        bytes32 hash = _hashTypedDataV4(messageHash);
        address recoveredAddress = ECDSAUpgradeable.recover(hash, signature);
        if (
            recoveredAddress == address(0)
                || recoveredAddress != factory.authorisedMutationSigner()
        ) {
            revert InvalidSignature();
        }
    }

    /// @notice Domain separator.
    function DOMAIN_SEPARATOR() external view returns (bytes32) {
        return _domainSeparatorV4();
    }
}

