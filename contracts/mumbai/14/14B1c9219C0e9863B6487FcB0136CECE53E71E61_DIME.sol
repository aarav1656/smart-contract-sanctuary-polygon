//SPDX-License-Identifier: GPL-3.0
pragma solidity ^0.8.2;

import "hardhat/console.sol";

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interfaces/IUserProfiles.sol";
import "./interfaces/IKUtils.sol";
import "./interfaces/IHashtags.sol";
import "./interfaces/ITagged.sol";
import "./interfaces/IPosts.sol";
import "./interfaces/ILikes.sol";
import "./interfaces/IMessageData.sol";
import "./interfaces/IDOOM.sol";
import "./interfaces/IComments.sol";
import "./interfaces/IGroups.sol";

contract DIME is Pausable, ReentrancyGuard {

    // Admins
    mapping (address => bool) admins;

    // Previous contract in chain
    address public prevContract;

    // Next contract in chain
    address public nextContract;

    // contract release number
    uint256 public contractRelease;

    // Total messages posted ever
    uint256 public msgCounter;

    // Total comments posted ever
    uint256 public commentCounter;

    // Total allowed tagged accounts per post
    uint256 public maxTaggedAccounts;

    // Total allowed hashtags per post
    uint256 public maxHashtags;

    // Max length of messages to save (UTF-8 single byte characters only)
    uint256 public maxMessageLength;

    // ERC20 Receiver for payment via ERC20
    IERC20 private paymentToken;

    // Cost to post a message
    uint256 public costToPost;

    // Cost to follow a user
    uint256 public costToFollow;

    // Cost to unfollow a user
    uint256 public costToUnfollow;

    // Cost to tag an account in a message
    uint256 public costToTagAccount;

    // Cost to add a hashtag to a message
    uint256 public costToAddHashtag;

    // Cost to like aa post
    uint256 public costToLike;

    // The max number of messages that can be returned at once
    uint256 public maxMsgReturnCount;

    // Link the User Profiles contract
    IUserProfiles public userProfiles;

    // Link to the KUtils
    IKUtils public KUtils;

    // Link to the Hashtags
    IHashtags public Hashtags;

    // Link to the Tagged Accounts
    ITagged public Tagged;

    // Link to the Posts
    IPosts public Posts;

    // Link to the Likes
    ILikes public Likes;

    // Link to the Message Data
    IMessageData public MessageData;

    // Link to the DOOM Token
    IDOOM public DOOM;

    // Link to the Comments
    IComments private Comments;

    // Link to the Groups
    IGroups private Groups;


    constructor(IERC20 _paymentToken, address _userProfiles, address _kutils) {
        //    constructor(IERC20 _paymentToken, uint256 _costToPost, uint256 _msgCounter, uint256 _maxMessageLength, address _prevContract, uint256 _maxMsgReturnCount, uint256 maxTaggedAccounts, uint256 maxHashtags, uint256 _maxHashtagLength, uint256 _costToTagAccount, uint256 _costToAddHashtag, uint256 _contractRelease) {

        // Setup the payment token
        paymentToken = IERC20(_paymentToken);

        // Setup link to User Profiles
        userProfiles = IUserProfiles(_userProfiles);

        // Setup link to KUtils
        KUtils = IKUtils(_kutils);

        // Setup the default Admin
        admins[msg.sender] = true;

        costToPost = 1;   // In wei
        msgCounter = 0;
        maxMessageLength = 140;
        prevContract = address(0);
        maxMsgReturnCount = 10;
        maxTaggedAccounts = 3;
        maxHashtags = 3;
        costToTagAccount = 1;   // In wei
        costToAddHashtag = 1;   // In wei
        contractRelease = 0;
        costToLike = 1; // In wei
        costToFollow = 1; // in wei
        costToUnfollow = 1; // in wei
    }


    /*

    MODIFIERS

    */

    modifier onlyAdmins() {
        require(admins[msg.sender], "Only admins can call this function.");
        _;
    }


    /*

    ADMIN FUNCTIONS

    */

    function addAdmin(address newAdmin) public onlyAdmins{
        admins[newAdmin] = true;
    }

    function removeAdmin(address oldAdmin) public onlyAdmins{
        admins[oldAdmin] = false;
    }

    function pause() public onlyAdmins {
        _pause();
    }

    function unpause() public onlyAdmins {
        _unpause();
    }

    function setNextContract(address _nextContract) public onlyAdmins {
        nextContract = _nextContract;
    }

    function setParams(uint256 _maxMsgReturnCount, uint256 _maxMessageLength) public onlyAdmins {
        maxMsgReturnCount = _maxMsgReturnCount;
        maxMessageLength = _maxMessageLength;
    }

    function updateContracts(IERC20 _payments, address _userProfiles, address _hashtags, address _tagged, address _posts, address _likes, address _messageData, address _doom, address _comments, address _groups) public onlyAdmins {
        // Update the contract address of the ERC20 token to be used as payment
        paymentToken = IERC20(_payments);

        // Update the User Profiles contract address
        userProfiles = IUserProfiles(_userProfiles);

        // Update the Hashtags address
        Hashtags = IHashtags(_hashtags);

        // Update the Tagged addresses
        Tagged = ITagged(_tagged);

        // Update the Posts addresses
        Posts = IPosts(_posts);

        // Update the Likes addresses
        Likes = ILikes(_likes);

        // Update the Message Data addresses
        MessageData = IMessageData(_messageData);

        // Update the DOOM Token address
        DOOM = IDOOM(_doom);

        // Update the Comments
        Comments = IComments(_comments);

        // Update the Comments
        Groups = IGroups(_groups);
    }

    /*

    PUBLIC FUNCTIONS

    */

    // Comment Attributes
    // 0 = Comment Level Allowed (0 = No comments Allowed, 1 = Comments Allowed)
    // 1 = Message ID of the post it is a comment to
    // 2 = Message ID of post if it's a repost of another post

    function postMsg(string calldata message, string[] memory _hashtags, address[] calldata taggedAccounts, uint256 totalTips, string calldata uri, uint256[3] calldata attribs, string calldata asGroup) public payable nonReentrant whenNotPaused {
        // Calculate the total cost to post
        uint256 totalCostToPost = costToPost + (costToAddHashtag * _hashtags.length) + (costToTagAccount * taggedAccounts.length);

        // require the approval of the sending of the token to the contract
        require(paymentToken.approve(msg.sender, totalCostToPost), append("Must approve ", KUtils.toString(totalCostToPost), " payment tokens to be transferred.",'',''));

        // Transfer the Payment Token to the contract
        require(paymentToken.transferFrom(msg.sender, address(this), totalCostToPost), "Didn't receive the Payment Token");

        // Make sure the message is within length limits
        require(bytes(message).length <= maxMessageLength && bytes(message).length > 0, "Message not within length limits");

        // Check for group membership
        if (bytes(asGroup).length > 1){
            require(Groups.isMemberOfGroup(asGroup, msg.sender), "You are not a member of that group");
        }

        // If there are tips, transfer them to the new owners
        uint256 tipPerTag = 0;
        if (totalTips > 0){
            require(taggedAccounts.length > 0, "You need to have tagged accounts to send the tips to");
            // Calculate how much to tip each person tagged
            tipPerTag = totalTips / taggedAccounts.length;
            for (uint t=0; t < taggedAccounts.length; t++) {
                // Transfer tips to tagged accounts last to prevent attacks
                address payTipTo = taggedAccounts[t];
                require(paymentToken.transferFrom(msg.sender, payTipTo, tipPerTag), "Failed to transfer tips to those tagged");
            }
        }


        // Check if the message is posted by contract or person
        uint256 postedByContract = 1;
        if (tx.origin == msg.sender){
            postedByContract = 0;
        }

        // Build the new message struct
        if (attribs[1] > 0){
            // Increment the amount of comments we have posted
            commentCounter++;

            // Update the hashtag Mapping with this message ID and hashtag
            if (_hashtags.length > 0){
                Hashtags.addHashtags(attribs[1], _hashtags);
            }

            // TODO: Tags need comments buckets to allow this
            // Update the tagged accounts with this message ID and tagged user address
//            Tagged.addTags(msgCounter, taggedAccounts);

            IComments.MsgData memory newMsg;

            newMsg.msgID = commentCounter;
            newMsg.poster = msg.sender;
            newMsg.message = message;
            newMsg.msgStats.time = block.timestamp;
            newMsg.msgStats.block = block.number;
            newMsg.tip = totalTips;
            newMsg.paid = totalCostToPost;
            newMsg.hashtags = _hashtags;
            newMsg.taggedAccounts = taggedAccounts;
            newMsg.asGroup = asGroup;
            newMsg.uri = uri;
            newMsg.commentLevel = attribs[0];
            newMsg.isCommentOf = attribs[1];
            newMsg.isRepostOf = attribs[2];
            newMsg.msgStats.postByContract = postedByContract;

            Comments.saveComment(newMsg);

            // Update that message commented on total comments stats
            MessageData.addStat(2, newMsg.isCommentOf, 1);
        } else {
            // Increment the amount of messages we have posted
            msgCounter++;

            // Update the hashtag Mapping with this message ID and hashtag
            if (_hashtags.length > 0){
                Hashtags.addHashtags(msgCounter, _hashtags);
            }

            // Update the tagged accounts with this message ID and tagged user address
            if (taggedAccounts.length > 0){
                Tagged.addTags(msgCounter, taggedAccounts);
            }

            IMessageData.MsgData memory newMsg;

            newMsg.msgID = msgCounter;
            newMsg.poster = msg.sender;
            newMsg.message = message;
            newMsg.msgStats.time = block.timestamp;
            newMsg.msgStats.block = block.number;
            newMsg.tip = totalTips;
            newMsg.paid = totalCostToPost;
            newMsg.hashtags = _hashtags;
            newMsg.taggedAccounts = taggedAccounts;
            newMsg.asGroup = asGroup;
            newMsg.uri = uri;
            newMsg.commentLevel = attribs[0];
            newMsg.isCommentOf = attribs[1];
            newMsg.isRepostOf = attribs[2];
            newMsg.msgStats.postByContract = postedByContract;

            MessageData.saveMsg(newMsg);
        }

        // Log the message post
        // TODO : NEED to revisit this to see what it's doing
        userProfiles.logPost(msg.sender, address(this), tipPerTag, taggedAccounts);
    }

    function eraseMsg(uint256 msgID, uint256 isCommentOf) public whenNotPaused nonReentrant {

        // Erase the message
        MessageData.removeMsg(msgID, msg.sender, isCommentOf);

        // TODO: I'm sure there's more to do here

    }

    function followUser(address userToFollow) public whenNotPaused nonReentrant {
        // require the approval of the sending of the token to the contract
        require(paymentToken.approve(msg.sender, costToFollow), append("Must approve ", KUtils.toString(costToFollow), " payment tokens to be transferred.",'',''));

        // Transfer the Payment Token to the contract
        require(paymentToken.transferFrom(msg.sender, address(this), costToFollow), "Didn't receive the Payment Token");

        // Follow the user
        userProfiles.followUser(msg.sender,userToFollow);
    }

    function unfollowUser(address userToUnfollow) public whenNotPaused nonReentrant {
        // require the approval of the sending of the token to the contract
        require(paymentToken.approve(msg.sender, costToUnfollow), append("Must approve ", KUtils.toString(costToFollow), " payment tokens to be transferred.",'',''));

        // Burn the Payment Token
        require(DOOM.burnTokens(msg.sender, costToUnfollow), "Didn't receive the Payment Token");

        // Follow the user
        userProfiles.unfollowUser(msg.sender, userToUnfollow);
    }

    function getMsgIDsByAddress(address usrAddress, uint256 startFrom) public view whenNotPaused returns (uint256[] memory) {
        return Posts.getMsgIDs(usrAddress, startFrom);
    }

    function getMsgIDsByHashtag(string memory hashtag, uint256 startFrom) public view whenNotPaused returns (uint256[] memory) {
        return Hashtags.getMsgIDsFromHashtag(hashtag, startFrom);
    }

    function getMsgIDsByTag(address taggedUser, uint256 startFrom) public view whenNotPaused returns (uint256[] memory) {
        return Tagged.getTaggedMsgIDs(taggedUser, startFrom);
    }

    function getMsgByIDs(uint256[] calldata msgIDs) public view whenNotPaused returns (string memory) {
        require(msgIDs.length <= maxMsgReturnCount , "Requested too many messages. Lower to the limit.");

        string memory messages = MessageData.getMsgsByIDs(msgIDs);

        return messages;
    }


    function getMsgCost(string[] calldata _hashtags, address[] calldata taggedAccounts) public view whenNotPaused returns (uint256){
        // Calculate the total cost to post
        return costToPost + (costToAddHashtag * _hashtags.length) + (costToTagAccount * taggedAccounts.length);
    }


    function append(string memory a, string memory b, string memory c, string memory d, string memory e) internal pure returns (string memory) {
        return string(abi.encodePacked(a, b, c, d, e));
    }
}

