        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
         for(uint i = 1; i <= maxSupply; i++) {
           _randomNumbers.push(i);
        }
    } */

    function initialize(
        string memory _name,
        string memory _symbol,
        uint256 _tokenPrice,
        uint256 _whiteListPrice,
        uint256 _maxSupply,
        address _feeAddress,
        string memory _contractURI,
        string memory _baseUri,
        uint64 subscriptionId,
        address _vrfCoordinator,
        bytes32 _s_keyHash
        ) public initializer {
        tokenPrice = _tokenPrice;
        whiteListPrice = _whiteListPrice;
        maxSupply = _maxSupply;
        feeAddress = _feeAddress;
        contractURI = _contractURI;
        baseUri = _baseUri;
        s_subscriptionId = subscriptionId;
        vrfCoordinator = _vrfCoordinator;
        s_keyHash = _s_keyHash;
        callbackGasLimit = 100000;
        requestConfirmations = 3;
        numWords = 1;
        isMintable = true;
        presaleStarted = false;
        __ERC721_init(_name, _symbol);
        _grantRole(MARKET_ADMIN_ROLE, _msgSender());
        __VRFConsumerBase_init(vrfCoordinator);
        COORDINATOR = VRFCoordinatorV2Interface(vrfCoordinator);
        for(uint i = 1;i <= maxSupply;i++) {
            _randomNumbers.push(i);
        }
    }

    modifier adminOnly() {
        require(hasRole(MARKET_ADMIN_ROLE, _msgSender()), "Must have market admin role");
        _;
    }

    modifier whenMintable() {
        require(isMintable, "Not mintable");
        _;
    }

    function fulfillRandomWords(
        uint256 requestId,
        uint256[] memory randomWords
    ) internal override {
        emit RequestFulfilled(requestId, randomWords);
    }

    function buyToken(uint256 _amount) public payable whenMintable returns (uint256 requestId) {
        if(presaleStarted) {
            require(whitelisted[_msgSender()], "You are not whitelisted");
        }
        require(_amount <= _randomNumbers.length, "Reached max supply");
        if (whitelisted[_msgSender()]) {
            price = whiteListPrice * _amount;
        } else {
            price = tokenPrice * _amount;
        }
        require(msg.value == price, "Not enough eth sent");
        payable(feeAddress).transfer(price);

        requestId = COORDINATOR.requestRandomWords(
            s_keyHash,
            s_subscriptionId,
            requestConfirmations,
            callbackGasLimit,
            numWords
        );
        for (uint i = 0; i < _amount; i++) {

    // get the random number, divide it by our array size and store the mod of that division.
    // this is to make sure the generated random number fits into our required range
            uint256 randomIndex = requestId % (_randomNumbers.length);

    // draw the current random number by taking the value at the random index
            uint256 resultNumber = _randomNumbers[randomIndex];

    // write the last number of the array to the current position.
    // thus we take out the used number from the circulation and store the last number of the array for future use
            _randomNumbers[randomIndex] = _randomNumbers[_randomNumbers.length - 1];

    // reduce the size of the array by 1 (this deletes the last record we’ve copied at the previous step)
            _randomNumbers.pop();

    // here be your logic where you mint and whatnot using the resultNumber as your unique random number
    // [ … ]
            super._mint(_msgSender(), resultNumber);

            emit TokensSold(address(this), resultNumber, price, _msgSender(), block.timestamp);

        }
    }

    function setMaxSupply(uint256 _newSupply) public adminOnly {
        for(uint i = maxSupply + 1; i <= _newSupply; i++) {
            _randomNumbers.push(i);
        }
        maxSupply = _newSupply;
    }

    function setTokenPrice(uint256 _newPrice) public adminOnly {
        tokenPrice = _newPrice;
    }

    function setFeeAddress(address _newFeeAddress) public adminOnly {
        feeAddress = _newFeeAddress;
    }

    function setBaseURI(string memory _newUri) external adminOnly {
        baseUri = _newUri;
    }

    function setContractURI(string memory _newUri) public adminOnly {
        contractURI = _newUri;
    }

    function setMintable(bool _mintStatus) public adminOnly {
        isMintable = _mintStatus;
    }

    function setWhitelistedPrice(uint256 _whiteListPrice) external adminOnly {
        whiteListPrice = _whiteListPrice;
    }

    function setWhitelistAddress(address[] calldata wtladdresses, bool _status)
        external
        adminOnly
    {
        for (uint16 i = 0; i < wtladdresses.length; i++) {
            whitelisted[wtladdresses[i]] = _status;
        }
        whitelistAddressCount += wtladdresses.length;
    }

    function findAddressInWhiteList(address user) external view returns (bool) {
        return whitelisted[user];
    }

    function setPreSale(bool _status) external adminOnly {
        presaleStarted = _status;
    }

    function grantRole(bytes32 role, address account)
        public
        virtual
        override(AccessControlUpgradeable, IAccessControlUpgradeable)
        adminOnly
    {
        return super._grantRole(role, account);
    }

    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function transferFrom(address from, address to, uint256 tokenId) public override onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory data)
        public
        override
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Upgradeable, AccessControlEnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function approve(address operator, uint256 tokenId) public override onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }
}

