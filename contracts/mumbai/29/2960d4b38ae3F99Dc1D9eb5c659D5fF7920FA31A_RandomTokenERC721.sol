contract RandomTokenERC721 is VRFConsumerBaseV2, ERC721Upgradeable, DefaultOperatorFiltererUpgradeable, AccessControlEnumerableUpgradeable {

    VRFCoordinatorV2Interface public COORDINATOR;

    uint64 public s_subscriptionId;
    address public vrfCoordinator;
    bytes32 public s_keyHash;
    uint32 public callbackGasLimit;
    uint16 public requestConfirmations;
    uint32 public numWords;
    bool public isMintable;
    uint256 public maxSupply;
    uint256 public price;
    uint256 public tokenPrice;
    uint256 public whiteListPrice;
    address public feeAddress;
    string public contractURI;
    string public baseUri;
    uint256[] private _randomNumbers;
    uint256 public mintLength;
    uint256 public whitelistAddressCount;
    // boolean to keep track of whether presale started or not
    bool public presaleStarted;

    // Set about the Whitelisted Person
    mapping(address => bool) public whitelisted;
    // mapping(address => saleItem) public saleItems;

    bytes32 public constant MARKET_ADMIN_ROLE = keccak256("MARKET_ADMIN_ROLE");

    event RequestFulfilled(uint256 requestId, uint256[] randomWords);
    event TokensSold(
        address market,
        uint256 tokenIds,
        uint256 price,
        address buyer,
        uint256 timestamp
    );

    /* constructor(
        string memory _name,
        string memory _symbol,
        uint256 _tokenPrice,
