pragma solidity 0.8.10;

import {AaveV3Mumbai} from 'aave-address-book/AaveAddressBook.sol';
import {IFlashLoanReceiver} from '../interfaces/IFlashLoanReceiver.sol';
import {SafeMath} from '@openzeppelin/contracts/utils/math/SafeMath.sol';
import {IERC20} from '@openzeppelin/contracts/interfaces/IERC20.sol';
import {SafeERC20} from '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import {DataTypes} from '../types/DataTypes.sol';

contract MergeHelper is IFlashLoanReceiver {
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  event Initiator(address initiator);

  function executeOperation(
    address[] calldata assets,
    uint256[] calldata amounts,
    uint256[] calldata premiums,
    address initiator,
    bytes calldata params
  ) external override returns (bool) {
    emit Initiator(initiator);
    require(msg.sender == address(AaveV3Mumbai.POOL), 'msg.sender is not pool');

    if (initiator == address(this)) {
      return _split(assets, amounts, premiums, params);
    }

    (DataTypes.UserPosition[] memory _userPositions) = abi.decode(params, (DataTypes.UserPosition[]));

    // Repay all Debt
    for(uint i=0; i<_userPositions.length; i++) {
      repayDebt(_userPositions[i].userAddress, _userPositions[i].debt);
    }

    // Transfer all the collateral
    for (uint i=0; i<_userPositions.length; i++) {
      transferCollateral(_userPositions[i].userAddress, initiator, _userPositions[i].collateral);
    }
    
    return true;
  }

  function split(
    DataTypes.UserPosition memory fromUser, 
    DataTypes.UserPosition[] memory toUsers
  ) external 
  {
    require(msg.sender == fromUser.userAddress, 'Invalid caller');

    // TODO: Add Health Factor and isBorrowEnabled checks

    bytes memory params = abi.encode(fromUser, toUsers);

    address[] memory assets = new address[](fromUser.debt.length);
    uint256[] memory amounts = new uint256[](fromUser.debt.length);

    // Flash Loan all the borrowed assets of fromUser
    for (uint i=0; i<fromUser.debt.length; i++) {
      assets[i] = fromUser.debt[i].baseTokenAddress;
      amounts[i] = (fromUser.debt[i].stableDebtAmount).add(fromUser.debt[i].variableDebtAmount);
    }

    _flashloan(assets, amounts, params);
  }

  function repayDebt(
    address user, 
    DataTypes.Debt[] memory userDebt
  ) internal
  {
    for(uint i=0; i<userDebt.length; i++) {
      // Repay StableDebt
      if (userDebt[i].stableDebtAmount != 0) {
        IERC20(userDebt[i].baseTokenAddress).approve(address(AaveV3Mumbai.POOL), userDebt[i].stableDebtAmount);
        AaveV3Mumbai.POOL.repay(userDebt[i].baseTokenAddress, userDebt[i].stableDebtAmount, 1, user);
      }
      // Repay VariableDebt
      if (userDebt[i].variableDebtAmount != 0) {
        IERC20(userDebt[i].baseTokenAddress).approve(address(AaveV3Mumbai.POOL), userDebt[i].variableDebtAmount);
        AaveV3Mumbai.POOL.repay(userDebt[i].baseTokenAddress, userDebt[i].variableDebtAmount, 2, user);
      }
    }
  }

  function transferCollateral(
    address from, 
    address to, 
    DataTypes.Collateral[] memory userCollateral
  ) internal
  {
    for (uint i=0; i<userCollateral.length; i++) {
      IERC20(userCollateral[i].aTokenAddress).safeTransferFrom(from, to, userCollateral[i].aTokenAmount);
    }
  }

  function openDebt(
    address user,
    DataTypes.Debt[] memory userDebt
  ) internal 
  {
    for (uint i=0; i<userDebt.length; i++) {
      if (userDebt[i].variableDebtAmount != 0) {
        AaveV3Mumbai.POOL.borrow(userDebt[i].baseTokenAddress, userDebt[i].stableDebtAmount, 1, 0, user);
      }
      if (userDebt[i].stableDebtAmount != 0) {
        AaveV3Mumbai.POOL.borrow(userDebt[i].baseTokenAddress, userDebt[i].stableDebtAmount, 1, 0, user);
      }
    }
  }

  function _flashloan(address[] memory assets, uint256[] memory amounts, bytes memory _params) internal {
    address receiverAddress = address(this);
    address onBehalfOf = address(this);
    bytes memory params = _params;
    uint16 referralCode = 0;
    uint256[] memory modes = new uint256[](assets.length);

    // 0 = no debt (flash)
    for (uint256 i = 0; i < assets.length; i++) {
      modes[i] = 0;
    }

    AaveV3Mumbai.POOL.flashLoan(
      receiverAddress,
      assets,
      amounts,
      modes,
      onBehalfOf,
      params,
      referralCode
    );
  }

  function openDebtWithFees(
    DataTypes.UserPosition memory firstUser,
    address[] calldata assets,
    uint256[] calldata premiums
  ) internal {
    bool[] memory isFeePaid = new bool[](assets.length);
    for (uint i=0; i<isFeePaid.length;i++) {
      isFeePaid[i] = false;
    }

    for (uint i=0; i< firstUser.debt.length; i++) {

      // if this user has variable debt, borrow variable debt with flash loan fees
      if (firstUser.debt[i].variableDebtAmount != 0) {
        for (uint j=0; j<assets.length; j++) {
          if (firstUser.debt[i].baseTokenAddress == assets[j]) {
            AaveV3Mumbai.POOL.borrow(firstUser.debt[i].baseTokenAddress, firstUser.debt[i].variableDebtAmount.add(premiums[j]), 2, 0, firstUser.userAddress);
            isFeePaid[j] = true;
          }
        }
      }

      // stable debt does not incur flash loan fees
      if (firstUser.debt[i].stableDebtAmount != 0) {
        AaveV3Mumbai.POOL.borrow(firstUser.debt[i].baseTokenAddress, firstUser.debt[i].stableDebtAmount, 1, 0, firstUser.userAddress);
      }

    }

    for (uint i=0; i<isFeePaid.length; i++) {
      if (isFeePaid[i] == false) {
        AaveV3Mumbai.POOL.borrow(firstUser.debt[i].baseTokenAddress, premiums[i], 2, 0, firstUser.userAddress);
      }
    }
  }

  function _split(
    address[] calldata assets,
    uint256[] calldata amounts,
    uint256[] calldata premiums,
    bytes calldata params
  ) internal returns (bool){

    (DataTypes.UserPosition memory _fromUser, DataTypes.UserPosition[] memory _toUsers) = abi.decode( params, (DataTypes.UserPosition, DataTypes.UserPosition[]) );

    repayDebt(_fromUser.userAddress, _fromUser.debt);

    // Transfer all the collateral
    for (uint i=0; i<_toUsers.length; i++) {
      transferCollateral(_fromUser.userAddress, _toUsers[i].userAddress, _toUsers[i].collateral);
    }

    // Borrow to open debt positions and repay the flashloan fees
    for (uint i=0; i<_toUsers.length; i++) {
      i == 0 ? openDebtWithFees(_toUsers[i], assets, premiums) : openDebt(_toUsers[i].userAddress, _toUsers[i].debt);
    }
  
    for (uint256 i = 0; i < assets.length; i++) {
      uint256 amountOwing = amounts[i].add(premiums[i]);
      IERC20(assets[i]).approve(address(AaveV3Mumbai.POOL), amountOwing);
    }

    return true;
  }

}

