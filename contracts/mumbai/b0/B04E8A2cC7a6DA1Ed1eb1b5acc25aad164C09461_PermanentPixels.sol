pragma solidity ^0.8.9;

import "erc721a/contracts/ERC721A.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "base64-sol/base64.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";

contract PermanentPixels is ERC721A, Ownable, VRFConsumerBaseV2 {
    using Strings for uint256;

    // Compiler will pack this into one "slot"
    struct RandomnessRequest {
        uint128 startId;
        uint128 endId; // Exclusive
    }

    // Token storing
    uint256 internal currentTokenIndex = 0;
    mapping(uint256 => RandomnessRequest) internal randomnessRequestInfo;

    // Use each bit to keep track of which tokens have been
    // fulfilled
    mapping(uint256 => uint256) internal validTokens;

    // Exclusive value, max number of tokens is 10 million
    uint256 internal constant TOKEN_LIMIT = 10000001;

    // Exclusive value, can only mint 1000 pixels
    uint256 internal constant MAX_MINT_AMOUNT = 1001;

    // Number of tokens we mint in a batch
    uint256 internal constant MAX_MINT_BATCH = 10;

    mapping(uint256 => uint256) internal tokenSeeds;

    // SVG Constants
    bytes internal constant SVG =
        "<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 50 50'>"
        "<rect x='0' y='0' height='50' width='50' fill='white'/>"
        "<path stroke-width='1' stroke='black' fill='none' d='M0 .5";
    bytes internal constant END_SVG = "'/></svg>";

    // SVG
    struct SvgBuilder {
        // Output
        uint256 blackPixels;
        bytes svgString;
        uint256 svgPtr;
        // Pixel tracking
        uint256 blackPixelsInARow;
    }

    bool public paused = false;

    VRFCoordinatorV2Interface internal coordinator;
    uint64 internal subscriptionId;
    bytes32 internal keyHash;

    function setKeyHash(bytes32 _keyHash) public onlyOwner {
        keyHash = _keyHash;
    }

    constructor(
        address vrfCoordinator,
        uint64 _subscriptionId,
        bytes32 _keyhash
    ) ERC721A("PermanentPixels", "PXL") VRFConsumerBaseV2(vrfCoordinator) {
        coordinator = VRFCoordinatorV2Interface(vrfCoordinator);
        subscriptionId = _subscriptionId;
        keyHash = _keyhash;
    }

    /**
        Mint a positive number of Permanent Pixels

        @param _mintAmount number of Pixels to mint
     */
    function mint(uint256 _mintAmount) public payable {
        require(!paused, "Try again later!");
        require(_mintAmount < MAX_MINT_AMOUNT, "1000 token mint limit!");
        require(
            currentTokenIndex + _mintAmount < TOKEN_LIMIT,
            "Mint fewer tokens!"
        );

        if (msg.sender != owner()) {
            // need to figure out how to set cost in MATIC- change '0.05 ether' to cost we want
            require(
                msg.value >= 0.0005 ether * _mintAmount,
                "Minting requires more funds!"
            );
        }

        _safeMint(msg.sender, _mintAmount);

        uint128 startToken = uint128(currentTokenIndex);
        currentTokenIndex += _mintAmount;

        uint128 lastEndToken = uint128(currentTokenIndex);

        uint16 confirmations = 3;

        uint32 numWords;

        for (; startToken < lastEndToken; startToken += 10) {
            if (startToken + MAX_MINT_BATCH > lastEndToken) {
                numWords = uint32(lastEndToken - startToken);
            } else {
                numWords = uint32(MAX_MINT_BATCH);
            }

            uint32 callbackGasLimit = 100000 + (20000 * numWords);

            uint256 requestId = getRandomness(
                confirmations,
                callbackGasLimit,
                numWords
            );
            RandomnessRequest storage request = randomnessRequestInfo[
                requestId
            ];

            request.startId = startToken;
            request.endId = startToken + numWords;
        }
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory svgUri)
    {
        require(_exists(tokenId), "Token hasn't been minted yet!");
        tokenId -= 1;
        require(isTokenFulfilled(tokenId), "Haven't recieved randomness yet!");
        svgUri = buildURIEncodedPP(tokenId);
    }

    function buildURIEncodedPP(uint256 tokenId)
        internal
        view
        returns (string memory)
    {
        uint256 randomSeed = tokenSeeds[tokenId];

        (string memory pp, uint256 blackPixels) = createRandomSVG(
            randomSeed,
            50
        );

        bytes memory skew = blackPixels < 1250
            ? bytes("white")
            : blackPixels > 1250
            ? bytes("black")
            : bytes("none");

        int256 difference = 1250 - int256(blackPixels);
        if (difference < 0) {
            difference = -difference;
        }

        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    string(
                        Base64.encode(
                            abi.encodePacked(
                                /* solhint-disable quotes */
                                '{"name": "Permanent Pixel ',
                                (tokenId + 1).toString(),
                                '","randomSeed": "',
                                randomSeed.toString(),
                                '","attributes": [{'
                                '"trait_type": "skew",'
                                '"value": "',
                                skew,
                                '"}, {'
                                '"trait_type": "rarity",'
                                '"value": ',
                                uint256(difference).toString(),
                                "}],"
                                '"image":"',
                                "data:image/svg+xml;base64,",
                                pp,
                                '"}'
                                /* solhint-enable quotes */
                            )
                        )
                    )
                )
            );
    }

    function createRandomSVG(uint256 randomness, int256 n)
        private
        pure
        returns (string memory, uint256)
    {
        require(n > 0, "n < 0");
        require(n < 51, "n > 50");

        SvgBuilder memory output;

        uint256 mask = 1;

        output.svgString = new bytes(calculateTotalSvgSize(uint256(n)));

        bool isBlackPixel;

        appendToSvg(output, SVG);

        // Keep track of how many black pixels we have in a row
        output.blackPixelsInARow = 0;

        // Where to start drawing black pixels
        uint256 startX = ~uint256(0);
        bytes memory tempIntBytes;

        // Store where we left off drawing so we can
        // Move relatively to our next start
        int256 lastX;
        int256 lastY;

        for (int256 y = 0; y < n; y++) {
            for (int256 x = 0; x < n; x++) {
                if (mask == 0) {
                    mask = 1;
                    randomness = uint256(keccak256(abi.encode(randomness)));
                }

                isBlackPixel = randomness & mask == 0;
                mask = mask << 1;
                output.blackPixels += isBlackPixel ? 1 : 0;

                if (!isBlackPixel) {
                    // We've gotten to a white pixel
                    // See if we need to draw any black pixels
                    if (output.blackPixelsInARow > 0) {
                        // output += " h <output.blackPixelsInARow"
                        appendToSvg(output, bytes("h"));

                        tempIntBytes = bytes(
                            output.blackPixelsInARow.toString()
                        );

                        appendToSvg(output, tempIntBytes);

                        // reset our start point
                        startX = ~uint256(0);
                        output.blackPixelsInARow = 0;

                        // Where to start our relative moven
                        lastX = (x);
                        lastY = (y);
                    }
                } else {
                    // Set our start point
                    if (startX == ~uint256(0)) {
                        // Need to move our cursor to the first black pixel
                        // output += " m x y "
                        // lowercase 'm' -> relative movement
                        appendToSvg(output, bytes("m"));

                        if (x - lastX < 0) {
                            appendByteToSvg(output, "-");
                            tempIntBytes = bytes(
                                uint256(0 - (x - lastX)).toString()
                            );
                        } else {
                            tempIntBytes = bytes(uint256(x - lastX).toString());
                        }

                        appendToSvg(output, tempIntBytes);
                        appendByteToSvg(output, " ");

                        if (y - lastY < 0) {
                            appendByteToSvg(output, "-");
                            tempIntBytes = bytes(
                                uint256(0 - (y - lastY)).toString()
                            );
                        } else {
                            tempIntBytes = bytes(uint256(y - lastY).toString());
                        }

                        appendToSvg(output, tempIntBytes);

                        startX = uint256(x);
                    }
                    output.blackPixelsInARow++;
                }
            }
            // Before we go on to the next row,
            // Check if we need to add any pixels
            if (output.blackPixelsInARow > 0) {
                tempIntBytes = bytes(output.blackPixelsInARow.toString());
                appendToSvg(output, bytes("h"));

                appendToSvg(output, tempIntBytes);

                output.blackPixelsInARow = 0;
                startX = ~uint256(0);
                lastX = n;
                lastY = y;
            }
        }

        appendToSvg(output, END_SVG);

        // Change the length of the svgString bytes array
        // so we don't encode a bunch of null bytes at the end
        // of our svg output

        // solhint-disable-next-line no-inline-assembly
        assembly {
            let svgPtr := mload(add(output, 0x20))
            let svgLength := mload(add(output, 0x40))
            mstore(svgPtr, svgLength)
        }

        return (Base64.encode(output.svgString), output.blackPixels);
    }

    function appendByteToSvg(SvgBuilder memory svg, bytes1 singleByte)
        private
        pure
    {
        svg.svgString[svg.svgPtr++] = singleByte;
    }

    function appendToSvg(SvgBuilder memory svg, bytes memory inputBytes)
        private
        pure
    {
        for (uint8 i = 0; i < inputBytes.length; i++) {
            svg.svgString[svg.svgPtr++] = inputBytes[i];
        }
    }

    function calculateTotalSvgSize(uint256 n) private pure returns (uint256) {
        // Theory: for a checker board pattern:
        // m1 0h1
        // This is 6 chars * (n * n / 2)
        return ((n * n) / 2) * 6;
    }

    function setPaused(bool _state) public onlyOwner {
        paused = _state;
    }

    /**
        Withdraw all chain currency to the owners wallet
     */
    function withdraw() public onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    /**
        Withdraw all of any ERC20 token to the owners wallet
     */
    function withdrawERC20Token(address tokenAddress) public onlyOwner {
        require(tokenAddress != address(0), "0 Token address");
        ERC20 tokenContract = ERC20(tokenAddress);

        tokenContract.transfer(owner(), tokenContract.balanceOf(address(this)));
    }

    function getRandomness(
        uint16 requestConfirmations,
        uint32 callbackGasLimit,
        uint32 numWords
    ) internal returns (uint256 requestId) {
        return
            coordinator.requestRandomWords(
                keyHash,
                subscriptionId,
                requestConfirmations,
                callbackGasLimit,
                numWords
            );
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        override
    {
        RandomnessRequest storage request = randomnessRequestInfo[requestId];
        require(
            request.startId != request.endId,
            "Randomness already received!"
        );
        // Signify that a request has been fulfilled
        request.endId = request.startId;
        setTokenSeeds(request.startId, randomWords);
    }

    /**
     * Set a range of token random seeds from start (inclusive) to end (exclusive)
     */
    function setTokenSeeds(uint128 start, uint256[] memory seeds) internal {
        for (uint256 i = 0; i < seeds.length; i++) {
            tokenSeeds[start + i] = seeds[i];
        }

        setTokenRangeFulfilled(start, seeds.length);
    }

    /**
        Set a range of tokens to be valid
        @param startIndex uint256 inclusive
        @param numTokens number of tokens starting with startIndex
        to be marked as valid
     */
    function setTokenRangeFulfilled(uint256 startIndex, uint256 numTokens)
        internal
    {
        uint256 batchOffset = startIndex % 256;
        uint256 batchTokens;
        uint256 batchIndex = startIndex / 256;

        if (batchOffset > 0) {
            // Max batch tokens
            batchTokens = 256 - batchOffset;
            if (numTokens > batchTokens) {
                numTokens -= batchTokens;
            } else {
                batchTokens = numTokens;
                numTokens = 0;
            }

            uint256 mask = ~uint256(0);

            mask = mask << (256 - batchTokens);
            mask = mask >> batchOffset;

            uint256 currentValid = validTokens[batchIndex];
            validTokens[batchIndex] = (currentValid | mask);

            batchIndex += 1;
        }

        // Indexes will always start at the beginning of a batch after this
        while (numTokens > 0) {
            if (numTokens > 256) {
                numTokens -= 256;
                batchTokens = 256;
            } else {
                batchTokens = numTokens;
                numTokens = 0;
            }

            uint256 mask = ~uint256(0);

            mask = mask << (256 - batchTokens);
            validTokens[batchIndex] = (validTokens[batchIndex] | mask);

            batchIndex += 1;
        }
    }

    function isTokenFulfilled(uint256 tokenIndex) internal view returns (bool) {
        uint256 index = tokenIndex / 256;
        uint256 remainder = tokenIndex % 256;
