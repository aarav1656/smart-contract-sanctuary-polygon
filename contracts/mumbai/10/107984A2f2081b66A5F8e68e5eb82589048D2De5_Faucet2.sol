    struct Token {
        address tokenAddress;
        address priceFeedAddress;
        uint256 minAmount;
        uint256 balance;
        Status status;
    }


    struct LockedAsset {
        address token;
        address payable beneficiary;
        uint amount;
        uint startDate;
        uint endDate;
        uint id;
        int priceInUSD;
        uint[][] option;
        bool isExchangable;
        Status status;
    }

