pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

import "./interfaces/IRegistryContract.sol";
import "./interfaces/IMetaForceContract.sol";
import "./interfaces/IHoldingContract.sol";
import "./interfaces/ICoreContract.sol";
import "./interfaces/ICoins.sol";
import "./interfaces/IMetaCore.sol";
import "./interfaces/IMetaPayment.sol";

contract Holding is Ownable, ReentrancyGuard, IHoldingContract {
    using SafeCast for uint256;
    using SafeERC20 for IERC20;

    IRegistryContract public immutable registry;

    mapping(uint256 => uint256) public lockupPeriods;

    Deposit[] private deposits;
    mapping(uint256 => uint256[]) private depositIds;

    modifier existingLevel(uint256 levelNumber) {
        if (registry.getHMFS(levelNumber) == address(0)) {
            revert MetaForceSpaceHoldingLevelDontExist(levelNumber);
        }
        _;
    }

    modifier allowedStage() {
        if (ICoreContract(registry.getCoreContract()).getWorkflowStage() != Workflow.SaleOpen) {
            revert MetaForceSpaceHoldingNotAllowed();
        }
        _;
    }

    constructor(IRegistryContract _registry, uint256[] memory _lockupPeriods) {
        registry = _registry;
        uint256 length = _lockupPeriods.length;
        for (uint256 i; i < length; ) {
            lockupPeriods[i + 1] = _lockupPeriods[i];
            unchecked {
                ++i;
            }
        }
    }

    function hold(uint256 levelNumber, uint256 amount) external override nonReentrant returns (uint256 depositId) {
        return _hold(_getUserId(msg.sender), true, levelNumber, amount);
    }

    function holdOnBehalf(
        uint256 beneficiaryId,
        uint256 levelNumber,
        uint256 amount
    ) external nonReentrant returns (uint256 depositId) {
        if (msg.sender != registry.getMetaForceContract()) {
            revert MetaForceSpaceHoldingNotAllowed();
        }
        if (beneficiaryId == 0) {
            revert MetaForceSpaceHoldingZeroAddress();
        }
        return _hold(beneficiaryId, false, levelNumber, amount);
    }

    function unhold(uint256 depositId, uint256 amount) external override nonReentrant {
        if (amount == 0) {
            revert MetaForceSpaceHoldingAmountIsZero(depositId);
        }
        uint256 length = deposits.length;
        if (depositId >= length) {
            revert MetaForceSpaceHoldingDepositDontExist(depositId);
        }
        uint256 userId = _getUserId(msg.sender);
        Deposit storage deposit = deposits[depositId];
        if (deposit.holderId != userId) {
            revert MetaForceSpaceHoldingNotAllowed();
        }
        if (!deposit.unholdingAllowed) {
            revert MetaForceSpaceHoldingNotAllowed();
        }
        if (amount > deposit.amount) {
            revert MetaForceSpaceHoldingInsufficientDepositAmount(depositId, amount);
        }
        deposit.amount -= amount.toUint224();
        _giveMFS(userId, amount);
        emit Unhold(depositId, amount);
    }

    function withdraw(uint256 depositId) external override nonReentrant {
        uint256 length = deposits.length;
        if (depositId >= length) {
            revert MetaForceSpaceHoldingDepositDontExist(depositId);
        }

        Deposit storage deposit = deposits[depositId];
        uint256 userId = _getUserId(msg.sender);
        if (deposit.holderId != userId) {
            revert MetaForceSpaceHoldingNotAllowed();
        }
        if (deposit.amount == 0) {
            revert MetaForceSpaceHoldingDepositInactive(depositId);
        }

        if (deposit.lockedUntil <= block.timestamp) {
            IRegistryContract tempRegistry = registry;
            address payment = tempRegistry.getMetaPayment();
            IHMFS(tempRegistry.getHMFS(deposit.levelNumber)).mint(payment, deposit.amount);
            IMetaPayment(payment).increaseBalance(tempRegistry.getHMFS(deposit.levelNumber), userId, deposit.amount);
        } else {
            revert MetaForceSpaceHoldingNotAllowed();
        }
        _giveMFS(userId, deposit.amount);
        deposit.amount = 0;
    }

    function setLockupPeriod(uint256 levelNumber, uint256 lockupPeriod) external onlyOwner existingLevel(levelNumber) {
        if (lockupPeriod == 0) {
            revert MetaForceSpaceHoldingLockupPeriodTooSmall(lockupPeriod);
        }

        lockupPeriods[levelNumber] = lockupPeriod;
    }

    function getDeposit(uint256 depositId) external view override returns (Deposit memory) {
        return deposits[depositId];
    }

    function getDepositIds(uint256 holder) external view override returns (uint256[] memory) {
        return depositIds[holder];
    }

    function _hold(
        uint256 beneficiary,
        bool unholdingAllowed,
        uint256 levelNumber,
        uint256 amount
    ) internal allowedStage existingLevel(levelNumber) returns (uint256 depositId) {
        if (amount == 0) {
            revert MetaForceSpaceHoldingAmountIsZero(depositId);
        }
        IRegistryContract tempRegistry = registry;
        IMetaPayment(tempRegistry.getMetaPayment()).decreaseBalance(
            tempRegistry.getMFS(),
            beneficiary,
            address(this),
            amount
        );
        uint256 lockedUntil = block.timestamp + lockupPeriods[levelNumber];
        assert(lockedUntil > block.timestamp);

        deposits.push(
            Deposit({
                holderId: beneficiary,
                unholdingAllowed: unholdingAllowed,
                levelNumber: levelNumber.toUint8(),
                amount: amount.toUint224(),
                lockedUntil: lockedUntil.toUint32()
            })
        );

        depositId = deposits.length - 1;
        depositIds[beneficiary].push(depositId);

        emit Hold(depositId, levelNumber, amount);
    }

    function _giveMFS(uint256 userId, uint256 amount) internal {
        IRegistryContract tempRegistry = registry;
        IMetaPayment payment = IMetaPayment(tempRegistry.getMetaPayment());
        IMFS mfs = IMFS(tempRegistry.getMFS());
        mfs.transfer(address(payment), amount);
        payment.increaseBalance(address(mfs), userId, amount);
    }

    function _getUserId(address user) internal view returns (uint256 userId) {
        IMetaCore metaCore = IMetaCore(registry.getMetaCore());
        userId = metaCore.checkRegistration(user);
    }
}

