    enum DisputeState {
        EVIDENCE,
        COMMIT,
        VOTE,
        APPEAL,
        EXECUTION
    }

    struct DelayTimestamp {
        uint256 selfRefund;
        uint256 evidence;
        uint256 commit;
        uint256 vote;
        uint256 appeal;
    }

    struct User {
        string metadata;
        uint256 inviterId;
        uint256 buyerUntil;
        uint256 buyerInvites;
        uint256 sellerUntil;
        uint256 sellerInvites;
        EnumerableSet.UintSet gigIds;
        EnumerableSet.UintSet offerIds;
