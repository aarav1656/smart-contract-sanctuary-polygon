        uint256 buyerId;
        uint256 sellerId;
        uint256 gigId;
        uint256 disputeId;
        bool disputed;
        Invoice invoice;
        OrderState state;
        EnumerableSet.UintSet reviewIds;
    }

    struct Evidence {
        uint256 userId;
        bytes32 role;
        string metadata;
