pragma solidity ^0.8.17;

// import "zk-merkle-tree/contracts/ZKTree.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./ZKTree.sol";

contract ZKTreeVote is ZKTree {
    using SafeERC20 for IERC20;
    
    event withdrawed(uint nullifier, uint root, uint amount, address recipient, address caller);

    address public owner;
    address public TOKEN;
    mapping(address => bool) public validators;
    mapping(uint256 => bool) uniqueHashes;
    uint numOptions;
    mapping(uint => uint) optionCounter;

    constructor(
        uint32 _levels,
        IHasher _hasher,
        IVerifier _verifier,
        uint _numOptions,
        address _token
    ) ZKTree(_levels, _hasher, _verifier) {
        owner = msg.sender;
        numOptions = _numOptions;
        TOKEN = _token;
        for (uint i = 0; i <= numOptions; i++) optionCounter[i] = 0;
    }

    function registerValidator(address _validator) external {
        require(msg.sender == owner, "Only owner can add validator!");
        validators[_validator] = true;
    }

    function registerCommitment(
        uint256 _uniqueHash,
        uint256 _commitment
    ) external {
        require(validators[msg.sender], "Only validator can commit!");
        require(
            !uniqueHashes[_uniqueHash],
            "This unique hash is already used!"
        );
        _commit(bytes32(_commitment));
        uniqueHashes[_uniqueHash] = true;
    }

    function vote(
        uint _option,
        uint256 _nullifier,
        uint256 _root,
        uint[2] memory _proof_a,
        uint[4] memory _proof_b,
        uint[2] memory _proof_c
    ) external {
        require(_option <= numOptions, "Invalid option!");
        _nullify(
            bytes32(_nullifier),
            bytes32(_root),
            _proof_a,
            _proof_b,
            _proof_c
        );
        optionCounter[_option] = optionCounter[_option] + 1;
    }

    // TODO
    function deposit(uint _uniqueHash, uint _commitment, uint amount) public {
        require(!uniqueHashes[_uniqueHash], "This unique hash is already used!");

        IERC20(TOKEN).safeTransferFrom(msg.sender, address(this), amount);

        _commit(bytes32(_commitment));
        uniqueHashes[_uniqueHash] = true;
    }

    function withdraw(
        uint _nullifier,
        uint _root,
        uint[2] memory _proof_a,
        uint[4] memory _proof_b,
        uint[2] memory _proof_c,
        uint _amount,
        address _recipient
    ) public {
        _nullify(
            bytes32(_nullifier),
            bytes32(_root),
            _proof_a,
            _proof_b,
            _proof_c
        );

        uint poolBalance = IERC20(TOKEN).balanceOf(address(this)); 
        require(poolBalance >= _amount, "insufficient pool balance");

        IERC20(TOKEN).safeTransfer(_recipient, _amount);

        // emit withdrawed(_nullifier, _root, _amount, _recipient, msg.sender);
    }
    function getOptionCounter(uint _option) external view returns (uint) {
        return optionCounter[_option];
    }

    function knownRoot(uint256 _root) public view returns (bool) {
        return isKnownRoot(bytes32(_root));
    }

    function getTopRoot() public view returns (uint256) {
        bytes32 r = getLastRoot();
        return uint(r);
    }
}

