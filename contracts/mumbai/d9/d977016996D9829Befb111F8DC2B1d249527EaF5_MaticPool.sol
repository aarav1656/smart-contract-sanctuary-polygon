    function placeorder(
        Order_SBT.sbt memory _sbt,
        address to,
        bool sell
    ) external {

        _sbt.orderId = ORDER_ID;
        int256 tokenAmt = _sbt.tokenAmount;
        int256 price = _sbt.price;
        if(sell){
            MaticSBT(erc20MaticSbt).InclockedAmt(to,tokenAmt);
            MaticSBT(erc20MaticSbt).DecunlockedAmt(to,tokenAmt);
        }
        else{
            USDTSbt(erc20USDTSbt).InclockedAmt(to,tokenAmt*price);
            USDTSbt(erc20USDTSbt).DecunlockedAmt(to,tokenAmt*price);
        }
        Order_SBT(SBT_Contract_Address).mint(to, _sbt, sell);
        emit mintOrder(to, ORDER_ID);
        ORDER_ID++;
    }
    
    // update SBTs and settle Amount 
    function updateSBTsAndsettle(uint256 matchedSbtId, uint256 sbtId, bool _sell, int256 _price) external{

     // USDT and Matic Pair    
    (int256 currMatchedAmount, address creatorMatched) = Order_SBT(SBT_Contract_Address).getAmountCreatoraddress(matchedSbtId);
    (int256 currAmount, address creator) = Order_SBT(SBT_Contract_Address).getAmountCreatoraddress(sbtId);
    address userSAMatched = ProxyStorage(proxyStorage).getSA(creatorMatched);
    address userSA = ProxyStorage(proxyStorage).getSA(creator);
    int256 settle = 0;
    lastTradePrice = _price;
    if(currAmount == currMatchedAmount){
        Order_SBT(SBT_Contract_Address).updateFilledandCancelSbt(matchedSbtId,false);
        Order_SBT(SBT_Contract_Address).updateFilledandCancelSbt(sbtId,false);
        settle = currAmount;
       
    }
    else if(currAmount > currMatchedAmount){
        Order_SBT(SBT_Contract_Address).updateFilledandCancelSbt(matchedSbtId,false);
        Order_SBT(SBT_Contract_Address).updatePartialSbt(sbtId,currMatchedAmount);
        settle = currMatchedAmount;
    }
    else{
        Order_SBT(SBT_Contract_Address).updatePartialSbt(matchedSbtId,currAmount);
        Order_SBT(SBT_Contract_Address).updateFilledandCancelSbt(sbtId,false);
        settle = currAmount;
    }
      // update code with a trade buyer and seller functionality (internal function)
     // Update Portfolio Sbt
        if(_sell){
            // orderPlacer
            //Matic //USDT
            MaticSBT(erc20MaticSbt).DeclockedAmt(userSA,settle);
            USDTSbt(erc20USDTSbt).IncunlockedAmt(userSA,settle*_price);
            
            // Matched Order
            MaticSBT(erc20MaticSbt).IncunlockedAmt(userSAMatched,settle);
            USDTSbt(erc20USDTSbt).DeclockedAmt(userSAMatched,settle*_price);

        }
        else {
            // Matched Order
            //Matic //USDT
            MaticSBT(erc20MaticSbt).DeclockedAmt(userSAMatched,settle);
            USDTSbt(erc20USDTSbt).IncunlockedAmt(userSAMatched,settle*_price);
            // orderPlacer
            MaticSBT(erc20MaticSbt).IncunlockedAmt(userSA,settle);
            USDTSbt(erc20USDTSbt).DeclockedAmt(userSA,settle*_price);
        }

    }

    function _msgSender()
        internal
        view
        override(BasicMetaTransaction)
        returns (address sender)
    {
        if (msg.sender == address(this)) {
            bytes memory array = msg.data;
            uint256 index = msg.data.length;
            assembly {
                // Load the 32 bytes word from memory with the address on the lower 20 bytes, and mask those.
                sender := and(
                    mload(add(array, index)),
                    0xffffffffffffffffffffffffffffffffffffffff
                )
            }
        } else {
            return msg.sender;
        }
    }
}

