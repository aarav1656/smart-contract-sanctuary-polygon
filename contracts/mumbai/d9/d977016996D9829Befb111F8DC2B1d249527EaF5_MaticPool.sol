pragma solidity ^0.8.0;
contract USDTSbt is ERC721 {
    address public owner;
    struct sbt {
       address owner;
       int256 lockedBalance;
       int256 unlockedBalance;
       string ABTTitle;
       string Description;
       int256 TokenSymbolID;
       int256 StakedBalance;
       int256 UnstakedBalance;
       bool LiquidStakingStatus;
       uint256 TradingLimitApproved;
       int8 ApplicableFeeDiscount;
    }
    uint256 public SBT_ID = 1;
    mapping(uint256 => int256) public lockedBalance;   // sbtId ka corresponding 
    mapping(uint256 => int256) public unlockedBalance;   // sbtId =>tokenId => balanace
    // mapping(uint256 => mapping(uint256=>uint256)) public lockedBalance;
    // mapping(uint256 => mapping(uint256=>uint256)) public unlockedBalance;
    mapping(address => uint256) public userSbtId;



    mapping(uint256 => sbt) public sbtInfo;
    event SBT_mint(address minter, address receiver, uint256 SBT_ID);
    event SBT_burn(uint256 SBT_ID);
    constructor() ERC721("Locked_UnlockedSBT", "SBT") {
        owner = _msgSender();
    }


    function mint(address _to, sbt memory _sbt, int256 _lockedBalance, int256 _unlockedBalance)
        external 
    {
        _mint(_to, SBT_ID);
        userSbtId[_to] = SBT_ID;
        sbtInfo[SBT_ID] = _sbt;
        lockedBalance[SBT_ID] = _lockedBalance;
        unlockedBalance[SBT_ID] = _unlockedBalance;
        emit SBT_mint(owner, _to, SBT_ID);
        SBT_ID++;
    }
    function InclockedAmt(address _user, int256 amt) external {
        uint256 sbtId =  userSbtId[_user];
        lockedBalance[sbtId] += amt;
        sbt memory Sbt = sbtInfo[sbtId];
        Sbt.lockedBalance = lockedBalance[sbtId];
        sbtInfo[sbtId] = Sbt;
    }
    function DeclockedAmt(address _user, int256 amt) external {
        uint256 sbtId =  userSbtId[_user];
        lockedBalance[sbtId] -= amt;
        sbt memory Sbt = sbtInfo[sbtId];
        Sbt.lockedBalance = lockedBalance[sbtId];
        sbtInfo[sbtId] = Sbt;
    }
    function IncunlockedAmt(address _user, int256 amt) external {
        uint256 sbtId =  userSbtId[_user];
        unlockedBalance[sbtId] += amt;
        sbt memory Sbt = sbtInfo[sbtId];
        Sbt.unlockedBalance = unlockedBalance[sbtId];
        sbtInfo[sbtId] = Sbt;
    }
    function DecunlockedAmt(address _user, int256 amt) external {
        uint256 sbtId =  userSbtId[_user];
        unlockedBalance[sbtId] -= amt;
        sbt memory Sbt = sbtInfo[sbtId];
        Sbt.unlockedBalance = unlockedBalance[sbtId];
        sbtInfo[sbtId] = Sbt;
    }
    function getLockedBalance(address _user) external view returns(int256){
        uint256 stbId = userSbtId[_user];
        return lockedBalance[stbId]; 
    } 
    function getUnlockedBalance(address _user) external view returns(int256){
        uint256 stbId = userSbtId[_user];
        return unlockedBalance[stbId]; 
    } 
    function _transfer(
        address from,
        address to,
        uint256 _sbtId
    ) internal override {
        require(false, "NON-transferable");
        super._transfer(from, to, _sbtId);
    }
}

