contract ProxyStorage {
     // SmartAccountProxy Address.
    address public immutable SmartAccountProxy;
    address public membershipSbt;
    constructor (address _SmartAccountProxy,address _membershipSbt) {
        SmartAccountProxy = _SmartAccountProxy;
        membershipSbt = _membershipSbt;
    }

    // Smart Account Count.
    uint64 public accounts = 1;
    mapping (address => address) public userLink;  // EOA => SA
    mapping (uint256 => address) public SmartAccountLink; // (EOA)MembershipSBTId => SA
    mapping (address => uint256) public SAMembershipId;  //  SA => MembershipSBTId(EOA)

    function init(address _user, address  _account, uint256 _membershipSbtId) external{
        require(msg.sender == SmartAccountProxy, "not-minimal Proxy!!");
        accounts++;
        userLink[_user] = _account;
        SmartAccountLink[_membershipSbtId] = _account;
        SAMembershipId[_account] = _membershipSbtId;
    }
    function getSA(address _user) view external returns(address){
        return userLink[_user];
    }
    function changeEOAandMintMembershipSbt(address EOA, address newEOA) external {
        // burn membership Sbt EOA and mint membershipSbt to newEOA 
        // mapping newEOA => SA 
        address _account = userLink[EOA];
        uint256 oldMembershipId = SAMembershipId[_account];
        SmartAccountLink[oldMembershipId] = 0x0000000000000000000000000000000000000000;
        userLink[EOA] = 0x0000000000000000000000000000000000000000;
        uint256 _membershipSbtId = MembershipSBT(membershipSbt).mintAndburnSbt(EOA,newEOA);
        userLink[newEOA] = _account;
        SmartAccountLink[_membershipSbtId] = _account;
        SAMembershipId[_account] = _membershipSbtId;
    }

}

