    }


    function mint(address _to, sbt memory _sbt)
        public returns(uint256)
    {
        _mint(_to, SBT_ID);
        userSbtId[_to] = SBT_ID;
        _sbt.MembershipID = SBT_ID;
        sbtInfo[SBT_ID] = _sbt;
        emit SBT_mint(_to, SBT_ID);
        SBT_ID++;
        return(SBT_ID);
    }
    function burn(uint256 _sbtId) public {
        _burn(_sbtId);
        emit SBT_burn(_sbtId);
    }
    function getMembershipId(address _to) external view returns(uint256){
        return userSbtId[_to];
    }
    function mintAndburnSbt(address user, address newUser) external returns(uint256) {
        uint256 _sbtId = userSbtId[user];
        sbt memory _sbt = sbtInfo[_sbtId];
        burn(_sbtId);
        uint256 memberShipSbt = mint(newUser,_sbt);
        return memberShipSbt; 
    }
