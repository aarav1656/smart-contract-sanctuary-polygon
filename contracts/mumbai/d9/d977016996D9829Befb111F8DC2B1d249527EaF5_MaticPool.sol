    address public owner;

    struct sbt {
        string orderType;
        uint256 orderId;
        string orderStatus;  // filled
        uint256 SbtId; 
        int256 price;
        int256 tokenAmount; // balance 0
        uint256 placeOrderAmount; 
        string buyTokenName;
        string sellTokenName;
        address orderCreator;
    }
    uint256 public SBT_ID = 1;
    mapping(uint256 => bool) public orderIDexist;  // orderID -> bool
    mapping(uint256 => sbt) public sbtInfo;
    mapping(uint256 => uint256) public OrderSbtId;   // orderId => sbtId
    mapping(uint256 => bool) public sbtExist;    
    mapping(uint256 => bool) public sellSbt;
    mapping(uint256 => bool) public buySbt;
    mapping(uint256 => bool) public cancelSbtId;  // cancel Sbts
    event SBT_mint(address minter, address receiver, uint256 SBT_ID);
    event SBT_burn(uint256 SBT_ID);
    constructor() ERC721("Buy_Sell_SBT", "SBT") {
        owner = _msgSender();
    }


    function mint(address _to, sbt memory _sbt, bool sell)
        external{
        _mint(_to, SBT_ID);
        _sbt.SbtId = SBT_ID;
        _sbt.orderStatus = "open";
        // _sbt.orderId = ORDER_ID;
        if(sell) sellSbt[SBT_ID]=true;
        else buySbt[SBT_ID]=true;
        sbtInfo[SBT_ID]=_sbt;
        orderIDexist[_sbt.orderId] = true;
        OrderSbtId[_sbt.orderId] = SBT_ID;
        sbtExist[SBT_ID] = true;
        emit SBT_mint(owner, _to, SBT_ID);
        SBT_ID++;
    }

    function updateFilledandCancelSbt(uint256 _sbtId, bool cancel) external {
        sbt memory _sbt = sbtInfo[_sbtId];
        // _burn(_sbtId);
        if(cancel==true){
             _sbt.orderStatus = "cancelled";
             cancelSbtId[_sbtId] =true;
        }
        else _sbt.orderStatus = "filled";
        _sbt.tokenAmount=0;
        sbtInfo[_sbtId] = _sbt;
        if(sellSbt[_sbtId]==true) sellSbt[_sbtId]=false;
        if(buySbt[_sbtId]==true) buySbt[_sbtId]=false;
        // emit SBT_burn(_sbtId);
    }

    function updatePartialSbt(uint256 sbtId, int256 amount) external {
        
        sbt memory _sbt = sbtInfo[sbtId];
        _sbt.tokenAmount = amount;
        _sbt.orderStatus = "partially";
        sbtInfo[sbtId] = _sbt;
    }
    
    function getSBTId() external view returns(uint256){
        return SBT_ID;
    }

    function getOrderSBTId(uint256 orderId) public view returns(uint256){
        uint256 sbtId=OrderSbtId[orderId];
        return sbtId;
    }

    function getSbt(uint256 sbtId) external view returns( 
    uint256, uint256, int256,int256){
        sbt memory _sbt = sbtInfo[sbtId];
        return(_sbt.orderId,_sbt.SbtId,_sbt.price,_sbt.tokenAmount);
    }

    function getAmountCreatoraddress(uint256 sbtId) external view returns( 
    int256, address){
        sbt memory _sbt = sbtInfo[sbtId];
        return(_sbt.tokenAmount,_sbt.orderCreator);
    }

    function getWithdrawalDetails(uint256 sbtId) external view returns(int256, int256, bool, address){
        sbt memory _sbt = sbtInfo[sbtId];
        return(_sbt.tokenAmount,_sbt.price,sellSbt[sbtId],_sbt.orderCreator);
    } 

    function getTokenAmount(uint256 sbtId) external view returns(int256){
        return sbtInfo[sbtId].tokenAmount;
    }
    function _transfer(
        address from,
        address to,
        uint256 _sbtId
    ) internal override {
        require(false, "NON-transferable");
        super._transfer(from, to, _sbtId);
    }
}

