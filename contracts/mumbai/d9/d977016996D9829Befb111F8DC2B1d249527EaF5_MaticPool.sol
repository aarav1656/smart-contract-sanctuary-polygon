    struct sbt{
       address ABTTitle;
       string Description;
       address OwnerAddress;
       address SmartACAddress;
       uint256 ReferredBy;
       uint256 MembershipID;
       bool zKYCVerificationStatus;
    }
    uint256 public SBT_ID = 1;
    mapping(address => uint256) public userSbtId;



    mapping(uint256 => sbt) public sbtInfo;
    event SBT_mint(address user, uint256 SBT_ID);
    event SBT_burn(uint256 SBT_ID);
