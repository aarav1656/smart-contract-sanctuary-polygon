import "./ERC1820/ERC1820Implementer.sol";

/**
 * @title UserCompliant
 *
 *
 * Error:
 * UC01: The transfer address cannot be address(0)
 * UC02: Sender not token address
 * UC03: Length mismatch
 * UC04: User or user id invalid
 * UC05: User id not found
 */

contract UserCompliant is
    IUserCompliant,
    ReentrancyPausableUpgradeable,
    ERC1820Client,
    ERC1820Implementer,
    KycAdmin,
    BlockListAdmin
{
    uint256 internal constant USER_VALID = 0;
    uint256 internal constant USER_INVALID = 0;

    // TOKEN
    string internal constant STTOKEN_INTERFACE_NAME = "STtoken";

    // Token extensions
    string internal constant ST_VALIDATOR = "ERC20STValidator";

    // Compliant
    string internal constant USER_COMPLIANT = "UserCompliant";

    // userIds => addresses
    mapping(uint256 => address[]) internal userIdAddresses;

    // address => userId
    mapping(address => uint256) internal addressUserId;

    /**
     * @notice Record user token transfer from history
     * @dev UserAddress => TokenAddress => year=>month=>day => DailyTransferRecord
     */
    mapping(address => mapping(address => mapping(uint256 => DailyTransferRecord)))
        internal addressDailyTransfers;

    // TODO month transfer history

    function _UserCompliant_init(address _erc1820Registry)
        external
        initializer
    {
        __PausableUpgradeable_init();
        _ERC1820Client_init(_erc1820Registry);

        ERC1820Client.setInterfaceImplementation(USER_COMPLIANT, address(this));
        ERC1820Implementer._setInterface(USER_COMPLIANT);
    }

    function attachUsers(uint256[] calldata usersId, address[] calldata users)
        public
        onlyKycAdmin
    {
        require(usersId.length == users.length, "UC03");
        for (uint256 i = 0; i < users.length; i++) {
            _attachUser(usersId[i], users[i]);
        }
    }

    function detachUsers(address[] calldata users) public onlyKycAdmin {
        for (uint256 i = 0; i < users.length; i++) {
            _detachUser(users[i]);
        }
    }

    function _attachUser(uint256 userId, address user) internal {
        require(userId != 0 && addressUserId[user] == 0, "UC04");
        userIdAddresses[userId].push(user);
        addressUserId[user] = userId;
    }

    function _detachUser(address user) internal {
        uint256 userId = addressUserId[user];

        // userIdAddresses remove
        for (uint256 i = 0; i < userIdAddresses[userId].length; i++) {
            if (user == userIdAddresses[userId][i]) {
                for (
                    uint256 j = i;
                    j < userIdAddresses[userId].length - 1;
                    j++
                ) {
                    userIdAddresses[userId][j] = userIdAddresses[userId][j + 1];
                }
                userIdAddresses[userId].pop();
                break;
            }
        }

        // addressUserId remove
        addressUserId[user] = 0;
    }

    /**
     * @dev Get user id by user address
     */
    function getUserId(address user) public view override returns (uint256) {
        uint256 userId = addressUserId[user];
        if (userId == 0) {
            revert("UC05");
        }
        return userId;
    }

    /**
     * @notice Check if the user is valid
     * @return result SUCCESS stands for user is valid
     * other result representative users do not meet some conditions
     */
    function isUserValid(address user, uint256[] memory requireKycLevels)
        public
        view
        override
        returns (uint256 result)
    {
        // address(0) no need
        if (user == address(0)) {
            return USER_VALID;
        }

        // get user id
        uint256 userId = getUserId(user);

        // blockList
        require(getUserBlockListData(userId) == USER_NOT_BLOCKLISTED, "BA01");

        // kyc no need
        if (requireKycLevels.length == 0) {
            return USER_VALID;
        }

        // kyc
        (uint256 kycResult, uint256 kycLevel) = getUserKycLevel(userId);
        require(kycResult == KYC_VALID, "KA01");

        bool IsKycLevelMatch = false;
        for (uint256 i = 0; i < requireKycLevels.length; i++) {
            if (kycLevel == requireKycLevels[i]) {
                IsKycLevelMatch = true;
                break;
            }
        }
        if (!IsKycLevelMatch) {
            revert("KA02");
        }
        return USER_VALID;
    }

    function updateTransfer(
        address _tokenAddress,
        address _from,
        address _to,
        uint256 _amount
    ) external override {
        require(_tokenAddress == msg.sender, "UC02");
        _updateTransfer(_tokenAddress, _from, _to, _amount);
    }

    /**
     * @dev Updates transfer daily history registries
     * @param _tokenAddress the address of the exchanged token
     * @param _from the sender of the tokens
     * @param _to the receiver of the tokens
     * @param _amount transfered tokens amount
     */
    function _updateTransfer(
        address _tokenAddress,
        address _from,
        address _to,
        uint256 _amount
    ) internal {
        require(_from != address(0) || _to != address(0), "UC01");

        uint256 date = getNowDate();

        // mint
        if (_from == address(0)) {
            addressDailyTransfers[_to][_tokenAddress][date].mint_ += _amount;
        }
        // burn
        else if (_to == address(0)) {
            addressDailyTransfers[_from][_tokenAddress][date].burn_ += _amount;
        }
        // transfer
        else {
            addressDailyTransfers[_from][_tokenAddress][date]
                .transferOut_ += _amount;
            addressDailyTransfers[_to][_tokenAddress][date]
                .transferIn_ += _amount;
        }
    }

    function getTransferRecord(
        address _userAddress,
        address _tokenAddress,
        uint256 _dateIndex
    ) public view returns (DailyTransferRecord memory dailyTransferRecord) {
        return addressDailyTransfers[_userAddress][_tokenAddress][_dateIndex];
    }

    function getNowDate() public view returns (uint256 date) {
        uint256 nowTimesTamp = block.timestamp;
        uint256 year = DateTime.getYear(nowTimesTamp);
        uint256 month = DateTime.getMonth(nowTimesTamp);
        uint256 day = DateTime.getDay(nowTimesTamp);
        date = year * 10000 + month * 100 + day;
    }
}

