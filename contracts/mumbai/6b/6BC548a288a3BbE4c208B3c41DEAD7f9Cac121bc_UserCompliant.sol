pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./utils/Roles.sol";

/**
 * @title BlockListAdmin
 * @dev BlockListAdmin are responsible for assigning and removing BlockList
 *
 * Error:
 * BA01: User blocklisted
 * BA02: Only blocklist admin
 * BA03: Length mismatch
 */
abstract contract BlockListAdmin is OwnableUpgradeable {
    uint256 internal constant USER_NOT_BLOCKLISTED = 0;
    uint256 internal constant USER_BLOCKLISTED = 1;

    using Roles for Roles.Role;

    /** BlockList */
    struct UserBlockListData {
        bool isBlockList;
        uint256 validTimestamp;
    }

    event BlockListAdminSeted(address[] indexed accounts);
    event BlockListAdminAdded(address indexed account);
    event BlockListAdminRemoved(address indexed account);

    event SetUsersBlockList(
        uint256[] indexed userIds,
        uint256[] newValidTimestamp
    );
    event CancelUsersBlockList(uint256[] indexed userIds);

    /**
     * @notice Record UserBlockListData
     * @dev user id => UserBlockListData
     */
    mapping(uint256 => UserBlockListData) internal userBlockListMapping;

    // Mapping from token to token blockList admins.
    Roles.Role private _blockListAdmins;

    modifier onlyBlockListAdmin() virtual {
        require(isBlockListAdmin(msg.sender), "BA02");
        _;
    }

    function isBlockListAdmin(address account) public view returns (bool) {
        if (account == owner()) {
            return true;
        }
        return _blockListAdmins.has(account);
    }

    function getBlockListAdmins() public view returns (address[] memory) {
        return _blockListAdmins.get();
    }

    function setBlockListAdmins(address[] memory accounts)
        public
        onlyBlockListAdmin
    {
        _blockListAdmins.set(accounts);
        emit BlockListAdminSeted(accounts);
    }

    function addBlockListAdmin(address account) public onlyBlockListAdmin {
        _addBlockListAdmin(account);
    }

    function addBlockListAdmins(address[] memory accounts)
        public
        onlyBlockListAdmin
    {
        for (uint256 i = 0; i < accounts.length; i++) {
            _addBlockListAdmin(accounts[i]);
        }
    }

    function removeBlockListAdmin(address account) public onlyBlockListAdmin {
        _removeBlockListAdmin(account);
    }

    function removeBlockListAdmins(address[] memory accounts)
        public
        onlyBlockListAdmin
    {
        for (uint256 i = 0; i < accounts.length; i++) {
            _removeBlockListAdmin(accounts[i]);
        }
    }

    function renounceBlockListAdmin() public {
        _removeBlockListAdmin(msg.sender);
    }

    function _addBlockListAdmin(address account) internal {
        _blockListAdmins.add(account);
        emit BlockListAdminAdded(account);
    }

    function _removeBlockListAdmin(address account) internal {
        _blockListAdmins.remove(account);
        emit BlockListAdminRemoved(account);
    }

    function setUsersBlockList(
        uint256[] calldata userIds,
        uint256[] calldata newValidTimestamp
    ) external onlyBlockListAdmin {
        require(userIds.length == newValidTimestamp.length, "BA03");
        for (uint256 i = 0; i < userIds.length; i++) {
            UserBlockListData memory newUserBlockListData;
            newUserBlockListData.isBlockList = true;
            newUserBlockListData.validTimestamp = newValidTimestamp[i];
            userBlockListMapping[userIds[i]] = newUserBlockListData;
        }
        emit SetUsersBlockList(userIds, newValidTimestamp);
    }

    function cancelUsersBlockList(uint256[] calldata userIds)
        external
        onlyBlockListAdmin
    {
        for (uint256 i = 0; i < userIds.length; i++) {
            UserBlockListData memory newUserBlockListData;
            newUserBlockListData.isBlockList = false;
            userBlockListMapping[userIds[i]] = newUserBlockListData;
        }
        emit CancelUsersBlockList(userIds);
    }

    function getUserBlockListData(uint256 userId)
        public
        view
        returns (uint256)
    {
        // BlockList
        UserBlockListData memory userBlockListData = userBlockListMapping[
            userId
        ];
        if (userBlockListData.isBlockList == true) {
            // zero means no expiration date
            if (userBlockListData.validTimestamp == 0) {
                return (USER_BLOCKLISTED);
            } else if (userBlockListData.validTimestamp > block.timestamp) {
                return (USER_BLOCKLISTED);
            } else {
                return (USER_NOT_BLOCKLISTED);
            }
        }
        return (USER_NOT_BLOCKLISTED);
    }
}

