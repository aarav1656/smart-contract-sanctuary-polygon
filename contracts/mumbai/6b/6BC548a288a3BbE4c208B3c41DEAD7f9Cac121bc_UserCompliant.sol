pragma solidity ^0.8.0;
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "./utils/Roles.sol";

/**
 * @title KycAdmin
 * @dev KycAdmin are responsible for assigning and removing Kyc
 *
 * Error:
 * KA01: KYC invalid
 * KA02: KYC level mismatch
 * KA03: Length mismatch
 */
abstract contract KycAdmin is OwnableUpgradeable {
    uint256 internal constant KYC_VALID = 0;
    uint256 internal constant USER_NOT_KYC = 1;
    uint256 internal constant USER_KYC_EXPIRED = 2;

    using Roles for Roles.Role;

    /** Kyc */
    struct UserKycData {
        bool isKyc;
        uint256 kycLevel;
        uint256 validTimestamp;
    }

    event KycAdminSeted(address[] indexed accounts);
    event KycAdminAdded(address indexed account);
    event KycAdminRemoved(address indexed account);

    event SetUsersKyc(
        uint256[] indexed userIds,
        uint256[] newKycLevel,
        uint256[] newValidTimestamp
    );

    event CancelUsersKyc(uint256[] indexed users);

    /**
     * @notice Record UserKycData
     * @dev User ID => TokenAddress => UserKycData
     */
    mapping(uint256 => UserKycData) internal userKycMapping;

    // Mapping from token to token kyc admins.
    Roles.Role private _kycAdmins;

    /** KYC admins */

    modifier onlyKycAdmin() virtual {
        require(isKycAdmin(msg.sender), "Not KycAdmin");
        _;
    }

    function isKycAdmin(address account) public view returns (bool) {
        if (account == owner()) {
            return true;
        }
        return _kycAdmins.has(account);
    }

    function getKycAdmins() public view returns (address[] memory) {
        return _kycAdmins.get();
    }

    function setKycAdmins(address[] memory accounts) public onlyKycAdmin {
        _kycAdmins.set(accounts);
        emit KycAdminSeted(accounts);
    }

    function addKycAdmin(address account) public onlyKycAdmin {
        _addKycAdmin(account);
    }

    function addKycAdmins(address[] memory accounts) public onlyKycAdmin {
        for (uint256 i = 0; i < accounts.length; i++) {
            _addKycAdmin(accounts[i]);
        }
    }

    function removeKycAdmin(address account) public onlyKycAdmin {
        _removeKycAdmin(account);
    }

    function removeKycAdmins(address[] memory accounts) public onlyKycAdmin {
        for (uint256 i = 0; i < accounts.length; i++) {
            _removeKycAdmin(accounts[i]);
        }
    }

    function renounceKycAdmin() public {
        _removeKycAdmin(msg.sender);
    }

    function _addKycAdmin(address account) internal {
        _kycAdmins.add(account);
        emit KycAdminAdded(account);
    }

    function _removeKycAdmin(address account) internal {
        _kycAdmins.remove(account);
        emit KycAdminRemoved(account);
    }

    /** User KYC */

    function setUserKyc(
        uint256 userId,
        uint256 newKycLevel,
        uint256 newValidTimestamp
    ) external onlyKycAdmin {
        _setUserKyc(userId, true, newKycLevel, newValidTimestamp);
    }

    function setUsersKyc(
        uint256[] calldata userIds,
        uint256[] calldata newKycLevel,
        uint256[] calldata newValidTimestamp
    ) external onlyKycAdmin {
        require(
            userIds.length == newKycLevel.length &&
                userIds.length == newValidTimestamp.length,
            "KA03"
        );
        for (uint256 i = 0; i < userIds.length; i++) {
            _setUserKyc(userIds[i], true, newKycLevel[i], newValidTimestamp[i]);
        }
        emit SetUsersKyc(userIds, newKycLevel, newValidTimestamp);
    }

    function cancelUsersKyc(uint256[] calldata userIds) external onlyKycAdmin {
        for (uint256 i = 0; i < userIds.length; i++) {
            _setUserKyc(userIds[i], false, 0, 0);
        }
        emit CancelUsersKyc(userIds);
    }

    function _setUserKyc(
        uint256 userId,
        bool isKyc,
        uint256 newKycLevel,
        uint256 newValidTimestamp
    ) internal {
        UserKycData memory newUserKycData;
        newUserKycData.isKyc = isKyc;
        if (newValidTimestamp == 0) {
            newUserKycData.validTimestamp = 0;
        } else {
            newUserKycData.validTimestamp = newValidTimestamp;
        }
        newUserKycData.kycLevel = newKycLevel;
        userKycMapping[userId] = newUserKycData;
    }

    /**
     * @notice check user kyc level
     * @dev called by kycTransfer rule
     * @return result 0 stands for user kyc is valid
     * other result representative users do not meet some conditions
     */
    function getUserKycLevel(uint256 userId)
        public
        view
        returns (uint256 result, uint256 kycLevel)
    {
        if (userKycMapping[userId].isKyc == true) {
            if (userKycMapping[userId].validTimestamp == 0) {
                return (KYC_VALID, userKycMapping[userId].kycLevel);
            } else if (
                userKycMapping[userId].validTimestamp > block.timestamp
            ) {
                return (KYC_VALID, userKycMapping[userId].kycLevel);
            } else {
                return (USER_KYC_EXPIRED, 0);
            }
        } else {
            return (USER_NOT_KYC, 0);
        }
    }
}

