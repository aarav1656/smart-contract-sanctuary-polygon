pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "erc721a/contracts/ERC721A.sol";
import "./Errors.sol";

contract Event is ERC721A {
    using Strings for uint256;

    string public baseURI = "";
    uint256 public maxSupply;
    uint256 public maxMintAmount;
    string[] public tickets;
    uint256[] public prices;
    bool public paused;
    address public organizer;

    struct TicketData {
        uint256 price;
        bool isPartOfEvent;
        uint256 supply;
        uint256 minted;
    }

    mapping(address => bool) public isStableAllowed;
    mapping(string => TicketData) public ticketData;
    mapping(address => uint) ticketBoughtByUsers;


    event TicketPriceDefined(string indexed ticketType, uint256 indexed price);
    event NewPaymentMethodAdded(address indexed _stableCoin);
    event NewTicketsMinted(string indexed ticketType, address indexed owner, uint ticketPrice, uint ticketsBougth);

    modifier onlyOwner() {
        require(
            msg.sender == organizer,
            "This function is restricted to the factory governance"
        );
        _;
    }

    constructor(
        string memory _name,
        uint256 _maxSupply,
        string[] memory _tickets,
        uint256[] memory _prices,
        uint256 _maxMint,
        address _organizer
    ) payable ERC721A(_name, _name) {
        require(_tickets.length == _prices.length, "lengths don't match");
        maxSupply = _maxSupply;
        tickets = _tickets;
        prices = _prices;
        maxMintAmount = _maxMint;
        organizer = _organizer;
        // assigned ticket category a price
        for (uint i; i<_tickets.length; i++) {
            ticketData[_tickets[i]] = TicketData({
                price: _prices[i],
                isPartOfEvent: true,
                supply: 0,
                minted: 0
            });
        }
        
    }

    function pause() public onlyOwner {
        paused = !paused;
    }

    /**
    *@notice set a supply number for ticket category, ex: STANDARD  ticket has 100 units
    *@param _ticketName category of ticket
    *@param _supply number of unit to assign 
    */
    function setTicketCategorySupply(string calldata _ticketName, uint256 _supply) external onlyOwner {
        if(ticketData[_ticketName].supply != 0) revert  TicketCategorySupplyAlreadySet();
        if(totalSupply() + _supply > maxSupply) revert ErrorTicketNumberToSet();
        ticketData[_ticketName].supply = _supply;
    }

    /**
    *@notice organiser can add new stable coin payment method
    *@param _stableCoin address stable coin 
    */
    function addNewStableCoin(address _stableCoin) public onlyOwner() {
        // check if the payment method has already been added
        if(isStableAllowed[_stableCoin]) revert PaymentMethodAlreadyAdded();
        isStableAllowed[_stableCoin] = true;
        emit NewPaymentMethodAdded(_stableCoin);
    }

    /**
    *@notice set price for ticket Category
    *@param _ticketName index representing the ticket category (standard, vip, ...)
    *@param _price price for the define category 
    */
    function setTicketPrice(string calldata _ticketName, uint _price) public onlyOwner {
        if(ticketData[_ticketName].isPartOfEvent == false) revert TicketIsNotPartOfEvent();
        ticketData[_ticketName].price = _price;
        emit TicketPriceDefined(_ticketName, _price);
    }


    /**
    *@notice allow user to pay new ticket for the event, the payment method allows only stablecoin
    *@param _to person who receives the ticket
    *@param _ticketName category of ticket, can be STANDARD, VIP, depending of the organizer
    *@param _paymentMethod stablecoin used to make the payment
    *@param _mintAmount amount of tickets to mint
    */
    function mint(
        address _to,
        address _paymentMethod,
        string calldata _ticketName,
        uint256 _mintAmount 
    ) public payable {
        uint256 supply = totalSupply();
        uint256 _ticketPrice = ticketData[_ticketName].price;
        // revert method
        if (paused) revert EventPaused();
        if (_mintAmount > maxMintAmount) revert MaxMintAmount();
        if (supply + _mintAmount > maxSupply) revert LimitTicketReached();
        if (!isStableAllowed[_paymentMethod]) revert InvalidPaymentMethod();
        if(ticketData[_ticketName].isPartOfEvent == false) revert TicketIsNotPartOfEvent();
        // if ticket supply for category by category has been set
        if (ticketData[_ticketName].supply != 0) {
            // get ticket SupplyForCategory
            uint256 categorySupply = ticketData[_ticketName].supply;
            // get amount minted for ticket type
            uint256 ticketMintedForCategory = ticketData[_ticketName].minted;
            // revert if all tickets type have been minted 
            if(_mintAmount + ticketMintedForCategory > categorySupply)  revert LimitForTicketCategoryReached();
        }
        uint256 amountToPay = _ticketPrice * _mintAmount;
        IERC20(_paymentMethod).transferFrom(msg.sender, address(this), amountToPay);
        // update ticket sold count
        unchecked {
            ticketData[_ticketName].minted += _mintAmount;
            // ticketSoldForCategory[_ticketName] += _mintAmount;
        }
        // mint Ticket
        _safeMint(_to, _mintAmount);
        emit NewTicketsMinted(_ticketName, _to, _ticketPrice, _mintAmount);
    }

    function getTicketPrice(string calldata _ticketName) public view returns (uint256) {
        return ticketData[_ticketName].price;
    }

    function getTicketStatus(string calldata _ticketName) public view returns (bool) {
        return ticketData[_ticketName].isPartOfEvent;
    }

    function handlePayment(uint256 amount, address erc20)
        internal
        returns (bool)
    {}

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory currentBaseURI = _baseURI();

        return
            string(
                abi.encodePacked(
                    "data:application/json,",
                    abi.encodePacked(
                        '{"name":"',
                        "COMING SOON",
                        '","description":"COMING SOON",',
                        '"image":"'
                        '","event_url":"',
                        string(abi.encodePacked(currentBaseURI)),
                        '"}'
                    )
                )
            );
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setmaxMintAmount(uint256 _newmaxMintAmount) public onlyOwner {
        maxMintAmount = _newmaxMintAmount;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }



    /**
    *@notice withdraw all amount from the contract
    *@param _coin organizer wants to withdraw
    *@param _to address where the funds are transfered
    *@param _amount amount to withdraw
    */
    function withdraw(address _coin, address _to, uint _amount) external onlyOwner {
       uint256 _balanceOf = IERC20(_coin).balanceOf(address(this));
       if(_balanceOf >= _amount) {
        IERC20(_coin).transfer(_to, _amount);
       }
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "./Event.sol";

contract Factory {
    address public owner;
    address public factoryGov;
    // uint256 public last_completed_migration;
    bool public live;
    
    mapping(uint256 => uint256) public fees;
    mapping(address => bool) public isStableAllowed;

    event NewPaymentMethodAdded(address indexed _stableCoin);

    error InvalidOwner();
    error CostNotSet();
    error InsufficientFunds();
    

    modifier onlyGov() {
        require(
            msg.sender == factoryGov,
            "This function is restricted to the factory governance"
        );
        _;
    }

    constructor(address _factoryGov) payable {
        owner = msg.sender;
        factoryGov = _factoryGov;
    }

    function setGovernance(address _factoryGov) public onlyGov {
        factoryGov = _factoryGov;
    }

    event EventMinted(string indexed eventName, address indexed eventAddress);
    event MintCostUpdated(
        uint256 oldCost,
        uint256 newMintCost,
        uint256 eventType
    );

    /**
    *@notice create new events
    *@param eventName name of the event organizer wants to creat
    *@param supply amount of ticket for this event
    *@param tickets array that contains all the tickets type available, exple: STANDARD, VIP, VVIP ...
    *@param prices array that contains price for all the tickets
    *@param maxMint maximum number of tickets per wallet
    *@param organizer address of the organizer 
    */
    function createEvent(
        string memory eventName,
        uint256 supply,
        string[] calldata tickets,
        uint256[] calldata prices,
        uint256 maxMint,
        address organizer
    ) public payable returns (address) {
        uint256 mintCost = getMintCost(supply);
        if (live != true) revert CostNotSet();
        payFee(mintCost);
        Event _eventAddress = new Event(
            eventName,
            supply,
            tickets,
            prices,
            maxMint,
            organizer
        );

        emit EventMinted(eventName, address(_eventAddress));
        return address(_eventAddress);
    }

    function setMintCost(uint256[] memory cost) external onlyGov {
        require(cost.length == 3, "invalid args");
        for(uint i; i < 2; i++) {
            fees[i] = cost[i];
        }
        live = true;
    }

    function payFee(uint mintCost) internal {
        if (msg.value < mintCost) revert InsufficientFunds();
    }

    function getMintCost(uint256 supply) internal view returns (uint256 cost) {
        if (supply <= 200) {
            cost =  fees[0];
        }
        if (200 < supply && supply <= 500) {
            cost = fees[1];
        }
        if (supply > 500) {
            cost =  fees[2];
        }
    }

    function withdraw() internal  {
        if (msg.sender != factoryGov) revert InvalidOwner();
        (bool os, ) = payable(factoryGov).call{value: address(this).balance}("");
        require(os);
    }

    /**
    *@notice withdraw all amount from the contract
    *@param _coin organizer wants to withdraw
    *@param _to address where the funds are transfered
    *@param _amount amount to withdraw
    */
    function withdraw(address _coin, address _to, uint _amount) external onlyGov {
       uint256 _balanceOf = IERC20(_coin).balanceOf(address(this));
       if(_balanceOf >= _amount) {
        IERC20(_coin).transfer(_to, _amount);
       }
    }
}

