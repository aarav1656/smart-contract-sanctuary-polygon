    /// @param offerId id of the sell offer to accept
    /// @param needTransfer internal flag, which shows does token tranfer needed,
    /// @param amount of nfts to buy
    /// if nft is buying by the native is equal to false, else true
    /// @return token token address to pay nft owner
    /// @return fee the fee function return variable,
    /// profit of nft seller in selected token with taking account of fee
    /// @return owner the sell offer owner, also nft owner, who gets profit
    function _acceptSellOffer(
        uint256 offerId,
        bool needTransfer,
        uint256 amount
    )
        internal
        offerExists(offerId)
        nonReentrant
        returns (
            address,
            uint256,
            address
        )
    {
        Offer storage offer = _offers[offerId];
        require(offer.direction == Direction.SELL, 'Could not buy not selling nft');
        require(offer.status == Status.PENDING, 'Not valid offer');
        require(offer.owner != msg.sender, 'caller is lot owner');
        require(amount <= offer.amount, 'Not valid amount');
        offer.lastNFtsActionAmount = amount;
        if (amount == offer.amount) offer.status = Status.ACCEPTED;
        else offer.amount -= amount;
        uint256 _totalPrice = offer.price * amount;
        if (needTransfer)
            IERC20(offer.token).safeTransferFrom(msg.sender, address(this), _totalPrice);

        _transferNFT(
            address(this),
            msg.sender,
            offer.nft.nftContract,
            offer.nft.tokenId,
            amount
        );
        emit SellOfferAccepted(block.timestamp, offer.owner, msg.sender, offerId);

        uint256 profit = _fee(_totalPrice, offer.token);

        _addUserActivity(msg.sender, offerId, _offers[offerId].price, Direction.BUY);
        _addUserActivity(offer.owner, offerId, profit, Direction.SELL);
        _addUserOffer(msg.sender, offerId, UserOfferType.ACCEPT);
        _addUserOffer(offer.owner, offerId, UserOfferType.ACCEPT);

        _addUserHistory(msg.sender, offerId, Direction.BUY);
        _addUserHistory(offer.owner, offerId, Direction.SELL);

        return (offer.token, profit, offer.owner);
    }

    function acceptSellOffer(uint256 offerId) external {
        (address token, uint256 profit, address owner_) = _acceptSellOffer(
            offerId,
            true,
            1
        );
        IERC20(token).safeTransfer(owner_, profit);
    }

    function acceptSellOfferWithAmount(uint256 offerId, uint256 amount)
        external
        notZeroAmount(amount)
    {
        (address token, uint256 profit, address owner_) = _acceptSellOffer(
            offerId,
            true,
            amount
        );
        IERC20(token).safeTransfer(owner_, profit);
    }

    function acceptSellOfferETH(uint256 offerId) external payable {
        _acceptSellOfferETH(offerId, 1);
    }

    function acceptSellOfferWithAmountETH(uint256 offerId, uint256 amount)
        external
        payable
        notZeroAmount(amount)
    {
        _acceptSellOfferETH(offerId, amount);
    }

    /// @dev Buying nft by the function caller only with ETH by selected price
    /// @param offerId id of the sell offer to accept
    function _acceptSellOfferETH(uint256 offerId, uint256 amount) internal {
        Offer memory offer = _offers[offerId];
        require(offer.token == WETH, 'This nft selling only for tokens');
        uint256 _totalPrice = offer.price * amount;
        if (msg.value < _totalPrice) revert NotEnoughETH();
        uint256 remain = msg.value - _totalPrice;

        if (remain > 0) payable(msg.sender).transfer(remain);

        IWETH(payable(WETH)).deposit{value: _totalPrice}();
        (, uint256 profit, ) = _acceptSellOffer(offerId, false, amount);
        IWETH(payable(WETH)).withdraw(profit);
        payable(offer.owner).transfer(profit);
    }

    function acceptBuyOffer(uint256 offerId) external {
        _acceptBuyOffer(offerId, 1);
    }

    function acceptBuyOfferWithAmount(uint256 offerId, uint256 amount)
        external
        notZeroAmount(amount)
    {
        _acceptBuyOffer(offerId, amount);
    }

    /// @dev Sells nft to by offer owner with selected token and price
    /// @param offerId if of the buy offer to accept
    function _acceptBuyOffer(uint256 offerId, uint256 amount)
        internal
        offerExists(offerId)
        nonReentrant
    {
        Offer storage offer = _offers[offerId];
        require(amount <= offer.amount, 'Not valid amount');
        require(offer.direction == Direction.BUY, 'This is not buy offer');
        require(offer.status == Status.PENDING, 'Not valid offer');
        require(offer.owner != msg.sender, 'caller is lot owner');
        offer.lastNFtsActionAmount = amount;
        if (amount == offer.amount) offer.status = Status.ACCEPTED;
        else offer.amount -= amount;
        _checkOwnership(offer.nft.nftContract, offer.nft.tokenId, msg.sender);
        _transferNFT(
            msg.sender,
            offer.owner,
            offer.nft.nftContract,
            offer.nft.tokenId,
            amount
        );

        uint256 profit = _fee(offer.price * amount, offer.token);
        if (offer.token != WETH) {
            IERC20(offer.token).safeTransfer(msg.sender, profit);
        } else {
            IWETH(payable(WETH)).withdraw(profit);
            payable(msg.sender).transfer(profit);
        }

        _addUserActivity(msg.sender, offer.id, offer.price, Direction.SELL);
        _addUserActivity(offer.owner, offer.id, profit, Direction.BUY);
        _addUserOffer(offer.owner, offerId, UserOfferType.ACCEPT);

        _addUserHistory(msg.sender, offerId, Direction.SELL);
        _addUserHistory(offer.owner, offerId, Direction.BUY);

        emit BuyOfferAccepted(block.timestamp, offer.owner, msg.sender, offerId);
    }

    /// @dev Allows to fulfill sell and buy offer with the same nft and parameters
    /// the function caller gets diff between sell and buy offers price
    /// @param sellOfferId id of sell offer to sell nft
    /// @param buyOfferId id of buy offer to buy nft
    function fulfillOffer(uint256 sellOfferId, uint256 buyOfferId)
        external
        offerExists(sellOfferId)
        offerExists(buyOfferId)
        nonReentrant
    {
        Offer storage buyOffer = _offers[buyOfferId];
        Offer storage sellOffer = _offers[sellOfferId];
        require(buyOffer.amount == 1 && sellOffer.amount == 1);
        require(buyOffer.direction == Direction.BUY);
        require(sellOffer.direction == Direction.SELL);
        require(sellOffer.status == Status.PENDING);
        require(buyOffer.status == Status.PENDING);
        require(sellOffer.price <= buyOffer.price);
        require(sellOffer.nft.nftContract == buyOffer.nft.nftContract);
        require(sellOffer.nft.tokenId == buyOffer.nft.tokenId);
        require(sellOffer.token == buyOffer.token);

        buyOffer.status = Status.ACCEPTED;
        sellOffer.status = Status.ACCEPTED;
        uint256 fulfillProfit = buyOffer.price - sellOffer.price;
        if (sellOffer.token == WETH) {
            IWETH(payable(WETH)).withdraw(
                buyOffer.price - (buyOffer.price * adminPanel.feeX1000()) / 1000
            );
            payable(msg.sender).transfer(_fee(fulfillProfit, WETH));
            payable(sellOffer.owner).transfer(_fee(sellOffer.price, WETH));
        } else {
            IERC20(sellOffer.token).safeTransfer(
                msg.sender,
                _fee(fulfillProfit, sellOffer.token)
            );
            IERC20(sellOffer.token).safeTransfer(
                sellOffer.owner,
                _fee(sellOffer.price, sellOffer.token)
            );
        }

        _addUserActivity(buyOffer.owner, buyOffer.id, buyOffer.price, Direction.BUY);
        _addUserActivity(sellOffer.owner, sellOffer.id, sellOffer.price, Direction.SELL);
        _addUserActivity(msg.sender, sellOffer.id, fulfillProfit, Direction.SELL);

        _addUserOffer(buyOffer.owner, buyOffer.id, UserOfferType.ACCEPT);
        _addUserOffer(sellOffer.owner, sellOffer.id, UserOfferType.ACCEPT);

        _addUserHistory(buyOffer.owner, buyOffer.id, Direction.BUY);
        _addUserHistory(sellOffer.owner, sellOffer.id, Direction.SELL);

        _transferNFT(
            address(this),
            buyOffer.owner,
            buyOffer.nft.nftContract,
            buyOffer.nft.tokenId,
            buyOffer.amount
        );

        emit FulFill(block.timestamp, sellOffer.id, buyOffer.id);
    }

    /// @dev Calculates fee amount and transfers it to fee address
    /// @param price the nft price in selected token
    /// @param token the fee transfer token
    /// @return profit with taking account of current fee
    function _fee(uint256 price, address token) internal returns (uint256) {
        IERC20(token).safeTransfer(
            adminPanel.feeAddress(),
            (price * adminPanel.feeX1000()) / 1000
        );
        return price - (price * adminPanel.feeX1000()) / 1000;
    }

    /// @dev Resturns lot with specific `offerId`
    /// @param offerId id of offer
    function getOffer(uint256 offerId) public view returns (OfferOutput memory) {
        OfferOutput memory offerOutput = OfferOutput(getOfferNFT(offerId));
        return offerOutput;
    }

    /// @dev Resturns lot with specific `offerId`
    /// @param offerId id of offer
    function getOfferNFT(uint256 offerId) public view returns (Offer memory) {
        require(offerId < _offers.length, 'Not existed lotId');
        Offer memory offer = _offers[offerId];
        offer.nft.tokenURI = _tokenURI(offer.nft.nftContract, offer.nft.tokenId);
        return offer;
    }

    /// @dev Returns array of offers with `offset` and `limit`
    /// @param offset start index
    /// @param limit length of array
    function offers(uint256 offset, uint256 limit)
        external
        view
        returns (OfferOutput[] memory)
    {
        OfferOutput[] memory offersOutput = new OfferOutput[](limit);
        for (uint256 i = 0; i < limit; i++) {
            if (i + offset >= _offers.length) break;
            offersOutput[i] = getOffer(offset + i);
        }
        return offersOutput;
    }

    /// @dev Returns array of lots with `offset` and `limit` with status `status`
    /// @param offset start index
    /// @param limit length of array
    /// @param status status of lot
    function offersStatus(
        uint256 offset,
        uint256 limit,
        Status status
    ) external view returns (OfferOutput[] memory) {
        OfferOutput[] memory offersOutput = new OfferOutput[](limit);
        uint256 skiped = 0;
        uint256 inIndex = 0;
        uint256 outIndex = 0;

        while (inIndex < _offers.length && outIndex < limit) {
            OfferOutput memory offerOutput = getOffer(inIndex);

            if (offerOutput.offer.status == status) {
                if (skiped >= offset) {
                    offersOutput[outIndex] = offerOutput;
                    outIndex++;
                } else skiped++;
            }
            inIndex++;
        }
        return offersOutput;
    }

    /// @dev Allows to get all activites for user
    /// @param user addres of user
    /// @param offset start index
    /// @param limit length of array
    function getActivities(
        address user,
        uint256 offset,
        uint256 limit
    ) external view returns (UserActivity[] memory) {
        uint256 length = _userActivities[user].length;
        UserActivity[] memory activities_ = new UserActivity[](limit);

        for (uint256 i = 0; i < limit; i++) {
            if (length < (offset + i) + 1) break;
            activities_[i] = _userActivities[user][length - (offset + i) - 1];
        }

        return activities_;
    }

    /// @dev Allows to get all user's history
    /// @param user addres of user
    /// @param offset start index
    /// @param limit length of array
    function getHistory(
        address user,
        uint256 offset,
        uint256 limit
    ) external view returns (UserHistory[] memory) {
        uint256 length = _userHistory[user].length;
        UserHistory[] memory userHistory = new UserHistory[](limit);

        for (uint256 i = 0; i < limit; i++) {
            if (length < (offset + i) + 1) break;
            userHistory[i] = _userHistory[user][length - (offset + i) - 1];
        }

        return userHistory;
    }

    /// @dev Allows to get all user's buy/sell history
    /// @param user addres of user
    /// @param offset start index
    /// @param limit length of array
    function getUserOffers(
        address user,
        uint256 offset,
        uint256 limit
    ) external view returns (UserOffer[] memory) {
        uint256 length = userOffers_[user].length;
        UserOffer[] memory userOffers__ = new UserOffer[](limit);

        for (uint256 i = 0; i < limit; i++) {
            if (length < (offset + i) + 1) break;
            userOffers__[i] = userOffers_[user][length - (offset + i) - 1];
        }

        return userOffers__;
    }

    /// @dev Allows to get all offers of one selected user
    /// @param user address to get all offers for
    /// @param offset start index
    /// @param limit length of array
    /// @return ids all offers id of selected user
    function userOffers(
        address user,
        uint256 offset,
        uint256 limit
    ) external view returns (uint256[] memory) {
        require(_userOffers[user].length > offset, 'Not existed element');

        uint256[] memory ids = new uint256[](limit);
        for (uint256 i = 0; i < limit; i++) {
            if (i + offset >= _userOffers[user].length) break;
            ids[i] = _userOffers[user][i + offset];
        }
        return ids;
    }

    /// @dev Sets new admin panel address
    /// @param newAdminPanel new admin panel address
    function setAdminPanel(address newAdminPanel) external onlyOwner {
        adminPanel = IAdminPanel(newAdminPanel);
    }
}

