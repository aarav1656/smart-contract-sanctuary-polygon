pragma solidity ^0.8.0;

/**
 * @title Market
 * @author gotbit
 */

import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/security/ReentrancyGuard.sol';
import './interfaces/IAdminPanel.sol';
import './interfaces/IMarket.sol';
import './NFTAssist.sol';
import './interfaces/IWETH.sol';
import 'hardhat/console.sol';

contract Market is IMarket, Ownable, NFTAssist, ReentrancyGuard {
    using SafeERC20 for IERC20;

    address public WETH;
    IAdminPanel public adminPanel;

    Offer[] private _offers;
    mapping(address => uint256[]) private _userOffers;
    mapping(address => bool) public validTokens;

    mapping(address => UserActivity[]) private _userActivities;
    mapping(address => UserOffer[]) private userOffers_;
    mapping(address => UserHistory[]) private _userHistory;

    event OfferCreated(
        uint256 indexed timestamp,
        address indexed owner,
        uint256 indexed offerId
    );
    event OfferCanceled(
        uint256 indexed timestamp,
        address indexed owner,
        uint256 indexed offerId
    );
    event SellOfferAccepted(
        uint256 indexed timestamp,
        address seller,
        address buyer,
        uint256 indexed offerId
    );
    event BuyOfferAccepted(
        uint256 indexed timestamp,
        address seller,
        address buyer,
        uint256 indexed offerId
    );
    event OfferPriceChanged(
        uint256 indexed timestamp,
        uint256 oldPrice,
        uint256 newPrice,
        uint256 indexed offerId
    );
    event FulFill(
        uint256 indexed timestamp,
        uint256 indexed sellOfferId,
        uint256 indexed buyOfferId
    );

    constructor(address WETH_, address adminPanel_) {
        WETH = WETH_;
        adminPanel = IAdminPanel(adminPanel_);
    }

    modifier offerExists(uint256 offerId) {
        require(_offers[offerId].id < _offers.length, 'Offer does not exist');
        _;
    }

    modifier notZeroAmount(uint256 amount) {
        require(amount > 0, 'Zero amount');
        _;
    }

    fallback() external payable {}

    receive() external payable {}

    /// @dev adds activity of user
    /// @param user address of user
    /// @param offerId id of offer
    /// @param direction direction of activity
    function _addUserActivity(
        address user,
        uint256 offerId,
        uint256 amount,
        Direction direction
    ) internal {
        _userActivities[user].push(
            UserActivity({
                timestamp: block.timestamp,
                offer: getOfferNFT(offerId),
                user: user,
                amount: amount,
                direction: direction
            })
        );
    }

    /// @dev adds offer for user
    /// @param user address of user
    /// @param offerId id of offer
    /// @param type_ type of offer
    function _addUserOffer(
        address user,
        uint256 offerId,
        UserOfferType type_
    ) internal {
        userOffers_[user].push(
            UserOffer({
                timestamp: block.timestamp,
                user: user,
                offer: getOfferNFT(offerId),
                type_: type_
            })
        );
    }

    /// @dev adds history for user
    /// @param user address of user
    /// @param offerId id of offer
    /// @param direction direction of trade
    function _addUserHistory(
        address user,
        uint256 offerId,
        Direction direction
    ) internal {
        _userHistory[user].push(
            UserHistory({
                timestamp: block.timestamp,
                user: user,
                offer: getOfferNFT(offerId),
                direction: direction
            })
        );
    }

    function createBuyOffer(
        address nft,
        uint256 tokenId,
        uint256 price_,
        address token_
    ) external payable {
        _createBuyOffer(nft, tokenId, price_, token_, 1);
    }

    function createBuyOfferWithAmount(
        address nft,
        uint256 tokenId,
        uint256 price_,
        address token_,
        uint256 amount_
    ) external payable notZeroAmount(amount_) {
        require(_isERC1155(nft), 'Not ERC1155');
        _createBuyOffer(nft, tokenId, price_, token_, amount_);
    }

    /// @dev Creates a Buy offer and sets it's info for selected nft address and token id
    /// @param nft address of nft to buy
    /// @param tokenId token id of nft to buy
    /// @param price_ suggested price to buy nft
    /// @param token_ token address of suggested tokens amount
    function _createBuyOffer(
        address nft,
        uint256 tokenId,
        uint256 price_,
        address token_,
        uint256 amount_
    ) internal nonReentrant {
        require(adminPanel.validTokens(token_), 'Not allowed token selected');
        require(adminPanel.belongsToWhitelist(nft), 'NFT does not belong to whitelist');
        uint256 _totalPrice = price_ * amount_;
        if (msg.value > 0) {
            if (msg.value < _totalPrice) revert NotEnoughETH();
            uint256 remain = msg.value - _totalPrice;
            if (remain > 0) payable(msg.sender).transfer(remain);
            require(token_ == WETH, 'Select weth as pay token, if you buy with eth');
            IWETH(payable(WETH)).deposit{value: _totalPrice}();
        } else {
            IERC20(token_).safeTransferFrom(msg.sender, address(this), _totalPrice);
        }
        Offer memory offer = Offer({
            id: _offers.length,
            nft: _getNFT(nft, tokenId),
            owner: msg.sender,
            token: token_,
            timeStarted: block.timestamp,
            price: price_,
            amount: amount_,
            lastNFtsActionAmount: amount_,
            status: Status.PENDING,
            direction: Direction.BUY
        });
        _offers.push(offer);
        _userOffers[msg.sender].push(offer.id);

        _addUserOffer(msg.sender, offer.id, UserOfferType.PLACED);
        _addUserActivity(msg.sender, offer.id, price_, Direction.BUY);

        emit OfferCreated(block.timestamp, msg.sender, _offers.length - 1);
    }

    function createSellOfferWithAmount(
        address nft,
        uint256 tokenId,
        uint256 price_,
        address token_,
        uint256 amount_
    ) external notZeroAmount(amount_) {
        require(_isERC1155(nft), 'Not ERC1155');
        _createSellOffer(nft, tokenId, price_, token_, amount_);
    }

    function createSellOffer(
        address nft,
        uint256 tokenId,
        uint256 price_,
        address token_
    ) external {
        _createSellOffer(nft, tokenId, price_, token_, 1);
    }

    /// @dev Creates a sell offer and sets it for selected nft address and id only by nft owner
    /// @param nft staking nft address
    /// @param tokenId staking nft token id
    /// @param price_ amount of nft cost in selected token
    /// @param token_ selected token to pay nft seller
    function _createSellOffer(
        address nft,
        uint256 tokenId,
        uint256 price_,
        address token_,
        uint256 amount_
    ) internal {
        _checkOwnership(nft, tokenId, msg.sender);
        require(adminPanel.validTokens(token_), 'Not allowed token selected');
        require(adminPanel.belongsToWhitelist(nft), 'NFT does not belong to whitelist');
        require(price_ != 0, 'Zero price');
        _transferNFT(msg.sender, address(this), nft, tokenId, amount_);
        Offer memory offer = Offer({
            id: _offers.length,
            nft: _getNFT(nft, tokenId),
            owner: msg.sender,
            token: token_,
            timeStarted: block.timestamp,
            price: price_,
            amount: amount_,
            lastNFtsActionAmount: amount_,
            status: Status.PENDING,
            direction: Direction.SELL
        });
        _offers.push(offer);

        _addUserOffer(msg.sender, offer.id, UserOfferType.PLACED);
        _userOffers[msg.sender].push(offer.id);
        emit OfferCreated(block.timestamp, msg.sender, _offers.length - 1);
    }

    /// @dev Cancel the offer handling all directoins by setting direction to null
    /// @param offerId id of the offer to cancel
    function cancelOffer(uint256 offerId) external offerExists(offerId) nonReentrant {
        Offer storage offer = _offers[offerId];

        require(offer.owner == msg.sender, 'Caller is not the creator of the offer');
        require(offer.status == Status.PENDING, 'Offer is not valid');
        offer.status = Status.CANCELED;
        uint256 _totalPrice = offer.price * offer.amount;
        if (offer.direction == Direction.BUY) {
            if (offer.token == address(WETH)) {
                IWETH(payable(WETH)).withdraw(_totalPrice);
                payable(msg.sender).transfer(_totalPrice);
            } else {
                IERC20(offer.token).safeTransfer(msg.sender, _totalPrice);
            }

            _addUserActivity(msg.sender, offer.id, offer.price, Direction.BUY);
            _addUserOffer(msg.sender, offer.id, UserOfferType.CANCELED);
        } else if (offer.direction == Direction.SELL) {
            _transferNFT(
                address(this),
                msg.sender,
                offer.nft.nftContract,
                offer.nft.tokenId,
                offer.amount
            );
            _addUserOffer(msg.sender, offer.id, UserOfferType.CANCELED);
        } else {
            revert NotValidOffer();
        }

        emit OfferCanceled(block.timestamp, msg.sender, offer.id);
    }

    /// @dev Internal function to realize general sell offer acception:
    /// receives tokens, changes the offer status, transfers nft to buyer
