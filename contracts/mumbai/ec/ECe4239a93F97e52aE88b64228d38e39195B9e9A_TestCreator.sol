        if (_testType == 0) {  // Open answers test, providing the [answerHashesRoot]
            _openAnswersRoot[_testId] = _solvingHashes[0];
        } else if (_testType > 0 && _testType < 100) {  /// Mixed test, providing [solutionHash, answerHashesRoot]
