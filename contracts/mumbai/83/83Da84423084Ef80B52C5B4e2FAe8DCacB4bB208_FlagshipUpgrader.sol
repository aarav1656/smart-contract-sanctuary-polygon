pragma solidity ^0.8.15;

import "./../interfaces/DPSInterfaces.sol";
import "./../interfaces/DPSStructs.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

contract FlagshipUpgrader is AccessControlEnumerable {
    DPSFlagshipI public flagship;
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    struct Flagship {
        uint16 flagshipId;
        uint8[7] parts;
    }

    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(UPGRADER_ROLE, _msgSender());
    }

    function upgradeParts(Flagship[] calldata _flagships) external onlyRole(UPGRADER_ROLE) {
        for (uint256 i; i < _flagships.length; ++i) {
            if (gasleft() < 270_000) break;
            upgradeParts(_flagships[i]);
        }
    }

    function upgradeParts(Flagship memory _flag) internal {
        for (uint8 i = 0; i < 7; i++) {
            if ((i != 0 && _flag.parts[i] != 1) || (i == 0 && _flag.parts[i] != 100)) {
                flagship.upgradePart(FLAGSHIP_PART(i), _flag.flagshipId, _flag.parts[i]);
            }
        }
    }

    function setFlagship(DPSFlagshipI _flagship) external onlyRole(DEFAULT_ADMIN_ROLE) {
        flagship = _flagship;
    }
}

