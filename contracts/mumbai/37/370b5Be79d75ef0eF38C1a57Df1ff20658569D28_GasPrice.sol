import "../interfaces/IMerfiVault.sol";
import "../interfaces/IMerfiStrategy.sol";
import "../interfaces/IMerfiHarvester.sol";
import "./gelato/OpsReady.sol";
import "./Manageable.sol";

abstract contract MerfiHarvester is Manageable, IMerfiHarvester, OpsReady {
    using SafeERC20 for IERC20;

    uint256 public delay = 10 minutes;

    function harvestVault(address _vault) external {
        IMerfiStrategy strategy = IMerfiStrategy(IMerfiVault(_vault).strategy());
        uint256 _rewardsAmount = strategy.rewardsAvailable();

        try strategy.harvest(address(this)) {
           emit HarvestSucceeded(_vault, _rewardsAmount);
        } catch {
           emit HarvestFailed(_vault, _rewardsAmount);
        }

        _payTxFee();
    }

    function willHarvestVault(address _vaultAddress)
        external
        view
        returns (
            bool _willHarvestVault
        )
    {
        bool shouldHarvestVault = _shouldHarvestVault(_vaultAddress);
        bool canHarvestVault = _canHarvestVault(_vaultAddress);

        _willHarvestVault = canHarvestVault && shouldHarvestVault;

        return _willHarvestVault;
    }

    function setDelay(uint256 _delay) external onlyManager { 
        delay = _delay;
        emit DelayForHarvestUpdated(_delay);
    }

     function _canHarvestVault(address vaultAddress_) internal view returns (bool canHarvest_) {
        IMerfiVault vault = IMerfiVault(vaultAddress_);
        IMerfiStrategy strategy = IMerfiStrategy(vault.strategy());

        bool isPaused = strategy.paused();

        canHarvest_ = !isPaused;

        return canHarvest_;
    }

    function _shouldHarvestVault(address vaultAddress_)
        internal
        view
        returns (
            bool shouldHarvestVault_
        )
    {
        IMerfiVault vault = IMerfiVault(vaultAddress_);
        IMerfiStrategy strategy = IMerfiStrategy(vault.strategy());

        /* solhint-disable not-rely-on-time */
        uint256 someTimeAgo = block.timestamp - delay;
        bool shouldHaverstAgain = strategy.lastHarvest() > someTimeAgo;
        /* solhint-enable not-rely-on-time */

        uint256 callRewardAmount_ = strategy.callReward();
        (uint256 feeAmount, ) = IOps(ops).getFeeDetails();

        bool isProfitableHarvest = shouldHaverstAgain && (callRewardAmount_ >= feeAmount);

        return (isProfitableHarvest);
    }

    function _payTxFee() internal { 
        (uint256 fee, address feeToken) = _getFeeDetails();
        _transfer(fee, feeToken);
    }

    /**
     * @dev Rescues random funds stuck.
     * @param _token address of the token to rescue.
     */
    function inCaseTokensGetStuck(address _token) external onlyManager {
        IERC20 token = IERC20(_token);

        uint256 amount = token.balanceOf(address(this));
        token.safeTransfer(msg.sender, amount);
    }

    receive() external payable {} // solhint-disable-line no-empty-blocks
}

