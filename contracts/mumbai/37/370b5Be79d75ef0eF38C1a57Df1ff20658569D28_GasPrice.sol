    uint256 public lastHarvest;
    bool public harvestOnDeposit;
    address public constant nullAddress = address(0);

    event StratHarvest(address indexed harvester, uint256 wantHarvested, uint256 tvl);
    event Deposit(uint256 tvl);
    event Withdraw(uint256 tvl);
    event ChargedFees(uint256 callFees, uint256 beefyFees, uint256 strategistFees);

    constructor(TokensUsed memory _tokens,
                Farm memory _farm, 
                Routes memory _routes,
                CommonAddresses memory _commonAddresses
    ) StratFeeManager(_commonAddresses) {
        tokens = _tokens;
        farm = _farm;
        routes = _routes;

        ///////// route checks /////////

        require(_routes.outputToNativeRoute.length == 2, "invalid route");

        // output -> first coin of the pair
        require(_routes.outputToLp0Route[0] == _tokens.output, "invalid output");
        require(_routes.outputToLp0Route[_routes.outputToLp0Route.length - 1] == _tokens.lpToken0, "invalid route");

        // output -> second coin of the pair
        require(_routes.outputToLp1Route[0] == _tokens.output, "invalid output");
        require(_routes.outputToLp1Route[_routes.outputToLp0Route.length - 1] == _tokens.lpToken1, "invalid route");

        // reward -> output
        require(_routes.rewardToOutputRoute[_routes.rewardToOutputRoute.length - 1] == _tokens.output, "_rewardToOutputRoute != output");
    }
    
     // puts the funds to work
    function deposit() public whenNotPaused {
        // save gas
        address _want = tokens.want;
        Farm memory _farm = farm;

        uint256 wantBal = IERC20(_want).balanceOf(address(this));
        if (wantBal > 0) {
            IMiniChefV2(_farm.chef).deposit(_farm.poolId, wantBal, address(this));
        }
    }

    function withdraw(uint256 _amount) external {
        require(msg.sender == vault, "!vault");
        address _want = tokens.want;

        uint256 wantBal = IERC20(_want).balanceOf(address(this));

        if (wantBal < _amount) {
            IMiniChefV2(farm.chef).withdraw(farm.poolId, _amount - wantBal, address(this));
            wantBal = IERC20(_want).balanceOf(address(this));
        }

        if (wantBal > _amount) {
            wantBal = _amount;
        }

        if (tx.origin != owner() && !paused()) {
            uint256 withdrawalFeeAmount = wantBal * withdrawalFee / WITHDRAWAL_MAX;
            wantBal = wantBal - withdrawalFeeAmount;
        }

        IERC20(_want).safeTransfer(vault, wantBal);

        emit Withdraw(balanceOf());
    }

    function beforeDeposit() external override {
        if (harvestOnDeposit) {
            require(msg.sender == vault, "!vault");
            _harvest(nullAddress);
        }
    }

    function harvest() external virtual {
        _harvest(nullAddress);
    }

    function harvest(address callFeeRecipient) external virtual {
        _harvest(callFeeRecipient);
    }

    function managerHarvest() external onlyManager {
        _harvest(nullAddress);
    }

    // compounds earnings and charges performance fee
    function _harvest(address callFeeRecipient) internal whenNotPaused {
        IMiniChefV2(farm.chef).harvest(farm.poolId, address(this));
        uint256 outputBal = IERC20(tokens.output).balanceOf(address(this));
        uint256 rewardBal = IERC20(tokens.reward).balanceOf(address(this));
        if (outputBal > 0 || rewardBal > 0) {
            chargeFees(callFeeRecipient);
            addLiquidity();
            deposit();
            lastHarvest = block.timestamp;
            emit StratHarvest(msg.sender, balanceOfWant(), balanceOf());
        }
    }

    // performance fees
    function chargeFees(address callFeeRecipient) internal {
        // v2 harvester rewards are in both output and reward, convert reward to output
        uint256 toOutput = IERC20(tokens.reward).balanceOf(address(this));
        if (toOutput > 0) {
            IUniswapRouterETH(unirouter).swapExactTokensForTokens(toOutput, 0, routes.rewardToOutputRoute, address(this), block.timestamp);
        }

        // transfer 4.5% of fees to this contract
        uint256 toNative = IERC20(tokens.output).balanceOf(address(this)) * 45 / DIVISOR; // retain 4.5%

        // Here we convert the Pool reward token to the native token
        // In this case: CAKE to BNB... is always SAFUr to retain the fees as native tokens.
        IUniswapRouterETH(unirouter).swapExactTokensForTokens(toNative, 0, routes.outputToNativeRoute, address(this), block.timestamp);

        uint256 nativeBal = IERC20(tokens.native).balanceOf(address(this));
        address _native = tokens.native;

        // reward the caller of this contract with a small fee
        IFeeConfig.FeeCategory memory fees = getFees();
        uint256 callFeeAmount = nativeBal * fees.call / DIVISOR;
        IERC20(_native).safeTransfer(callFeeRecipient, callFeeAmount); // send this to the caller

        // reward the platform/treasury with a small fee
        uint256 platformFeeAmount = nativeBal * fees.merfi / DIVISOR;
        IERC20(_native).safeTransfer(feeRecipient, platformFeeAmount); // send this fee to the 'Platform' or 'Treasury' management contract

        // reward the creator of this strategy with a small fee
        uint256 strategistFee = nativeBal * fees.strategist / DIVISOR; // send this to the 'Strategist' wallet.
        IERC20(_native).safeTransfer(strategist, strategistFee);

        emit ChargedFees(callFeeAmount, platformFeeAmount, strategistFee);
    }

    // Adds liquidity to AMM and gets more LP tokens.
    function addLiquidity() internal {
        uint256 outputHalf = IERC20(tokens.output).balanceOf(address(this)) / 2;

        if (tokens.lpToken0 != tokens.output) {
            IUniswapRouterETH(unirouter).swapExactTokensForTokens(outputHalf, 0, routes.outputToLp0Route, address(this), block.timestamp);
        }

        if (tokens.lpToken1 != tokens.output) {
            IUniswapRouterETH(unirouter).swapExactTokensForTokens(outputHalf, 0, routes.outputToLp1Route, address(this), block.timestamp);
        }

        uint256 lp0Bal = IERC20(tokens.lpToken0).balanceOf(address(this));
        uint256 lp1Bal = IERC20(tokens.lpToken1).balanceOf(address(this));
        IUniswapRouterETH(unirouter).addLiquidity(tokens.lpToken0, tokens.lpToken1, lp0Bal, lp1Bal, 1, 1, address(this), block.timestamp);
    }

    // calculate the total underlaying 'want' held by the strat.
    function balanceOf() public view returns (uint256) {
        return balanceOfWant() + balanceOfPool();
    }

    // it calculates how much 'want' this contract holds.
    function balanceOfWant() public view returns (uint256) {
        return IERC20(tokens.want).balanceOf(address(this));
    }

    // it calculates how much 'want' the strategy has working in the farm.
    function balanceOfPool() public view returns (uint256) {
        (uint256 _amount, ) = IMiniChefV2(farm.chef).userInfo(farm.poolId, address(this));
        return _amount;
    }

    // called as part of strat migration. Sends all the available funds back to the vault.
    function retireStrat() external {
        require(msg.sender == vault, "!vault");

        IMiniChefV2(farm.chef).emergencyWithdraw(farm.poolId, address(this));

        uint256 wantBal = IERC20(tokens.want).balanceOf(address(this));
        IERC20(tokens.want).transfer(vault, wantBal);
    }

    // returns rewards unharvested
    function rewardsAvailable() public view returns (uint256) {
        return IMiniChefV2(farm.chef).pendingBanana(farm.poolId, address(this));
    }

    // native reward amount for calling harvest
    function callReward() public view returns (uint256) {
        IFeeConfig.FeeCategory memory fees = getFees();
        uint256 outputBal = rewardsAvailable();
        uint256 nativeOut;
         if (outputBal > 0) {
            try IUniswapRouterETH(unirouter).getAmountsOut(outputBal, routes.outputToNativeRoute)
                returns (uint256[] memory amountOut) 
            {
                nativeOut = amountOut[amountOut.length -1];
            }
            catch {}
        }

        uint256 pendingNative;
        address rewarder = IMiniChefV2(farm.chef).rewarder(farm.poolId);
        if (rewarder != nullAddress) {
            pendingNative = IRewarder(rewarder).pendingToken(farm.poolId, address(this));
        } 
        
        return (pendingNative + nativeOut) / DIVISOR * fees.call / DIVISOR;
    }

    function setHarvestOnDeposit(bool _harvestOnDeposit) external onlyManager {
        harvestOnDeposit = _harvestOnDeposit;

        if (harvestOnDeposit) {
            setWithdrawalFee(0);
        } else {
            setWithdrawalFee(10);
        }
    }

    // pauses deposits and withdraws all funds from third party systems.
    function panic() public onlyManager {
        pause();
        IMiniChefV2(farm.chef).emergencyWithdraw(farm.poolId, address(this));
    }

    function pause() public onlyManager {
        _pause();

        _removeAllowances();
    }

    function unpause() external onlyManager {
        _unpause();

        _giveAllowances();

        deposit();
    }

    function _giveAllowances() internal {
        IERC20(tokens.want).safeApprove(farm.chef, type(uint256).max);
        IERC20(tokens.output).safeApprove(unirouter, type(uint256).max);
        IERC20(tokens.reward).safeApprove(unirouter, type(uint256).max);

        IERC20(tokens.lpToken0).safeApprove(unirouter, 0);
        IERC20(tokens.lpToken0).safeApprove(unirouter, type(uint256).max);

        IERC20(tokens.lpToken1).safeApprove(unirouter, 0);
        IERC20(tokens.lpToken1).safeApprove(unirouter, type(uint256).max);
    }

    function _removeAllowances() internal {
        IERC20(tokens.want).safeApprove(farm.chef, 0);
        IERC20(tokens.output).safeApprove(unirouter, 0);
        IERC20(tokens.reward).safeApprove(unirouter, 0);
        IERC20(tokens.lpToken0).safeApprove(unirouter, 0);
        IERC20(tokens.lpToken1).safeApprove(unirouter, 0);
    }

    function outputToNative() external view returns (address[] memory) {
        return routes.outputToNativeRoute;
    }

    function rewardToOutput() external view returns (address[] memory) {
        return routes.rewardToOutputRoute;
    }

    function outputToLp0() external view returns (address[] memory) {
        return routes.outputToLp0Route;
    }

    function outputToLp1() external view returns (address[] memory) {
        return routes.outputToLp1Route;
    }

    receive() external payable {}

}

