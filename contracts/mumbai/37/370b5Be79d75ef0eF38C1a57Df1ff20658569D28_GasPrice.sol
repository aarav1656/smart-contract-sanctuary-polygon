pragma solidity ^0.8.0;

import "./MergeVault.sol";
import "lib/openzeppelin-contracts-upgradeable/contracts/proxy/ClonesUpgradeable.sol";

// Merge Vault Proxy Factory
// Minimal proxy pattern for creating new Merge vaults
contract MergeVaultFactory {
  using ClonesUpgradeable for address;

  // Contract template for deploying proxied Merge vaults
  MergeVault public instance;

  event ProxyCreated(address proxy);

  // Initializes the Factory with an instance of the Merge Vault
  constructor(address _instance) {
    if (_instance == address(0)) {
      instance = new MergeVault();
    } else {
      instance = MergeVault(_instance);
    }
  }

  // Creates a new Merge Vault as a proxy of the template instance
  // A reference to the new proxied Merge Vault
  function cloneVault() external returns (MergeVault) {
    return MergeVault(cloneContract(address(instance)));
  }

  // Deploys and returns the address of a clone that mimics the behaviour of `implementation`
  function cloneContract(address implementation) public returns (address) {
    address proxy = implementation.clone();
    emit ProxyCreated(proxy);
    return proxy;
  }
}