        require(deadline >= block.timestamp, 'UniswapV2Router: EXPIRED');
        _;
    }

    function initialize(address _factory, address _WETH) public initializer {
        OwnableUpgradeable.initialize();
        factory = _factory;
        WETH = _WETH;
