        uint256 userId,
        uint256 limit,
        uint256 amount,
        uint256 nonce,
        string uri,
        uint256 price,
        address payoutAddr
    );

    function initialize(
        string memory name_,
        string memory symbol_,
        address controller_,
        address treasury_,
        uint256 defaultBps_,
