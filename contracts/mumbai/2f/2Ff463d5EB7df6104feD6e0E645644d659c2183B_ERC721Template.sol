pragma solidity ^0.8.4;

import "./NFTCommon.sol";
import "./interfaces/IController.sol";
import "./interfaces/IERC721Template.sol";

contract ERC721Template is NFTCommon, IERC721Template {
    IController public controller;
    mapping(uint256 => bool) internal _nonce; // for nonce that has already used
    mapping(uint256 => mapping(uint256 => uint256)) internal _hasMinted; //dropId => userId => amount
    mapping(uint256 => uint256) internal _hasMintedTotal; //dropId => amount
    mapping(uint256 => string) internal _uris; //tokenId => amount
    address public erc721Entry;

    modifier checkNonce(uint256 nonce) {
        require(!_nonce[nonce], "ERC721Template: duplicate nonce");
        _nonce[nonce] = true;
        _;
    }
    modifier onlyERC721Entry() {
        require(msg.sender == erc721Entry, "ERC721Template: only erc721Entry");
        _;
    }

    function initialize(
        string calldata name_,
        string calldata symbol_,
        address _controller,
        address _treasury,
        uint256 _defaultBps,
        address _erc721Entry
    ) public initializer {
        __ERC721_init(name_, symbol_);
        __Ownable_init();
        controller = IController(_controller);
        treasury = _treasury;
        defaultBps = _defaultBps;
        erc721Entry = _erc721Entry;
    }

    function tokenURI(
        uint256 tokenId
    ) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        return _uris[tokenId];
    }

    function hasMinted(
        uint256 dropId,
        uint256 userId
    ) public view returns (uint256) {
        return _hasMinted[dropId][userId];
    }

    function hasMintedTotal(
        uint256 dropId
    ) public view returns (uint256) {
        return _hasMintedTotal[dropId];
    }

    function batchMint(
        address[] calldata tos,
        uint256[] calldata amounts,
        string[] calldata uris
    ) external onlyERC721Entry returns (uint256, uint256){
        require(tos.length == amounts.length, "ERC721Template: invalid param");
        require(tos.length == uris.length, "ERC721Template: invalid param");
        uint256 startId = totalSupply() + 1;
        for (uint i = 0; i < tos.length;) {
            _setURI(totalSupply() + 1, amounts[i], uris[i]);
            _mintAmount(tos[i], amounts[i]);
        unchecked {
            ++i;
        }
        }
        return (startId, totalSupply());
    }

    function _mintAmount(address to, uint256 amount) internal {
        if (amount == 0) {
            return;
        }
        for (uint i = 0; i < amount; i++) {
            _mint(to, totalSupply() + 1);
        }
    }

    function burn(uint256 tokenId) external {
        require(controller.isCanBurn(address(this)), "can not burn");
        require(ownerOf(tokenId) == msg.sender, "not the owner");
        _burn(tokenId);
        delete _uris[tokenId];
    }

    function claim(
        address receiver,
        IController.CheckParam calldata param,
        bytes[] calldata sigs
    ) external checkNonce(param.nonce) onlyERC721Entry returns (uint256, uint256){
        require(
            param.limit == 0 || _hasMinted[param.dropId][param.userId] + param.amount <= param.limit,
            "ERC721Template: over limit"
        );
        require(
            param.totalLimit == 0 || _hasMintedTotal[param.dropId] + param.amount <= param.totalLimit,
            "ERC721Template: over totalLimit"
        );
        require(controller.checkClaimSigs(address(this), receiver, param, sigs), "ERC721Template invalid claim sig");
        uint256 id = totalSupply() + 1;
        _setURI(id, param.amount, param.uri);
        _mintAmount(receiver, param.amount);
        _hasMinted[param.dropId][param.userId] += param.amount;
        _hasMintedTotal[param.dropId] += param.amount;
        emit Claim(
            param.dropId,
            param.userId,
            param.limit,
            param.amount,
            param.nonce,
            param.uri
        );
        return (id, totalSupply());
    }

    function buy(
        address receiver,
        IController.CheckParam calldata param,
        uint256 price,
        address payoutAddr,
        bytes[] calldata sigs
    ) external payable checkNonce(param.nonce) onlyERC721Entry returns (uint256, uint256){
        require(
            param.limit == 0 || _hasMinted[param.dropId][param.userId] + param.amount <=
        param.limit,
            "ERC721Template: over limit"
        );
        require(
            param.totalLimit == 0 || _hasMintedTotal[param.dropId] + param.amount <= param.totalLimit,
            "ERC721Template: over totalLimit"
        );
        require(
            price * param.amount <= msg.value,
            "ERC721Template: msg.value is too low"
        );
        require(
            controller.checkBuySigs(
                address(this),
                receiver,
                param,
                price,
                payoutAddr,
                sigs
            ),
            "ERC721Template: invalid buy sig"
        );
        uint256 startId = totalSupply() + 1;
        _setURI(startId, param.amount, param.uri);
        _mintAmount(receiver, param.amount);
        _hasMinted[param.dropId][param.userId] += param.amount;
        _hasMintedTotal[param.dropId] += param.amount;
        payable(payoutAddr).transfer(msg.value);
        emit Buy(
            param.dropId,
            param.userId,
            param.limit,
            param.amount,
            param.nonce,
            param.uri,
            price,
            payoutAddr
        );
        return (startId, totalSupply());
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        from;
        to;
        tokenId;
        require(
            from == address(0) || controller.isCanTransfer(address(this)),
            "ERC721Template: can not transfer"
        );
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _setURI(
        uint256 startId,
        uint256 amount,
        string calldata uri
    ) internal {
        for (uint i = 0; i < amount;) {
            _uris[startId] = uri;
            startId += 1;
        unchecked {
            ++i;
        }
        }
    }
}

