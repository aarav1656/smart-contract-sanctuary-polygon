    /// @param tokenId The identifier for a token.
    function burnAuth(uint256 tokenId) public view returns(BurnAuth) {
        require((tokenId != 0) && (tokenId <= _tokenIdCounter.current()), "Token ID invalid.");
        return contractBurnAuth;
    }

    /// @notice Checks wether a anchor is valid, using Merkle tree.
    /// @dev Using the Openzeppelin Merkle tree implementation
    /// @param proof Proof can be optained from the Merkle tree issuer.
    /// @param anchor anchor is the hashed original anchor number
    function isValidAnchor(bytes32[] calldata proof, bytes32 anchor) public view returns (bool) {
        return MerkleProof.verifyCalldata(
            proof,
            validAnchors,
            keccak256(bytes.concat(keccak256(abi.encode(anchor))))
        );
    }

    /// @notice Returns how many drops are left for a specific anchor. Returns the maxDropsPerAnchor for non-existent anchors and valid but not used anchors alike.
    /// @param anchor anchor is the hashed original anchor number
    function dropsLeft(bytes32 anchor) public view returns (uint16) {
        return maxDropsPerAnchor - anchorDroppedCount[anchor];
    }

    /// @notice Checks wether a anchor can be dropped, implying it needs to be isValidAnchor() and have not reached dropsPerAnchor yet.
    /// @dev Using the Openzeppelin Merkle tree implementation
    /// @param proof Proof can be optained from the Merkle tree issuer.
    /// @param anchor anchor is the hashed original anchor number
    function isDropableAnchor(bytes32[] calldata proof, bytes32 anchor) public view returns (bool) {
        return isValidAnchor(proof, anchor) 
                && (anchorDroppedCount[anchor] < maxDropsPerAnchor);
    }

    /// @notice Adjust the BurnAuth here to change the workflow for burning.
    /// @param _burnAuth A valid EIP-5484 BurnAuth
    function setContractBurnAuth(BurnAuth _burnAuth) public onlyRole(MAINTAINER_ROLE) {
        contractBurnAuth = _burnAuth;
    }

    /// @notice Set the default maximum number of drops per anchor
    /// @param _dropsPerAnchor An integer >= 0
    function setMaxDropsPerAnchor(uint16 _dropsPerAnchor) public onlyRole(MAINTAINER_ROLE) {
        maxDropsPerAnchor = _dropsPerAnchor;
    }

    /// @notice Update the Merkle root containing the valid anchors. Consider salt-leaves!
    /// @dev Proof (safeMint, dropAnchor) needs to provided from this tree. The merkle-tree needs to contain at least one "salt leaf" in order to not publish the complete merkle-tree when all anchors should have been dropped at least once. 
    /// @param _validAnchors The root, containing all anchors we want validated.
    function updateValidAnchors(bytes32 _validAnchors) public onlyRole(MAINTAINER_ROLE) {
        validAnchors = _validAnchors;
    }

    /// @notice Reset the Used count for a anchor. After calling, the anchor will show 0 uses.
    /// @dev To check effective usage please rely on the indexed events.
    /// @param _anchor The anchor, were the usage-count should be re-set.
    function resetAnchorUsedCount(bytes32 _anchor) public onlyRole(MAINTAINER_ROLE) {
        delete(anchorDroppedCount[_anchor]);
    }

    /// @notice Set a new BaseURI. Can be used with dynamic NFTs that have server APIs, IPFS-buckets
    /// or any other suitable system. Refer tokenURI(tokenId) for anchor-based or tokenId-based format.
    /// @param tokenBaseURI The token base-URI. Must end with slash '/'.
    function updateBaseURI(string calldata tokenBaseURI) public onlyRole(MAINTAINER_ROLE) {
        baseURI = tokenBaseURI;
    }

    /// @notice Set a new contractURI. Refer `contractURI` for details.
    /// @param _contractURI The token base-URI. Must end with slash '/'.
    function updateContractURI(string calldata _contractURI) public onlyRole(MAINTAINER_ROLE) {
        contractUri = _contractURI;
    }

    /// @notice Transfers ownership over the contract. The owner has no updating power in this contract, but is returned when calling owner(). 
    /// This allows the owner to act/sign accordinlgy external software applications / marketplaces etc.
    /// The owner can be changed by MAINTAINER_ROLE or the current owner.
    /// @dev Overrides Ownable's function. For MetaAnchor, the MAINTAINER_ROLE can also change ownership. 
    /// Do NOT use onlyOwner modifier in this contract, as this would give power to the owner, which is not desired in our use-case.
    /// @param newOwner The new owner
    function transferOwnership(address newOwner) override public virtual 
        ownerOrMaintainer() {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        _transferOwnership(newOwner); // Calls the Ownable implementation
    }

    modifier ownerOrMaintainer() {
         require(hasRole(MAINTAINER_ROLE, msg.sender) || msg.sender == owner(), "Caller must be maintainer or owner");
         _;
    }

    modifier validAnchor(bytes32[] calldata proof, bytes32 anchor) {
        require(isValidAnchor(proof, anchor), "This anchor is not valid for this contract");
        _;
    }

    modifier usableAnchor(bytes32 anchor) {
        require(anchorDroppedCount[anchor] < maxDropsPerAnchor, "This anchor has been used up and can no longer be dropped" );
        _;
    }

    modifier notDroppedAnchor(bytes32 anchor) {
        require(tokenByAnchor[anchor] == 0, "Anchor is already dropped at another account");
        _;
    }
}