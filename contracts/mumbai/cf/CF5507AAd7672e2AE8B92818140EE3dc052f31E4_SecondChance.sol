pragma solidity ^0.8.17;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "./interfaces/IMetaData.sol";
import "./interfaces/ISecondChance.sol";
import "./utils/RandomUtils.sol";
import "./utils/ArrayUtils.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

contract SecondChance is ISecondChance, VRFConsumerBaseV2, Ownable {
    using AddressArrayUtils for address[];
    using UintArrayUtils for uint256[];

    uint16 private constant REQUEST_CONFIRMATIONS = 3;

    bytes32 private immutable i_keyHash;
    uint32 private immutable i_callbackGasLimit;
    uint64 private immutable i_subscriptionId;
    VRFCoordinatorV2Interface private immutable i_vrfCoordinator;
    IMetaData private immutable i_metaData;

    mapping(uint256 => bytes) private s_poolOpenings;
    mapping(uint256 => uint256) private s_requestIdToPoolId;

    SecondChancePrizePool[] s_pools;

    constructor(
        // VRF Requirements
        uint64 subscriptionId,
        bytes32 keyHash,
        uint32 callbackGasLimit,
        address vrfCoordinator,
        // VRF Requirements
        IMetaData metaData,
        uint256[4][] memory prizePoolSettings
    ) Ownable() VRFConsumerBaseV2(vrfCoordinator) {
        i_subscriptionId = subscriptionId;
        i_keyHash = keyHash;
        i_callbackGasLimit = callbackGasLimit;
        i_vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinator);
        i_metaData = metaData;

        unchecked {
            for (uint256 id = 0; id < prizePoolSettings.length; id++) {
                uint256 opensAt = prizePoolSettings[id][
                    uint256(PrizePoolSettings.OPENS_AT)
                ];
                uint256 openUntil = 0;
                uint256 prizeCount = prizePoolSettings[id][
                    uint256(PrizePoolSettings.PRIZE_COUNT)
                ];
                uint256 prizeTotal = prizePoolSettings[id][
                    uint256(PrizePoolSettings.PRIZE_TOTAL)
                ];
                uint256 jackpot = prizePoolSettings[id][
                    uint256(PrizePoolSettings.JACKPOT)
                ];
                bool isOpen = false;

                uint256 largePrize = getLargePrize(prizeTotal, jackpot, 3 + id);
                uint256 smallPrize = getSmallerPrize(
                    prizeTotal,
                    jackpot,
                    prizeCount,
                    3 + id
                );

                address[] memory users;
                uint256[][] memory tokens;
                address[] memory winners;

                SecondChancePrizePool memory prizePool = SecondChancePrizePool(
                    id,
                    opensAt,
                    openUntil,
                    prizeCount,
                    prizeTotal,
                    jackpot,
                    largePrize,
                    smallPrize,
                    isOpen,
                    users,
                    tokens,
                    winners
                );

                uint256 salt = 1337;

                s_poolOpenings[opensAt] = abi.encodePacked(id, salt);
                s_pools.push(prizePool);
            }
        }
    }

    function openPrizePool(uint256 poolId, uint256 openForSeconds) external {
        if (poolId > s_pools.length - 1) {
            revert SecondChance_PrizePoolNotFound(poolId);
        }

        if (msg.sender != address(i_metaData) && msg.sender != owner()) {
            revert SecondChance_Unauthorized();
        }

        SecondChancePrizePool memory pool = s_pools[poolId];

        pool.isOpen = true;
        pool.openUntil = block.timestamp + openForSeconds;

        s_pools[poolId] = pool;

        emit SecondChance_PoolOpened(poolId, pool.openUntil);
    }

    function initiateRaffle(
        uint256 poolId,
        address[] memory owners,
        uint256[][] memory tokenIds
    ) external onlyOwner {
        SecondChancePrizePool[] memory pools = s_pools;

        if (!pools[poolId].isOpen) {
            revert SecondChance_PoolNotOpen(poolId);
        }

        if (pools[poolId].openUntil > block.timestamp) {
            revert SecondChance_PoolNotReadyForRaffle(
                pools[poolId].openUntil - block.timestamp
            );
        }

        pools[poolId].isOpen = false;
        pools[poolId].registeredUsers = owners;
        pools[poolId].registeredTokens = tokenIds;
        s_pools[poolId] = pools[poolId];

        uint256 requestId = i_vrfCoordinator.requestRandomWords(
            i_keyHash,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            100
        );

        s_requestIdToPoolId[requestId] = poolId;

        emit SecondChance_RaffleInitiated(poolId, requestId);
    }

    function fulfillRandomWords(
        uint256 requestId,
        uint256[] memory randomWords
    ) internal override {
        SecondChancePrizePool memory pool = s_pools[
            s_requestIdToPoolId[requestId]
        ];

        uint256 offset = 0;

        address[] memory winners = new address[](
            Math.min(pool.registeredUsers.length, pool.prizeCount)
        );
        uint256[] memory entries = new uint256[](pool.registeredUsers.length);

        unchecked {
            for (uint256 i = 0; i < pool.registeredUsers.length; i++) {
                entries[i] = calculateEntries(pool.registeredTokens[i].length);
            }
        }

        uint256 totalEntries = entries.sum();

        unchecked {
            uint256 iterator = 0;

            while (winners[winners.length - 1] == address(0)) {
                uint256 winnerIndex = RandomUtils.weightDistributedRandom(
                    randomWords[offset],
                    entries,
                    totalEntries,
                    true
                );

                if (!winners.contains(pool.registeredUsers[winnerIndex])) {
                    winners[iterator] = pool.registeredUsers[winnerIndex];
                    iterator++;
                }

                offset++;
            }
        }

        s_pools[pool.id].winners = winners;

        emit SecondChance_RaffleResults(pool.id, winners);
    }

    function calculateEntries(
        uint256 numOfScratchedTickets
    ) public pure override returns (uint256) {
        if (numOfScratchedTickets >= 1 && numOfScratchedTickets < 5) {
            return numOfScratchedTickets * 10;
        }

        if (numOfScratchedTickets >= 5 && numOfScratchedTickets < 10) {
            return numOfScratchedTickets * 15;
        }

        if (numOfScratchedTickets >= 10 && numOfScratchedTickets < 15) {
            return numOfScratchedTickets * 20;
        }

        if (numOfScratchedTickets >= 15 && numOfScratchedTickets < 20) {
            return numOfScratchedTickets * 25;
        }

        if (numOfScratchedTickets >= 20 && numOfScratchedTickets < 25) {
            return numOfScratchedTickets * 30;
        }

        if (numOfScratchedTickets >= 25 && numOfScratchedTickets < 30) {
            return numOfScratchedTickets * 35;
        }

        if (numOfScratchedTickets >= 30) {
            return 1200;
        }

        return 0;
    }

    function needsToOpenPool(
        uint256 scratchedOffCount
    ) external view override returns (bool, uint256) {
        bytes memory empty = new bytes(0);
        if (keccak256(s_poolOpenings[scratchedOffCount]) == keccak256(empty)) {
            return (false, 0);
        }

        (uint256 id, ) = abi.decode(
            s_poolOpenings[scratchedOffCount],
            (uint256, uint256)
        );

        return (true, id);
    }

    function getPrizePools()
        external
        view
        returns (SecondChancePrizePool[] memory)
    {
        return s_pools;
    }

    function getLargePrize(
        uint256 prizeTotal,
        uint256 jackpot,
        uint256 numOfLargePrizes
    ) internal pure returns (uint256) {
        return (prizeTotal - jackpot) / 2 / (numOfLargePrizes);
    }

    function getSmallerPrize(
        uint256 prizeTotal,
        uint256 jackpot,
        uint256 numOfPrizes,
        uint256 numOfLargePrizes
    ) internal pure returns (uint256) {
        return
            (prizeTotal - jackpot) / 2 / (numOfPrizes - numOfLargePrizes - 1);
    }

    function validateEligibleTokenIds(
        uint256[] calldata tokenIds
    ) public view returns (uint256[] memory) {
        uint256[] memory eligibleTokenIds;
        uint256 counter = 0;
        unchecked {
            for (uint256 i = 0; i < tokenIds.length; i++) {
                IMetaData.TokenMetaData memory metadata = i_metaData
                    .getTokenMetaData(tokenIds[i]);
                if (
                    metadata.status == IMetaData.NFTStatus.DONE &&
                    metadata.winner == false
                ) {
                    eligibleTokenIds[counter] = (tokenIds[i]);
                    counter++;
                }
            }
        }

        return eligibleTokenIds;
    }
}

