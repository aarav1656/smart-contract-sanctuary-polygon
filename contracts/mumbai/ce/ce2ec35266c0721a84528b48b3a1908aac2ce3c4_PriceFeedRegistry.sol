import {Owned} from "solmate/auth/Owned.sol";
import {ERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";

import {PriceFeedConversionLib} from "./libs/PriceFeedConversionLib.sol";
import {IPriceFeedTokenToToken, IPriceFeedTokenToUSD} from "./interfaces/IPriceFeed.sol";
import {IPriceFeedRegistry} from "./interfaces/IPriceFeedRegistry.sol";
import "forge-std/console.sol";

/// @title Registry interface for price feeds
/// @author urlaubaitos
/// @notice Intended as a singleton, that allows for dynamic accounting of price feeds.
///         Useful for multi interaction between feeds, swapping of feed implementations, or different address registration per chain
contract PriceFeedRegistry is IPriceFeedRegistry, Owned {
    mapping(address => mapping(address => IPriceFeedTokenToToken)) private tokenToTokenFeeds;
    mapping(address => IPriceFeedTokenToUSD) private tokenToUSDFeeds;

    event RegisteredPriceFeed(address indexed feed);
    event UnregisteredPriceFeed(address indexed feed);

    constructor() Owned(msg.sender) {}

    /// @notice Registers token-to-token feed. Replacing existing feed if token token pair was previously registered.
    function registerTokenToTokenFeed(IPriceFeedTokenToToken feed) external onlyOwner {
        (address base, address quote) = (address(feed.baseToken()), address(feed.quoteToken()));
        if (base > quote) {
            (base, quote) = (quote, base);
        }

        tokenToTokenFeeds[base][quote] = feed;
        emit RegisteredPriceFeed(address(feed));
    }

    /// @notice Forgets previously registered feed. Nothing happens if passed feed was not registered prior.
    function unregisterTokenToTokenFeed(IPriceFeedTokenToToken feed) external onlyOwner {
        (address base, address quote) = (address(feed.baseToken()), address(feed.quoteToken()));
        if (base > quote) {
            (base, quote) = (quote, base);
        }

        delete tokenToTokenFeeds[base][quote];
        emit UnregisteredPriceFeed(address(feed));
    }

    /// @notice Registers token-to-usd feed. Replacing existing feed if token usd pair was previously registered.
    function registerTokenToUSDFeed(IPriceFeedTokenToUSD feed) external onlyOwner {
        tokenToUSDFeeds[address(feed.baseToken())] = feed;
        emit RegisteredPriceFeed(address(feed));
    }

    /// @notice Forgets previously registered feed. Nothing happens if passed feed was not registered prior.
    function unregisterTokenToUSDFeed(IPriceFeedTokenToUSD feed) external onlyOwner {
        delete tokenToUSDFeeds[address(feed.baseToken())];
        emit UnregisteredPriceFeed(address(feed));
    }

    /// @notice Returns the current onchain price of the pair. The price is returned as a range, where it is likely the true price lies. Each term is given with 18 significant digits.
    function price(ERC20 baseToken, ERC20 quoteToken) external returns (uint256 low, uint256 mid, uint256 high) {
        (address base, address quote) = (address(baseToken), address(quoteToken));
        if (base > quote) {
            (base, quote) = (quote, base);
            return PriceFeedConversionLib.reciprocalPrice(tokenToTokenFeeds[base][quote]);
        }

        return tokenToTokenFeeds[base][quote].price();
    }

    /// @notice Returns the current onchain price of the token in USD. The price is returned as a range, where it is likely the true price lies. Each term is given with 18 significant digits.
    function priceInUSD(ERC20 token) public returns (uint256 low, uint256 mid, uint256 high) {
        return tokenToUSDFeeds[address(token)].priceInUSD();
    }

    /// @notice A custom getter, that is more convienent than that generated by the compiler.
    /// @dev Given storage can flip the ordering of the passed token addresses.
    function tokenToTokenFeed(ERC20 _base, ERC20 _quote) external view returns (IPriceFeedTokenToToken) {
        (address base, address quote) = (address(_base), address(_quote));
        if (base > quote) {
            (base, quote) = (quote, base);
        }

        return tokenToTokenFeeds[base][quote];
    }

    function tokenToUSDFeed(ERC20 _token) external view returns (IPriceFeedTokenToUSD) {
        return tokenToUSDFeeds[address(_token)];
    }

    /// @notice Given an amount of tokens, returns the USD value based on oracle pricing.
    function valueInUSD(ERC20 token, uint256 amount) public returns (uint256) {
        (, uint256 mid, ) = priceInUSD(token);
        return _value(amount, mid, token.decimals());
    }

    /// @notice Given a USD value, returns the amount of tokens based on oracle pricing.
    function amountFromUSD(ERC20 token, uint256 value) public returns (uint256) {
        (, uint256 mid, ) = priceInUSD(token);
        return _amount(value, mid, token.decimals());
    }

    /// @notice Given an Record, returns the USD credit and debt values associated with it.
    function valueRecordInUSD(
        address[] memory tokens,
        int256[] memory amounts
    ) external returns (uint256 credit, uint256 debt) {
        ERC20 token;
        int256 amount;
        uint256 length = tokens.length;
        if (amounts.length != length) revert MismatchedLength();
        for (uint256 i = 0; i < length; i++) {
            token = ERC20(tokens[i]);
            amount = amounts[i];

            if (amount > 0) {
                credit += valueInUSD(token, uint256(amount));
            } else {
                debt += valueInUSD(token, uint256(-amount));
            }
        }

        return (credit, debt);
    }

    function _value(uint256 amount, uint256 priceInBase, uint256 decimals) internal pure returns (uint256) {
        return (amount * priceInBase) / (10 ** decimals);
    }

    function _amount(uint256 value, uint256 priceInBase, uint256 decimals) internal pure returns (uint256) {
        return (value * (10 ** decimals)) / priceInBase;
    }
}

