    error MismatchedLength();

    /// @notice Registers token-to-token feed. Replacing existing feed if token token pair was previously registered.
    function registerTokenToTokenFeed(IPriceFeedTokenToToken feed) external;

    /// @notice Forgets previously registered feed. Nothing happens if passed feed was not registered prior.
    function unregisterTokenToTokenFeed(IPriceFeedTokenToToken feed) external;

    /// @notice Registers token-to-usd feed. Replacing existing feed if token usd pair was previously registered.
    function registerTokenToUSDFeed(IPriceFeedTokenToUSD feed) external;

    /// @notice Forgets previously registered feed. Nothing happens if passed feed was not registered prior.
    function unregisterTokenToUSDFeed(IPriceFeedTokenToUSD feed) external;

    /// @notice Returns the current onchain price of the pair. The price is returned as a range, where it is likely the true price lies. Each term is given with 18 significant digits.
    function price(ERC20 baseToken, ERC20 quoteToken) external returns (uint256 low, uint256 mid, uint256 high);

    /// @notice Returns the current onchain price of the token in USD. The price is returned as a range, where it is likely the true price lies. Each term is given with 18 significant digits.
    function priceInUSD(ERC20 token) external returns (uint256 low, uint256 mid, uint256 high);

    function valueInUSD(ERC20 token, uint256 amount) external returns (uint256);

    function amountFromUSD(ERC20 token, uint256 value) external returns (uint256);

    function valueRecordInUSD(
        address[] memory tokens,
