import {ERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import {FixedPointMathLib} from "solmate/utils/FixedPointMathLib.sol";

import {IPriceFeedTokenToToken} from "../interfaces/IPriceFeed.sol";

/// @notice Price conversion math for price feeds
/// @author urlaubaitos
library PriceFeedConversionLib {
    error PriceFeedConversionLibPathNotCompatible();

    /// @notice Price if the base and quote tokens were flipped
    function reciprocalPrice(IPriceFeedTokenToToken self) external returns (uint256 _low, uint256 _mid, uint256 _high) {
        (uint256 low, uint256 mid, uint256 high) = self.price();
        return (
            FixedPointMathLib.divWadDown(1e18, high),
            FixedPointMathLib.divWadUp(1e18, mid),
            FixedPointMathLib.divWadUp(1e18, low)
        );
    }

    /// @notice Price conversion between feeds
    /// @param self Price feed for the relationship between x, y. x is the base token
    /// @param other Price feed for the relationship between y, z. z is the quotation token
    /// @param base Base token for the returned price conversion
    /// @param quote Quote token for the returned price conversion
    function convertPrice(
        IPriceFeedTokenToToken self,
        IPriceFeedTokenToToken other,
        ERC20 base,
        ERC20 quote
    ) external returns (uint256 low, uint256 mid, uint256 high) {
        (uint256 selfLow, uint256 selfMid, uint256 selfHigh) = self.price();
        (uint256 otherLow, uint256 otherMid, uint256 otherHigh) = other.price();

        ERC20 selfBase = self.baseToken();
        ERC20 selfQuote = self.quoteToken();
        ERC20 otherBase = other.baseToken();
        ERC20 otherQuote = other.quoteToken();

        // Can calculate the conversion without the taking the reciprocal of either
        if (selfBase == base && otherQuote == quote) {
            if (selfQuote != otherBase) {
                revert PriceFeedConversionLibPathNotCompatible();
            }

            low = FixedPointMathLib.mulWadDown(selfLow, otherLow);
            mid = FixedPointMathLib.mulWadUp(selfMid, otherMid);
            high = FixedPointMathLib.mulWadUp(selfHigh, otherHigh);
            return (low, mid, high);
        }

        // Need to take the reciporcal of both feeds
        if (selfQuote == base && otherBase == quote) {
            if (selfBase != otherQuote) {
                revert PriceFeedConversionLibPathNotCompatible();
            }

            selfLow = FixedPointMathLib.divWadUp(1e18, selfLow);
            selfMid = FixedPointMathLib.divWadDown(1e18, selfMid);
            selfHigh = FixedPointMathLib.divWadDown(1e18, selfHigh);

            otherLow = FixedPointMathLib.divWadUp(1e18, otherLow);
            otherMid = FixedPointMathLib.divWadUp(1e18, otherMid);
            otherHigh = FixedPointMathLib.divWadDown(1e18, otherHigh);

            low = FixedPointMathLib.mulWadDown(selfHigh, otherHigh);
            mid = FixedPointMathLib.mulWadUp(selfMid, otherMid);
            high = FixedPointMathLib.mulWadUp(selfLow, otherLow);
            return (low, mid, high);
        }

        // Only the reciprocal of self is needed
        if (selfQuote == base) {
            if (selfBase != otherBase) {
                revert PriceFeedConversionLibPathNotCompatible();
            }

            selfLow = FixedPointMathLib.divWadUp(1e18, selfLow);
            selfMid = FixedPointMathLib.divWadDown(1e18, selfMid);
            selfHigh = FixedPointMathLib.divWadDown(1e18, selfHigh);

            low = FixedPointMathLib.mulWadDown(selfHigh, otherLow);
            mid = FixedPointMathLib.mulWadUp(selfMid, otherMid);
            high = FixedPointMathLib.mulWadUp(selfLow, otherHigh);
            return (low, mid, high);
        }

        // Only the reciprocal of other is needed

        if (selfQuote != otherQuote) {
            revert PriceFeedConversionLibPathNotCompatible();
        }

        otherLow = FixedPointMathLib.divWadUp(1e18, otherLow);
        otherMid = FixedPointMathLib.divWadUp(1e18, otherMid);
        otherHigh = FixedPointMathLib.divWadDown(1e18, otherHigh);

        low = FixedPointMathLib.mulWadDown(selfLow, otherHigh);
        mid = FixedPointMathLib.mulWadUp(selfMid, otherMid);
        high = FixedPointMathLib.mulWadUp(selfHigh, otherLow);
    }
}

