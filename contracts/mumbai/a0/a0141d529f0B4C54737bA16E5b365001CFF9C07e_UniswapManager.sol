import "@uniswap/v3-periphery/contracts/interfaces/INonfungiblePositionManager.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Multicall.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract UniswapManager is Multicall, Ownable {

    event event_refund(address tokenRefund, address recipient, uint256 amountRefund);
    event event_Approval(address token, address spender);
    event event_decreaseLiquidity(uint256 amount0decrease, uint256 amount1decrease);
    event event_collect(uint256 amount0liquidity, uint256 amount1liquidity);
    event  event_Swap(int256 token0, int256 token1);

    mapping(address => bool) public wallet_is_here;
    mapping(uint256 => mapping(address => int256)) private positionBalances;

    INonfungiblePositionManager public immutable nonfungiblePositionManager;
    ISwapRouter public immutable swapRouter;

    constructor(address AddressNonfungiblePositionManager, address AddressSwapRouter) {
        nonfungiblePositionManager = INonfungiblePositionManager(AddressNonfungiblePositionManager);
        swapRouter = ISwapRouter(AddressSwapRouter);
        wallet_is_here[address(this)] = true;
    }

    function ClosePosition(uint256 tokenId, uint128 liquidity, address token0, address token1, uint256 deadline)
        external
        onlyOwner
    {
        uint256 amount0decrease;
        uint256 amount1decrease;
        uint256 amount0liquidity;
        uint256 amount1liquidity;

        (amount0decrease, amount1decrease) = nonfungiblePositionManager.decreaseLiquidity(
            INonfungiblePositionManager.DecreaseLiquidityParams(
                tokenId, 
                liquidity, 
                0, 
                0,
                deadline
            )
        );
        emit event_decreaseLiquidity(amount0decrease, amount1decrease);

        INonfungiblePositionManager.CollectParams memory collectParams = 
            INonfungiblePositionManager.CollectParams(
                tokenId, 
                address(this), 
                type(uint128).max, 
                type(uint128).max  
            );

        (amount0liquidity, amount1liquidity) = nonfungiblePositionManager.collect(collectParams);
        emit event_collect(amount0liquidity, amount1liquidity);

        nonfungiblePositionManager.burn(tokenId);

        positionBalances[tokenId][token0] = int256(amount0liquidity);
        positionBalances[tokenId][token1] = int256(amount1liquidity);
    }

    function SwapExactInput(uint256 tokenId, address token0, address token1, bytes[] calldata data, uint256 deadline) 
        external
        onlyOwner
    {
        uint256 rest0 = IERC20(token0).balanceOf(address(this));
        uint256 rest1 = IERC20(token1).balanceOf(address(this));

        checkApprovals(token0, token1, address(swapRouter));
        swapRouter.multicall(deadline, data);

        uint256 _rest0 = IERC20(token0).balanceOf(address(this));
        uint256 _rest1 = IERC20(token1).balanceOf(address(this));
        
        int256 delta0 = int256(_rest0) - int256(rest0);
        int256 delta1 = int256(_rest1) - int256(rest1);

        positionBalances[tokenId][token0] = positionBalances[tokenId][token0] + delta0;
        positionBalances[tokenId][token1] = positionBalances[tokenId][token1] + delta1;

        emit event_Swap( delta0, delta1);
    }

    struct CallCreatePosition { 
        int24 tickLower;
        int24 tickUpper;
        uint24 poolFee;
        address token0;
        address token1;
        address recipient;
        uint256 amount0ToAdd;
        uint256 amount1ToAdd;
        uint256 deadline;
        uint256 tokenId;
    }

    function CreatePosition(CallCreatePosition calldata params)
        external
        onlyOwner
    {
        uint256 amount0 = uint256(positionBalances[params.tokenId][params.token0] + int256(params.amount0ToAdd));
        uint256 amount1 = uint256(positionBalances[params.tokenId][params.token1] + int256(params.amount1ToAdd));

        if (!wallet_is_here[address(this)]) {
            IERC20(params.token0).transferFrom( msg.sender, address(this), params.amount0ToAdd);
            IERC20(params.token1).transferFrom( msg.sender, address(this), params.amount1ToAdd);
        }

        require(amount0 <= IERC20(params.token0).balanceOf(address(this)), "amount0_gr_balanceOf0");
        require(amount1 <= IERC20(params.token1).balanceOf(address(this)), "amount0_gr_balanceOf1");

        checkApprovals(params.token0, params.token1, address(nonfungiblePositionManager));

        INonfungiblePositionManager.MintParams memory mint_params = 
            INonfungiblePositionManager.MintParams({
                token0: params.token0,
                token1: params.token1,
                fee: params.poolFee,
                tickLower: params.tickLower,
                tickUpper: params.tickUpper,
                amount0Desired: amount0,
                amount1Desired: amount1,
                amount0Min: 0,
                amount1Min: 0,
                recipient: params.recipient,
                deadline: params.deadline
            });

        nonfungiblePositionManager.mint(mint_params);

        delete positionBalances[params.tokenId][params.token0];
        delete positionBalances[params.tokenId][params.token1];
    }

    function checkApprovals(address token0, address token1, address spender) 
        internal 
    {
        uint256 allowance0 = IERC20(token0).allowance(address(this), spender);
        if (allowance0 < IERC20(token0).balanceOf(address(this))) {
            SafeERC20.safeApprove(IERC20(token0), spender, type(uint256).max);
            emit event_Approval(token0, spender);
        }
        uint256 allowance1 = IERC20(token1).allowance(address(this), spender);
        if (allowance1 < IERC20(token1).balanceOf(address(this))) {
            SafeERC20.safeApprove(IERC20(token1), spender, type(uint256).max);
            emit event_Approval(token1, spender);
        }
    }

    function Refund(address tokenRefund)
        external
        onlyOwner
    {
        checkApprovals(tokenRefund, tokenRefund, msg.sender);
        
        uint256 amountRefund = IERC20(tokenRefund).balanceOf(address(this));
        IERC20(tokenRefund).transfer(msg.sender, amountRefund);
        emit event_refund(tokenRefund, msg.sender, amountRefund);
    }

    function ChangeWallet()
        external
        onlyOwner
    {
        if (wallet_is_here[address(this)]) { wallet_is_here[address(this)] = false; }
        else  {wallet_is_here[address(this)] = true; }
    }

    /*exchange WETH for ETH and transfer ETH to a wallet*/
    function withdrawETH(address addressWeth, uint256 wad) 
        external 
        onlyOwner
    {
        IWETH9 weth = IWETH9(addressWeth);
        weth.withdraw(wad);
        payable(msg.sender).transfer(address(this).balance);
    }

    receive() external payable { }

}

interface IWETH9 is IERC20 {
    function withdraw(uint256 wad) external;
}