pragma solidity ^0.8.0;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "operator-filter-registry/src/RevokableDefaultOperatorFilterer.sol";
import "operator-filter-registry/src/UpdatableOperatorFilterer.sol";
import "../UriStorage.sol";

abstract contract OnchainNFT is RevokableDefaultOperatorFilterer,Ownable {
    constructor(){
        setDefaultRoyalty(0xC826746f37D2daEF338CC8b55Ef8eaBa8BC2Ab61, 1000);
    }

    UriStorageInterface internal uriStorage_;
    function setStorageContract(address _token) external onlyOwner{
        uriStorage_ = UriStorageInterface(_token);
    }

    uint256 internal totalIssues_ = 0;//発行済みの枚数（ERC1155の場合はアイテム数じゃなくて、全アイテムの合計枚数）
    function getTotalIssues() external view returns(uint256){
        return totalIssues_;
    }
    uint256 internal maximumIssueLimit_ = 0;//発行上限
    function getMaximumIssueLimit() external view returns(uint256){
        return maximumIssueLimit_;
    }
    function setMaximumIssueLimit(uint256 _limit) external onlyOwner{
        maximumIssueLimit_ = _limit;
    }

    modifier mintable(uint256 _amount){
        checkMintable(_amount);
        _;
    }
    function checkMintable(uint256 _amount) internal view virtual {
        require(_amount > 0, "The value less than or equal to 0 cannot be specified for the \"amount\".");
        require(maximumIssueLimit_ == 0 || maximumIssueLimit_ >= totalIssues_ + _amount, "It will exceed the maximum issue limit.");
    }

    function getUri(uint256 _tokenId) internal view virtual returns (string memory) {
        string memory result = uriStorage_.getUri(_tokenId);
        if (bytes(result).length == 0){
            return string(abi.encodePacked(
                'data:application/json,{"name":"',
                Strings.toString(_tokenId),
                '", "description": "No data in storage."}'
            ));
        }
        return result;
    }

    //ERC2981---------------------------

    function _royaltyInfo(uint256 _tokenId, uint256 _salePrice) internal view returns (address, uint256) {
        RoyaltyInfo memory royalty = _tokenRoyaltyInfo[_tokenId];
        if (royalty.receiver == address(0)) {
            royalty = _defaultRoyaltyInfo;
        }

        uint256 royaltyAmount = (_salePrice * royalty.royaltyFraction) / feeDenominator();

        return (royalty.receiver, royaltyAmount);
    }

    struct RoyaltyInfo {
        address receiver;
        uint96 royaltyFraction;
    }
    RoyaltyInfo private _defaultRoyaltyInfo;
    mapping(uint256 => RoyaltyInfo) private _tokenRoyaltyInfo;

    function feeDenominator() public pure returns (uint96) {
        return 10000;
    }
    function setDefaultRoyalty(address _receiver, uint96 _feeNumerator) public onlyOwner {
        require(_feeNumerator <= feeDenominator(), "ERC2981: royalty fee will exceed salePrice");
        require(_receiver != address(0), "ERC2981: invalid receiver");

        _defaultRoyaltyInfo = RoyaltyInfo(_receiver, _feeNumerator);
    }

    function setTokenRoyalty(uint256 _tokenId, address _receiver, uint96 _feeNumerator) external onlyOwner {
        require(_feeNumerator <= feeDenominator(), "ERC2981: royalty fee will exceed salePrice");
        require(_receiver != address(0), "ERC2981: Invalid parameters");

        _tokenRoyaltyInfo[_tokenId] = RoyaltyInfo(_receiver, _feeNumerator);
    }
    function resetTokenRoyalty(uint256 _tokenId) external onlyOwner {
        delete _tokenRoyaltyInfo[_tokenId];
    }

    //OpenSea filter--------------------

    function owner() public view override(Ownable, UpdatableOperatorFilterer) returns (address){
        return super.owner();
    }
}

