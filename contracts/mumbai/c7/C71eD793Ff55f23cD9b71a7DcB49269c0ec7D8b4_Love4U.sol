import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "./lib_akane/Onchain721.sol";

/*
 * giverを追加するとき
 * 1) giverIDに紐付けてstorageに画像データを保存
 * 2) setGiverNameすると giverMintが可能になる
 * 
 * appendUri時の注意
 * 末尾の</svg>タグは消しておくこと
 * 
 * サイト側
 * 1) giverとテキストを決めてもらう
 * 2) checkUsedTextで使えるテキストか確認
 *    空欄だったらチェックしなくていい　前後の余白を削除してチェック・URIエンコードして送信すること
 * 3) wethのapprove
 * 4) giverMint
 */

struct Metadata {
    uint256 giver;
    string  text;
    string  uri;
}

contract Love4U is Onchain721 {
    constructor() Onchain721("Love4U", "Love4U"){
    }

    //key: tokenId ---------------------------------------------------------
    //uri取得時の負荷を下げる
    mapping(uint256 => Metadata) private uri_;

    //key: giverId ---------------------------------------------------------
    //giverの名前
    mapping(uint256 => string)   private giverName_;
    //同じgiverで同じセリフは言えない
    mapping(uint256 => mapping(string => bool)) private usedText_;

    //ミントすると増える
    uint256 private mintedId_ = 1;


    //todo: weth支払い
    function giverMint(address _to, uint256 _giver, string memory _text) external mintable(1){
        if (bytes(giverName_[_giver]).length == 0) revert("Giver is not ready.");

        string memory img = uriStorage_.getUri(_giver);
        if (bytes(img).length == 0)       revert("Invalid giver.");
        if (checkUsedText(_giver, _text)) revert("That dialogue has already been used.");

        string memory text = "";
        string memory name = "";

        if (bytes(_text).length == 0){
            //todo 空文字列の場合は特別なSVGを挿入する?
            _text = "";
            name = string(abi.encodePacked(
                //',"name":"#',
                ',%22name%22:%22#',
                Strings.toString(mintedId_)
            ));
        }else{
            //todo URLエンコード
            text = string(abi.encodePacked(
                //'<style>.text{font-size: 80px;font-weight: bold;font-family: \'Montserrat\',\'Helvetica Neue\',Arial,\'Hiragino Kaku Gothic ProN\',\'Hiragino Sans\',Meiryo,sans-serif;text-anchor: middle;dominant-baseline: middle;}</style>',
                //"<style>.text{font-size: 80px;font-weight: bold;font-family: 'Montserrat','Helvetica Neue',Arial,'Hiragino Kaku Gothic ProN','Hiragino Sans',Meiryo,sans-serif;text-anchor: middle;dominant-baseline: middle;}</style>",
                '%3Cstyle%3E%20.text%7B%20font-size:%2080px;%20font-weight:%20bold;%20font-family:%20\'Montserrat\',\'Helvetica%20Neue\',Arial,\'Hiragino%20Kaku%20Gothic%20ProN\',\'Hiragino%20Sans\',Meiryo,sans-serif;%20text-anchor:%20middle;%20dominant-baseline:%20middle;%20%7D%20%3C/style%3E',
                //'<text id=\'text\' class=\'text\' x=\'500\' y=\'500\' fill=\'#fff\' stroke=\'#000\' stroke-width=\'2\'>',
                //"<text id='text' class='text' x='500' y='500' fill='#fff' stroke='#000' stroke-width='2'>",
                '%3Ctext%20id=\'text\'%20class=\'text\'%20x=\'500\'%20y=\'500\'%20fill=\'#fff\'%20stroke=\'#000\'%20stroke-width=\'2\'%3E',
                _text,
                //'</text></svg>"'
                '%3C/text%3E%3C/svg%3E%22'
            ));
            name = string(abi.encodePacked(
                //',"name":"#',
                ',%22name%22:%22#',
                Strings.toString(mintedId_),
                //' ',
                '%20',
                _text
            ));
        }

        Metadata memory meta;
        meta.giver = _giver;
        meta.text  = _text;
        meta.uri   = string(abi.encodePacked(
            text,
            name,
            //'", "description":"They give message with love for you.", "attributes":[{"trait_type":"Giver", "value":"',
            '%22,%20%22description%22:%22They%20give%20message%20with%20love%20for%20you.%22,%20%22attributes%22:%5B%7B%22trait_type%22:%22Giver%22,%20%22value%22:%22',
            giverName_[_giver],
            //'"}]}'
            '%22%7D%5D%7D'
        ));

        uri_[mintedId_] = meta;

        mint(_to, mintedId_++);
        usedText_[_giver][_text] = true;
    }
    function burn(uint256 _id) public override{
        address owner = ERC721.ownerOf(_id);
        require(
            owner == _msgSender() || isApprovedForAll(owner, _msgSender()),
            "Sender is not token owner nor approved"
        );

        //テキスト使用履歴からも消す
        Metadata memory uri = uri_[_id];
        usedText_[uri.giver][uri.text] = false;

        _burn(_id);
        totalIssues_ -= 1;
    }

    function checkUsedText(uint256 _giver, string memory _text) public view returns (bool){
        //空文字列はスルー
        return bytes(_text).length > 0 && usedText_[_giver][_text];
    }

    //名前をセットすると誰でもmint可能になるので最後にやること
    function setGiverName(uint256 _giver, string memory _giverName) external onlyOwner{
        giverName_[_giver] = _giverName;
    }

    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        _requireMinted(_tokenId);

        return string(abi.encodePacked(
            //'data:application/json,{"image":"data:image/svg+xml,',
            'data:application/json,%7B%22image%22:%22data:image/svg%2Bxml,',
            getUri(_tokenId),
            uri_[_tokenId].uri
        ));
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "./lib/SSTORE2.sol";
import "./lib/Memory.sol";

struct Token {
    uint256 bytesLength;
    address[] addresses;
}

interface UriStorageInterface {
    function getUri(uint256 tokenId) external view returns (string memory);
}
contract UriStorage is AccessControl, UriStorageInterface {
    bytes32 public constant ROLE_ADMIN = keccak256("ADMIN");
    bytes32 public constant ROLE_READ  = keccak256("READER");
    bytes32 public constant ROLE_WRITE = keccak256("WRITER");
    constructor(){
        _setupRole(ROLE_ADMIN, _msgSender());
        _setupRole(ROLE_READ,  _msgSender());
        _setupRole(ROLE_WRITE, _msgSender());
    }

    mapping(uint256 => Token) private tokens_;
    mapping(uint256 => Token) private metas_;

    bool private freezeFlg_ = false;
    function freeze() external onlyRole(ROLE_ADMIN){
        freezeFlg_ = true;
    }

    function appendUri(uint256 _tokenId, bytes[] calldata _values) public onlyRole(ROLE_WRITE) {
        if (freezeFlg_) revert("This storage is already frozen.");
        for (uint256 i = 0; i < _values.length; i++) {
            tokens_[_tokenId].addresses.push(SSTORE2.write(_values[i]));
            tokens_[_tokenId].bytesLength += _values[i].length;
        }
    }
    function clearUri(uint256 _tokenId) public onlyRole(ROLE_WRITE) {
        if (freezeFlg_) revert("This storage is already frozen.");
        delete tokens_[_tokenId].addresses;
        tokens_[_tokenId].bytesLength = 0;
    }

    //コンテンツとその他を別に保存したい
    function writeMeta(uint256 _tokenId, bytes[] calldata _values) public onlyRole(ROLE_WRITE) {
        if (freezeFlg_) revert("This storage is already frozen.");
        //まず初期化
        delete metas_[_tokenId].addresses;
        metas_[_tokenId].bytesLength = 0;

        for (uint256 i = 0; i < _values.length; i++) {
            metas_[_tokenId].addresses.push(SSTORE2.write(_values[i]));
            metas_[_tokenId].bytesLength += _values[i].length;
        }
    }

    function getData(Token memory _target) private view returns (string memory){
        bytes memory uri = new bytes(_target.bytesLength);
        (uint256 uriAddr, ) = Memory.fromBytes(uri);
        for (uint256 i = 0; i < _target.addresses.length; i++) {
            bytes memory data = SSTORE2.read(_target.addresses[i]);
            (uint256 dataAddr, uint256 dataLen) = Memory.fromBytes(data);
            Memory.copy(dataAddr, uriAddr, dataLen);
            uriAddr += dataLen;
        }
        return string(uri);
    }
    function getUri(uint256 _tokenId) public view onlyRole(ROLE_READ) returns (string memory){
        if (metas_[_tokenId].bytesLength == 0){
            return getData(tokens_[_tokenId]);
        }else{
            return string(abi.encodePacked(
                'data:application/json,{',
                getData(tokens_[_tokenId]),
                ',',
                getData(metas_[_tokenId]),
                '}'
            ));
        }
    }

    function addReader(address _token) public onlyRole(ROLE_ADMIN){
        _setupRole(ROLE_READ,  _token);
    }
    function revokeReader(address _token) public onlyRole(ROLE_ADMIN){
        _revokeRole(ROLE_READ,  _token);
    }
    function addWriter(address _token) public onlyRole(ROLE_ADMIN){
        _setupRole(ROLE_WRITE,  _token);
    }
    function revokeWriter(address _token) public onlyRole(ROLE_ADMIN){
        _revokeRole(ROLE_WRITE,  _token);
    }
}

