        bytes32[] memory prizes = abi.decode(_prizes, (bytes32[]));
        uint256[] memory weights = abi.decode(_weights, (uint256[]));

        
        raffle[Raffle_id].prizes = prizes;
        raffle[Raffle_id].weights = weights;
        
    }

