import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "./Ownable.sol";
import 'hardhat/console.sol';


contract RaffleTest_15_Dec is Ownable{

    // token contract interface
   IERC721 public token;
    // ownable contract
    Ownable public ownable;
    address public deployer;
    address public custodialWallet;
    // Raffle ID
    uint256 private RaffleID;

    uint256 public nonce;   //for generating randomnumber

    string invalidArgument = "Invalid argument.";
    string callerNotOwner =  "Caller is not the Owner.";
    string RI_notExist = "Raffle round doesn't exist";

    event RaffleCreated( address creator, uint RaffleID);
    event RaffleAnnouced(uint RaffleID, uint roundID, address[] winners, bytes32[] prizes);
    event userRegistered(uint RaffleID, address registerUser);

    enum RaffleStatus {NotInitialized, Initialized, Running, Completed}
    enum RaffleCategory{web3, web2}
    

    //for web3
    struct Raffle{
        address creator;
        uint256 totalRounds;
        uint256 totalWinners;
        uint256 startingRounds; // first index for the number of rounds and second for the winners of that round.
        uint256 endingRound;
        uint256 threshold;
        uint256 currentRound;
        // uint256 currentElement;
        bytes32[] prizes;
        address[] participants;
        uint256[] weights;
        RaffleStatus status;
        bool ignorePreviousWinner;
        // bool claimApplied;
        bool regRequired;
        // bool RaffleType;
    }

    struct web2Raffle{
        uint256 RaffleId;
        uint256 totalRounds;
        uint256 totalWinners;
        uint256 winnersAnnounced;
        uint256 currentRound;
        uint256 threshold;
        bool isRegistrationRequired;
        RaffleStatus status;
        // address[] winnerAddresses;
        // uint256[] awards;
    }

    // mapping(uint => uint =>)
    mapping(uint => web2Raffle) private web2raffle;

    // raffle id => raffle data
    mapping(uint => Raffle) private raffle;

    //for getting winners of the raffle Id
    // raffle id => round id => winners addresses
    mapping(uint => mapping(uint => address[])) private winnersOfTheRaffleID;  

    //for award of the winner
    //      raffleId   => round num     => address  => prize in bytes
    mapping(uint => mapping(uint => mapping(address => bytes32))) private winnersAward;

    //for claim track of the prizes of winner
    //Raffle Id     => round Id    => walletAddress => true
    mapping(uint => mapping(uint => mapping(address => bool))) private isClaimed;
    
    mapping(uint => mapping(address => bool)) public isRegistered;

    mapping (uint => uint) private registeredUsers;
  

    modifier onlyOwner() override {
        if(address(ownable) != address(0)){
        require(
        ownable.verifyOwner(msg.sender) == true ||
        verifyOwner(msg.sender) == true,
        callerNotOwner
        );
        } 
        else{
        require(
        verifyOwner(msg.sender) == true,
        callerNotOwner );
        }
        _;
    }


    modifier onlyDeployer() {
        require(msg.sender == deployer, 
            "Caller in not deployer"
        );
        _;
    } 

    modifier onlyCustodial(){
         require(msg.sender == custodialWallet,
         "Function is only callable with custodial wallet"
        );
        _;
    }

    modifier checkRaffle(uint Raffle_id){
        require(raffle[Raffle_id].status == RaffleStatus.Initialized || raffle[Raffle_id].status == RaffleStatus.Running ||
        web2raffle[Raffle_id].status == RaffleStatus.Initialized || web2raffle[Raffle_id].status == RaffleStatus.Running,
         "Raffle is not Intialized or Running");       

        require(raffle[Raffle_id].currentRound + 1 <= raffle[Raffle_id].totalRounds ||
            web2raffle[Raffle_id].currentRound + 1 <=  web2raffle[Raffle_id].totalRounds, 
        RI_notExist);
        _;
    }


    constructor( address _custodialWallet, address _token){
        token = IERC721(_token);
        custodialWallet = _custodialWallet;
        deployer= msg.sender;
    }

    function startWeb2Raffle(
        uint256 RaffleId,
        uint256 totalRounds,
        uint256 totalWinners,
        uint256 threshold,
        bool isRegistrationRequired
    ) external onlyCustodial {
        require(threshold > 0, "Invalid threhold");
        require(totalWinners >= totalRounds, "Invalid amount  of winners.");
        require(web2raffle[RaffleId].status == RaffleStatus.NotInitialized, "Raffle is already Initialized");

        web2raffle[RaffleId] = web2Raffle(
        RaffleId,
        totalRounds,
        totalWinners,
        0,
        0,
        threshold,
        isRegistrationRequired,
        RaffleStatus.Initialized
        );
    }

    function sendWinnersAndAwards(uint256 raffleId, uint256 roundId, bytes memory winnersOfTheRaffle, bytes memory prizesOfTheWinners) external onlyCustodial checkRaffle(raffleId){
        require((getWeb2Raffle(raffleId).currentRound + 1) == roundId,"wrong round Id");

        bool regRequired = getWeb2Raffle(raffleId).isRegistrationRequired;
        
        if(getWeb2Raffle(raffleId).currentRound < getWeb2Raffle(raffleId).totalRounds){
        web2raffle[raffleId].currentRound++ ;
        web2raffle[raffleId].status =  RaffleStatus.Running; 
        }

        address[] memory winners = abi.decode(winnersOfTheRaffle, (address[]));
        bytes32[] memory prizes = abi.decode(prizesOfTheWinners, (bytes32[]));
        require((winners.length + web2raffle[raffleId].winnersAnnounced) <= web2raffle[raffleId].totalWinners, "winners quantity exceeded.");
        require(winners.length == prizes.length, "Quantity of winners differ from quantity of prizes");

        winnersOfTheRaffleID[raffleId][roundId]= winners;

        for(uint i = 0; i < winners.length; i++){
            winnersAward[raffleId][roundId][winners[i]] = prizes[i];

            if(regRequired == false){
                isClaimed[raffleId][roundId][winners[i]] = true;  
            }

        }

        web2raffle[raffleId].winnersAnnounced += winners.length;

        if(getWeb2Raffle(raffleId).totalRounds == roundId){
            web2raffle[raffleId].status = RaffleStatus.Completed;
        }

    }


    /////////////////////////////////////////////////////////////////////////////////////////    
    //                                                                                     //
    //                                         START RAFFLE                                //
    //                                                                                     //
    /////////////////////////////////////////////////////////////////////////////////////////


    function startRaffle(
        uint256 Raffle_id, 
        uint256 totalRounds, 
        uint256 totalWinners, 
        uint256 threshold,
        bool ignorePreviousWinner,
        // bool claimApplied,
        bool regRequired
        // bool raffleType
