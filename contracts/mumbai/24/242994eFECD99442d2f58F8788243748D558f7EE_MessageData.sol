pragma solidity ^0.8.2;

import "hardhat/console.sol";

import "./interfaces/IKUtils.sol";
import "./interfaces/IHashtags.sol";
import "./interfaces/ITagged.sol";
import "./interfaces/IPosts.sol";
import "./interfaces/IComments.sol";
import "./interfaces/IMessageFormat.sol";
import "./interfaces/IFollowers.sol";

contract MessageData {

    // Admins
    mapping (address => bool) admins;

    // Trusted Contracts
    mapping (address => bool) trustedContracts;

    // Maximum amount of records to return on query
    uint16 maxItemsReturn = 0;

    // Max number of items to store in a bucket in a mapping
    uint256 public maxItemsPerBucket;

    // Set the Bucket Key and Count
    uint256[2] public buckets = [0,0];

    // Message Stats
    struct MsgStats {
        int likes;
        int comments;
        int reposts;
        int tipsReceived;
        uint postByContract;
        uint256 time;
        uint256 block;
    }


    // The Message data struct
    struct MsgData {
        uint msgID;
        address[2] postedBy;
        string message;
        uint256 tip;
        uint256 paid;
        string[] hashtags;
        address[] taggedAccounts;
        uint256 asGroup;
        uint256[] inGroups;
        string uri;
        uint256 commentLevel;
        uint256 isRepostOf;
        MsgStats msgStats;
    }


    // Map of all the message buckets (posts is static)
    // posts-0, posts-1, posts-2 ...
    mapping (string => uint256[]) public msgMap;
    mapping (string => mapping (uint256 => bool)) public msgMapMap;

    // Bucket Key => Msg ID => Msg Data
    mapping (string => mapping (uint256 => MsgData)) public msgData;

    // Link the KUtils contract
    IKUtils private KUtils;

    // Link to the Hashtags
    IHashtags private Hashtags;

    // Link to the Tagged Accounts
    ITagged private Tagged;

    // Link to the Posts Owners
    IPosts private Posts;

    // Link to the Comments
    IComments private Comments;

    // Link to the Message Formatter
    IMessageFormat private MessageFormat;

    // Link to the Followers
    IFollowers private Followers;

    constructor(address _kutils) {
        // Setup the default Admin
        admins[msg.sender] = true;
        trustedContracts[msg.sender] = true;

        maxItemsReturn = 4;
        maxItemsPerBucket = 3;

        require((maxItemsPerBucket + 1) >= maxItemsReturn, "Invalid Setup");
        
        // Setup link to User Profiles
        KUtils = IKUtils(_kutils);
    }


    /*

    EVENTS

    */

    event logNewMsg(uint256 msgID, address[2] postedBy, MsgData newMsg, uint256 indexed asGroup, uint256[] inGroups);
    event logRemoveMsg(uint256 msgID, uint256 indexed isCommentOf);
    event logUpdateMsgStats(uint256 msgID, uint256 indexed statType, int256 statValue);



    /*

    MODIFIERS

    */

    modifier onlyAdmins() {
        require(admins[msg.sender], "Only admins can call this function.");
        _;
    }

    modifier onlyTrustedContracts() {
        require(trustedContracts[msg.sender], "Only trusted contracts can call this function.");
        _;
    }


    /*

    ADMIN FUNCTIONS

    */

    function addAdmin(address newAdmin) public onlyAdmins{
        admins[newAdmin] = true;
    }

    function removeAdmin(address oldAdmin) public onlyAdmins{
        admins[oldAdmin] = false;
    }

    function addContract(address newContract) public onlyAdmins{
        trustedContracts[newContract] = true;
    }

    function removeContract(address oldContract) public onlyAdmins{
        trustedContracts[oldContract] = false;
    }

    function updateContracts(address _kutils, address _hashtags, address _tagged, address _posts, address _messageFormat, address _comments, address _followers) public onlyAdmins {
        // Update the User Profiles contract address
        KUtils = IKUtils(_kutils);

        // Update the Hashtags address
        Hashtags = IHashtags(_hashtags);

        // Update the Tagged addresses
        Tagged = ITagged(_tagged);

        // Update the Posts addresses
        Posts = IPosts(_posts);

        // Update the Comments
        Comments = IComments(_comments);

        // Update the Message Formatting
        MessageFormat = IMessageFormat(_messageFormat);

        // Update the Followers addresses
        Followers = IFollowers(_followers);
    }


    /*

    Public Functions

    */


    function saveMsg(MsgData memory newMsg) public onlyAdmins {

        // Check if this is a repost, and if so, update that messages updateStats
        if (newMsg.isRepostOf > 0){
            addStat(3, newMsg.isRepostOf, 1);
        }

        // Get the bucket to store the message
        if (buckets[1] == maxItemsPerBucket){
            // If so, update the buckets and then return the next bucket ID
            buckets[0] += 1;
            buckets[1] = 0;
        }

        uint256 bucketKeyID = buckets[0];
        string memory thisBucketKey = append("posts", '-',KUtils.toString(bucketKeyID),'','');

        msgMap[thisBucketKey].push(newMsg.msgID);

        // Save the Message MessageData
        msgData[thisBucketKey][newMsg.msgID] = newMsg;
    
        // Record the post to the poster
        Posts.addPost(newMsg.msgID, newMsg.postedBy[0]);

        // Increase the bucket counter
        buckets[1] += 1;

        // Emit to the logs for external reference
        emit logNewMsg(newMsg.msgID, newMsg.postedBy, newMsg, newMsg.asGroup, newMsg.inGroups);

        delete bucketKeyID;
        delete thisBucketKey;
        delete newMsg;

    }


    function removeMsg(uint256 msgID, address requester, uint256 isCommentOf) public onlyAdmins {
        // Find the bucket containing the message
        string memory thisBucketKey = getBucketKeyByID(msgID);

        // Get the poster address from the message for a post
        MsgData memory thisMsgData = msgData[thisBucketKey][msgID];

        // Only the message poster can erase it
        require(thisMsgData.postedBy[0] == requester, "Only the message owner can erase it");

        if (isCommentOf > 0){
            // Find the bucket containing the message commented on
            string memory thisBucketKeyTmp = getBucketKeyByID(isCommentOf);

            // If it's a comment, erase the comment
            Comments.removeComment(msgID, requester, isCommentOf);

            // Remove a comment stat
            msgData[thisBucketKeyTmp][isCommentOf].msgStats.comments -= 1;
        } else {
            // Remove the Hashtags
            Hashtags.removeHashtags(msgID, thisMsgData.hashtags);

            // Remove the Tagged accounts
            Tagged.removeTags(msgID, thisMsgData.taggedAccounts);

            // Remove the post from the poster
            Posts.removePost(msgID, msgData[thisBucketKey][msgID].postedBy[0]);
        }
        
        thisMsgData.message = "[nil]";

        // Emit to the logs for external reference
        emit logRemoveMsg(msgID, isCommentOf);
    }


    function getMsgsByIDs(uint256[] memory msgIDs) public view returns (string memory){
        string memory allData;

        for (uint i=0; i < msgIDs.length; i++) {

            string memory thisBucketKey = getBucketKeyByID(msgIDs[i]);

            // Check to make sure the message exists
            require(bytes(thisBucketKey).length > 0, "Invalid Message ID");

            // Get the message data from the bucket
            MsgData memory thisMsgData = msgData[thisBucketKey][msgIDs[i]];

            uint256[] memory thisData = new uint256[](13);

            thisData[0] = thisMsgData.msgID;
            thisData[1] = thisMsgData.msgStats.time;
            thisData[2] = thisMsgData.msgStats.block;
            thisData[3] = thisMsgData.tip;
            thisData[4] = thisMsgData.paid;
            thisData[5] = thisMsgData.msgStats.postByContract;
            thisData[6] = uint(thisMsgData.msgStats.likes);
            thisData[7] = uint(thisMsgData.msgStats.reposts);
            thisData[8] = uint(thisMsgData.msgStats.comments);
            thisData[9] = 0;
            thisData[10] = thisMsgData.isRepostOf;
            thisData[11] = uint(thisMsgData.commentLevel);
            thisData[12] = thisMsgData.asGroup;

            // Get the formatted message
            string memory thisMsg = MessageFormat.buildMsg(thisData, thisMsgData.message, thisMsgData.postedBy, thisMsgData.hashtags, thisMsgData.taggedAccounts, thisMsgData.uri, thisMsgData.inGroups);

            if (i == 0){
                allData = append('{"Messages": [', thisMsg, '','','');
            } else {
                allData = append(allData, ',', thisMsg,'','');
            }

            delete thisMsg;
            delete thisData;
        }

        if (bytes(allData).length > 0){
            allData = append(allData, ']}', '','','');
        }

        return allData;
    }

    function getMsgCommentLevel(uint256 msgID) public view returns (uint256){
        // Find the bucket key for the message
        string memory thisBucketKey = getBucketKeyByID(msgID);

        // Check to make sure the message exists
        require(bytes(thisBucketKey).length > 0, "Invalid message ID to comment on");

        // Get the message data from the bucket
        MsgData memory thisMsgData = msgData[thisBucketKey][msgID];

        return thisMsgData.commentLevel;
    }


    // statType
    // 1 = like
    // 2 = comment
    // 3 = repost
    // 4 = tip

    function addStat(uint8 statType, uint256 msgID, int amount) public onlyAdmins {
        // Find the bucket where this message is
        string memory thisBucketKey = getBucketKeyByID(msgID);

        MsgData memory thisMsgData = msgData[thisBucketKey][msgID];

        if (statType == 1){
            thisMsgData.msgStats.likes += amount;
        } else if (statType == 2){
            thisMsgData.msgStats.comments += amount;
        } else if (statType == 3){
            thisMsgData.msgStats.reposts += amount;
        } else if (statType == 4){
            thisMsgData.msgStats.tipsReceived += amount;
        }

        msgData[thisBucketKey][msgID] = thisMsgData;

        // Emit to the logs for external reference
        emit logUpdateMsgStats(msgID, statType, amount);
    }


    function getBucketKeyByID(uint256 msgID) internal view returns (string memory){
        // Initialize the bucket key
        string memory thisBucketKey;

        // Go through each bucket to see if it's there in reverse
        for (uint b=buckets[0]; b >= 0; b--) {
            // Get the next bucket key
            thisBucketKey = append('posts-',KUtils.toString(b),'','','');

            // Get the poster address from the message for a post
            address poster = msgData[thisBucketKey][msgID].postedBy[0];
            
            if (poster != address(0)) {
                // We found the bucket with the message in it
                break;
            } else if (b == 0){
                thisBucketKey = '';
                break;
            }
        }

        return thisBucketKey;
    }

    function append(string memory a, string memory b, string memory c, string memory d, string memory e) internal pure returns (string memory) {
        return string(abi.encodePacked(a, b, c, d, e));
    }
}

