interface IEstateRegistry {
    function getTokenOwner(uint256 tokenId) external view returns (address);

    function approveToken(address to, uint256 tokenId) external;

    function getOperator(uint256 tokenId) external view returns (address);

    function resetPermissions(uint256 tokenId) external;

    function mint(address to) external returns (uint256);

    function areLandsValidForEstate(
        uint256[] calldata x,
        uint256[] calldata y
    ) external returns (bool);

    function canAddLandsToEstate(
        uint256[] calldata x,
        uint256[] calldata y,
        uint256 estateId
    ) external returns (bool);

    // EVENTS

    event SaleIsActive(bool indexed _saleIsActive);

    event AddLand(uint256 indexed _estateId, uint256 indexed _landId);

    event EstateMinted(address indexed _to, uint256 indexed estateId);

    event EstateDissolved(uint256 indexed _estateId, uint256[] indexed landIds);

    event OperatorAdded(address indexed _owner, address indexed _operator, uint256 indexed tokenId);
    event OperatorRemoved(
        address indexed _owner,
        address indexed _operator,
        uint256 indexed tokenId
    );

    event ResetPermission(uint256 indexed tokenId);

    event RentRegistrySet(address indexed _registry);
    event LandRegistrySet(address indexed _registry);

    event UpdateManager(
        address indexed _owner,
        address indexed _operator,
        address indexed _caller,
        bool _approved
    );
}

