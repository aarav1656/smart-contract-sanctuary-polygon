import "./../estate/EstateStorage.sol";
import "./../estate/IEstateRegistry.sol";
import "./LANDStorage.sol";

contract LANDRegistry is
    LANDStorage,
    Initializable,
    UUPSUpgradeable,
    ILANDRegistry,
    ERC721EnumerableUpgradeable,
    IERC2981Upgradeable,
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable
{
    function initialize() public initializer {
        __ERC721_init("Coderblock LAND", "LAND");
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    modifier onlyOwnerOfLand(uint256 tokenId) {
        require(
            msg.sender == ownerOf(tokenId),
            "This function can only be called by the owner of the LAND"
        );
        _;
    }

    modifier canTransfer(uint256 landId) {
        require(_isApprovedOrOwner(msg.sender, landId), "Only owner or operator can transfer");
        require(!isCurrentlyRented(landId), "Land is curretly rented.");
        _;
    }

    modifier canMultipleTransfer(uint256[] calldata x, uint256[] calldata y) {
        for (uint256 i; i < x.length; i++) {
            uint256 landId = getTokenId(x[i], y[i]);
            require(_isApprovedOrOwner(msg.sender, landId), "Only owner or operator can transfer");
            require(!isCurrentlyRented(landId), "Land is curretly rented.");
        }
        _;
    }

    modifier areValidCoords(uint256[] memory x, uint256[] memory y) {
        require(x.length == y.length, "The coordinates should have the same length");
        require(x.length > 0, "You should supply at least one coordinate");
        for (uint256 i = 0; i < x.length; i++) {
            require(x[i] <= getGridWidth() && x[i] > 0, "X coordinate is out of boundaries");
            require(y[i] <= getGridHeight() && y[i] > 0, "Y coordinate is out of boundaries");
        }
        _;
    }

    modifier areValidSingleCoords(uint256 x, uint256 y) {
        require(x <= getGridWidth() && x > 0, "X coordinate is out of boundaries");
        require(y <= getGridHeight() && y > 0, "Y coordinate is out of boundaries");
        _;
    }

    modifier onlyDeployer() {
        require(authorizedDeploy[msg.sender]);
        _;
    }

    function isCurrentlyRented(uint256 tokenId) public view returns (bool) {
        if (isRentRegistrySet()) {
            return (rentRegistry.isRented(address(this), tokenId));
        }
        return (false);
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default, can be overriden in child contracts.
     */

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    /**
     * @dev See {IERC721-ownerOf}.
     */
    function getTokenOwner(uint256 tokenId) public view virtual returns (address) {
        return ownerOf(tokenId);
    }

    function getTokenIdsFromOwner(
        address owner
    ) public view returns (uint256[] memory tokensArray) {
        uint256 arrayLength = balanceOf(owner);

        uint256[] memory ownerTokenIds = new uint256[](arrayLength);

        for (uint256 i = 0; i < arrayLength; i++) {
            ownerTokenIds[i] = tokenOfOwnerByIndex(owner, i);
            console.log(ownerTokenIds[i]);
        }

        return ownerTokenIds;
    }

    function getTokenOfManyOwners(
        uint256[] calldata x,
        uint256[] calldata y
    ) external view returns (address[] memory) {
        require(x.length > 0, "You should supply at least one coordinate");
        require(x.length == y.length, "The coordinates should have the same length");

        address[] memory addrs = new address[](x.length);
        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);
            addrs[i] = getTokenOwner(tokenId);
        }

        return addrs;
    }

    function setDeployer(address _deployer) external onlyOwner {
        require(_deployer != address(0), "invalid address");
        require(authorizedDeploy[_deployer] == false, "address is already authorized");
        authorizedDeploy[_deployer] = true;

        emit DeployerAdded(msg.sender, _deployer);
    }

    function forbidDeploy(address _deployer) external onlyOwner {
        require(_deployer != address(0), "invalid address");
        require(authorizedDeploy[_deployer], "address is already forbidden");
        authorizedDeploy[_deployer] = false;

        emit DeployerRemoved(msg.sender, _deployer);
    }

    function isDeploymentSet(address _deployer) public view returns (bool) {
        return authorizedDeploy[_deployer];
    }

    function setOperator(address _owner, address _operator, uint256 x, uint256 y) public {
        require(!isCurrentlyRented(getTokenId(x, y)));
        require(_owner == ownerOf(getTokenId(x, y)), "Unauthorized user");

        operatorsTokenIds[_operator].push(getTokenId(x, y));
        operators[getTokenId(x, y)] = _operator;

        emit OperatorAdded(_owner, _operator, getTokenId(x, y));
    }

    function getOperator(uint256 tokenId) public view returns (address) {
        return operators[tokenId];
    }

    function resetPermissions(uint256 x, uint256 y) public {
        require(msg.sender == address(rentRegistry));
        _removeOperator(x, y);

        emit ResetPermission(getTokenId(x, y));
    }

    function removeOperator(uint256 x, uint256 y) public {
        require(msg.sender == getTokenOwner(getTokenId(x, y)));
        _removeOperator(x, y);
    }

    function _removeOperator(uint256 x, uint256 y) internal {
        address _operator = getOperator(getTokenId(x, y));

        operators[getTokenId(x, y)] = address(0);

        for (uint256 i = 0; i < operatorsTokenIds[_operator].length; i++) {
            if (operatorsTokenIds[_operator][i] == getTokenId(x, y)) {
                uint256 tmp = operatorsTokenIds[_operator][operatorsTokenIds[_operator].length - 1];
                operatorsTokenIds[_operator][
                    operatorsTokenIds[_operator].length - 1
                ] = operatorsTokenIds[_operator][i];
                operatorsTokenIds[_operator][i] = tmp;
                operatorsTokenIds[_operator].pop();
            } else {
                revert("Not the operator of estate.");
            }
        }

        emit OperatorRemoved(msg.sender, _operator, getTokenId(x, y));
    }

    function isValidTokenId(uint256 _tokenId) public pure virtual returns (bool) {
        uint256 supply = getTotalGridSupply();
        return _tokenId < supply;
    }

    function hasEnoughSupply() public view returns (bool) {
        uint256 supply = getTotalGridSupply();
        return totalSupply() < supply;
    }

    function isNotMinted(uint256 _tokenId) public view returns (bool) {
        return bytes(_tokenURIs[_tokenId]).length == 0;
    }

    function isLandAvailable(uint256 _tokenId) public view returns (bool) {
        return !_exists(_tokenId);
    }

    function isLandCoordsAvailable(uint256 x, uint256 y) public view returns (bool) {
        return !_exists(getTokenId(x, y));
    }

    function canMint(uint256 _tokenId) public view returns (bool) {
        return
            hasEnoughSupply() &&
            isValidTokenId(_tokenId) &&
            isLandAvailable(_tokenId) &&
            isNotMinted(_tokenId);
    }

    function canMintByCoords(uint256 x, uint256 y) public view returns (bool) {
        return
            hasEnoughSupply() &&
            isValidTokenId(getTokenId(x, y)) &&
            isLandCoordsAvailable(x, y) &&
            isNotMinted(getTokenId(x, y));
    }

    //
    // Authorization getters
    //
    function approveToken(address to, uint256 tokenId) public virtual {
        address owner = ERC721Upgradeable.ownerOf(tokenId);

        require(to != owner, "ERC721: approval to current owner");
        require(owner == msg.sender, "You must own the token");

        _approve(to, tokenId);

        owner = ERC721Upgradeable.ownerOf(tokenId);
    }

    //
    // LAND Create
    //

    function mintToken(address to, uint256 x, uint256 y) public {
        uint256 landId = getTokenId(x, y);

        mintToken(to, landId);
    }

    function mintToken(address to, uint256 landId) public {
        require(
            msg.sender == owner() || authorizedDeploy[msg.sender],
            "Only the owner or authorized deployer can mint lands"
        );
        require(saleIsActive, "Sale is not active for this contract");
        require(canMint(landId), "Minting is not allowed, already minted");
        _safeMint(to, landId);
        _setTokenURI(landId, StringsUpgradeable.toString(landId));

        emit LandMinted(to, landId);
    }

    function mintMultipleTokens(address to, uint256[] memory landIds) public {
        require(
            msg.sender == owner() || authorizedDeploy[msg.sender],
            "Only the owner or authorized deployer can air drop lands"
        );
        for (uint256 i = 0; i < landIds.length; i++) {
            require(saleIsActive, "Sale is not active for this contract");
            require(canMint(landIds[i]), "Minting is not allowed, already minted");

            _safeMint(to, landIds[i]);

            _setTokenURI(landIds[i], StringsUpgradeable.toString(landIds[i]));

            emit MultipleLandsMinted(to, landIds);
        }
    }

    function mintMultipleTokens(address to, uint256[] calldata x, uint256[] calldata y) public {
        require(x.length == y.length, "The coordinates should have the same length");

        uint256[] memory landIds = new uint256[](x.length);

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);
            landIds[i] = tokenId;
        }

        mintMultipleTokens(to, landIds);
    }

    //
    // LAND Getters
    //

    function getGridWidth() public pure returns (uint256) {
        return GRID_SIZE_X;
    }

    function getGridHeight() public pure returns (uint256) {
        return GRID_SIZE_Y;
    }

    function getTotalGridSupply() public pure returns (uint256) {
        return (getGridWidth() * 2 + 1) * (getGridHeight() * 2 + 1);
    }

    function getX(uint256 tokenId) public pure returns (uint256) {
        uint256 unshiftedX = uint256(tokenId / ((getGridHeight() * 2) + 1));
        return unshiftedX - uint256(getGridWidth());
    }

    function getY(uint256 tokenId) public pure returns (uint256) {
        uint256 unshiftedY = uint256(tokenId % ((getGridWidth() * 2) + 1));
        return unshiftedY - uint256(getGridHeight());
    }

    function getTokenId(
        uint256 x,
        uint256 y
    ) public pure areValidSingleCoords(x, y) returns (uint256) {
        uint256 paddedX = uint256(x + uint256(getGridWidth()));
        uint256 paddedY = uint256(y + uint256(getGridHeight()));
        return paddedX * ((getGridHeight() * 2) + 1) + paddedY;
    }

    function exists(uint256 x, uint256 y) external view returns (bool) {
        return _exists(getTokenId(x, y));
    }

    //
    // LAND tokenURI
    //

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721URIStorage: URI query for nonexistent token");

        string memory _tokenURI = _tokenURIs[tokenId];
        string memory base = _baseURI();

        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return _tokenURI;
        }
        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(base, _tokenURI));
        }

        return super.tokenURI(tokenId);
    }

    // Sets '_tokenURI' as the tokenURI of 'tokenId'.

    function _setTokenURI(uint256 tokenId, string memory _tokenURI) public {
        require(_exists(tokenId), "ERC721URIStorage: URI set of nonexistent token");
        _tokenURIs[tokenId] = _tokenURI;
    }

    //
    // LAND Transfer - Transfer a parcel to another address using the parcel's token id.
    //

    function transferLand(uint256 x, uint256 y, address to) public {
        uint256 tokenId = getTokenId(x, y);
        transferLand(tokenId, to);

        // resetPermissions(x, y);
    }

    function transferLand(uint256 tokenId, address to) public canTransfer(tokenId) {
        safeTransferFrom(ownerOf(tokenId), to, tokenId);

        // resetPermissions(x, y);
    }

    function transferLandDissolve(address from, address to, uint256 tokenId) external {
        require(address(estateRegistry) == msg.sender);
        transferFrom(from, to, tokenId);
    }

    function transferManyLand(
        uint256[] calldata x,
        uint256[] calldata y,
        address to
    ) external canMultipleTransfer(x, y) {
        require(x.length > 0, "You should supply at least one coordinate");
        require(x.length == y.length, "The coordinates should have the same length");

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);
            safeTransferFrom(ownerOf(tokenId), to, tokenId);
        }
    }

    function transferFrom(address from, address to, uint256 tokenId) public override canTransfer(tokenId) {
        super.transferFrom(from, to, tokenId);
    }

    //
    // LAND TO ESTATE - Add a parcel to an Estate using the parcel's coordinates.
    //

    function transferLandToEstate(
        uint256[] calldata x,
        uint256[] calldata y,
        uint256 estateId
    ) external canMultipleTransfer(x, y) {
        require(
            estateRegistry.canAddLandsToEstate(x, y, estateId),
            "Land is not valid to be added to the estate"
        );

        uint256[] memory landIds = new uint256[](x.length);

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);

            landIds[i] = tokenId;

            require(
                estateRegistry.getTokenOwner(estateId) == msg.sender,
                "You must own the Estate you want to transfer to"
            );

            bytes memory estateTokenIdBytes = toBytes(estateId);

            safeTransferFrom(msg.sender, address(estateRegistry), tokenId, estateTokenIdBytes);
        }

        emit LandsAddedToEstate(landIds, estateId);
    }

    //
    // function airDrop
    //

    function airDropLand(address _recipient, uint256 x, uint256 y) public {
        require(
            msg.sender == owner() || authorizedDeploy[msg.sender],
            "Only the owner or authorized deployer can air drop lands"
        );
        uint256 tokenId = getTokenId(x, y);
        if (_exists(tokenId)) {
            require(!isCurrentlyRented(tokenId), "Land is curretly rented.");
            safeTransferFrom(msg.sender, _recipient, tokenId);
        } else if (!_exists(tokenId)) {
            mintToken(_recipient, x, y);
        }

        emit getAirDrop(_recipient, tokenId);
    }

    function airDropMultipleLand(
        address _recipient,
        uint256[] calldata x,
        uint256[] calldata y
    ) public {
        require(
            msg.sender == owner() || authorizedDeploy[msg.sender],
            "Only the owner or authorized deployer can air drop lands"
        );

        uint256[] memory landIds = new uint256[](x.length);

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);
            landIds[i] = tokenId;
            if (_exists(tokenId)) {
                require(msg.sender == ownerOf(tokenId), "You are not the token owner.");
                require(!isCurrentlyRented(tokenId), "Land is curretly rented.");
                safeTransferFrom(msg.sender, _recipient, tokenId);
            } else if (!_exists(tokenId)) {
                mintMultipleTokens(_recipient, x, y);
            }

        }
            emit getMultipleAirDrop(_recipient, landIds);
    }

    //
    // Estate generation
    //

    function isRentRegistrySet() public view returns (bool) {
        return address(rentRegistry) != address(0);
    }

    function setRentRegistry(address registry) external onlyOwner {
        rentRegistry = IRENTRegistry(registry);

        emit RentRegistrySet(registry);
    }

    function setEstateRegistry(address registry) external onlyOwner {
        estateRegistry = IEstateRegistry(registry);

        emit EstateRegistrySet(registry);
    }

    function createEstate(uint256[] calldata x, uint256[] calldata y) external returns (uint256) {
        return _createEstate(x, y);
    }

    function _createEstate(uint256[] calldata x, uint256[] calldata y) internal returns (uint256) {
        require(x.length > 1, "You should supply at least two coordinate");
        require(x.length == y.length, "The coordinates should have the same length");
        require(address(estateRegistry) != address(0), "The Estate registry should be set");
        require(
            estateRegistry.areLandsValidForEstate(x, y) == true,
            "Coordinates are not adjacent"
        );

        for (uint256 i = 0; i < x.length; i++) {
            require(
                ownerOf(getTokenId(x[i], y[i])) == msg.sender,
                "Sender is not current land owner."
            );
        }

        uint256 estateTokenId = estateRegistry.mint(msg.sender);
        bytes memory estateTokenIdBytes = toBytes(estateTokenId);

        uint256[] memory landIds = new uint256[](x.length);

        for (uint256 i = 0; i < x.length; i++) {
            uint256 tokenId = getTokenId(x[i], y[i]);

            landIds[i] = tokenId;

            safeTransferFrom(msg.sender, address(estateRegistry), tokenId, estateTokenIdBytes);
        }

        emit CreateEstate(landIds, estateTokenId);

        return estateTokenId;
    }

    function toBytes(uint256 x) internal pure returns (bytes memory b) {
        b = new bytes(32);
        // solium-disable-next-line security/no-inline-assembly
        assembly {
            mstore(add(b, 32), x)
        }
    }

    //
    // SALE ACTIVATION
    //

    function setSaleIsActive(bool saleIsActive_) external onlyOwner {
        saleIsActive = saleIsActive_;

        emit SaleIsActive(saleIsActive_);
    }

    //
    // ROYALTIES
    //

    function royaltyInfo(
        uint256,
        uint256 salePrice
    ) external view returns (address receiver, uint256 royaltyAmount) {
        return (address(this), (salePrice * 500) / 10000);
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC721EnumerableUpgradeable, IERC165Upgradeable) returns (bool) {
        return (interfaceId == type(IERC2981Upgradeable).interfaceId ||
            super.supportsInterface(interfaceId));
    }

    function _bytesToUint(bytes memory b) internal pure returns (uint256) {
        return uint256(_bytesToBytes32(b));
    }

    function _bytesToBytes32(bytes memory b) internal pure returns (bytes32) {
        bytes32 out;

        for (uint256 i = 0; i < b.length; i++) {
            out |= bytes32(b[i] & 0xFF) >> (SafeMathUpgradeable.mul(i, 8));
        }

        return out;
    }

    /** UPGRADE FUNCTION **/

    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}
}

