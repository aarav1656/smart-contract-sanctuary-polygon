pragma solidity ^0.8.9;

import 'hardhat/console.sol';
import '../interfaces/IOttopiaStore.sol';
import '../interfaces/IOttoItemFactory.sol';
import '../interfaces/IOttoItem.sol';
import '../interfaces/IERC20.sol';
import '../interfaces/IOtto.sol';
import '../interfaces/IERC721.sol';
import '../interfaces/IERC1155.sol';
import '../libraries/BaseContract.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155ReceiverUpgradeable.sol';
import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';

interface IClamERC20 is IERC20 {
    function burn(uint256 amount_) external;
}

interface IOttoItemERC1155 is IOttoItem, IERC1155Preset {}

contract OttopiaStore is
    BaseContract,
    IOttopiaStore,
    IERC1155ReceiverUpgradeable
{
    using ECDSA for bytes32;
    bytes32 public constant WHITELISTED_CONTRACT =
        keccak256('WHITELISTED_CONTRACT');
    uint256 public constant NUM_RARITIES = 12;
    uint16 public constant HUNDRED_PERCENT = 10000;

    struct Signature {
        string nonce;
        bytes32 digest;
        bytes signed;
    }

    event BuyItem(
        address indexed buyer,
        address indexed receiver,
        uint256 indexed itemId,
        uint256 amount,
        address token, // zero address for matic
        uint256 price
    );

    event CreateProduct(uint256 indexed id_);
    event UpdateProduct(uint256 indexed id_);
    event DeleteProduct(uint256 indexed id_);
    event BuyProduct(
        uint256 indexed id,
        address indexed buyer,
        address indexed to,
        uint256 price,
        uint256 amount
    );
    event RedeemProduct(
        uint256 indexed id,
        address indexed redeemer,
        uint256 amount
    );
    event BuyFish(address indexed buyer, uint256 spent, uint256 bought);
    mapping(uint256 => Product) public products;
    // product id => factories contract
    mapping(uint256 => address) public factories;

    IClamERC20 public CLAM;
    address public DAO;
    address public REWARD;
    IERC721 public OTTO;

    uint256 private _idCounter;
    string private _baseURI;

    // Airdrop info
    struct AirdropInfo {
        uint256 startId;
        uint256 endId;
        uint256 amount;
    }

    struct CouponInfo {
        uint256 productId;
        bool created;
    }
    // product id => airdrop info
    mapping(uint256 => AirdropInfo) public airdropInfos;
    // product id => otto id => claimed amount
    mapping(uint256 => mapping(uint256 => uint256)) public claimed;
    // item id (coupon) => product id
    mapping(uint256 => CouponInfo) public coupons;
    IOttoItemERC1155 public ITEM;
    IERC20Mintable public FISH;
    uint256 private _fishPerClam;
    address public signer;
    mapping(bytes => bool) public usedSignatures;
    uint256 public fishPerMatic;
    uint256 public minMaticToBuyFish;

    modifier validProduct(uint256 id_) {
        require(
            products[id_].amount != 0 && bytes(products[id_].typ).length != 0,
            'product not created'
        );
        _;
    }

    modifier forSaleProduct(uint256 id_) {
        if (products[id_].startedAt != 0) {
            require(
                block.timestamp >= products[id_].startedAt,
                'product sale not started'
            );
        }
        if (products[id_].endedAt != 0) {
            require(
                block.timestamp < products[id_].endedAt,
                'product sale ended'
            );
        }
        _;
    }

    modifier onlyEOA() {
        require(tx.origin == msg.sender, 'not EOA');
        _;
    }

    modifier callerIsUserOrWhitelistedContract() {
        require(
            tx.origin == msg.sender ||
                hasRole(WHITELISTED_CONTRACT, msg.sender),
            'Not whitelied'
        );
        _;
    }

    function initialize(
        address clam_,
        address dao_,
        address reward_,
        address otto_
    ) public initializer {
        __BaseContract_init();
        CLAM = IClamERC20(clam_);
        DAO = dao_;
        REWARD = reward_;
        OTTO = IERC721(otto_);
    }

    function setSigner(address signer_) external onlyAdmin {
        signer = signer_;
    }

    function initializeFish(address fish_) external onlyAdmin {
        FISH = IERC20Mintable(fish_);
        _fishPerClam = 6000;
    }

    function setFishPerClam(uint256 fishPerClam_) external onlyAdmin {
        _fishPerClam = fishPerClam_;
    }

    function setFishPerMatic(uint256 fishPerMatic_, uint256 minMatic_)
        external
        onlyAdmin
    {
        fishPerMatic = fishPerMatic_;
        minMaticToBuyFish = minMatic_;
    }

    function toFish(uint256 clamAmount_) public view returns (uint256) {
        return
            clamAmount_ *
            _fishPerClam *
            10**(FISH.decimals() - CLAM.decimals());
    }

    function maticToFish(uint256 maticAmount_) public view returns (uint256) {
        return maticAmount_ * fishPerMatic;
    }

    function buyFishWithMatic()
        external
        payable
        callerIsUserOrWhitelistedContract
    {
        require(msg.value >= minMaticToBuyFish, 'at least 1 matic');
        uint256 fishAmount_ = maticToFish(msg.value);
        FISH.mint(_msgSender(), fishAmount_);
        _distributeMatic(msg.value);
        emit BuyFish(_msgSender(), msg.value, fishAmount_);
    }

    function buyFish(uint256 clamAmount_)
        external
        callerIsUserOrWhitelistedContract
    {
        require(clamAmount_ >= 10**CLAM.decimals(), 'at least 1 clam');
        CLAM.transferFrom(_msgSender(), address(this), clamAmount_);
        uint256 fishAmount_ = toFish(clamAmount_);
        FISH.mint(_msgSender(), fishAmount_);
        _distribute(clamAmount_);
        emit BuyFish(_msgSender(), clamAmount_, fishAmount_);
    }

    function setItem(address item_) external onlyAdmin {
        ITEM = IOttoItemERC1155(item_);
    }

    function toggleWhitelist(address contract_) external onlyAdmin {
        if (hasRole(WHITELISTED_CONTRACT, contract_)) {
            _revokeRole(WHITELISTED_CONTRACT, contract_);
        } else {
            _grantRole(WHITELISTED_CONTRACT, contract_);
        }
    }

    function setBaseURI(string calldata baseURI_) external onlyAdmin {
        _baseURI = baseURI_;
    }

    function upgradeForFlashSale() external onlyAdmin {
        products[0].metaItemId = 16646144;
        products[1].metaItemId = 16646144;
        products[2].metaItemId = 16646144;
        products[3].metaItemId = 16646208;
        products[4].metaItemId = 16646208;
        products[5].metaItemId = 16646208;
        products[6].metaItemId = 16646230;
        products[7].metaItemId = 16646230;
        products[8].metaItemId = 16646230;
        products[9].metaItemId = 16711702;
    }

    function _distribute(uint256 amount_) private {
        uint256 toBurn = _calcPercentage(amount_, 1000);
        uint256 toDao = _calcPercentage(amount_, 7000);
        uint256 rewards = amount_ - toBurn - toDao;
        CLAM.burn(toBurn);
        CLAM.transfer(DAO, toDao);
        CLAM.transfer(REWARD, rewards);
    }

    function _distributeMatic(uint256 amount_) private {
        uint256 toDao = _calcPercentage(amount_, 5000);
        uint256 rewards = amount_ - toDao;
        payable(DAO).call{value: toDao}('');
        payable(REWARD).call{value: rewards}('');
    }

    function buy(
        address to_,
        uint256 id_,
        uint256 amount_
    ) external override {
        buyNoChainlink(to_, id_, amount_);
    }

    function buyNoChainlink(
        address to_,
        uint256 id_,
        uint256 amount_
    )
        public
        callerIsUserOrWhitelistedContract
        validProduct(id_)
        forSaleProduct(id_)
    {
        Product memory product = products[id_];
        uint256 price_ = product.discountPrice;
        price_ *= amount_;
        CLAM.transferFrom(_msgSender(), address(this), price_);
        if (factories[id_] != address(0)) {
            IOttoItemFactory(factories[id_]).shipNoChainlink(
                address(this),
                id_,
                to_,
                amount_
            );
        }
        _distribute(price_);
        emit BuyProduct(id_, msg.sender, to_, product.discountPrice, amount_);
    }

    function _verify(bytes32 hash_, Signature calldata sig_) private {
        require(!usedSignatures[sig_.signed], 'sig used');
        usedSignatures[sig_.signed] = true;
        require(
            signer == hash_.toEthSignedMessageHash().recover(sig_.signed),
            'sig mismatch'
        );
        require(hash_ == sig_.digest, 'hash mismatch');
    }

    function _sendContents(address to_, uint256[] calldata contents_) private {
        for (uint256 i = 0; i < contents_.length; i++) {
            ITEM.mint(to_, contents_[i], 1, '');
        }
    }

    function buyWithMatic(
        address to_,
        uint256 itemId_,
        uint256 amount_,
        uint256[] calldata contents_,
        Signature calldata sig_
    ) external payable onlyEOA {
        bytes32 hash = keccak256(
            abi.encodePacked(
                msg.value,
                msg.sender,
                address(this),
                to_,
                itemId_,
                amount_,
                contents_,
                sig_.nonce
            )
        );
        _verify(hash, sig_);
        _distributeMatic(msg.value);
        if (itemId_ / (1 << 16) == 254) {
            require(amount_ == contents_.length, 'amount mismatch');
            _sendContents(to_, contents_);
        } else {
            require(contents_.length == 0, 'not zero contents');
            ITEM.mint(to_, itemId_, amount_, '');
        }
        emit BuyItem(msg.sender, to_, itemId_, amount_, address(0), msg.value);
    }

    function openChest(
        address to_,
        uint256 itemId_,
        uint256 amount_,
        uint256[] calldata contents_,
        Signature calldata sig_
    ) external onlyEOA {
        bytes32 hash = keccak256(
            abi.encodePacked(
                msg.sender,
                address(this),
                to_,
                itemId_,
                amount_,
                contents_,
                sig_.nonce
            )
        );
        _verify(hash, sig_);
        require(itemId_ / (1 << 16) == 254, 'not chest');
        ITEM.burn(msg.sender, itemId_, amount_);
        _sendContents(to_, contents_);
    }

    function buySigned(
        address to_,
        uint256 itemId_,
        uint256 amount_,
        address token_,
        uint256 price_,
        Signature calldata sig_
    ) external onlyEOA {
        bytes32 hash = keccak256(
            abi.encodePacked(
                msg.sender,
                address(this),
                to_,
                itemId_,
                amount_,
                token_,
                price_,
                sig_.nonce
            )
        );
        _verify(hash, sig_);
        if (token_ == address(CLAM)) {
            CLAM.transferFrom(_msgSender(), address(this), price_);
            _distribute(price_);
        } else if (token_ == address(FISH)) {
            IERC20Burnable(token_).burnFrom(msg.sender, price_);
        } else {
            revert('invalid token');
        }
        ITEM.mint(to_, itemId_, amount_, '');
        emit BuyItem(msg.sender, to_, itemId_, amount_, token_, price_);
    }

    function claim(uint256 productId_, uint256[] calldata ottoIds_) external {
        claimNoChainlink(productId_, ottoIds_);
    }

    function claimNoChainlink(uint256 productId_, uint256[] calldata ottoIds_)
        public
        validProduct(productId_)
        forSaleProduct(productId_)
        callerIsUserOrWhitelistedContract
    {
        AirdropInfo storage info = airdropInfos[productId_];
        uint256 total = 0;
        for (uint256 i = 0; i < ottoIds_.length; i++) {
            uint256 ottoId = ottoIds_[i];
            if (info.startId <= ottoId && ottoId <= info.endId) {
                uint256 amount = info.amount - claimed[productId_][ottoId];
                if (amount > 0) {
                    claimed[productId_][ottoId] += amount;
                    total += amount;
                }
            }
        }
        if (total > 0) {
            IOttoItemFactory(factories[productId_]).shipNoChainlink(
                address(this),
                productId_,
                _msgSender(),
                total
            );
        }
    }

    function airdropClaimableAmount(
        uint256 productId_,
        uint256[] calldata ottoIds_
    ) external view validProduct(productId_) returns (uint256) {
        uint256 amount = 0;
        uint256 ottoCount = ottoIds_.length;
        for (uint256 i = 0; i < ottoCount; i++) {
            uint256 ottoId = ottoIds_[i];
            if (
                airdropInfos[productId_].startId <= ottoId &&
                ottoId <= airdropInfos[productId_].endId
            ) {
                amount +=
                    airdropInfos[productId_].amount -
                    claimed[productId_][ottoId];
            }
        }
        return amount;
    }

    function setAirdropInfo(
        uint256 productId_,
        uint256 startId_,
        uint256 endId_,
        uint256 amount_
    ) external validProduct(productId_) forSaleProduct(productId_) onlyAdmin {
        airdropInfos[productId_] = AirdropInfo(startId_, endId_, amount_);
    }

    function createProduct(Product memory product_, address factory_)
        external
        onlyAdmin
    {
        require(
            product_.amount != 0 && bytes(product_.typ).length != 0,
            'invalid amount or type'
        );
        uint256 id_ = _idCounter++;
        products[id_] = product_;
        factories[id_] = factory_;
        emit CreateProduct(id_);
    }

    function updateProduct(
        uint256 id_,
        Product memory product_,
        address factory_
    ) external onlyAdmin validProduct(id_) {
        products[id_] = product_;
        factories[id_] = factory_;
        emit UpdateProduct(id_);
    }

    function deleteProduct(uint256 id_) external onlyAdmin validProduct(id_) {
        delete products[id_];
        delete factories[id_];
        emit DeleteProduct(id_);
    }

    function createCoupon(uint256 itemId_, uint256 productId_)
        external
        onlyAdmin
        validProduct(productId_)
    {
        (uint8 slot_, ) = ITEM.decode(itemId_);
        require(slot_ == 254, 'item is not a coupon');
        require(!coupons[itemId_].created, 'coupon already created');
        coupons[itemId_].productId = productId_;
        coupons[itemId_].created = true;
    }

    function updateCoupon(uint256 itemId_, uint256 productId_)
        external
        onlyAdmin
        validProduct(productId_)
    {
        require(coupons[itemId_].created, 'coupon not created');
        coupons[itemId_].productId = productId_;
    }

    function deleteCoupon(uint256 itemId_) external onlyAdmin {
        require(coupons[itemId_].created, 'coupon not created');
        delete coupons[itemId_];
    }

    function uri(uint256 id_)
        external
        view
        override
        validProduct(id_)
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(_baseURI, StringsUpgradeable.toString(id_))
            );
    }

    function amountOf(uint256 id_)
        external
        view
        override
        validProduct(id_)
        returns (uint256)
    {
        return products[id_].amount;
    }

    function discountPriceOf(uint256 id_)
        external
        view
        override
        validProduct(id_)
        returns (uint256)
    {
        return products[id_].discountPrice;
    }

    function _calcPercentage(uint256 price_, uint256 percentage_)
        private
        pure
        returns (uint256)
    {
        return (price_ * percentage_) / 10000;
    }

    function totalSupply() external view returns (uint256) {
        return _idCounter;
    }

    function _authorizeUpgrade(address) internal override onlyAdmin {}

    function onERC1155Received(
        address,
        address from_,
        uint256 itemId_,
        uint256 amount_,
        bytes memory _data
    ) public virtual override returns (bytes4) {
        if (_msgSender() == address(ITEM)) {
            CouponInfo memory coupon = coupons[itemId_];
            require(coupon.created, 'coupon not created');
            ITEM.burn(address(this), itemId_, amount_);
            IOttoItemFactory(factories[coupon.productId]).shipNoChainlink(
                address(this),
                coupon.productId,
                from_,
                amount_
            );
            emit RedeemProduct(coupon.productId, from_, amount_);
        }
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes memory
    ) public virtual override returns (bytes4) {
        if (_msgSender() == address(ITEM)) {
            revert('not implemented');
        }
        return this.onERC1155BatchReceived.selector;
    }
}

