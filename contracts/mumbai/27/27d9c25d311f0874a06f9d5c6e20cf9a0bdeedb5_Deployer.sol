        return _checkData(destination, data);
    }

    function _checkData(address contractAddress, bytes calldata data) internal view virtual returns (bool) {
        if (_checkContractAddress(contractAddress)) return true;
        return _checkContractData(contractAddress, data);
    }

    function _checkContractAddress(address contractAddress) internal view returns (bool) {
        bytes32 noSigId = keccak256(abi.encodePacked(contractAddress));
        return _bytes32Storage[noSigId];
    }

    function _checkContractData(address contractAddress, bytes calldata data) internal view returns (bool) {
        bytes4 selector = bytes4(BytesMask.sliceBytesToFixLen(data, 4));
        bytes memory mask = _masks[contractAddress][selector];
        bytes memory slicedData = BytesMask.sliceBytesToFixLen(data, mask.length);
        bytes memory maskedData = new bytes(mask.length);
        BytesMask.maskArrayReplace(maskedData, slicedData, mask);
        bytes32 id = keccak256(abi.encodePacked(contractAddress, maskedData));
        return _bytes32Storage[id];
    }

    function checkContractAddress(address contractAddress) public view returns (bool) {
        return _checkContractAddress(contractAddress);
    }

    function checkContractData(address contractAddress, bytes calldata data) public view returns (bool) {
        return _checkContractData(contractAddress, data);
    }

    function getMask(address contractAddress, bytes4 selector) public view returns (bytes memory) {
        return _masks[contractAddress][selector];
    }
}

