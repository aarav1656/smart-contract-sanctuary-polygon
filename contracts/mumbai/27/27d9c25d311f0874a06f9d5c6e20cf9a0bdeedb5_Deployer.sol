pragma solidity 0.8.16;

import {BeaconProxy} from "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import {UpgradeableBeacon} from "@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol";
import {Ownable2StepUpgradeable} from "@openzeppelin/contracts-upgradeable/access/Ownable2StepUpgradeable.sol";
import {UUPSUpgradeable} from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {TimelockController} from "@openzeppelin/contracts/governance/TimelockController.sol";
import {IHuman} from "./interfaces/IHuman.sol";

contract HumanFactory is Initializable, UUPSUpgradeable, Ownable2StepUpgradeable {
    ISafeProxyFactory public gnosisFactory;
    address public fallbackHandler;
    address public singleton;
    address public signer;
    address public policies;
    address public entryPoint;
    address public beacon;

    event UpdateSafe(address gnosisFactory, address fallbackHandler, address singleton);
    event UpdateConfig(address signer, address policies, address entryPoint);
    event NewSafe(address indexed safe, address[] owners, uint256 salt, uint256 threshold);
    event NewTimeLockController(address indexed timelock, uint256 delay, address[] proposers, address[] executors);
    event NewHuman(
        address indexed proxy,
        address indexed owner,
        string stringSalt,
        address guardian,
        address master,
        uint256 inactivityTime
    );

    constructor() {
        _disableInitializers();
    }

    function initialize(
        address human_,
        address owner_,
        address signer_,
        ISafeProxyFactory gnosisFactory_,
        address fallbackHandler_,
        address singleton_,
        address policies_,
        address entryPoint_
    ) public initializer {
        __UUPSUpgradeable_init_unchained();
        __Ownable2Step_init_unchained();
        _transferOwnership(owner_);

        UpgradeableBeacon beacon_ = new UpgradeableBeacon(human_);
        beacon_.transferOwnership(owner_);
        beacon = address(beacon_);
        signer = signer_;
        gnosisFactory = gnosisFactory_;
        fallbackHandler = fallbackHandler_;
        singleton = singleton_;
        policies = policies_;
        entryPoint = entryPoint_;
    }

    function getHuman(string memory stringSalt) public view returns (address) {
        uint256 salt = uint256(keccak256(abi.encode(stringSalt)));
        bytes memory creationCode = type(BeaconProxy).creationCode;
        bytes memory bytecode = abi.encodePacked(creationCode, abi.encode(beacon, ""));
        bytes32 hash = keccak256(abi.encodePacked(bytes1(0xff), address(this), salt, keccak256(bytecode)));

        // NOTE: cast last 20 bytes of hash to address
        return address(uint160(uint256(hash)));
    }

    //solhint-disable-next-line no-empty-blocks
    function _authorizeUpgrade(address newImplementation) internal virtual override onlyOwner {}

    function updateSafe(ISafeProxyFactory gnosisFactory_, address fallbackHandler_, address singleton_)
        external
        onlyOwner
    {
        gnosisFactory = gnosisFactory_;
        fallbackHandler = fallbackHandler_;
        singleton = singleton_;
        emit UpdateSafe(address(gnosisFactory), fallbackHandler, singleton);
    }

    function updateConfig(address signer_, address policies_, address entryPoint_) external onlyOwner {
        signer = signer_;
        policies = policies_;
        entryPoint = entryPoint_;
        emit UpdateConfig(signer, policies, entryPoint);
    }

    function deployHuman(
        address[] calldata safeOwners,
        uint256 safeSaltNonce,
        uint256 timelockMinDelay,
        address owner,
        address master,
        uint96 humanInactivityRequiredTime,
        string calldata create2StringSalt
    ) external returns (address payable) {
        require(msg.sender == signer, "HumanFactory:");
        return payable(
            _deployHuman(
                safeOwners,
                safeSaltNonce,
                timelockMinDelay,
                owner,
                master,
                humanInactivityRequiredTime,
                create2StringSalt
            )
        );
    }

    function _deployHuman(
        address[] calldata safeOwners,
        uint256 safeSaltNonce,
        uint256 timelockMinDelay,
        address owner,
        address master,
        uint96 humanInactivityRequiredTime,
        string calldata create2StringSalt
    ) private returns (address human) {
        address guardian = _deployGuardian(safeOwners, safeSaltNonce, timelockMinDelay);
        {
            bytes memory initializeCalldata = abi.encodeWithSelector(
                IHuman.initialize.selector, owner, master, guardian, entryPoint, policies, humanInactivityRequiredTime
            );
            human = _createBeaconProxy(initializeCalldata, create2StringSalt);
        }
        {
            emit NewHuman(human, owner, create2StringSalt, guardian, master, humanInactivityRequiredTime);
        }
        return human;
    }

    function _createBeaconProxy(bytes memory initializeCalldata, string calldata stringSalt)
        private
        returns (address proxyAddress)
    {
        uint256 salt = uint256(keccak256(abi.encode(stringSalt)));
        bytes memory creationCode = type(BeaconProxy).creationCode;
        // NOTE: BeaconProxy data param is empty so create2 address does not depend on it
        bytes memory bytecode = abi.encodePacked(creationCode, abi.encode(beacon, ""));
        // solhint-disable-next-line no-inline-assembly
        assembly {
            proxyAddress := create2(0, add(bytecode, 0x20), mload(bytecode), salt)
            if iszero(extcodesize(proxyAddress)) { revert(0, 0) }
        }
        // NOTE: BeaconProxy is not initialized on constructor so its initialized after deployment
        // solhint-disable-next-line avoid-low-level-calls
        (bool success,) = proxyAddress.call(initializeCalldata);
        require(success, "HumanFactory: initialize");
    }

    function _deployGuardian(address[] calldata initialOwners, uint256 saltNonce, uint256 minDelay)
        private
        returns (address guardian)
    {
        address safe = _deploySafeMultisig(initialOwners, saltNonce);
        address[] memory proposers = new address[](1);
        proposers[0] = safe;
        address[] memory executors = new address[](1);
        executors[0] = address(0);
        guardian = _deployTimeLockController(minDelay, proposers, executors, address(0));
    }

    function _deployTimeLockController(
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors,
        address admin
    ) private returns (address guardian) {
        guardian = address(new TimelockController(minDelay, proposers, executors, admin));
        emit NewTimeLockController(guardian, minDelay, proposers, executors);
        return guardian;
    }

    function _deploySafeMultisig(address[] calldata initialOwners, uint256 saltNonce) private returns (address) {
        uint256 threshold = (initialOwners.length / 2) + 1;
        bytes memory safeCalldata = abi.encodeWithSignature(
            "setup(address[],uint256,address,bytes,address,address,uint256,address)",
            initialOwners,
            threshold,
            address(0),
            new bytes(0),
            fallbackHandler,
            address(0),
            0,
            address(0)
        );

        address safe = gnosisFactory.createProxyWithNonce(singleton, safeCalldata, saltNonce);
        emit NewSafe(safe, initialOwners, saltNonce, threshold);
        return safe;
    }
}

interface ISafeProxyFactory {
    function createProxyWithNonce(address singleton, bytes memory initializer, uint256 saltNonce)
        external
        returns (address proxy);
}

