pragma solidity 0.8.16;

import {ERC1967Proxy} from "@openzeppelin/contracts/proxy/ERC1967/ERC1967Proxy.sol";
import {HumanFactory} from "src/HumanFactory.sol";
import {HumanExecutePolicies} from "src/HumanExecutePolicies.sol";
import {HumanEntryPoint} from "src/HumanEntryPoint.sol";

contract Deployer {
    address public factory;
    address public policies;
    address public entryPoint;

    constructor(
        address ms,
        address signer,
        address safeFactory,
        address safeFallbackHandler,
        address safeSingleton,
        address humanImp,
        address executePoliciesImp,
        address entryPointImp,
        address factoryImp
    ) {
        {
            bytes memory initializeCalldata = abi.encodeWithSelector(HumanExecutePolicies.initialize.selector, ms);
            policies = _createProxy(executePoliciesImp, initializeCalldata);
        }

        {
            bytes memory initializeCalldata = abi.encodeWithSelector(HumanEntryPoint.initialize.selector, ms);
            entryPoint = _createProxy(entryPointImp, initializeCalldata);
        }

        {
            bytes memory initializeCalldata = abi.encodeWithSelector(
                HumanFactory.initialize.selector,
                humanImp,
                ms,
                signer,
                safeFactory,
                safeFallbackHandler,
                safeSingleton,
                address(policies),
                address(entryPoint)
            );
            factory = _createProxy(factoryImp, initializeCalldata);
        }
    }

    function _createProxy(address proxyImplementation, bytes memory initializeCalldata)
        private
        returns (address proxyAddress)
    {
        proxyAddress = address(new ERC1967Proxy(proxyImplementation, initializeCalldata));
    }
}

