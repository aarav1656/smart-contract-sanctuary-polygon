pragma solidity ^0.8.0;
import '@chainlink/contracts/src/v0.8/ChainlinkClient.sol';
import '@chainlink/contracts/src/v0.8/ConfirmedOwner.sol';
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@opengsn/contracts/src/interfaces/IRelayHub.sol";
import "./community.sol";
import "./chat.sol";

contract sbttest is ChainlinkClient, ConfirmedOwner {
    bytes32 private jobId;
    uint256 private fee;
    using Chainlink for Chainlink.Request;
    using Strings for uint256;
    using Strings for string;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    Counters.Counter private _communityIds;
    event Transfer(
        address indexed _from,
        address indexed _to,
        uint256 indexed _tokenId
    );
    struct SBT {
        string name;
        string data;
    }
    struct COM {
        uint256 id;
        string name;
        string symbol;
        address owner;
        address ctadd;
    }
    struct cpSBT {
        uint256 tokenid;
        uint256 comid;
        bool mint;
    }
    struct fbSBT {
        uint256 tokenid;
        address user;
        bool mint;
    }
    mapping (uint256 => bool[]) mintd;
    mapping (bytes4 => bool)  interfaced;
    mapping (uint256 => address) ownerd;
    mapping (uint256 => SBT) nft;
    mapping (uint256 => COM) com;
    mapping (bytes32 => fbSBT) sbtid;
    mapping (bytes32 => cpSBT) cpsbtid;
    mapping (address => uint256) balance;
    address chatCont;
    constructor() ConfirmedOwner(msg.sender){
        interfaced[(bytes4)(0x01ffc9a7)] = true;
        interfaced[(bytes4)(0x5b5e139f)] = true;
        setChainlinkToken(0x326C977E6efc84E512bB9C30f76E30c160eD06FB);
        setChainlinkOracle(0x40193c8518BB267228Fc409a613bDbD8eC5a97b3);
        chatCont = address(new chat(address(this)));
        jobId = '7da2702f37fd48e5b1b9a5715e3509b6';
        fee = (1 * LINK_DIVISIBILITY) / 10;
    }
    event createcommunity(uint256 indexed id,string indexed name,string symbol,address indexed owner);
    function createCommunity(string memory _name,string memory _symbol,address owner) public onlyOwner {
        _communityIds.increment();
        uint256 newCommunityId = _communityIds.current();
        community Community = new community(_name,_symbol,owner,newCommunityId,chatCont);
        com[newCommunityId].id = newCommunityId;
        com[newCommunityId].name = _name;
        com[newCommunityId].symbol = _symbol;
        com[newCommunityId].owner = owner;
        com[newCommunityId].ctadd = address(Community);
        emit createcommunity(newCommunityId,_name,_symbol,owner);
    }
    function mint(string memory _name) public returns (bytes32 requestId) {
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.Confirm.selector);
        require(balance[msg.sender] == 0,"not allow");
        uint256 newItemId = _tokenIds.current() + 1;
        nft[newItemId].name = _name;
        string memory link = uri("sbt",address(this),newItemId);
        req.add('get', link);
        req.add('path', 'data');
        bytes32 requestId = sendChainlinkRequest(req, fee);
        sbtid[requestId] = fbSBT(newItemId,msg.sender,true);
        return requestId;
    }
    event SBTConfirm(uint256 _tokenid);
    function Confirm(bytes32 _requestId, bytes memory _data) public recordChainlinkFulfillment(_requestId) {
        if (sbtid[_requestId].mint == true) {
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        ownerd[newItemId] = sbtid[_requestId].user;
        balance[sbtid[_requestId].user] = 1;
        emit Transfer(address(0), sbtid[_requestId].user, newItemId);
        }
        nft[sbtid[_requestId].tokenid].data = string(_data);
        emit SBTConfirm(sbtid[_requestId].tokenid);
    }
    function supportsInterface(bytes4 _interfaceID) public view returns (bool)
    {
        return interfaced[_interfaceID];
    }
    function name() public view returns (string memory) {
        return "TESTSBT";
    }
    function symbol() public view returns (string memory) {
        return "TSBT";
    }
    function mintCommunityPassport(uint256 _tokenId,uint256 comId) public returns (bytes32 requestId) {
        require(ownerOf(_tokenId) == msg.sender, "not allow");
        require(com[comId].id != 0);
        require(mintd[_tokenId][comId] == false);
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.setcpSBTConfirm.selector);
        req.add('get', uri("cpsbt",_viewCont(comId),_tokenId));
        req.add('path', 'data');
        bytes32 requestId = sendChainlinkRequest(req, fee);
        cpsbtid[requestId].tokenid = _tokenId;
        cpsbtid[requestId].comid = comId;
        cpsbtid[requestId].mint = true;
        return requestId;
    }
    function setSBT(uint256 _tokenId) public returns (bytes32 requestId){
        require(ownerOf(_tokenId) == msg.sender);
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.Confirm.selector);
        req.add('get', uri("sbt",address(this),_tokenId));
        req.add('path', 'data');
        bytes32 requestId = sendChainlinkRequest(req, fee);
        sbtid[requestId] = fbSBT(_tokenId,address(0),false);
        return requestId;
    }
    function setcpSBT(uint256 _tokenId,uint256 comId) public returns (bytes32 requestId){
        require(ownerOf(_tokenId) == msg.sender);
        require(com[comId].id != 0);
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.setcpSBTConfirm.selector);
        req.add('get', uri("cpsbt",_viewCont(comId),_tokenId));
        req.add('path', 'data');
        bytes32 requestId = sendChainlinkRequest(req, fee);
        cpsbtid[requestId].tokenid = _tokenId;
        cpsbtid[requestId].comid = comId;
        return requestId;
    }
    event setcpSBTDConfirm(uint256 comid,uint256 _tokenId);
    function setcpSBTConfirm(bytes32 _requestId, bytes memory _data) public recordChainlinkFulfillment(_requestId) {
        uint256 comid = cpsbtid[_requestId].comid;
        community Comu = community(com[comid].ctadd);
        uint256 tokenid = cpsbtid[_requestId].tokenid;
        if (cpsbtid[_requestId].mint == true) {
            Comu.mint(tokenid,string(_data));
            mintd[tokenid][comid] = true;
        } else {
            Comu.setData(tokenid,string(_data));
        }
        emit setcpSBTDConfirm(comid,tokenid);
    }
    function _viewName(uint256 _tokenId) public view returns (string memory) {
        return nft[_tokenId].name;
    }
    function _viewCont(uint256 communityId) public view returns (address) {
        return com[communityId].ctadd;
    }
    function tokenURI(uint256 _tokenId) public view returns (string memory) {
        return nft[_tokenId].data;
    }
    function ownerOf(uint256 _tokenId) public view returns (address) {
        return ownerd[_tokenId];
    }
    function balanceOf(address owner) public view returns (uint256) {
        return balance[owner];
    }
    function uri(string memory _type,address ct,uint256 tokenId) public view returns (string memory){
        return string(abi.encodePacked('http://api.fan-base.xyz/api/', _type ,'/' ,Strings.toHexString(uint256(uint160(ct)), 20), '/' , tokenId.toString()));
    }
