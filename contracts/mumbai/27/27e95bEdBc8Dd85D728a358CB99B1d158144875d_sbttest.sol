pragma solidity ^0.8.0;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./sbttest.sol";
contract community is Ownable {
    using Strings for uint256;
    using Strings for string;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    event Transfer(
        address indexed _from,
        address indexed _to,
        uint256 indexed _tokenId
    );
    struct NFT {
        string name;
        string data;
    }
    string named;
    string symbold;
    address ctowner;
    uint256 ctid;
    address chatcont;
    constructor(string memory _name,string memory _symbol,address _ctowner,uint256 _id,address _chatcont) {
        named = _name;
        symbold = _symbol;
        ctowner = _ctowner;
        ctid = _id;
        chatcont = _chatcont;
        interfaced[(bytes4)(0x01ffc9a7)] = true;
        interfaced[(bytes4)(0x5b5e139f)] = true;
    }
    mapping (bytes4 => bool)  interfaced;
    mapping (uint256 => address) ownerd;
    mapping (uint256 => NFT) nft;
    mapping (uint256 => uint256) pair;
    mapping (address => uint256) balance;
    event getCommunityPassport(uint256 indexed tokenId, address indexed user);
    function mint(uint256 _tokenId,string memory _data) public onlyOwner returns (uint256){
        sbttest proxy = sbttest(owner());
        address user = proxy.ownerOf(_tokenId);
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        ownerd[newItemId] = user;
        nft[newItemId].name = proxy._viewName(_tokenId);
        pair[_tokenId] = newItemId;
        balance[user] = 1;
        nft[newItemId].data = _data;
        emit Transfer(address(0), user , newItemId);
        emit getCommunityPassport(_tokenId,user);
        return newItemId;
    }
    function burn(uint256 _tokenId) public onlyOwner payable  {
        uint256 id = pair[_tokenId];
        balance[ownerd[id]] = 0;
        require(msg.sender == owner(),"SBT: OnlyOwner");
        address currentOwner = ownerd[id];
        delete ownerd[id];
        emit Transfer(currentOwner, address(0), _tokenId);
    }
    function communityId() public view returns (uint256) {
        return ctid;
    }
    function communityOwner() public view returns (address) {
        return ctowner;
    }
    function supportsInterface(bytes4 _interfaceID) public view returns (bool)
    {
        return interfaced[_interfaceID];
    }
    function name() public view returns (string memory) {
        return named;
    }
    function symbol() public view returns (string memory) {
        return symbold;
    }
    event setcpSBTD(uint256 indexed _tokenId);
    function setData(uint256 _tokenId,string memory data) public onlyOwner {
        uint256 id = pair[_tokenId];
        nft[id].data = data;
        emit setcpSBTD(_tokenId);
    }
    event ChatMessage(uint256 _tokenId,string name,bytes32 text);
    function chat(uint256 _tokenId,string memory _name,bytes32 text) public {
        require(msg.sender == chatcont);
        emit ChatMessage(_tokenId,_name,text);
    }
    function tokenURI(uint256 _tokenId) public view returns (string memory) {
        return nft[_tokenId].data;
    }
    function ownerOf(uint256 _tokenId) public view returns (address) {
        return ownerd[_tokenId];
    }
    function balanceOf(address owner) public view returns (uint256) {
        return balance[owner];
    }
}

