contract Escrow is Initializable, Signature {
    // using Address for address;
    // address admin;
    event ContractCreated(address indexed _from, address indexed _to);

    uint8 public current_milestone;
    bool dispute;
    uint256[] public milestones_amounts;
    // current milestone should be equal to 1 to start the contract
    // current value of milestone reflects the milestone in progress

    struct Parties {
        address client;
        address freelancer;
    }

    struct Escrow_account {
        Parties parties;
        address token; //for native token such as MATIC will be null
        address disputer;
        string content_id;
    }

    Escrow_account public account_info;

    // initlilaze the escrow account
    function init(
        address _client,
        address _freelancer,
        string memory cid,
        string memory _msg,
        uint256[] memory _milestones_amounts,
        address _token,
        bytes memory _clientSig,
        uint256 _clientSigTimestamp,
        bytes memory _freelancerSig,
        uint256 _freelancerSigTimestamp
    ) public initializer {
        require(_client != _freelancer, "address can't be same !");
        require(
            _client != address(0) && _freelancer != address(0),
            "addresses can't be zero"
        );
        require(
            _token == address(0) || isContract(_token),
            "Not a valid token address"
        );
        // require(cid.length > 0,"Not Valid");
        require(
            verify(_client, _msg, _clientSigTimestamp, _clientSig),
            "Client is not agreeing"
        );

        // check that client is agreeing with the agreement
        require(
            verify(_freelancer, _msg, _freelancerSigTimestamp, _freelancerSig),
            "freelancer is not agreeing"
        );

        // check address aren't zero/same
        // check inittime < endtime
        // check content_id is not null
        // create the account object with send values and default ones

        account_info.parties.client = _client;
        account_info.parties.freelancer = _freelancer;
        account_info.content_id = cid;
        milestones_amounts = _milestones_amounts;
        account_info.token = _token;

        emit ContractCreated(_client, _freelancer);
    }

    // function to return the current balance in escrow
    function balance() public view returns (uint256) {
        if (account_info.token == address(0)) {
            return address(this).balance;
        } else {
            return IERC20(account_info.token).balanceOf(address(this));
        }
    }

    // function to make the payment;
    function payment(uint256 _amount) public payable {
        if (account_info.token == address(0)) {
            require(msg.value >= _amount && _amount > 0, "can't be less");
        } else {
            bytes memory data = abi.encodeWithSignature(
                "transferFrom(address,address,uint256)",
                msg.sender,
                address(this),
                _amount
            );

            // call the token contract to transfer the tokens
            (bool success, bytes memory returndata) = account_info.token.call{
                value: msg.value
            }(data);
            verifyCallResultFromTarget(
                account_info.token,
                success,
                returndata,
                "failed"
            );
        }

        // start the contract
        if (current_milestone == 0) {
            current_milestone = 1;
        }
    }

    // function to mark the contract dispute [only client and freelancer]
    function mark_dispute() external {
        require(
            msg.sender == account_info.parties.client ||
                msg.sender == account_info.parties.freelancer,
            "Not a valid party"
        );
        require(!dispute, "Marked disputed !");
        dispute = true;
    }

    // Only client can update the milestone and the payment of the milestone will be released to the freelancer.
    // The next milestone will be started

    function update_milestone() public {
        // only client can update the milestone
        require(msg.sender == account_info.parties.client, "! Not Client");

        // the escrow should have balance of available milestone
        if (account_info.token == address(0)) {
            require(
                address(this).balance >=
                    milestones_amounts[current_milestone - 1],
                "Address: insufficient balance for call"
            );
            _withdraw(
                account_info.parties.freelancer,
                milestones_amounts[current_milestone - 1]
            );
            current_milestone++;
        } else {
            require(
                IERC20(account_info.token).balanceOf(address(this)) >=
                    milestones_amounts[current_milestone - 1],
                "not sufficent amount"
            );
            _withdraw(
                account_info.parties.freelancer,
                milestones_amounts[current_milestone - 1]
            );
            current_milestone++;
        }
    }

    // function to upgrade the milestones [Only client but needs freelancers'approval]
    function upgrade_milestones(
        uint8[] memory _index,
        uint256[] memory _milestones_amounts,
        string memory _msg,
        bytes memory _freelancerSig,
        uint256 _freelancerSigTimestamp
    ) external {
        // the sender should be client
        require(msg.sender == account_info.parties.client, "Not the client");

        // freelancer should be agreeing for the change
        require(
            verify(
                account_info.parties.freelancer,
                _msg,
                _freelancerSigTimestamp,
                _freelancerSig
            ),
            "freelancer is not agreeing"
        );
        require(_index.length == _milestones_amounts.length, "Should be equal");

        for (uint8 i = 0; i < _index.length; ++i) {
            // if index is greater than the current ones add new ones

            require(
                _index[i] >= current_milestone,
                "Can't update a past milestone"
            );

            if (_index[i] > (milestones_amounts.length - 1)) {
                milestones_amounts.push(_milestones_amounts[i]);
            }
            // update the index
            else {
                milestones_amounts[_index[i]] = _milestones_amounts[i];
            }
        }
    }

    // check both methods (pass as argument, load from storage) to make sure this one is gas efficient
    function _withdraw(address _payee, uint256 _amount) internal {
        if (account_info.token == address(0)) {
            // check that user has sufficient amount before withdraw
            require(balance() >= _amount, "Not sufficent amount");
            // send the money
            (bool success, bytes memory data) = payable(_payee).call{
                value: _amount
            }("");
            verifyCallResult(success, data, "failed");
        } else {
            // check that user has sufficient amount before withdraw
            require(balance() >= _amount, "Not sufficent amount");

            // send the money
            bytes memory data = abi.encodeWithSignature(
                "transfer(address,uint256)",
                _payee,
                _amount
            );

            (bool success, bytes memory returndata) = account_info.token.call{
                value: msg.value
            }(data);
            verifyCallResultFromTarget(
                account_info.token,
                success,
                returndata,
                "failed"
            );
            // Address.functionCall(account_info.token,data);
        }
    }

    // withdraw the money (dispute [by disputer])
    // Disputer amount needs to be implemented
    function withdraw_by_disputer(address _payee, uint256 _amount) public {
        // the receiver must be freelancer or client
        require(dispute, "not disputed");
        require(
            account_info.disputer != address(0) &&
                msg.sender == account_info.disputer,
            "Not the disputer"
        );
        require(
            _payee == account_info.parties.client ||
                _payee == account_info.parties.freelancer,
            "Not a valid party"
        );
        // for example using 5 % fee for the disputer
        uint256 _disputer_fee = (_amount * 5) / 100;
        _amount = (_amount - (_amount * 5) / 100);

        // pay to the party
        _withdraw(_payee, _amount);
        // pay the disputer
        _withdraw(msg.sender, _disputer_fee);
    }

    // set disputer for the contract
    function set_disputer(
        address payable _disputer,
        string memory _msg,
        bytes memory _clientSig,
        uint256 _clientSigTimestamp,
        bytes memory _freelancerSig,
        uint256 _freelancerSigTimestamp
    ) external {
        // check that both are the real parties and agreeing
        require(
            _disputer != account_info.parties.client &&
                _disputer != account_info.parties.freelancer,
            "Failed"
        );

        require(
            verify(
                account_info.parties.client,
                _msg,
                _clientSigTimestamp,
                _clientSig
            ),
            "Client is not agreeing"
        );
        require(
            verify(
                account_info.parties.freelancer,
                _msg,
                _freelancerSigTimestamp,
                _freelancerSig
            ),
            "freelancer is not agreeing"
        );
        require(_disputer != address(0));
        // parties can't be the disputer
        require(dispute, "Not Disputed");
        account_info.disputer = _disputer;
    }

    // // function to delete the contract data
    // function clear_data() external onlyOwner{
    //     if(account_info.token == address(0)){
    //         require(address(this).balance == 0,"Balance is not zero");
    //         delete account_info;
    //         // delete current_milestone;
    //         // delete dispute;
    //         // delete milestones_amounts;
    //         // delete Empty;
    //       }else{
    //         require(balance() == 0,"Balance is not zero");
    //         delete account_info;
    //         // delete current_milestone;
    //         // delete dispute;
    //         // delete milestones_amounts;
    //         // delete Empty;
    //       }
    // }

    function isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }

    function verifyCallResultFromTarget(
        address target,
        bool success,
        bytes memory returndata,
        string memory errorMessage
    ) internal view returns (bytes memory) {
        if (success) {
            if (returndata.length == 0) {
                // only check isContract if the call was successful and the return data is empty
                // otherwise we already know that it was a contract
                require(isContract(target), "Address: call to non-contract");
            }
            return returndata;
        } else {
            _revert(returndata, errorMessage);
        }
    }

    function verifyCallResult(
        bool success,
        bytes memory returndata,
        string memory errorMessage
    ) internal pure returns (bytes memory) {
        if (success) {
            return returndata;
        } else {
            _revert(returndata, errorMessage);
        }
    }

    function _revert(bytes memory returndata, string memory errorMessage)
        private
        pure
    {
        // Look for revert reason and bubble it up if present
        if (returndata.length > 0) {
            // The easiest way to bubble the revert reason is using memory via assembly
            /// @solidity memory-safe-assembly
            assembly {
                let returndata_size := mload(returndata)
                revert(add(32, returndata), returndata_size)
            }
        } else {
            revert(errorMessage);
        }
    }
}

