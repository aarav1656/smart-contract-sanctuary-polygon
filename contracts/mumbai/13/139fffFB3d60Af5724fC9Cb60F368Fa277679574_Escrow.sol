contract Escrow is Initializable,Signature,ReentrancyGuard{

    event ContractCreated(address indexed _from, address indexed _to);
    event WithdrawlSuccessful(address indexed _from, address indexed _to, uint _amount);

    // global variables
    address public protocol;
    uint protocol_fee;
    bool approved;
    uint8 public current_milestone;
    uint _init_time;
    bool dispute;
    bool paying;
    uint [] public milestones_amounts;

    // current milestone should be equal to 1 to start the contract
    // current value of milestone reflects the milestone in progress
    
    using SafeMath for uint256;

    struct Parties{
        address client;
        address freelancer;
    }

    enum PaymentType{
        Flat,
        Milestone_based,
        Recurring
    }

    struct EscrowAccount{
        Parties parties;
        address token; //for native token such as MATIC will be null
        address disputer;
        string content_id;
        PaymentType payment_type;
        uint timesframe;
      }


    EscrowAccount public accountInfo;

    function updateApproval(bool _newstatus) external{
        require(msg.sender == accountInfo.parties.client, "! Not Client");
        approved = _newstatus;
    }

    //initlilaze the escrow account
    function initContract(
        address [] memory _address,
        string memory cid,
        string memory _msg,
        uint [] memory _milestones_amounts,
        bytes [] memory _signatures,
        uint [] memory _timestamps,
        uint _protocol_fee,
        PaymentType _payment_type,
        uint _timeframe
      ) public initializer{

        /**
         * Following conditions will be checked
         * address aren't zero/same
         * check content_id is not null
         * Signatures are valid
         * Send token address should be a contract
         */
        require(_address[0] != _address[1],"address can't be same !");
        require(_address[0] != address(0) && _address[1] != address(0),"addresses can't be zero");
        require(_address[2] == address(0) || isContract(_address[2]),"Not a valid token address");
        require(bytes(cid).length > 0," !Not Valid length");
        require(verify(_address[0],_msg,_timestamps[0],_signatures[0]),"Client is not agreeing");

        require(verify(_address[1],_msg,_timestamps[1],_signatures[1]),"freelancer is not agreeing");
        
        /**assign the variables in the contract*/

        accountInfo.parties.client = _address[0];
        accountInfo.parties.freelancer = _address[1];
        accountInfo.content_id = cid;
        milestones_amounts = _milestones_amounts;
        accountInfo.token = _address[2];
        accountInfo.payment_type = _payment_type;

        /**The timeframe should be in unix timestamp format */
        accountInfo.timesframe = _timeframe;

        /**set the protocol variables */
        protocol = _address[3];
        protocol_fee = _protocol_fee;

        /**Emit the contract created event */
        emit ContractCreated(_address[0], _address[1]);            

    }

    
    /* function to return the current balance in escrow */
    function balance() public view returns(uint){
        if(accountInfo.token == address(0)){
            return address(this).balance;
        }else{
            return IERC20(accountInfo.token).balanceOf(address(this));
        }
    }

    /* function to make the payment*/
    function payment(uint _amount) public payable{

        uint _protocol_fee;

        if(accountInfo.token == address(0)){
                 /* calculate the fee for the protocol [current 2.5%] */
                 _protocol_fee = _amount.mul(protocol_fee).div(1000);
                //  _protocol_fee = (_amount*protocol_fee)/1000;
                /* Protocol Fee amount */
                console.log("Protocol Fee : ",_protocol_fee);
                require(msg.value == _amount.add(_protocol_fee) && _amount > 0,"can't be less");
                
        }else{
            /**Get the equivalent MATIC of the send amount */
            /**Get the MATIC/USD Price from Oracle */
            uint256 _price = 111232670; /**the price will be sent with 8 decimals */
           
            /**Calculate the protocol Fee */
            _protocol_fee = (((_amount.mul(10**26)).div(_price)).mul(protocol_fee)).div(1000);
            console.log("protocol fee with safe math : ",_protocol_fee);

            /**Check the Fee has been send with required allowance */            
            require(msg.value >= _protocol_fee,"! Pay the fee");
            require(IERC20(accountInfo.token).allowance(msg.sender,(address(this))) >= _amount,"Not enough allowance");
             bytes memory data = abi.encodeWithSignature("transferFrom(address,address,uint256)",msg.sender,address(this),_amount);

            /* call the token contract to transfer the tokens */
            (bool _success, bytes memory returndata) = accountInfo.token.call{value:0}(data);
            verifyCallResultFromTarget(accountInfo.token, _success, returndata,"failed");
        }
        
        /* sent the fee to the protocol */
        (bool success,) = payable(protocol).call{value:(_protocol_fee)}("");
        require(success,"failed to transfer");
        /**Start the contract*/
        if(current_milestone == 0){
            if(accountInfo.payment_type == PaymentType.Recurring){                
                _init_time = block.timestamp;
            }
            current_milestone = 1;
        }
    }

    function _withdraw(address _payee, uint _amount) internal{
         if(accountInfo.token == address(0)){
            // check that user has sufficient amount before withdraw
            require(balance() >= _amount,"Not sufficent amount");
            // send the money 
             (bool success, bytes memory data) = 
                payable(_payee).call{value:_amount}("");
                verifyCallResult(success,data,"failed");
          }else{
            // check that user has sufficient amount before withdraw
            require(balance() >= _amount,"Not sufficent amount");

            // send the money
            bytes memory data = abi.encodeWithSignature(
                "transfer(address,uint256)",
                _payee,_amount
            );

            (bool success, bytes memory returndata) = accountInfo.token.call{value:msg.value}(data);
            verifyCallResultFromTarget(accountInfo.token, success, returndata,"failed");
            // Address.functionCall(accountInfo.token,data);
          }
          /**Emit the event for successful withdrawl */
          emit WithdrawlSuccessful(msg.sender,_payee,_amount);
    }

    /**Release the payment  [add reentrancy guard here]*/
    function releasePayment(uint _amount) public nonReentrant{

        /**Check the Payment type
         * For Flat Payments
         */
        require(!dispute,"! Disputed ");

        if(accountInfo.payment_type == PaymentType.Flat){
            require(msg.sender == accountInfo.parties.client,"! Not Client");
            _withdraw(accountInfo.parties.freelancer,_amount);
        }

        /**For Milestone based payments*/
        if(accountInfo.payment_type == PaymentType.Milestone_based){
            require(msg.sender == accountInfo.parties.client, "! Not Client");
            if(current_milestone != milestones_amounts.length){
                require(balance() >= milestones_amounts[current_milestone],"Add money to update the milestone");
                _withdraw(accountInfo.parties.freelancer,milestones_amounts[current_milestone-1]);
                current_milestone++;
                return;
            }
                require(balance() >= milestones_amounts[current_milestone-1],"Add money to update the milestone");
                _withdraw(accountInfo.parties.freelancer,milestones_amounts[current_milestone-1]);
        }
        
        /**For Recurring payments */
        if(accountInfo.payment_type == PaymentType.Recurring){
            if(approved){
                require(block.timestamp >= _init_time + accountInfo.timesframe," ! Not passed the time");
            }else{
                require(msg.sender == accountInfo.parties.client, "! Not Client");
            }
            _init_time = block.timestamp;
            _withdraw(accountInfo.parties.freelancer,milestones_amounts[current_milestone-1]);
            current_milestone++;
        }
    }

    // function to mark the contract dispute [only client and freelancer]
    function markDispute() external{
            require(msg.sender == accountInfo.parties.client || msg.sender == accountInfo.parties.freelancer,"Not a valid party");
            require(!dispute,"Marked disputed !");               
            dispute = true;
    }

    /* function to upgrade the milestones [Only client but needs freelancers'approval]*/
    function upgradeMilestones(
        uint8 [] memory _index,
        uint [] memory _milestones_amounts,
        string memory _msg,
        bytes memory _freelancerSig,
        uint _freelancerSigTimestamp
    ) external{
        // the sender should be client
        require(msg.sender == accountInfo.parties.client,"Not the client");

        // freelancer should be agreeing for the change
        require(verify(accountInfo.parties.freelancer,_msg,_freelancerSigTimestamp,_freelancerSig),"freelancer is not agreeing");
        require(_index.length == _milestones_amounts.length, "Should be equal");


        for(uint8 i = 0; i < _index.length; ++i){
            // if index is greater than the current ones add new ones
            
            require(_index[i] >= current_milestone && _milestones_amounts[i] > 0 , "Not Upgradable");
            if(_index[i] > (milestones_amounts.length-1)){
                milestones_amounts.push(_milestones_amounts[i]);
            } 
            // update the index 
            else{
                milestones_amounts[_index[i]] = _milestones_amounts[i];
            }
        }


    }

    /*withdraw the money (dispute [by disputer])
        Disputer amount needs to be implemented
     */
    
    function withdrawByDisputer(address _payee, uint _amount) public nonReentrant{
        // the receiver must be freelancer or client
        require(dispute,"not disputed");
        require(accountInfo.disputer != address(0) && msg.sender == accountInfo.disputer,"Not the disputer");
        require(_payee == accountInfo.parties.client || _payee == accountInfo.parties.freelancer,"Not a valid party");
            // for example using 5 % fee for the disputer
            uint _disputer_fee = (_amount.mul(5)).div(100);
            _amount = _amount.sub(_disputer_fee);

            console.log("disputer Fee : ",_disputer_fee);
            // pay to the party
            _withdraw(_payee,_amount);
            // pay the disputer
            _withdraw(msg.sender,_disputer_fee);
    }

    // set disputer for the contract
    function setDisputer(
            address payable _disputer,
            string memory _msg,
            bytes memory _clientSig,
            uint _clientSigTimestamp,
            bytes memory _freelancerSig,
            uint _freelancerSigTimestamp
    ) external{
        // check that both are the real parties and agreeing 
        require(_disputer != accountInfo.parties.client && _disputer != accountInfo.parties.freelancer,"Failed");
        
        require(verify(accountInfo.parties.client,_msg,_clientSigTimestamp,_clientSig),"Client is not agreeing");
        require(verify(accountInfo.parties.freelancer,_msg,_freelancerSigTimestamp,_freelancerSig),"freelancer is not agreeing");
        require(_disputer != address(0));
        // parties can't be the disputer
        require(dispute,"Not Disputed");
        accountInfo.disputer = _disputer;
  }

    function isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }

      function verifyCallResultFromTarget(
        address target,
        bool success,
        bytes memory returndata,
        string memory errorMessage
    ) internal view returns (bytes memory) {
        if (success) {
            if (returndata.length == 0) {
                // only check isContract if the call was successful and the return data is empty
                // otherwise we already know that it was a contract
                require(isContract(target), "Address: call to non-contract");
            }
            return returndata;
        } else {
            _revert(returndata, errorMessage);
        }
    }

    function verifyCallResult(
            bool success,
            bytes memory returndata,
            string memory errorMessage
                ) internal pure returns (bytes memory) {
            if (success) {
                return returndata;
            } else {
                _revert(returndata, errorMessage);
            }
        }

    function _revert(bytes memory returndata, string memory errorMessage) private pure {
        // Look for revert reason and bubble it up if present
        if (returndata.length > 0) {
            // The easiest way to bubble the revert reason is using memory via assembly
            /// @solidity memory-safe-assembly
            assembly {
                let returndata_size := mload(returndata)
                revert(add(32, returndata), returndata_size)
            }
        } else {
            revert(errorMessage);
        }
    }


}

