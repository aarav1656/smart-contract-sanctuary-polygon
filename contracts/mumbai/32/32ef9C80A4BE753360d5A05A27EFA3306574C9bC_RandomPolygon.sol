pragma solidity 0.8.17;

import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract RandomPolygon is VRFConsumerBaseV2, AccessControlEnumerable {
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    VRFCoordinatorV2Interface private COORDINATOR;
    LinkTokenInterface private LINKTOKEN;
    bytes32 private keyHash;
    uint64 private immutable s_subscriptionId;
    uint16 private immutable requestConfirmations = 3;
    uint32 private immutable callbackGasLimit = 1000000;

    struct Participant {
        address account;
        bytes32 twitterId;
        bytes32 twitterUsername;
        bytes32 discordId;
        bytes32 discordUsername;
        address mainWallet;
        uint256 timeRegistration;
    }

    struct Lot {
        uint256 lotId;
        address lotOwner;
        Participant[] participants;
    }

    struct WinningLot {
        uint256 id;
        Participant participant;
    }

    mapping(address => mapping(uint256 => Participant[])) private _lotOwnerToIdLotToInfo;
    mapping(address => mapping(uint256 => WinningLot)) private _lotOwnerToIdLotToWinningLot;
    mapping(uint256 => Lot) private _lotteryRandomnessRequest;

    event ParticipantAdded(uint256 lotId, address lotOwner, Participant participant);
    event LotteryStarted(uint256 indexed lotId, address indexed lotOwner, Participant[] participant, uint256 requestId);
    event WinnerDetermined(uint256 indexed lotId, address indexed lotOwner);

    function getParticipantsByLotOwnerAndLotId(uint256 lotId, address lotOwner) public view returns (Participant[] memory) {
        return _lotOwnerToIdLotToInfo[lotOwner][lotId];
    }

    function getWinningLotByLotOwnerAndLotId(uint256 lotId, address lotOwner) public view returns (WinningLot memory) {
        return _lotOwnerToIdLotToWinningLot[lotOwner][lotId];
    }

    function getParticipantByLotIdAndLotOwner(
        address lotOwner,
        uint256 lotId,
        address account,
        bytes32 twitterUsername,
        bytes32 discordUsername
    ) public view returns (Participant memory participantInfo) {
        require(_lotOwnerToIdLotToInfo[lotOwner][lotId].length != 0, "No participants");

        Participant[] memory allParticipant = _lotOwnerToIdLotToInfo[lotOwner][lotId];

        if (account != address(0)) {
            for (uint256 i = 0; i < allParticipant.length; i++) {
                if (allParticipant[i].account == account) {
                    participantInfo = allParticipant[i];
                }
            }
        } else if (twitterUsername != bytes32(0)) {
            for (uint256 i = 0; i < allParticipant.length; i++) {
                if (allParticipant[i].twitterUsername == twitterUsername) {
                    participantInfo = allParticipant[i];
                }
            }
        } else if (discordUsername != bytes32(0)) {
            for (uint256 i = 0; i < allParticipant.length; i++) {
                if (allParticipant[i].discordUsername == discordUsername) {
                    participantInfo = allParticipant[i];
                }
            }
        } else {
            revert("Participants not found");
        }
    }

    function getParticipants(
        address lotOwner,
        uint256 lotId,
        uint256 offset,
        uint256 limit
    ) public view returns (Participant[] memory participants) {
        if (_lotOwnerToIdLotToInfo[lotOwner][lotId].length == 0) return new Participant[](0);
        uint256 participantsCount = _lotOwnerToIdLotToInfo[lotOwner][lotId].length;
        if (offset > participantsCount) return new Participant[](0);
        uint256 to = offset + limit;
        if (participantsCount < to) to = participantsCount;
        participants = new Participant[](to - offset);
        for (uint256 i = 0; i < participants.length; i++) participants[i] = _lotOwnerToIdLotToInfo[lotOwner][lotId][offset + i];
    }

    function isParticipated(
        address lotOwner,
        uint256 lotId,
        address account,
        bytes32 twitterId,
        bytes32 discordId,
        address mainWallet
    ) public view returns (bool partAccount, bool partTwitter, bool partDiscord, bool partWallet) {
        for (uint256 i; i < _lotOwnerToIdLotToInfo[lotOwner][lotId].length; i++) {
            if (account == _lotOwnerToIdLotToInfo[lotOwner][lotId][i].account) {
                partAccount = true;
            }
            if (twitterId == _lotOwnerToIdLotToInfo[lotOwner][lotId][i].twitterId) {
                partTwitter = true;
            }
            if (discordId == _lotOwnerToIdLotToInfo[lotOwner][lotId][i].discordId) {
                partDiscord = true;
            }
            if (mainWallet == address(0)) {
                partWallet = false;
            } else if (_lotOwnerToIdLotToInfo[lotOwner][lotId][i].mainWallet == mainWallet) {
                partWallet = true;
            }
        }
        return (partAccount, partTwitter, partDiscord, partWallet);
    }

    function giveawayParticipantsCount(
        address lotOwner,
        uint256 lotId
    ) public view returns (uint256 count, address winner, bytes32 twitter, bytes32 discord) {
        return (
            _lotOwnerToIdLotToInfo[lotOwner][lotId].length,
            _lotOwnerToIdLotToWinningLot[lotOwner][lotId].participant.account,
            _lotOwnerToIdLotToWinningLot[lotOwner][lotId].participant.twitterUsername,
            _lotOwnerToIdLotToWinningLot[lotOwner][lotId].participant.discordUsername
        );
    }

    function giveawaysParticipantsCount(address lotOwner, uint256[] memory array) public view returns (uint256[] memory maxParticipants) {
        uint256[] memory IdArray = new uint256[](array.length);
        for (uint256 i; i < array.length; i++) {
            IdArray[i] = _lotOwnerToIdLotToInfo[lotOwner][array[i]].length;
        }
        return IdArray;
    }

    constructor(bytes32 _keyHash, uint64 subscriptionId, address _vrfCoordinator, address _link) VRFConsumerBaseV2(_vrfCoordinator) {
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        LINKTOKEN = LinkTokenInterface(_link);
        s_subscriptionId = subscriptionId;
        keyHash = _keyHash;
        address deployer = msg.sender;
        _grantRole(DEFAULT_ADMIN_ROLE, deployer);
        _grantRole(ADMIN_ROLE, deployer);
    }

    function addAdmin(address account) external onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        _grantRole(ADMIN_ROLE, account);
        return true;
    }

    function setWinner(address lotOwner, uint256 lotId) external onlyRole(ADMIN_ROLE) returns (bool) {
        require(lotOwner != address(0), "LotOwner eq zero address");
        require(_lotOwnerToIdLotToInfo[lotOwner][lotId].length != 0, "No participants");
        uint256 s_requestId = COORDINATOR.requestRandomWords(keyHash, s_subscriptionId, requestConfirmations, callbackGasLimit, 1);
        Lot storage lot = _lotteryRandomnessRequest[s_requestId];
        lot.lotId = lotId;
        lot.lotOwner = lotOwner;
        lot.participants = _lotOwnerToIdLotToInfo[lotOwner][lotId];
        emit LotteryStarted(lotId, lotOwner, _lotOwnerToIdLotToInfo[lotOwner][lotId], s_requestId);
        return true;
    }

    function addParticipant(
        address account,
        bytes32 twitterId,
        bytes32 twitterUsername,
        bytes32 discordId,
        bytes32 discordUsername,
        address mainWallet,
        address lotOwner,
        uint256 lotId
    ) external onlyRole(ADMIN_ROLE) returns (bool) {
        Participant memory participant = Participant(
            account,
            twitterId,
            twitterUsername,
            discordId,
            discordUsername,
            mainWallet,
            block.timestamp
        );
        _lotOwnerToIdLotToInfo[lotOwner][lotId].push(participant);
        emit ParticipantAdded(lotId, lotOwner, participant);
        return true;
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomness) internal override {
        Lot memory lot = _lotteryRandomnessRequest[requestId];
        uint256 numberParticipants = lot.participants.length;
        uint256 winnerIndex = (randomness[0] % numberParticipants);
        _lotOwnerToIdLotToWinningLot[lot.lotOwner][lot.lotId].id = winnerIndex;
        _lotOwnerToIdLotToWinningLot[lot.lotOwner][lot.lotId].participant = lot.participants[winnerIndex];
        emit WinnerDetermined(lot.lotId, lot.lotOwner);
        delete _lotteryRandomnessRequest[requestId];
    }
}

