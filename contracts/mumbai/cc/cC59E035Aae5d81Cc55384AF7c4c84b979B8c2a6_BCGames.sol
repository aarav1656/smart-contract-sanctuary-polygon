contract BCGames is Ownable {
    // TODO: Where is right place for these???
    // TODO: Go back to matching name of contract?
    CharInterface public chars;
    UtilsInterface public utils;
    MapInterface public maps;
    EventsInterface public bcEvents;
    ItemsInterface public items;
    PlayersInterface public players;

    address public lobbiesAddress;
    address public charsAddress;
    address public actionsAddress;

    // BCChars bcChars;
    // Maps bcMaps;

    function setContractAddresses(
        address _charAddress,
        address _utilsAddress,
        address _mapsAddress,
        address _lobbiesAddress,
        address _actionsAddress,
        address _eventsAddress,
        address _itemsAddress,
        address _playersAddress)
        public onlyOwner {
        chars = CharInterface(_charAddress);
        // bcChars = BCChars(_charAddress);
        utils = UtilsInterface(_utilsAddress);
        maps = MapInterface(_mapsAddress);
        // bcMaps = Maps(_mapsAddress);
        lobbiesAddress = _lobbiesAddress;
        actionsAddress = _actionsAddress;
        bcEvents = EventsInterface(_eventsAddress);
        items = ItemsInterface(_itemsAddress);
        players = PlayersInterface(_playersAddress);
    }

    uint constant DEFAULT_ACTIONS_NUM = 3;

    uint8 constant SPACE_ROOM = 0;

    BCTypes.Position public StartLocation = BCTypes.Position(10, 6); // TODO: duped on maps

    // BCTypes.Player[] public players;

    BCTypes.Game[] public games;

    mapping (address => uint[]) public addressToGame;

    constructor() {
        // TODO: CRITICAL!!! BURN PLAYER 0
    }

    function startGame(uint[] memory _playerIndexes, uint _numPlayers, uint _debugRoomId) public returns (uint) {
        // TODO: CRITICAL -> Add tests to confirm require works
        // TODO: Add turn and overall time limits for game
        require(msg.sender == lobbiesAddress, "Not lobby contract");
        BCTypes.Game storage newGame = games.push();
        newGame.active = true;
        newGame.turnsTaken = 0;
        newGame.playerIndexes = _playerIndexes;
        newGame.numPlayers = _numPlayers;
        newGame.eventTracker = BCTypes.EventTracker(0, 0, 0, 0);

        newGame.mapContract = address(maps);

        // TODO: CRITICAL -> Remove DEBUG
        if (_debugRoomId == 0) {
            newGame.mapId = maps.createMap();
        } else {
            newGame.mapId = maps.debugBuildMapOneRoom(_debugRoomId);
        }
        // Move the players to the start // TODO: Hardcoded
        // And map the owners to this game
        for (uint i = 0; i <= _numPlayers-1; i++) {
            // Warning: using _movePlayer here will attempt to discover, etc.
            _movePlayer(newGame.mapId, StartLocation, newGame.playerIndexes[i]);
            address joiner = players.extGetPlayerOwner(_playerIndexes[i]);
            addressToGame[joiner].push(games.length-1);
        }

        // TODO DEAL WITH ITEMS

        // TODO: Player who pays to start gets to go first?
        // TODO: Event
        return games.length-1;
    }
                                        // TODO: Will be used to add choice later, ie drop items but no risk of death
    function resolveEvent(uint _gameId, uint _choice) public {
        // TODO: CRITICAL -> Require msg.sender to be player owner
        BCTypes.Game storage eventGame = games[_gameId];
        players.extChooseEffect(_choice, eventGame.eventPlayerId, _gameId, eventGame.mapId, eventGame.eventPosition, eventGame.eventNumber, eventGame.eventIsTile);

        eventGame.eventPlayerId = 0;
        eventGame.eventNumber = 0;
        eventGame.eventIsTile = false; // TODO: Could save gas by not writing this
    }

    function endPlayerAction(uint _gameId, uint _playerId) public {
        require(msg.sender == actionsAddress, "Not actions contract");

        players.incrementPlayerActions(_playerId);
        BCTypes.Player memory currentPlayer = players.extGetPlayer(_playerId);
        BCTypes.Game storage currentGame = games[_gameId];

        // TODO: Revise this to handle manual passing the turn
        if (currentPlayer.actionsTaken >= DEFAULT_ACTIONS_NUM) {
            // TODO: Get items and abilities for actions
            // TODO: This will prevent free actions after last
            // TODO: Add FE and BE mechanisim to have a default checkbox "Pass the turn"
            players.resetPlayerActions(_playerId);
            if (currentGame.currentPlayerTurnIndex < currentGame.playerIndexes.length-1) {
                currentGame.currentPlayerTurnIndex++;
            } else {
                _endPlayerRound(_gameId, currentGame.mapId);
                currentGame.currentPlayerTurnIndex = 0;
            }
        }
    }

    function _endPlayerRound(uint _gameId, uint _mapId) internal {
        // TODO: All denizen logic
        BCTypes.Game storage currentGame = games[_gameId];
        // TODO: Consider shuffling the index here

        maps.resetOpenDoors(_mapId);
        // TODO: Add ability to change distance for endgame
        maps.extSpreadHazards(_mapId, 1);
        // TODO: Hazards should damage denizens

        players.applyHazardDamage(currentGame.mapId, currentGame.playerIndexes);
    }

    function extMovePlayer(uint _gameId, BCTypes.Position memory _position, uint _playerId) public {
        require((msg.sender == actionsAddress || msg.sender == address(players)), "Not actions/players contract");
        _movePlayer(_gameId, _position, _playerId);
    }

    function _movePlayer(uint _gameId, BCTypes.Position memory _position, uint _playerId) internal {

        // extMovePlayer returns true if the player was killed by moving into space
        if(!players.extMovePlayer(_position, _playerId, games[_gameId].mapId)) {
            // Discover room and process event, if undiscovered
            // If roomId is 0, room was already discovered (it's not space)
            // TODO: This is confusing
            uint discoveredRoomId = maps.discoverRoom(games[_gameId].mapId, _position.row, _position.col);
            if (discoveredRoomId > 2) {
                BCTypes.RoomTile memory discoveredRoom = maps.extGetRoomById(games[_gameId].mapId, discoveredRoomId);
                // TODO: I could save function bytes4 representations in a mapping then call iterators with this.call
                // Increment the event tracker
                if (discoveredRoom.eventType == BCTypes.BCEventType.BUG) {
                    games[_gameId].eventTracker.bugEvents++;
                }
                if (discoveredRoom.eventType == BCTypes.BCEventType.MYSTERY) {
                    games[_gameId].eventTracker.mysteryEvents++;
                }
                if (discoveredRoom.eventType == BCTypes.BCEventType.SCAVENGER) {
                    games[_gameId].eventTracker.scavEvents++;
                }
                if (discoveredRoom.eventType == BCTypes.BCEventType.SHIP_SECURITY) {
                    games[_gameId].eventTracker.shipEvents++;
                }

                // TODO: Process in-room event or draw event
                if (discoveredRoom.eventNum > 0) {
                    // Set the in-room event ONLY if this is not a permanant event
                    BCEvents.BCEvent memory currentEvent = bcEvents.extGetRoomEvent(discoveredRoom.eventNum);
                    if(!currentEvent.permanent) {
                        games[_gameId].eventPlayerId = _playerId;
                        games[_gameId].eventNumber = discoveredRoom.eventNum;
                        games[_gameId].eventIsTile = true;
                        games[_gameId].eventPosition = _position;
                    }
                } else if (discoveredRoom.eventType != BCTypes.BCEventType.NONE) {
                    // Draw a card for an event
                    // This also covers rooms like the medbay
                }
            }
        }
        // TODO: Emit event
    }

    function extGetGamePlayerIndexes(uint _gameId) public view returns (uint[] memory) {
        uint[] memory indexes = new uint[](games[_gameId].playerIndexes.length);
        for (uint i = 0; i < indexes.length; i++) {
            indexes[i] = games[_gameId].playerIndexes[i];
        }
        return indexes;
    }

    function extGetGame(uint _id) public view returns(BCTypes.Game memory) {
        return games[_id];
    }

    function extGetGamesOfPlayer(address _playerAddress) public view returns (uint[] memory) {
        uint[] memory gamesList = new uint[](addressToGame[_playerAddress].length);

        for (uint i = 0; i < gamesList.length; i++) {
            gamesList[i] = addressToGame[_playerAddress][i];
        }

        return gamesList;
    }
}

