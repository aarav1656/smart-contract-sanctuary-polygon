pragma solidity ^0.8.17;

// solhint-disable max-line-length

import {TransferHelper} from "../../uniswap/libraries/TransferHelper.sol";
import {IProxy} from "../../interfaces/IProxy.sol";
import "../../../lekker/factory/ProxyDeployer.sol";
import "./ILekkerAavePoolInitialize.sol";
import "./ILekkerEulerPoolInitialize.sol";
import {EnumerableSet, LekkerFactoryStorageGenesis} from "../../factory/LekkerFactoryStorage.sol";

// the Goerli deployment allows multiple admins
contract LekkerFactoryStorageGenesisGoerli is LekkerFactoryStorageGenesis {
    // add further slots here that are specific to Goerli
}

contract LekkerFactoryGoerli is LekkerFactoryStorageGenesisGoerli, ProxyDeployer {
    using EnumerableSet for EnumerableSet.AddressSet;

    modifier onlyAdmin() {
        require(isAdmin[msg.sender] || msg.sender == admin, "Only Admins can call");
        _;
    }

    /**
     * @notice Sets this contract as the implementation for a proxy input
     * @param proxy the proxy contract to accept this implementation
     */
    function _become(IProxy proxy) external {
        require(msg.sender == proxy.admin(), "only proxy admin can change brains");
        proxy._acceptImplementation();
    }

    constructor() {
        isAdmin[msg.sender] = true;
    }

    function initialize(
        address _uniswapV3Factory,
        address _minimalRouter,
        address _uniswapOracle,
        address _euler,
        address _eulerMarkets,
        address _aaveDataProvider,
        address _aaveAddressProvider,
        address _aaveV3Pool
    ) external onlyAdmin {
        require(!initialized, "INIT");

        // Uniswap data
        uniswapV3Factory = _uniswapV3Factory;
        minimalRouter = _minimalRouter;

        // Euler data
        eulerMarkets = _eulerMarkets;
        euler = _euler;

        // AAVE data
        aaveAddressProvider = _aaveAddressProvider;
        aaveV3Pool = _aaveV3Pool;
        aaveDataProvider = _aaveDataProvider;

        uniswapOracle = _uniswapOracle;

        initialized = true;
    }

    // pool creation

    function createEulerPool(
        address collateral,
        address debt,
        uint256 initialAmount,
        uint256 debtToSwap,
        uint24 poolFee
    ) external returns (address pool) {
        // add to arrays
        pool = deployProxy(eulerPoolImplementation, address(this), new bytes(0));
        pools.push(pool);
        allEulerPools.push(pool);
        _eulerPoolSet.add(pool);
        eulerPools[collateral][debt].add(pool);
        isLekkerPool[pool] = true;

        // add to counters
        latestEulerId += 1;
        latestId += 1;

        address _collateral = collateral;
        // transfer amounts from caller to this contract
        TransferHelper.safeTransferFrom(_collateral, msg.sender, address(this), initialAmount);
        // approve spending on pool
        TransferHelper.safeApprove(_collateral, pool, type(uint256).max);
        ILekkerEulerPoolInitialize(pool).initialize(
            BasePoolInitialParameters({
                uniswapFactory: uniswapV3Factory,
                minimalRouter: minimalRouter,
                collateral: _collateral,
                debt: debt,
                initialAmount: initialAmount,
                debtToSwap: debtToSwap,
                poolFee: poolFee
            }),
            EulerSetupParameters({euler: euler, eulerMarkets: eulerMarkets, tokenCollateral: collateral, tokenBorrow: debt}),
            AssetPricerParameters({collateral: collateral, debt: debt, oracle: uniswapOracle, fee: poolFee})
        );
    }

    function createAavePool(
        address collateral,
        address debt,
        uint256 initialAmount,
        uint256 debtToSwap,
        uint24 poolFee
    ) external returns (address pool) {
        // add to arrays
        pool = deployProxy(aavePoolImplementation, address(this), new bytes(0));
        pools.push(pool);
        _aavePoolSet.add(pool);
        allAavePools.push(pool);
        aavePools[collateral][debt].add(pool);
        isLekkerPool[pool] = true;

        // add to counters
        latestAaveId += 1;
        latestId += 1;

        address _collateral = collateral;
        // transfer amounts from caller to this contract
        TransferHelper.safeTransferFrom(_collateral, msg.sender, address(this), initialAmount);
        // approve spending on pool
        TransferHelper.safeApprove(_collateral, pool, type(uint256).max);
        uint256 totalCollateral = ILekkerAavePoolInitialize(pool).initialize(
            BasePoolInitialParameters({
                uniswapFactory: uniswapV3Factory,
                minimalRouter: minimalRouter,
                collateral: _collateral,
                debt: debt,
                initialAmount: initialAmount,
                debtToSwap: debtToSwap,
                poolFee: poolFee
            }),
            AaveSetupParameters({
                addressProvider: aaveAddressProvider,
                dataProvider: aaveDataProvider,
                tokenCollateral: collateral,
                tokenBorrow: debt
            }),
            AssetPricerParameters({collateral: collateral, debt: debt, oracle: uniswapOracle, fee: poolFee})
        );

        //transfer lekker tokens to the caller
        TransferHelper.safeTransfer(pool, msg.sender, totalCollateral);
    }

    function upgradePool(address pool, address logic) external onlyAdmin {
        upgradeLogic(pool, logic);
    }

    // setters

    function setAdmin(address _admin) external {
        require(msg.sender == admin);
        address adminToConsider = _admin;
        if (isAdmin[adminToConsider]) {
            isAdmin[adminToConsider] = false;
        } else {
            isAdmin[adminToConsider] = true;
        }
    }

    function setEulerPoolCreator(address _lekkerEulerPoolCreator) external onlyAdmin {
        eulerPoolCreator = _lekkerEulerPoolCreator;
    }

    function setAavePoolCreator(address _lekkerAavePoolCreator) external onlyAdmin {
        aavePoolCreator = _lekkerAavePoolCreator;
    }

    function setEulerPoolImplementation(address _lekkerEulerPoolImplementation) external onlyAdmin {
        eulerPoolImplementation = _lekkerEulerPoolImplementation;
    }

    function setAavePoolImplementation(address _lekkerAavePoolImplementation) external onlyAdmin {
        aavePoolImplementation = _lekkerAavePoolImplementation;
    }

    function setAaveAddressProvider(address _newAddressProvider) external onlyAdmin {
        aaveAddressProvider = _newAddressProvider;
    }

    function setAaveDataProvider(address _newDataProvider) external onlyAdmin {
        aaveDataProvider = _newDataProvider;
    }

    function setEuler(address _newEuler) external onlyAdmin {
