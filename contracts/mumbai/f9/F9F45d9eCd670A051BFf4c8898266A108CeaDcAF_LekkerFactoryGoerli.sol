    function pushAavePool(address _pool) external onlyAdmin {
        allAavePools.push(_pool);
        _aavePoolSet.add(_pool);
        latestAaveId += 1;
    }

    function pushEulerPool(address _pool) external onlyAdmin {
        allEulerPools.push(_pool);
        _eulerPoolSet.add(_pool);
        latestEulerId += 1;
    }

    // only adds to enumerable sets
    function setAavePoolToSet(address _pool) external onlyAdmin {
        _aavePoolSet.add(_pool);
    }

    function setEulerPoolToSet(address _pool) external onlyAdmin {
        _eulerPoolSet.add(_pool);
    }

    // only adds to enumerable sets
    function removeAavePoolFromSet(address _pool) external onlyAdmin {
        _aavePoolSet.remove(_pool);
    }

    function removeEulerPoolFromSet(address _pool) external onlyAdmin {
        _eulerPoolSet.remove(_pool);
    }

    // delete functions

    function deletePool(uint256 _poolId) external onlyAdmin {
        delete pools[_poolId];
        latestId -= 1;
    }

    function deleteAavePool(uint256 _poolId) external onlyAdmin {
        delete allAavePools[_poolId];
        latestAaveId -= 1;
    }

    function deleteEulerPool(uint256 _poolId) external onlyAdmin {
        delete allEulerPools[_poolId];
        latestEulerId -= 1;
    }

