  function bookstate(
    address _src,
    address _dst,
    uint16  _dst_cid
  ) public view returns (
    address src,
    address dst,
    uint16  dst_chain,
    bool    wiring,
    bool    mkrs,
    bool    colt
  ){

    Book storage b = books[booktag(_src,_dst,_dst_cid)];
    return ( b.src, b.dst, b.dst_chain, b.wiring, b.mkrs, b.colt);

  }

  function sums(
    address src, address dst, uint16 dst_c
  ) public view returns (
    uint epoch, uint sum, uint msum, uint summand,
    uint msummand, uint dstsum, uint dstmsum
  ) {

    Sums storage s = books[booktag(src, dst, dst_c)].sums;

    return ( s.epoch, s.sum, s.summand, s.msum,
             s.msummand, s.dstsum, s.dstmsum );

  }

  function makers(
    address _src,
    address _dst,
    uint16  _dst_cid,
    uint256 _ix
  ) public view returns (Maker memory) {

    Book storage book = books[booktag(_src, _dst, _dst_cid)];

    return book.ms.ms[_ix];

  }

  function makerixs(
    address _src,
    address _dst,
    uint16  _dst_cid
  ) public view returns (uint head, uint step, uint tail) {

    Makers storage ms = books[booktag(_src, _dst, _dst_cid)].ms;

    return (ms.hd, ms.stp, ms.tl);

  }

  function activetraders(
    address src,
    address dst,
    uint16  dst_c
  ) public view returns (Trader[] memory) {

    mapping(uint => Trader) storage ts = books[booktag(src, dst, dst_c)].ts.ts;
    uint hd = books[booktag(src, dst, dst_c)].ts.hd;
    uint tl = books[booktag(src, dst, dst_c)].ts.tl;

    uint num;
    Trader storage t = ts[hd];
    while (t.next <= tl) ( t = ts[t.next], num++ );

    uint i; t = ts[hd];
    Trader[] memory trdrs = new Trader[](num);
    while (i < num) trdrs[i++] = t = ts[t.next];

    return trdrs;

  }


  function activemakers(
    address src,
    address dst,
    uint16  dst_c
  ) public view returns (Maker[] memory) {

    mapping(uint => Maker) storage ms = books[booktag(src, dst, dst_c)].ms.ms;
    uint hd = books[booktag(src, dst, dst_c)].ms.hd;
    uint tl = books[booktag(src, dst, dst_c)].ms.tl;

    uint num;
    Maker storage m = ms[hd];
    while (m.next <= tl) ( m = ms[m.next], num++ );

    uint i; m = ms[hd];
    Maker[] memory mkrs = new Maker[](num);
    while (i < num) mkrs[i++] = m = ms[m.next];

    return mkrs;

  }


  function _send(bytes memory _payload, bytes memory _params) virtual internal {

    lz.send{value: address(this).balance}(
      HUB_CID,
      abi.encodePacked(HUB, address(this)),
      _payload,
      payable(address(this)),
      address(0),
      _params
    );

  }

  function _send(uint16 cid, address dst, bytes memory _payload) virtual internal {

    lz.send{value: address(this).balance}(
      cid,
      abi.encodePacked(dst, address(this)),
      _payload,
      payable(address(this)),
      address(0),
      bytes("")
    );

  }

  function lzReceive(
    uint16 _srcChainId,
    bytes calldata _srcAddress,
    uint64 _nonce,
    bytes calldata _payload
  ) public {

    uint _type_ = abi.decode(_payload, (uint));

    if      (_type_ == LZ_RESOLVE) return resolve(_payload);
    else if (_type_ == LZ_SETTLE)  return settle(_payload);
    else if (_type_ == LZ_INIT)    return init(_payload);
    else if (_type_ == LZ_CONNECT) return connect(_payload);
    else if (_type_ == LZ_HINT)    return hint(_payload);

  }

  function cid () internal returns (uint16) { 

    return lz_cid;

  }

  receive() external payable {}

}