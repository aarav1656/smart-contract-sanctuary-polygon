pragma solidity ^0.8.13;

import "layer-zero/interfaces/ILayerZeroEndpoint.sol";
import "layer-zero/interfaces/ILayerZeroReceiver.sol";
import "layer-zero/interfaces/ILayerZeroUserApplicationConfig.sol";

import "openzeppelin-contracts/contracts/interfaces/IERC20.sol";

import "forge-std/console.sol";

contract SpokeMatcher is 
    ILayerZeroReceiver,
    ILayerZeroUserApplicationConfig
{

  address private constant MKR = address(uint160(uint(keccak256("MKR"))));
  address private constant FIN = address(uint160(uint(keccak256("FIN"))));
  address private constant GAP = address(uint160(uint(keccak256("GAP"))));
  address private constant CUT = address(uint160(uint(keccak256("CUT"))));
  address private constant CLT = address(uint160(uint(keccak256("CLT"))));

  uint private constant MAX = type(uint).max;

  uint private constant MAX_BPS = 1e4;

  uint constant LZ_CHOKE = 10;
  uint constant LZ_SLICE = 11;

  uint public MARGIN_BPS;
  uint public REPO_BPS;

  uint16 public immutable HUB_CID;

  address public immutable HUB;

  ILayerZeroEndpoint public lz;
  uint16 lz_cid;

  event Log(uint256);
  event Log(address);

  // incoming from hub
  uint256 constant LZ_RESOLVE = uint(keccak256("RESOLVE"));
  uint256 constant LZ_SETTLE  = uint(keccak256("SETTLE"));
  uint256 constant LZ_INIT    = uint(keccak256("INIT"));
  uint256 constant LZ_HINT    = uint(keccak256("HINT"));

  // outgoing to hub
  uint256 constant LZ_PAIR    = uint(keccak256("PAIR"));
  uint256 constant LZ_PRIME   = uint(keccak256("PRIME"));

  // outgoing and incoming to and from hub
  uint256 constant LZ_CONNECT = uint(keccak256("CONNECT"));

  struct Trader {
    bool head;
    bool funded;
    bool cancelled;
    address trader;
    uint256 collateral;
    uint256 capital;
    uint256 placed;
    uint256 prev;
    uint256 next;
  }

  struct Maker {
    bool head;
    bool cancelled;
    address maker;
    uint256 collateral;
    uint256 capital;
    uint256 ordered;
    uint256 placed;
    uint256 prev;
    uint256 next;
  }

  struct Traders {
    uint                    hd;
    uint                   stp;
    uint                    tl;
    mapping(uint => Trader) ts;
  }

  struct Makers {
    uint                    hd;
    uint                   stp;
    uint                    tl;
    mapping(uint => Maker)  ms;
  }

  struct Book {
    address                    src;
    address                    dst;
    uint16               dst_chain;
    uint                     epoch;
    bool                    wiring;
    bool                      mkrs;
    bool                      colt;
    Sums                      sums;
    Traders                     ts;
    Makers                      ms;
  }

  struct Sums {
    uint epoch;
    uint sum;
    uint summand;
    uint msum;
    uint msummand;
    uint dstsum;
    uint dstmsum;
    uint seizings;
  }

  struct TapeOut {
    address u;
    uint256 n0; // if Order, amount. If COL, seized collateral
    uint256 n1; // if Order, id. Empty if GAP, FIN, MKR, COL.
  }

  struct TapeIn {
    address user;
    uint256 amount;
  }

  mapping (uint => Book) internal books;

  mapping (uint16 => address) public peers;

  uint internal _epoch_;
  uint internal _epochmoment_;
  uint public epochspan;

  constructor (
    address _lz, 
    uint16  _lz_cid, 
    address _domino,
    uint16  _domino_chain_id
  ) { 

    lz = ILayerZeroEndpoint(_lz);
    lz_cid = _lz_cid;

    HUB = _domino;
    HUB_CID = _domino_chain_id;

    MARGIN_BPS = 5;
    REPO_BPS   = 2;

  }

  function init(
    bytes memory _payload
  ) internal {

    (,uint _epoch, uint _epochspan, uint _epochmoment ) 
      = abi.decode(_payload, (uint,uint,uint,uint));

    _epoch_ = _epoch;
    epochspan = _epochspan;
    _epochmoment_ = _epochmoment;

  }

  function epoch () public view returns (uint) {

    return _epoch_ + (( block.timestamp - _epochmoment_ ) / epochspan);

  }

  function epochmoment () public view returns (uint) {

    uint news = (block.timestamp - _epochmoment_) / epochspan;

    return _epochmoment_ + ( epochspan * news );

  }

  function _ticktockepoch_ () internal {

    uint news = (block.timestamp - _epochmoment_) / epochspan;

    if (0 < news) {

      _epoch_ += news;
      _epochmoment_ += news * epochspan;

    }

  }

  function placeTraderFunded (
    address _src, 
    address _dst, 
    uint16  _dst_cid,
    uint256 _capital
  ) public payable returns (uint link_) {

    _ticktockepoch_();

    link_ = _writetrader(_src, _dst, _dst_cid, _capital, 0);

    IERC20(_src).transferFrom(msg.sender, address(this), _capital);

  }


  function placeTraderUnfunded (
    address _src, 
    address _dst, 
    uint16  _dst_cid,
    uint256 _capital
  ) public payable returns (uint link_) {

    _ticktockepoch_();

    uint _collateral = _capital * MARGIN_BPS / MAX_BPS;

    link_ = _writetrader(_src, _dst, _dst_cid, _capital, _collateral);

    IERC20(_src).transferFrom(msg.sender, address(this), _collateral);

  }

  function _writetrader (
    address _src,
    address _dst,
    uint16  _dst_cid,
    uint256 _capital,
    uint256 _collateral
  ) internal returns (uint link_) {

    Book storage book = getbook(_src, _dst, _dst_cid);

    Trader storage link;
    link_ = book.ts.tl + 1;
    link = book.ts.ts[link_];
    book.ts.tl = link_;

    link.funded       = _collateral == 0;
    link.trader       = msg.sender;
    link.capital      = _capital;
    link.collateral   = _collateral;
    link.placed       = block.timestamp;  // TODO: block.timestamp for now
    if (link.prev == 0)
      link.prev       = link_ - 1;
    link.next         = link_ + 1;

    book.ts.ts[link_ + 1].prev = link_;

    _sumhintprime(book.sums, _src, _dst, _dst_cid, _capital, 0);

  }

  function sumhintprime (address src, address dst, uint16 dst_c) public {

    _ticktockepoch_();

    Sums storage sums = getbook(src, dst, dst_c).sums;

    _sumhintprime(sums, src, dst, dst_c, 0, 0);

    sums.epoch = MAX;

  }

  function _sumhintprime (Sums storage sums, address src_t,
   address dst_t, uint16 dst_c, uint summand, uint msummand) internal {

    uint epo = _epoch_; uint epo_s = sums.epoch;

    if (epo_s == MAX) sums.epoch = epo_s = epo;
    if (epo_s != epo) {

      // TODO: one side may not have traders waiting but may have makers waiting
      // and makers persist throughout epochs.

      // If prior summand is not 0, traders are waiting and this may be primed
      if (sums.summand != 0 || sums.msummand != 0) 
        _send(abi.encode(LZ_PRIME, cid(), src_t, dst_c, dst_t, epo_s), bytes(""));

      sums.sum += sums.summand;
      sums.summand = summand;

      sums.msum += sums.msummand;
      sums.msummand = msummand;

      sums.epoch = epo;

      // Q: placement of args? dst_t, src_t, cid() - this fits pattern
      // of looking up book with src, dst, dst_c on opposing peer
      _send(dst_c, peers[dst_c], abi.encode(LZ_HINT, 
        dst_t, src_t, cid(), sums.sum, sums.msum));

    } else ( sums.summand += summand, sums.msummand += msummand );

  }

  function placeMaker (
    address src, 
    address dst, 
    uint16  dst_c, 
    uint256 capital
  ) public payable returns (uint256 link_) {

    _ticktockepoch_();

    uint _collat = capital * MARGIN_BPS / MAX_BPS;

    Book storage book = getbook(src, dst, dst_c);

    Maker storage link;
    link_ = book.ms.tl + 1;
    link = book.ms.ms[link_];
    book.ms.tl = link_;

    link.maker        = msg.sender;
    link.collateral   = _collat;
    link.capital      = capital;
    link.ordered      = capital;
    link.placed       = block.timestamp;
    if (link.prev == 0)
      link.prev       = link_ - 1;
    link.next         = link_ + 1;

    book.ms.ms[link_ + 1].prev = link_;

    IERC20(book.src).transferFrom(address(msg.sender), address(this), _collat);

    _sumhintprime(book.sums, src, dst, dst_c, 0, capital);

  }

  function hint (bytes memory payload) internal {

    ( ,address src_t,
       address dst_t,
       uint16  dst_c,
       uint      sum,
       uint     msum 
    ) = abi.decode(payload, (uint, address, address, uint16, uint, uint));

    Sums storage sums = getbook(src_t, dst_t, dst_c).sums;

    sums.dstsum = sum;
    sums.dstmsum = msum;

  }

  function resolve (
    bytes memory payload
  ) internal {

    _ticktockepoch_();

    ( ,address src_t, address dst_t, uint16  dst_c ) = 
      abi.decode(payload, (uint,address,address,uint16));

    Book storage book = getbook(src_t, dst_t, dst_c);

    book.wiring = true;

    _resolve(book, src_t, dst_t, dst_c);

  }

  function resolve (
    address src_t, 
    address dst_t, 
    uint16  dst_c
  ) public {

    Book storage book = getbook(src_t, dst_t, dst_c);

    require(book.wiring);

    require(book.ts.tl != book.ts.stp || book.ms.stp != book.ms.tl, 
                                        "transmissions exhausted");

    _resolve(book, src_t, dst_t, dst_c);

  }

  function _resolve (
    Book storage book, 
    address src_t, 
    address dst_t, 
    uint16 dst_c
  ) internal {

    uint gas = gasleft();

    ( TapeOut[] memory tape, bool done ) = lock_orders(book);

    bytes memory payload = abi.encode(LZ_PAIR, cid(), src_t, dst_c, dst_t, tape);

    _send(payload, bytes(""));

    console.log("gas used in resolve", cid(), gas - gasleft());

  }

  function lock_orders (
    Book storage book
  ) internal returns (
    TapeOut[] memory,
    bool done
  ) {

    // length should be const
    TapeOut[] memory tape = new TapeOut[](LZ_SLICE); 
    uint tix = 0; uint id; uint pile; 
    IERC20 src = IERC20(book.src); 

    uint need; 
    Sums memory sums = book.sums;
    if (sums.sum < sums.dstsum) {

      // if destination sum is greater than local sum how many 
      // makers are available to satisfy destination sum?
      if (sums.msum <= sums.dstsum - sums.sum) 
           need = sums.sum + sums.msum;
      else need = sums.dstsum;

    } else {

      // if local sum is greater than destination sum, how many 
      // makers does the destination have to satisfy the local sum?
      if (sums.dstmsum <= sums.sum - sums.dstsum)  
           need = sums.dstsum + sums.dstmsum;
      else need = sums.sum;

    }

    { // TODO: no need to iterate when step is tail
      Traders storage ts = book.ts;
      Trader  storage t  = ts.ts[ts.stp]; 
      ( t, id, pile, sums ) = tnext(src, pile, ts, t, sums);
      if (id != MAX) while (true) {

        tape[tix++] = TapeOut(t.trader, t.capital, id);

        if (tix == LZ_CHOKE) { 

          tape[tix++] = TapeOut(GAP, 0, 0);

          book.sums.sum = sums.sum; 
          book.sums.dstsum = sums.dstsum; 
          book.sums.dstmsum = sums.dstmsum;
          book.sums.seizings = sums.seizings;
          ts.stp = id; return (tape, false); 

        }

        if (need <= pile) { ts.stp = id; break; }

        ( t, id, pile, sums ) = tnext(src, pile, ts, t, sums);

        if (id == MAX) { ts.stp = t.prev; break; }

      }
    }

    if (pile < need) {

      pile = 0;
      need = sums.dstsum;

      Makers storage ms = book.ms;
      Maker  storage m  = ms.ms[ms.stp]; 
      ( m, id, pile, sums ) = mnext(src, pile, ms, m, sums);
      if (id != MAX) {

        tape[tix++] = TapeOut(MKR, sums.seizings, 0);

        sums.seizings = 0;

        while (true) {

          tape[tix++] = TapeOut(m.maker, m.capital, id);

          if (tix >= LZ_CHOKE ) { 

            tape[tix++] = TapeOut(GAP, 0, 0);

            book.sums.sum = sums.sum;
            book.sums.msum = sums.msum;
            book.sums.dstsum = sums.dstsum;
            book.sums.seizings = sums.seizings;
            ms.stp = id; return (tape, false); 

          }

          if (need <= pile) { ms.stp = id; break; }

          ( m, id, pile, sums ) = mnext(src, pile, ms, m, sums);

          if (id == MAX) break;

        }
      } 
    }

    tape[tix++] = TapeOut(FIN, sums.seizings, 0);

    sums.seizings = 0;
    book.sums = sums;

    // resize array downwards
    assembly { mstore(tape, sub(mload(tape), sub(LZ_SLICE, tix))) }

    return ( tape, true );

  }


  function tnext(
    IERC20            src,
    uint             pile,
    Traders storage    ts,
    Trader storage      p,
    Sums memory      sums
  ) internal returns (
    Trader storage n/*ext*/, 
    uint id, 
    uint pile_,
    Sums memory
  ) {

    uint prevdotnext = p.next;

    n = ts.ts[prevdotnext];

    while (true) {

      // TODO: insert epoch eligibility
      if (0 == n.placed) {

        return (n, MAX, pile, sums);

      // TODO: cancellation criteria
      } else if (n.cancelled) {

        tremit(n, src);

        toust(ts, n);

        n = ts.ts[n.next]; continue;

      } else if (!n.funded) {

        try src.transferFrom(n.trader, address(this), n.capital - n.collateral){}
        catch {

          sums.seizings += n.capital;

          toust(ts, n); 

          n = ts.ts[n.next]; continue;

        }

      } 

      uint cptl = n.capital;

      sums.sum -= cptl;
      if (sums.dstsum <= cptl) {
        if (cptl - sums.dstsum >= sums.dstmsum) sums.dstmsum = 0;
        else sums.dstmsum -= cptl - sums.dstsum;
        sums.dstsum = 0;
      } else sums.dstsum -= cptl;

      return (n, prevdotnext, pile + cptl, sums);

    }
  }

  function mnext(
    IERC20            src,
    uint             pile,
    Makers storage     ms,
    Maker storage    prev,
    Sums memory      sums
  ) internal returns (
    Maker storage next, 
    uint id, 
    uint pile_, 
    Sums memory
  ) {

    uint prevdotnext = prev.next;

    next = ms.ms[prevdotnext];

    while (true) {

      // TODO: eligibility criteria
      if (0 == next.placed) {

        return (next, MAX, pile, sums);

      // TODO: cancellation criteria
      } else if (next.cancelled) {

        mremit(next, src);

        moust(ms, next);

        next = ms.ms[next.next]; continue;

      } else try src.transferFrom(next.maker, address(this), next.ordered) {

        uint capital = next.capital = next.ordered;

        sums.msum -= capital;
        sums.dstsum -= sums.dstsum > capital ? capital : sums.dstsum;

        return (next, prevdotnext, pile + capital, sums);

      } catch {

        sums.seizings += next.ordered;

        moust(ms, next); 

        next = ms.ms[next.next]; continue;

      }

    }

  }

  function toust (
    Traders storage ts,
    Trader storage link
  ) internal {

    if (link.head) return;

    uint linkdotprev = link.prev; 
    Trader storage prev = ts.ts[linkdotprev];

    if (linkdotprev == ts.hd) {

      link.head = true;
      ts.hd = prev.next;

    } else {

      uint linkdotnext = link.next;
      Trader storage next = ts.ts[linkdotnext];

      prev.next = linkdotnext;
      next.prev = linkdotprev;

    }

  }

  function moust (
    Makers storage ms,
    Maker storage link
  ) internal {

    if (link.head) return;

    uint linkdotprev = link.prev; 
    Maker storage prev = ms.ms[linkdotprev];

    if (linkdotprev == ms.hd) {

      link.head = true;
      ms.hd = prev.next;

    } else {

      uint linkdotnext = link.next;
      Maker storage next = ms.ms[linkdotnext];

      prev.next = linkdotnext;
      next.prev = linkdotprev;

    }

  }


  function tremit (
    Trader storage trader,
    IERC20 _token
  ) internal {

    _token.transfer(trader.trader,
      trader.funded ? trader.capital + trader.collateral : trader.collateral);

  }

  function mremit (
    Maker storage maker,
    IERC20 _token
  ) internal {

    _token.transfer(maker.maker, maker.collateral);

  }

  function settle (bytes memory payload) internal {

    uint gas = gasleft();

    ( ,address src_t, 
       address dst_t, 
       uint16 dst_c, 
       TapeIn[] memory tape 
    ) = abi.decode(payload, (uint,address,address,uint16,TapeIn[]));

    Book storage book = getbook(src_t, dst_t, dst_c);

    uint cut; uint len = tape.length; IERC20 src = IERC20(book.src);

    for (uint i = 0; i < len; i++) {

      address signal = tape[i].user;
      uint value = tape[i].amount;

      if      (signal == CUT) cut += value;
      else if (signal == MKR) processMKR(book, value);
      else if (signal == GAP) processGAP(book, value, cut);
      else if (signal == FIN) processFIN(book, value, cut);
      else if (signal == CLT) processCLT(book);
      else                    processSND(book, signal, value, src);

    }

    console.log("gas used in settling", cid(), gas - gasleft());

  }

  function processSND (Book storage book, address dst, uint amount, IERC20 src) internal {

    if (book.colt) {
      uint seized = amount * REPO_BPS / MAX_BPS;
      uint collat = amount = amount * MARGIN_BPS / MAX_BPS - seized;
    }

    src.transfer(dst, amount);

  }

  function processCLT (Book storage book) internal {

    book.colt = true;

  }

  // @dev it does not matter this may be triggered before 
  //      tape that are filling maker orders. the only 
  //      important thing here is signifying maker orders
  //      have been exercised and setting the trader head
  function processMKR (Book storage book, uint value) internal {

    book.mkrs = true;
    book.ts.hd = value;
    book.ts.ts[value].head = true;

  }

  function processGAP (Book storage book, uint value, uint cut) internal {

    if (book.mkrs) {

      Maker storage m = book.ms.ms[book.ms.ms[value].next];

      m.capital -= cut;

    } else {

      book.ts.hd = value;

      Trader storage t = book.ts.ts[book.ts.ts[value].next]; 

      t.capital -= cut;

    }

  }

  function processCLT (Book storage book, uint value ) internal {

    book.colt = true;

  }

  function processFIN (Book storage book, uint value, uint cut) internal {

    if (book.mkrs) {

      if (book.ms.tl != value) remitmakers(book, cut, value);

    } else {

      if (book.ts.stp != value) remittraders(book, cut, value);
      else remitmakers(book, 0, book.ms.hd);

      book.ts.hd = book.ts.stp = value;
      book.ts.ts[value].head = true;

    }

    // TODO: if cancellations occur while 
    // wiring are there any problems here?
    book.ms.stp = book.ms.hd;

    delete book.mkrs;
    delete book.wiring;
    delete book.colt;

  }

  function remittraders (
    Book storage book,
    uint          cut,
    uint      remitix
  ) internal {

    Traders storage ts = book.ts;
    Trader storage t = ts.ts[ts.ts[remitix].next];

    uint stop = book.ts.stp;

    t.capital -= cut;

    // resize collateral compared to amount cut
    t.collateral = t.capital * MARGIN_BPS / MAX_BPS;

    IERC20 src = IERC20(book.src);

    while (true) {

      if (!t.funded) src.transfer(t.trader, t.capital - t.collateral);

      t = ts.ts[t.next];

      if (t.prev == stop || t.trader == address(0)) 
        return remitmakers(book, 0, book.ms.hd);

    }

  }

  function remitmakers (
    Book storage book,
    uint          cut,
    uint      remitix
  ) internal {

    Makers storage ms = book.ms;
    Maker storage m = ms.ms[ms.ms[remitix].next];

    uint stop = ms.stp;

    m.capital -= cut;

    IERC20 src = IERC20(book.src);

    while (true) {

      if (m.prev == stop || m.maker == address(0)) break;

      src.transfer(m.maker, m.capital);

      m = ms.ms[m.next];

    }

  }

  function getbook (address src, address dst, uint16 dst_c)
    internal returns (Book storage book) {

    book = books[booktag(src, dst, dst_c)];

    if (book.src == address(0)) {

      if (peers[dst_c] == address(0)) revert("spokes not connected");

      book.sums.epoch = _epoch_;
      book.src        = src;
      book.dst        = dst;
      book.dst_chain  = dst_c;

      Trader storage thead = book.ts.ts[0]; thead.next = 1; thead.head = true;
      Maker storage mhead = book.ms.ms[0]; mhead.next = 1; mhead.head = true;
    }
  }

  function connect (uint dst_c, uint hop_fee)  public payable {

      _send(
        abi.encode(LZ_CONNECT, cid(), dst_c),
        abi.encodePacked(uint16(2), uint(400000), hop_fee, HUB)
      );

  }

  function connect (bytes memory payload) internal {

    ( ,uint16 c, 
      address peer
    ) = abi.decode(payload, (uint, uint16, address));

    peers[c] = peer;

  }

  function toksort(
    address t0,
    address t1
  ) public pure returns (address t0_, address t1_) {

    if (t0 > t1) (t0, t1) = (t1, t0); t0_ = t0; t1_ = t1;

  }

  function booktag (
    address _src, 
    address _dst, 
    uint256 _dst_cid
  ) public view returns (uint bookkey_) {

    ( address t0, address t1 ) = toksort(_src, _dst);

    bookkey_ = uint256(
      keccak256(abi.encodePacked(t0, t1, _dst_cid)));

  }

  function forceResumeReceive(uint16 _srcChainId, bytes calldata _srcAddress) public {
      lz.forceResumeReceive(_srcChainId, _srcAddress);
  }

  function setReceiveVersion(uint16 _version) public {
      lz.setReceiveVersion(_version);
  }

  function setSendVersion(uint16 _version) public {
      lz.setSendVersion(_version);
  }

  function setConfig(uint16 _version, uint16 _chainId, 
     uint256 _configType, bytes calldata _config) public {

      lz.setConfig(_version, _chainId, _configType, _config);

  }

  function traders(
    address _src,
    address _dst,
    uint16  _dst_cid,
    uint256 _ix
  ) public view returns (Trader memory) {

    Book storage book = books[booktag(_src, _dst, _dst_cid)];

    return book.ts.ts[_ix];

  }

  function traderixs(
    address _src,
    address _dst,
    uint16  _dst_cid
  ) public view returns (uint head, uint step, uint tail) {

    Traders storage ts = books[booktag(_src, _dst, _dst_cid)].ts;

    return (ts.hd, ts.stp, ts.tl);

