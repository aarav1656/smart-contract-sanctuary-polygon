    function pickWinners(uint256 raffleID, uint256 roundNum) public onlyOwner checkRaffle(raffleID){
        require(roundNum == raffle[raffleID].currentRound + 1, "Wrong round number.");
        require(getWinner_TokenIds(raffleID, roundNum).length == 0, "Winners already announced.");
        require(getTokenIDs(raffleID).length >= getQtyOfWinners(raffleID, (getRaffle(raffleID).currentRound + 1)), "Not enough participants");
        //will pick winners according to the round qty of the raffle id
        randomWinners(raffleID, roundNum);     
    }


    function setOwnable(address ownableAddr) external onlyDeployer {
        require(ownableAddr != address(0) && ownableAddr != address(this), "Invalid ownable address.");
        ownable = Ownable(ownableAddr);
    }

    function setRandomNumberContract(address _generateRandom) public onlyDeployer {
       require(_generateRandom != address(0) && _generateRandom != address(this), "Invalid contract address.");
        generateRandom = IGenerateRandom(_generateRandom);
    }


    // function setCustodialWallet(address _custodialWallet) external onlyOwner{
    //     require(_custodialWallet != address(0) && _custodialWallet != address(this),
    //     "Invalid address passed");

    //     custodialWallet = _custodialWallet;
    // }
    

    function setPrizesAndWeights(uint Raffle_id, bytes memory _prizes, bytes memory _weights, uint256 sumOfSeats) external onlyOwner checkRaffle(Raffle_id) {
  
        require(sumOfSeats >= getRaffle(Raffle_id).totalWinners, "wrong amount of parameters.");

        bytes32[] memory prizes = abi.decode(_prizes, (bytes32[]));
        uint256[] memory weights = abi.decode(_weights, (uint256[]));
        
        raffle[Raffle_id].prizes = prizes;
        raffle[Raffle_id].weights = weights;
        
    }

    function randomWinners(uint256 raffleID, uint256 roundNum) internal{
        
    
        uint256 totalWinners = getQtyOfWinners(raffleID, roundNum);

        uint256[] memory tokenIDS = getTokenIDs(raffleID);

        if(tokenIDS.length  == totalWinners){

            for(uint i = 0; i < totalWinners; i++){
                uint winner = tokenIDS[i];
                winnersOfTheRaffleID[raffleID][roundNum].push(winner);
            }

                if(raffle[raffleID].ignorePreviousWinner == true){
                    delete raffle[raffleID].TokenIDS;
                }

        }else{
            uint256 r_number = generateRandom.fullfillRandomWords(requestID);
            for(uint i = 0; i < totalWinners; i++){
          
            uint256 winnerIndex = ((r_number ) % (tokenIDS.length - 1)) + 1;
            console.log("r_number -", ((r_number % (tokenIDS.length-1)) + 1));

            uint winner = tokenIDS[winnerIndex];
       
            winnersOfTheRaffleID[raffleID][roundNum].push(winner);
            requestID++;
            r_number -= ((i*3) + (i * i));
            tokenIDS[winnerIndex] = tokenIDS[tokenIDS.length - 1];  // winner index moved to last index
            tokenIDS = tokenIDsPop(tokenIDS);
            }
        }

        if(raffle[raffleID].ignorePreviousWinner == true){
            raffle[raffleID].TokenIDS = tokenIDS;
        }
    }
    

    function getTokenIDs(uint Raffle_id) public view returns(uint256[] memory){
        return raffle[Raffle_id].TokenIDS;
    }
    

    function getPrizesAndWeights(uint Raffle_id) public view returns(bytes32[] memory, uint256[] memory){
        require(raffle[Raffle_id].prizes.length > 0, "Wrong Raffle Id passed.");
        return (raffle[Raffle_id].prizes, raffle[Raffle_id].weights);
    }


    function getWinner_TokenIds(uint Raffle_id, uint Round_No) public view returns(uint256[] memory){
        return winnersOfTheRaffleID[Raffle_id][Round_No];
    }
    

    function startWeb2Raffle(
        uint256 RaffleId,
        uint256 totalRounds,
        uint256 totalWinners,
        uint256 threshold,
        bool isRegistrationRequired
        ) external onlyCustodial isGenerateRandomSet{
        require(threshold > 0, "Invalid threhold");
        require(web2raffle[RaffleId].status == RaffleStatus.NotInitialized, "Raffle is already Initialized");
        require(totalWinners >= totalRounds, "Invalid amount  of winners.");

        web2raffle[RaffleId] = web2Raffle(
        RaffleId,
        totalRounds,
        totalWinners,
        0,
        0,
        threshold,
        isRegistrationRequired,
        RaffleStatus.Initialized
        );
    }

    function sendWinnersAndAwards(uint256 raffleId, uint256 roundId, bytes memory winnersOfTheRaffle, bytes memory prizesOfTheWinners) external onlyCustodial checkRaffle(raffleId){
        require((getWeb2Raffle(raffleId).currentRound + 1) == roundId,"wrong round Id");
       
        bool regRequired = getWeb2Raffle(raffleId).isRegistrationRequired;
        
        if(getWeb2Raffle(raffleId).currentRound < getWeb2Raffle(raffleId).totalRounds){
        web2raffle[raffleId].currentRound++ ;
        web2raffle[raffleId].status =  RaffleStatus.Running; 
        }

        uint256[] memory winners = abi.decode(winnersOfTheRaffle, (uint256[]));
        bytes32[] memory prizes = abi.decode(prizesOfTheWinners, (bytes32[]));
        require((winners.length + web2raffle[raffleId].winnersAnnounced) <= web2raffle[raffleId].totalWinners, "winners quantity exceeded.");
        require(winners.length == prizes.length, "Quantity of winners differ from quantity of prizes");
 
        winnersOfTheRaffleID[raffleId][roundId]= winners;

        for(uint i = 0; i < winners.length; i++){
            winnersAward[raffleId][roundId][winners[i]] = prizes[i];

            if(regRequired == false){
                isClaimed[raffleId][roundId][winners[i]] = true;  
            }
        }

        web2raffle[raffleId].winnersAnnounced += winners.length;

        if(getWeb2Raffle(raffleId).totalRounds == roundId){
            web2raffle[raffleId].status = RaffleStatus.Completed;
        }
    }


    function tokenIDsPop(uint256[] memory tokenIDS) internal pure returns(uint256[] memory){

        uint256[] memory newTokenIDS = new uint256[](tokenIDS.length -1);

        for(uint i = 0; i < tokenIDS.length - 1; i++){
                newTokenIDS[i] = tokenIDS[i];
        }

        return newTokenIDS;
    }


    function awardsPop(bytes32[] memory awards) internal pure returns(bytes32[] memory){

        bytes32[] memory newAwards = new bytes32[](awards.length -1);

        for(uint i = 0; i < awards.length - 1; i++){
                newAwards[i] = awards[i];
        }

        return newAwards;
    }


    function weightsPop(uint256[] memory weights) internal pure returns(uint256[] memory){

        uint256[] memory newWeights = new uint256[](weights.length -1);

        for(uint i = 0; i < weights.length - 1; i++){
                newWeights[i] = weights[i];
        }

        return newWeights;
    }


    function getAwardsOfTheWinner(uint256 raffleID, uint256 roundNum, uint256 winnerTokenID) public view returns(bytes32){
       return winnersAward[raffleID][roundNum][winnerTokenID];
    }

    function getCurrentRound(uint Raffle_id) public view returns(uint){
        if(getWeb2Raffle(Raffle_id).threshold >1){
            return getWeb2Raffle(Raffle_id).currentRound;
        } else{
            return getRaffle(Raffle_id).currentRound;
        }
        
    }


    function getQtyOfWinners(uint256 Raffle_id, uint256 roundNumber) public view returns(uint256 NumberOfWinners){
        
        require(raffle[Raffle_id].threshold > 0, RI_notExist);

        if(roundNumber <= raffle[Raffle_id].totalRounds - 1){
            return getRaffle(Raffle_id).startingRounds;
        } else if (roundNumber == raffle[Raffle_id].totalRounds){
            return getRaffle(Raffle_id).endingRound;
        }
    }

    function getWeb2Raffle(uint256 raffleID) public view returns (web2Raffle memory ){
        return web2raffle[raffleID];
    }

