pragma solidity ^0.8.17;

import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "./interfaces/ILPToken.sol";
import "./interfaces/IFactoryDIF.sol";
import "./interfaces/IDIF.sol";

contract DIF is IDIF {
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    using SafeERC20 for IERC20;

    uint256 public constant GRANULARITY = 1e8;

    bool private _initialized;
    IFactoryDIF.DIFPrefences private _settings;
    ILPToken private _lpToken;
    IFactoryDIF private _factory;
    // Mapping of address of tokens to price of LP-tokens
    EnumerableMap.AddressToUintMap private _assets;

    /**
     * Pre-initialization of the base contract
     */
    constructor() {
        _initialized = true;
    }

    /**
     * @dev Initial minting function buys asset tokens for stable USD tokens
     * and mints LP-tokens. At the time of purchase price of 1 LP token == 1 USD token
     * @param amount Amount of LP tokens minted to creator of DIF
     * @param creator Address of creator
     * @param settings Addresses of the tokens, Uniswap router and vault
     * @param weights What part of USD tokens will be used to purchase asset tokens
     * @param paths Paths of Uniswap router for multihop exchange
     */
    function initialMint(
        uint256 amount,
        address creator,
        address lpToken,
        IFactoryDIF.DIFPrefences memory settings,
        uint256[] memory weights,
        bytes[] memory paths
    ) external {
        require(!_initialized, "Already initialized");
        _factory = IFactoryDIF(msg.sender);
        _settings = settings;
        _lpToken = ILPToken(lpToken);
        _lpToken.mint(creator, amount);
        for (uint256 i = 0; i < paths.length; i++) {
            address inToken = _toAddress(paths[i], 0);
            // amountIn - cost in USDT with decimals
            uint256 amountIn = (amount * weights[i]) /
                GRANULARITY /
                (10 ** (18 - IERC20Metadata(inToken).decimals()));
            IERC20(_settings.USDToken).approve(
                address(_settings.uniswapRouter),
                amountIn
            );
            uint256 amountOut = _settings.uniswapRouter.exactInput(
                ISwapRouter.ExactInputParams({
                    path: paths[i],
                    recipient: _settings.vault,
                    deadline: block.timestamp,
                    amountIn: amountIn,
                    amountOutMinimum: 0
                })
            );
            address outToken = _toAddress(paths[i], paths[i].length - 20);
            /**
             * FIXME: check amount formula
             */
            _assets.set(
                outToken,
                (amountOut *
                    weights[i] *
                    10 ** IERC20Metadata(inToken).decimals()) /
                    amountIn /
                    GRANULARITY
            );
        }
        _initialized = true;
    }

    /**
     * @dev Mint function takes USD-tokens, buys asset tokens and mints LP-tokens
     * @param amount Amount of LP-tokens which the user will receive
     * @param paths Paths of Uniswap router for multihop exchange
     */
    function mint(uint256 amount, bytes[] memory paths) external {
        require(
            paths.length > 0 && paths.length <= _assets.length(),
            "Invalid length of arrays"
        );
        _lpToken.mint(msg.sender, amount);
        for (uint256 i = 0; i < _assets.length(); i++) {
            (bool success, uint256 price) = _assets.tryGet(
                _toAddress(paths[i], 0)
            );
            require(success, "Invalid token address");
            /**
             * FIXME: check formula
             */
            uint256 amountOut = (price * amount) / 1e18;
            uint256 amountIn = _settings.uniswapQuoter.quoteExactOutput(
                paths[i],
                amountOut
            );
            _settings.USDToken.safeTransferFrom(
                msg.sender,
                address(this),
                amountIn
            );
            _settings.USDToken.safeApprove(
                address(_settings.uniswapRouter),
                amountIn
            );
            _settings.uniswapRouter.exactOutput(
                ISwapRouter.ExactOutputParams({
                    path: paths[i],
                    recipient: _settings.vault,
                    deadline: block.timestamp,
                    amountOut: amountOut,
                    amountInMaximum: amountIn
                })
            );
        }
    }

    /**
     * @dev Issue function takes asset tokens and mints LP-tokens
     * @param amount Amount of LP-tokens which the user will receive
     */
    function issue(uint256 amount) external {
        _lpToken.mint(msg.sender, amount);
        for (uint256 i = 0; i < _assets.length(); i++) {
            (address token, uint256 price) = _assets.at(i);
            /**
             * FIXME: check amount formula
             */
            IERC20(token).safeTransferFrom(
                msg.sender,
                _settings.vault,
                (price * amount) / 1e18
            );
        }
    }

    /**
     * @dev Redeem function burns LP-token and transfers asset tokens
     * @param amount Amount of LP-tokns
     */
    function redeemAssets(uint256 amount) external {
        for (uint256 i = 0; i < _assets.length(); i++) {
            (address token, uint256 price) = _assets.at(i);
            /**
             * FIXME: check amount formula
             */
            _factory.transferAsset(token, msg.sender, (amount * price) / 1e18);
        }
        _lpToken.burn(msg.sender, amount);
    }

    /**
     * @dev Redeem function burns LP-token, 
     * @dev purchases asset tokens for USD and transfers CRUS tokens
     * @param amount Amount of LP-tokns
     */
    function redeem(uint256 amount, bytes[] memory paths) external {
        require(
            paths.length > 0 && paths.length == _assets.length(),
            "Invalid length of arrays"
        );
        for (uint256 i = 0; i < paths.length; i++) {
            address token = _toAddress(paths[i], 0);
            (bool success, uint256 price) = _assets.tryGet(token);
            require(success, "Invalid token address");
            /**
             * FIXME: check amount formula
             */
            uint256 assetAmount = (amount * price) / 1e18;
            _factory.transferAsset(token, address(this), assetAmount);
            IERC20(token).safeApprove(
                address(_settings.uniswapRouter),
                assetAmount
            );
            _settings.uniswapRouter.exactInput(
                ISwapRouter.ExactInputParams({
                    path: paths[i],
                    recipient: address(msg.sender),
                    deadline: block.timestamp,
                    amountIn: assetAmount,
                    amountOutMinimum: 0
                })
            );
        }
        _lpToken.burn(msg.sender, amount);
    }

    function _toAddress(
        bytes memory _bytes,
        uint256 _start
    ) internal pure returns (address) {
        // require(_start + 20 >= _start, "toAddress_overflow");
        require(_bytes.length >= _start + 20, "toAddress_outOfBounds");
        address tempAddress;

        assembly {
            tempAddress := div(
                mload(add(add(_bytes, 0x20), _start)),
                0x1000000000000000000000000
            )
        }
        return tempAddress;
    }

    function _subtractPercentage(
        uint256 amount,
        uint256 percent
    ) internal pure returns (uint256 remains, uint256 share) {
        share = _calcPercent(amount, percent);
        return (amount - share, share);
    }

    function _calcPercent(
        uint256 amount,
        uint256 percent
    ) internal pure returns (uint256 share) {
        return ((amount * percent) / GRANULARITY);
    }
}

