        if (_msgSender() != auction.owner) revert NotOwnerNotApproved();

        if (_startTime < 1) revert InvalidStartTime();
        if (auction.startTime + 60 < _getNow()) revert AuctionInProgress();
        if (_startTime + 300 > auction.endTime) revert InvalidEndTime();

        // Ensure auction not already resulted
        if (auction.resulted) revert AuctionEnded();
        if (auction.endTime < 1) revert NoAuctionExists();

