/// @notice This is the main NFT contract for Tero retirement.
/// @dev This contract is used to mint NFTs for Tero retirement.
contract TeroAposentadoriaV2 is ERC721AUpgradeable, OwnableUpgradeable {
    using Strings for uint256;

    uint256 public minted;
    string public baseURI;
    bool public paused;

    uint256 public MainCertificate_id;
