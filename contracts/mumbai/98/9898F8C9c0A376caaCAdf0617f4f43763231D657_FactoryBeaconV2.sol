    uint256 certificate_id, address _factory)initializerERC721A initializer public {
        
        __ERC721A_init(_name, _Sid);
        __Ownable_init();
        transferOwnership(_owner);

        factory = _factory;

        MainCertificate_id = certificate_id;
        paused = true;
    }

    /* Utility Methods */

    function setFactory(address _factory) public {
        factory = _factory;
    }

    function setMainCertificate(address _MainCertificate) public {
        require(IFactoryBeacon(factory).getAllowed(msg.sender),"Tero Aposentadoria: Not allowed");
        require(_MainCertificate != address(0),"Tero Aposentadoria: Cannot use zero address");
        MainCertificate = _MainCertificate;
    }

    function getCertificateId() public view returns(uint256){
        return MainCertificate_id;
    }

    // function getBits(uint256 _input, uint256 _startBit, uint256 _length) private pure returns (uint256) {
    //     uint256 bitMask = ((1 << _length) - 1) << _startBit;

    //     uint256 outBits = _input & bitMask;

    //     return outBits >> _startBit;
    // }

    /* Pausable */

    function setPaused(bool _state) external payable {
        require(IFactoryBeacon(factory).getAllowed(msg.sender),"Tero Aposentadoria: Not allowed");
        paused = _state;
        emit Pause(msg.sender, _state);
    }

    /* ETH Withdrawals */

    function ownerPullETH() external {
        require(IFactoryBeacon(factory).getAllowed(msg.sender),"Tero Aposentadoria: Not allowed");        
        payable(msg.sender).transfer(address(this).balance);
        require(address(this).balance == 0, "Tero Aposentadoria: ETH Transfer Failed");
    }

    /* Minting */


    /// @notice mint function
    /// @param _amount The amount of NFTs to be minted
    
    function mintRetire(address to,uint256 _amount) external whenNotPaused returns(uint256){
        require(IFactoryBeacon(factory).getAllowed(msg.sender),"Tero Aposentadoria: Not allowed");
        require(MainCertificate != address(0), "Tero Aposentadoria: Main Certificate Not Set Yet");
        require(msg.sender == MainCertificate, "Tero Aposentadoria: Address Not Allowed");

        minted += _amount;
        _setAux(to, _getAux(to) + uint64(_amount));

        _mint(to, _amount);

        emit MintedRetirement(to, _nextTokenId() - 1);

        return _nextTokenId() - 1;
    }

