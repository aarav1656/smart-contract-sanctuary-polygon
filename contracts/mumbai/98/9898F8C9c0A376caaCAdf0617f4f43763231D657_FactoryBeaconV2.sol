pragma solidity >=0.8.15;


import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";


import "./Interfaces/ITeroAposentadoria.sol";
import "./Interfaces/ITeroDestoken.sol";
import "./Interfaces/ITeroTransfer.sol";
import "./Interfaces/IFactoryBeacon.sol";




contract TeroCarbonV2 is ERC1155Upgradeable,
OwnableUpgradeable,ReentrancyGuardUpgradeable, UUPSUpgradeable{ 
    using StringsUpgradeable for uint256;


    event NewProject(uint256 indexed id, uint256 indexed supply,uint256 indexed totalCO2,address creator);
    event TokenURIChanged(address indexed creator, uint256 indexed id, string indexed newuri);
    event Blacklisted(address indexed blacklisted);
    event Frozen(address indexed frozenAccount);
    event Quarantined(address indexed caller, bool indexed situation);
    event Retirement(address indexed owner, uint256 indexed projectID, uint256 indexed amount);
    event Destoken(address indexed account, uint256 indexed projectID);
    event Retoken(address indexed receiver,address olderAddress,uint256 indexed projectID,
    uint256 indexed retiredTokenID);

    //Transfer batch function not available on this contract
    error notAvailable();

    //Retirement policy for CO2 compensation
    mapping(uint256 => address) public retirement;
    // ITeroAposentadoria public aposentadoria;
    ITeroDestokenizacao public destokenizationContract;
    ITeroTransfer public transferCertificate;
    IFactoryBeacon public factoryBeacon;

    //ERC1155 variables
    mapping(uint256 => uint256) public minted;
    mapping(uint256 => uint256) public burned;
    mapping(uint256 => uint256) public frozen;

    // CO2 certificate program compliance
    struct Project {
        string certificateDescription;
        uint256 certificateId;
        bool quarantine;
        uint256 totalCO2;
    }
    mapping(uint256 => Project) public certificates;

    mapping(address => bool) public blacklist;
    mapping(address => bool) public frozenAccounts;

    // tokens URI
    string private _baseUri;
    mapping(uint256 => string) private _tokenURIs;

    // ERC1155 self address
    // address private immutable __self = address(this);

    /// -----------------------------------------------------------------------
    /// Constructor
    /// -----------------------------------------------------------------------

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }



    /// @notice constructor for ERC1155 tokens of CO2
    /// @param allowed_controller Owner of the contract
    function initialize(address allowed_controller,address _factory) public initializer {
        __ERC1155_init("");
        __ReentrancyGuard_init();
        __Ownable_init();

        transferOwnership(allowed_controller);
        factoryBeacon = IFactoryBeacon(_factory);
    }


    /// -----------------------------------------------------------------------
    /// OZ UUPS REQUIREMENT
    /// -----------------------------------------------------------------------

    function _authorizeUpgrade(address) internal override onlyOwner {}


    /// -----------------------------------------------------------------------
    /// MINT PROJECTS
    /// -----------------------------------------------------------------------

    function mintNewProject(
        string memory _certificate,
        uint256 _supply,
        uint256 _totalCO2,
        address[] memory _distributions,
        uint256[] memory _shares,
        uint256 _id,
        address _retirementAddress
    ) public onlyProxy returns (Project memory) {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        require(_distributions.length == _shares.length,"Tero Carbon: Wrong amount of initial holders and amounts");

        uint256 total;

        for (uint256 i = 0; i < _shares.length; i++) {
            total += _shares[i];
        }
        require(total == 100, "Tero Carbon: Not summing right");

        retirement[_id] = _retirementAddress;

        _distribute(_distributions, _shares, _supply, _id);
        setTokenURI(_id, _id.toString());
        certificates[_id] = Project(_certificate, _id, false, _totalCO2);

        emit NewProject(_id, _supply, _totalCO2, msg.sender);

        return certificates[_id];
    }

    ///@dev Private function to distribute amounts on the mint of a Project
    function _distribute(
        address[] memory _distributions,
        uint256[] memory _shares,
        uint256 _supply,
        uint256 _ProjectID
    ) private {
        uint256 amount;
        uint256 mint;

        for (uint256 i = 0; i < _shares.length; i++) {
            amount = (_shares[i] * _supply) / 100;
            mint += amount;
            _mint(_distributions[i], _ProjectID, amount, "");
        }

        if (mint < _supply) {
            _mint(owner(), _ProjectID, (_supply - mint), "");
        }

        minted[_ProjectID] += _supply;
    }

    /// -----------------------------------------------------------------------
    /// ERC1155 functions
    /// -----------------------------------------------------------------------

    function uri(uint256) public view override returns (string memory) {
        return _baseUri;
    }

    function setBaseURI(string memory _newuri) public onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        require(
            keccak256(bytes(_newuri)) != keccak256(bytes(_baseUri)),
            "New URI is the same as current URI"
        );
        _baseUri = _newuri;
    }

    function setTokenURI(uint256 _tokenId, string memory _newuri) public onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        require(minted[_tokenId] > 0, "Token Not minted yet");
        require(
            keccak256(bytes(_newuri)) != keccak256(bytes(_tokenURIs[_tokenId])),
            "New URI is the same as current URI"
        );
        _tokenURIs[_tokenId] = _newuri;
        emit TokenURIChanged(msg.sender, _tokenId, _newuri);
    }

    function tokenURI(uint256 _tokenId) public view returns (string memory) {
        require(minted[_tokenId] != 0, "Tero Carbon: Token Id not minted yet!");

        string memory _tokenURI = _tokenURIs[_tokenId];
        string memory base = _baseUri;

   
        if (bytes(_tokenURI).length == 0) {
            return string(abi.encodePacked(base, _tokenId.toString(), ".json"));
        }
       
        if (bytes(_tokenURI).length > 0) {
            return _tokenURI;
        }

        return "Tero Carbon: Base set and tokenId URI not";
    }

    /**
     * @dev See {IERC1155-setApprovalForAll}.
     */
    function setApprovalForAll(address _address, bool _approved) public override onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        _setApprovalForAll(_address, address(this), _approved);
    }

    /// -----------------------------------------------------------------------
    /// User and Project controllers
    /// -----------------------------------------------------------------------
    function getProjectInfo(uint256 _ProjectID) public view returns (Project memory) {
        return certificates[_ProjectID];
    }

    function addBlacklist(address _addr) public onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        blacklist[_addr] = true;
        emit Blacklisted(_addr);
    }

    function removeBlacklist(address _addr) public onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        blacklist[_addr] = false;
    }

    function _freeze(address _addr) internal {
        frozenAccounts[_addr] = true;
        emit Frozen(_addr);
    }

    function _unfreeze(address _addr) internal {
        frozenAccounts[_addr] = false;
    }

    function startQuarantine(uint256 _ProjectID) public quarantined(_ProjectID) onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        certificates[_ProjectID].quarantine = true;
        emit Quarantined(msg.sender, true);
    }

    function stopQuarantine(uint256 _ProjectID) public onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        require(certificates[_ProjectID].quarantine, "Tero Carbon: Not quarantined!");
        certificates[_ProjectID].quarantine = false;
        emit Quarantined(msg.sender, false);
    }

    ///@dev CHECK THIS FUNCTION
    function adminMint(uint256 _id, uint256 _amount) external payable quarantined(_id) onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        _mint(msg.sender, _id, _amount, "");
        minted[_id] += _amount;
    }

    function setTransferCertificate(address _transferCertif) public onlyProxy {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
        transferCertificate = ITeroTransfer(_transferCertif);
    }

    /// -----------------------------------------------------------------------
    /// Retirement
    /// -----------------------------------------------------------------------

    function retire(
        address _from,
        uint256 _amount,
        uint256 _ProjectID
    ) public checkFrozen(_from) quarantined(_ProjectID) nonReentrant onlyProxy returns (uint256) {
        require(factoryBeacon.getAllowed(msg.sender), "Tero Carbon: Not allowed");
