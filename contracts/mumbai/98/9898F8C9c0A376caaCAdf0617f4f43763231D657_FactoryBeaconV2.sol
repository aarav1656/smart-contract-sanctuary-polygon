import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts/proxy/beacon/IBeacon.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

///@param TeroCarbon is the ERC1155Upgradeable regarding the certificate
import "./TeroAposentadoria.sol";
import "./TeroCarbon.sol";


///@dev This is the contract that will be cloned and will 
//allow implementations of new certificates
contract FactoryBeaconV2 is Ownable{

    struct Project {
        string certificateDescription;
        uint256 certificateId;
        bool quarantine;
        uint256 totalCO2;
    }

    struct _deployed {
        Project ProjectCO2;
        address proxyRetirement;
    }

    address public transferCertif;
    address public destokenization;

    address public beacon1155;
    address public beacon721;

    address public address1155;

    mapping(uint256 => _deployed)private teroCertific;
    mapping(address => bool) private allow;


    event BuiltNewProject(uint256 indexed id, address[] indexed distributions, uint256[] indexed shares);
    event ChangeAllowed(address indexed newAllow, bool indexed state);
    event ChangedAddress(address indexed oldAddr, address indexed newAddr);

    ///@dev the implementation contract will own the TeroBeacon
    constructor(address _owner){
       
        require(_owner != address(0), "Owner on zero address");

        transferOwnership(_owner);
        allow[msg.sender] = true;
        allow[address(this)] = true;
    }

    function buildTero(address _owner, string memory _name, string memory _Sid,
        uint256 certificate_id, string memory _projectDescription, uint256 _supply
        , uint256 _totalCO2, address[] memory _distributions, uint256[] memory _shares) 
    external payable allowed returns(_deployed memory){

        require(teroCertific[certificate_id].proxyRetirement == address(0),"Factory Tero: Project Id already exists");

        BeaconProxy teroAposentadoriaProxy = new BeaconProxy(beacon721,
        abi.encodeWithSelector(TeroAposentadoriaV2(address(0)).initialize.selector, 
        _owner, _name,_Sid,certificate_id, address(this)));

        TeroCarbonV2(address1155).mintNewProject(_projectDescription, _supply, _totalCO2,
        _distributions, _shares, certificate_id, address(teroAposentadoriaProxy));

        TeroAposentadoriaV2(address(teroAposentadoriaProxy)).setMainCertificate(address1155);

        addAllow(address(teroAposentadoriaProxy));

        teroCertific[certificate_id] = _deployed({
            ProjectCO2:Project(_projectDescription,certificate_id,false,_totalCO2), 
            proxyRetirement:address(teroAposentadoriaProxy)});

        emit BuiltNewProject(certificate_id, _distributions, _shares);

        return teroCertific[certificate_id];


    }


    function setAddresses(address _beacon1155, address _beacon721,address _transferCertif, address destoken)
        public allowed{
        require(_beacon1155 != address(0), "Address1155 on zero address");
        require(_beacon721 != address(0), "Beacon721 on zero address");
        require(_transferCertif != address(0), "Transfer Certificate on zero address");
        require(destoken != address(0), "Destokenization on zero address");

        // BeaconProxy teroCarbonProxy = new BeaconProxy(_beacon1155,
        // abi.encodeWithSelector(TeroCarbonV2(address(0)).initialize.selector, 
        // owner(), address(this)));


        // address1155 = address(teroCarbonProxy);
        address1155 = _beacon1155;
        beacon721 = _beacon721;
        transferCertif = _transferCertif;
        destokenization = destoken;

        addAllow(address1155);
        addAllow(_beacon1155);
        addAllow(_beacon721);
        addAllow(_transferCertif);
        addAllow(destoken);

    }

    function getTeroAddresses(uint256 _Idcertificate) public view returns (_deployed memory) {
        return teroCertific[_Idcertificate];
    }

    function get1155() public view returns (address){
        return address1155;
    }

    function getBeacon721()public view returns (address) {
        return beacon721;
    }
    function getImplementation721() public view returns (address){
        return IBeacon(beacon721).implementation();
    }

    function getAllowed(address addr) public view returns(bool){
        return allow[addr];
    }

    function changeTransferContract(address _newAddr) public allowed returns(address){
        emit ChangedAddress(transferCertif, _newAddr);
        return transferCertif = _newAddr;
    }
    function changeDestokenContract(address _newAddr) public allowed returns(address){
        emit ChangedAddress(destokenization, _newAddr);
        return destokenization = _newAddr;
    }
    function changeBeacon721Contract(address _newAddr) public allowed returns(address){
        emit ChangedAddress(beacon721, _newAddr);
        return beacon721 = _newAddr;
    }
    function changeERC1155Contract(address _newAddr) public allowed returns(address){
        emit ChangedAddress(address1155, _newAddr);
        return address1155 = _newAddr;
    }


    function addAllow(address _addr) public allowed {
        // require(!allow[_addr],"Already allowed!");
        allow[_addr] = true;

        emit ChangeAllowed(_addr, true);
    }

    function removeAllow(address _addr) public allowed {
        require(allow[_addr],"Not allowed yet!");
        allow[_addr] = false;
        emit ChangeAllowed(_addr, false);
    }

    /// -----------------------------------------------------------------------
    /// unlock funds
    /// -----------------------------------------------------------------------

    function withdraw(address _addr) allowed public{
        require(_addr != address(0), "Transfer to zero address");
        payable(_addr).transfer(address(this).balance);
    }




    modifier allowed(){
        require(allow[msg.sender] || msg.sender == owner(),"Not alowed");
        _;
    }


}

