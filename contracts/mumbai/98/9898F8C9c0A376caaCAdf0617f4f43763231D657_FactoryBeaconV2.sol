         string(abi.encodePacked(baseURI, tokenId.toString(),".json"));
    }

    function setTokenURI(uint256 tokenId, string memory _tokenURI) public {
        require(IFactoryBeacon(factory).getAllowed(msg.sender),"Tero Aposentadoria: Not allowed");
        require(_exists(tokenId), "Tero Aposentadoria: URI set of nonexistent token");
        _tokenURIs[tokenId] = _tokenURI;
        emit TokenURIChange(msg.sender, tokenId, _tokenURI);
    }



    function getBaseURI() external view returns (string memory) {
        return baseURI;
    }



