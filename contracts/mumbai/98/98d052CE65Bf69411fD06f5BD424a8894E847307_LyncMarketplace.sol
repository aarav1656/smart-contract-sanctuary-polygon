pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./Structs.sol";
import "hardhat/console.sol";

error PriceNotMet(address nftAddress, uint256 tokenId, uint256 price);
error ItemNotForSale(address nftAddress, uint256 tokenId);
error TokenNotListed();
error AlreadyListed(address nftAddress, uint256 tokenId);
error NoProceeds();
error NotOwner();
error NoSuchListing();
error NotApprovedForMarketplace();
error SelectValidTokenStandard();
error PriceMustBeAboveZero(uint256 _price);
error NftAddressDoesNotExist(address nftAddress);

contract LyncMarketplace is
    Ownable,
    ReentrancyGuard,
    ERC721Holder,
    ERC1155Holder
{
    event ItemsListed(
        NFTStandard nftStandard,
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 tokenQuantity,
        uint256 price
    );
    event ItemsCanceled(
        NFTStandard nftStandard,
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 tokenQuantity
    );

    event ItemsBought(
        NFTStandard nftStandard,
        address indexed buyer,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 tokenQuantity,
        uint256 price
    );

    event ItemsModified(
        NFTStandard nftStandard,
        address indexed seller,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 tokenQuantity,
        uint256 price
    );
    // State Variables

    address public nftaddress;

    //@dev contract can have multiple admins
    //address payable[] public adminAddresses;
    address payable public adminAddress;

    //@dev percentFeesAdmin id
    uint256 percentFeesAdmin = 4;

    tokenBuyer[] public tokenBuyersArray;
    mapping(address => mapping(uint256 => tokenSeller[])) public s_listings;
    mapping(address => uint256) private s_proceeds;

    //@dev calculation to made with 100x for better decimal handling
    uint256 internal constant FLOAT_HANDLER_TEN_4 = 10000;

    //@dev contructor takes only one argument, adminFees, to be set in perecentage
    constructor() {
        // percentFeesAdmin = _adminFeePercent;
        adminAddress = payable(msg.sender);
    }

    modifier onlyAdmin() {
        // require(checkIfAdmin(msg.sender), "Only admins can call this fucntion");
        require(
            msg.sender == adminAddress,
            "Only admins can call this fucntion"
        );
        _;
    }

    //@dev modifier checks if the NFT is listed in Marketplace
    // Function modifiers
    modifier notListed(
        address nftAddress,
        uint256 tokenId,
        uint256 tokenQuantity
    ) {
        bool check = false;

        tokenSeller[] memory listings = s_listings[nftAddress][tokenId];
        for (uint256 i = 0; i < listings.length; i++) {
            tokenSeller memory tempStructSeller = listings[i];
            if (
                tempStructSeller.sellerAddress == msg.sender &&
                tempStructSeller.tokenQuantity > 0
            ) {
                check = true;
                break;
            }
        }
        if (check == true) {
            revert AlreadyListed(nftAddress, tokenId);
        }
        _;
    }

    //@dev modifier checks if the requested NFT lisitng is owned
    //by sender
    function checkIsOwner(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity
    ) internal view returns (bool isOwner) {
        if (_nftStandard == NFTStandard.E1155) {
            IERC1155 nft = IERC1155(_nftAddress);

            uint256 amount = nft.balanceOf(msg.sender, _tokenId);
            if (amount < _tokenQuantity) {
                revert NotOwner();
            } else {
                if (
                    !IERC1155(_nftAddress).isApprovedForAll(
                        msg.sender,
                        address(this)
                    )
                ) {
                    revert NotApprovedForMarketplace();
                } else {
                    isOwner = true;
                }
            }
        } else if (_nftStandard == NFTStandard.E721) {
            require(
                _tokenQuantity == 1,
                "for ERC721 nft, Quantity should be 1"
            );
            address owner = IERC721(_nftAddress).ownerOf(_tokenId);
            if (msg.sender != owner) {
                revert NotOwner();
            } else {
                if (
                    IERC721(_nftAddress).getApproved(_tokenId) != address(this)
                ) {
                    revert NotApprovedForMarketplace();
                } else {
                    isOwner = true;
                }
            }
        } else {
            revert SelectValidTokenStandard();
        }
        return isOwner;
    }

    //@dev checks if the NFT is approved for the Marketplace to call
    //SafeTrasferFrom
    function checkIfApproved(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId
    ) internal view returns (bool isApproved) {
        if (_nftStandard == NFTStandard.E1155) {
            if (
                !IERC1155(_nftAddress).isApprovedForAll(
                    msg.sender,
                    address(this)
                )
            ) {
                isApproved = false;
            } else {
                isApproved = true;
            }
        } else if (_nftStandard == NFTStandard.E721) {
            IERC721 nft = IERC721(_nftAddress);
            if (nft.getApproved(_tokenId) != address(this)) {
                isApproved = false;
            } else {
                isApproved = true;
            }
        } else {
            revert SelectValidTokenStandard();
        }
        return isApproved;
    }

    //@dev modifier checks if the NFT is already listed in Marketplace
    modifier isListed(
        tokenSeller[] memory listings,
        uint256 tokenQuantity,
        address spender
    ) {
        bool check = false;
        //tokenSeller[] memory listings = s_listings[nftAddress][tokenId];
        for (uint256 i = 0; i < listings.length; i++) {
            if (
                listings[i].sellerAddress == spender &&
                listings[i].tokenQuantity >= tokenQuantity
            ) {
                check = true;
                _;
                break;
            }
        }
        if (check == false) {
            revert TokenNotListed();
        }
    }

    //@dev modifier checks if the NFT is already listed in Marketplace
    modifier isListedWithMarketplace(
        tokenSeller[] memory listings,
        address spender
    ) {
        bool check = false;
        //tokenSeller[] memory listings = s_listings[nftAddress][tokenId];
        for (uint256 i = 0; i < listings.length; i++) {
            if (listings[i].sellerAddress == spender) {
                check = true;
                _;
                break;
            }
        }
        if (check == false) {
            revert TokenNotListed();
        }
    }

    modifier isListedSeller(address _nftAddress, uint256 _tokenId) {
        bool value = false;
        tokenSeller[] memory listings = s_listings[_nftAddress][_tokenId];
        for (uint256 i = 0; i < listings.length; i++) {
            if (listings[i].sellerAddress == msg.sender) {
                value = true;
                break;
            }
        }
        if (value = false) {
            revert NoSuchListing();
        }
        _;
    }

    function listItem(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity,
        uint256 _price
    ) external notListed(_nftAddress, _tokenId, _tokenQuantity) {
        //this fucntion checks the owner as well as approved for this contract
        bool isOwnerAndApproved = checkIsOwner(
            _nftStandard,
            _nftAddress,
            _tokenId,
            _tokenQuantity
        );
        //bool isApproved = checkIfApproved(_nftStandard,_nftAddress,_tokenId);
        if (isOwnerAndApproved == true) {
            if (_price <= 0) {
                revert PriceMustBeAboveZero(_price);
            }

            tokenSeller memory tempSellerStruct;

            if (
                _nftStandard == NFTStandard.E1155 ||
                _nftStandard == NFTStandard.E721
            ) {
                tempSellerStruct.nftStandard = _nftStandard;
                tempSellerStruct.sellerAddress = msg.sender;
                tempSellerStruct.tokenQuantity = _tokenQuantity;
                tempSellerStruct.tokenPrice = _price;
            } else {
                revert SelectValidTokenStandard();
            }

            //tokenSeller[] storage tempArray = s_listings[nftAddress][tokenId];
            s_listings[_nftAddress][_tokenId].push(tempSellerStruct);
            if (_nftStandard == NFTStandard.E1155) {
                IERC1155(_nftAddress).safeTransferFrom(
                    msg.sender,
                    address(this),
                    _tokenId,
                    _tokenQuantity,
                    ""
                );
            } else if (_nftStandard == NFTStandard.E721) {
                IERC721(_nftAddress).safeTransferFrom(
                    msg.sender,
                    address(this),
                    _tokenId
                );
            }
            // s_listings[nftAddress][tokenId] = tempArray;
            emit ItemsListed(
                _nftStandard,
                msg.sender,
                _nftAddress,
                _tokenId,
                _tokenQuantity,
                _price
            );
            console.log("item listed");
        } else {
            revert NotOwner();
        }
    }

    function cancelListing(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity
    )
        external
        isListed(s_listings[_nftAddress][_tokenId], _tokenQuantity, msg.sender)
        nonReentrant
    {
        tokenSeller[] storage listings = s_listings[_nftAddress][_tokenId];
        uint256 index = getIndexOfArray(listings, msg.sender);
        require(
            listings[index].sellerAddress == msg.sender,
            "Only listing address can revoke the listing"
        );
        if (listings[index].nftStandard == NFTStandard.E1155) {
            IERC1155(_nftAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenId,
                _tokenQuantity,
                ""
            );
        } else if (listings[index].nftStandard == NFTStandard.E721) {
            IERC721(_nftAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenId
            );
        }

        removeListing(listings, index);

        emit ItemsCanceled(
            _nftStandard,
            msg.sender,
            _nftAddress,
            _tokenQuantity,
            _tokenId
        );

        console.log("Items listing cancelled");
    }

    function modifyListing(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity,
        uint256 _newPrice
    )
        external
        isListedWithMarketplace(s_listings[_nftAddress][_tokenId], msg.sender)
        nonReentrant
    {
        require(_newPrice != 0, "Listing price cannot be zero");
        tokenSeller[] storage listings = s_listings[_nftAddress][_tokenId];
        uint256 index = getIndexOfArray(listings, msg.sender);
        require(
            listings[index].sellerAddress == msg.sender,
            "Only listing address can revoke the listing"
        );
        require(
            listings[index].nftStandard == _nftStandard,
            "Incorrect NFT standard"
        );

        if (listings[index].nftStandard == NFTStandard.E1155) {
            if (listings[index].tokenQuantity > _tokenQuantity) {
                IERC1155(_nftAddress).safeTransferFrom(
                    address(this),
                    msg.sender,
                    _tokenId,
                    listings[index].tokenQuantity - _tokenQuantity,
                    ""
                );
            } else if (_tokenQuantity > listings[index].tokenQuantity) {
                IERC1155(_nftAddress).safeTransferFrom(
                    msg.sender,
                    address(this),
                    _tokenId,
                    _tokenQuantity - listings[index].tokenQuantity,
                    ""
                );
            }
            listings[index].tokenQuantity = _tokenQuantity;
            listings[index].tokenPrice = _newPrice;
        } else if (listings[index].nftStandard == NFTStandard.E721) {
            listings[index].tokenPrice = _newPrice;
        }

        emit ItemsModified(
            _nftStandard,
            msg.sender,
            _nftAddress,
            _tokenId,
            _tokenQuantity,
            _newPrice
        );
        console.log("listing modified");
    }

    function buyItem(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity,
        address _sellerAddress
    )
        external
        payable
        isListed(
            s_listings[_nftAddress][_tokenId],
            _tokenQuantity,
            _sellerAddress
        )
        nonReentrant
    {
        tokenSeller[] storage listings = s_listings[_nftAddress][_tokenId];

        uint256 index = getIndexOfArray(listings, _sellerAddress);
        require(
            msg.value >= (listings[index].tokenPrice) * (_tokenQuantity),
            "Price not met"
        );

        if (listings[index].nftStandard == NFTStandard.E1155) {
            IERC1155(_nftAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenId,
                _tokenQuantity,
                ""
            );
        } else if (listings[index].nftStandard == NFTStandard.E721) {
            IERC721(_nftAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenId
            );
        }

        UpdateTokenBuyersArray(
            _nftStandard,
            _nftAddress,
            _tokenId,
            _tokenQuantity,
            _sellerAddress
        );

        //calculates fees and sends them to seller
        splitFeesAdminSeller(msg.value, _sellerAddress);

        emit ItemsBought(
            _nftStandard,
            msg.sender,
            _nftAddress,
            _tokenId,
            _tokenQuantity,
            listings[index].tokenPrice
        );
        if (listings[index].tokenQuantity - _tokenQuantity == 0) {
            removeListing(listings, index);
        } else {
            uint256 newQuantity = listings[index].tokenQuantity -
                _tokenQuantity;
            updateListingData(listings, index, newQuantity);
        }
        console.log("Item bought");
    }

    function updateListingData(
        tokenSeller[] storage tokenSellers,
        uint256 _index,
        uint256 _newQuantity
    ) internal {
        tokenSellers[_index].tokenQuantity = _newQuantity;
    }

    function UpdateTokenBuyersArray(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity,
        address _sellerAddress
    ) private {
        tokenBuyer memory tokenBuyers;
        tokenBuyers.nftStandard = _nftStandard;
        tokenBuyers.nftAddress = _nftAddress;
        tokenBuyers.tokenId = _tokenId;
        tokenBuyers.tokenQuantity = _tokenQuantity;
        tokenBuyers.sellerAddress = _sellerAddress;
        tokenBuyers.buyerAddress = msg.sender;

        tokenBuyersArray.push(tokenBuyers);
    }

    //@dev fucntion updates listing for exsisting listing,
    // tokenQuantity can be changed only for E1155 listings
    //price can be changed for both E1155 and E721
    function updateListingPriceAndQuantity(
        NFTStandard _nftStandard,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _tokenQuantity,
        uint256 _newPrice
    ) external isListedSeller(_nftAddress, _tokenId) nonReentrant {
        bool isOwnerAndApproved = checkIsOwner(
            _nftStandard,
            _nftAddress,
            _tokenId,
            _tokenQuantity
        );
        if (isOwnerAndApproved == true) {
            if (_newPrice == 0) {
                revert PriceMustBeAboveZero(_newPrice);
            }
            tokenSeller[] storage listings = s_listings[_nftAddress][_tokenId];
            uint256 index = getIndexOfArray(listings, msg.sender);
            listings[index].tokenQuantity = _tokenQuantity;
            listings[index].tokenPrice = _newPrice;

            emit ItemsListed(
                _nftStandard,
                msg.sender,
                _nftAddress,
                _tokenId,
                _tokenQuantity,
                _newPrice
            );
        }
    }

    // @dev function allows admin to withdraw all token Amounts
    function withdrawContractProceeds() external onlyAdmin {
        payable(msg.sender).transfer(address(this).balance);
    }

    //@dev this function gets the listing for perticular NFT Address and TokenId
    function getListingOfCaller(address nftAddress, uint256 tokenId)
        external
        view
        returns (tokenSeller memory)
    {
        tokenSeller[] storage listings = s_listings[nftAddress][tokenId];
        uint256 index = getIndexOfArray(listings, msg.sender);
        return listings[index];
    }

    // @dev Function is used to remove listing either bycancel listing or after the Item is soldout
    function removeListing(tokenSeller[] storage sellerArray, uint256 _index)
        internal
    {
        sellerArray[_index] = sellerArray[sellerArray.length - 1];
        sellerArray.pop();
    }

    //@dev this fucntion is used to get Index of Address in a tokenSeller array
    function getIndexOfArray(
        tokenSeller[] storage sellerArray,
        address _sellerAddress
    ) internal view returns (uint256) {
        uint256 index;
        bool value = false;
        for (uint256 i = 0; i < sellerArray.length; i++) {
            if (sellerArray[i].sellerAddress == _sellerAddress) {
                index = i;
                value = true;
            }
        }
        if (value == false) {
            revert NoSuchListing();
        }
        return index;
    }

    //@dev this fucntion is used to set percent fees for every transaction
    // on the marketpalce
    function setFeesForAdmin(uint256 _percentFees) external onlyAdmin {
        require(_percentFees < 100, "Fees cannot exceed 100 %");
        percentFeesAdmin = _percentFees;
    }

    //@dev fucntion enables changing for admin address
    function changeAdminAddress(address payable _newAdminAddress)
        external
        onlyAdmin
    {
        require(_newAdminAddress != address(0), "Null address cannot be admin");
        adminAddress = _newAdminAddress;
    }

    //@dev Fucntion calculates admin fees to be deducted from total amount
    //and split the totalamout according to fees structure
    function splitFeesAdminSeller(uint256 _totalAmount, address _sellerAddress)
        internal
    {
        require(_totalAmount != 0, "_totalAmount must be greater than 0");
        uint256 amountToSeller = (_totalAmount * (100 - percentFeesAdmin)) /
            100;

        payable(_sellerAddress).transfer(amountToSeller);
        console.log(
            "fees admin, seller",
            _totalAmount - amountToSeller,
            amountToSeller
        );
    }

    //@dev functions gives us the data for listing data for
    function getListingdata(address _nftAddress, uint256 _tokenId)
        public
        view
        returns (
            // address _sellerAddress
            tokenSeller memory
        )
    {
        tokenSeller[] storage listings = s_listings[_nftAddress][_tokenId];
        uint256 index = getIndexOfArray(listings, msg.sender);
        return listings[index];
    }
}

