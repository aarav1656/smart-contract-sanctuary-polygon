contract Dex is Ownable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    struct Token {
        address tokenAddress;
        uint256 weight;
    }

struct Pair {
    Token tokenA;
    Token tokenB;
    uint256 totalSupply;
    mapping(address => uint256) balance;
    mapping(address => uint256) lastBlock;
    mapping(address => uint256) rewards;
    uint256 lpAmount; // Add lpAmount as a member of the struct
}


    uint256 public constant SWAP_FEE = 300; // 0.3%
    uint256 public constant REWARD_PER_BLOCK = 1; // Reward per block in LP tokens
    uint256 public constant BLOCKS_PER_REWARD = 10; // Number of blocks per reward

    mapping(address => Pair) public pairs;
    mapping(address => bool) public isPair;
    address public feeRecipient;
    uint256 public totalSupply;

    event AddLiquidity(address indexed user, address indexed pairAddress, uint256 amountA, uint256 amountB, uint256 lpAmount);
    event RemoveLiquidity(address indexed user, address indexed pairAddress, uint256 lpAmount, uint256 amountA, uint256 amountB);
    event Swap(address indexed user, address indexed pairAddress, address indexed tokenIn, address tokenOut, uint256 amountIn, uint256 amountOut);
    event ClaimReward(address indexed user, address indexed pair, uint256 rewards);
    event PairCreated(address indexed pair, address indexed token0, address indexed token1, uint256 weight0, uint256 weight1);
    event EmergencyWithdraw(address indexed user, address indexed token, uint256 amount);
    event EmergencyWithdrawETH(address indexed user, uint256 amount);

    constructor(address _feeRecipient) {
        feeRecipient = _feeRecipient;
    }

    function addLiquidity(address pairAddress, uint256 amountA, uint256 amountB) external {
        require(isPair[pairAddress], "Pair does not exist");
        Pair storage pair = pairs[pairAddress];

        IERC20(pair.tokenA.tokenAddress).safeTransferFrom(msg.sender, address(this), amountA);
        IERC20(pair.tokenB.tokenAddress).safeTransferFrom(msg.sender, address(this), amountB);

        uint256 lpAmount;
        if (pair.totalSupply == 0) {
            lpAmount = sqrt(amountA.mul(amountB));
        } else {
            uint256 reserves = sqrt(pair.totalSupply.mul(pair.tokenA.weight.mul(amountB).div(pair.tokenB.weight)).mul(pair.tokenB.weight).div(pair.tokenA.weight));
            lpAmount = reserves.mul(sqrt(amountA.mul(amountB))).div(pair.totalSupply);
        }

        pair.totalSupply = pair.totalSupply.add(lpAmount);
        pair.balance[msg.sender] = pair.balance[msg.sender].add(lpAmount);
        pair.lastBlock[msg.sender] = block.number;

        emit AddLiquidity(msg.sender, pairAddress, amountA, amountB, lpAmount);
    }

    function removeLiquidity(address pairAddress, uint256 lpAmount) external {
        require(isPair[pairAddress], "Pair does not exist");
        Pair storage pair = pairs[pairAddress];

        require(pair.balance[msg.sender] >= lpAmount, "Insufficient LP balance");

        uint256 amountA = lpAmount.mul(pair.tokenA.weight).div(sqrt(pair.totalSupply)).mul(pair.tokenA.weight).div(pair.tokenB.weight);
        uint256 amountB = lpAmount.mul(pair.tokenB.weight).div(sqrt(pair.totalSupply)).mul(pair.tokenB.weight).div(pair.tokenA.weight);

        pair.totalSupply = pair.totalSupply.sub(lpAmount);
        pair.balance[msg.sender] = pair.balance[msg.sender].sub(lpAmount);
        pair.lastBlock[msg.sender] = block.number;
            IERC20(pair.tokenA.tokenAddress).safeTransfer(msg.sender, amountA);
    IERC20(pair.tokenB.tokenAddress).safeTransfer(msg.sender, amountB);

    emit RemoveLiquidity(msg.sender, pairAddress, lpAmount, amountA, amountB);
}

function swap(address pairAddress, address tokenIn, address tokenOut, uint256 amountIn) external {
    require(isPair[pairAddress], "Pair does not exist");

    uint256 amountOut = getSwapOutput(pairAddress, tokenIn, tokenOut, amountIn);
    require(amountOut > 0, "Insufficient swap output");

    IERC20(tokenIn).safeTransferFrom(msg.sender, address(this), amountIn);
    IERC20(tokenOut).safeTransfer(msg.sender, amountOut);

    emit Swap(msg.sender, pairAddress, tokenIn, tokenOut, amountIn, amountOut);
}

function getSwapOutput(address pairAddress, address tokenIn, address tokenOut, uint256 amountIn) public view returns (uint256) {
    require(isPair[pairAddress], "Pair does not exist");
    Pair storage pair = pairs[pairAddress];

    Token memory tokenA = pair.tokenA;
    Token memory tokenB = pair.tokenB;

    uint256 reservesA = sqrt(pair.totalSupply.mul(tokenA.weight).mul(tokenB.weight).div(tokenB.weight));
    uint256 reservesB = sqrt(pair.totalSupply.mul(tokenB.weight).mul(tokenA.weight).div(tokenA.weight));

    if (tokenIn == tokenA.tokenAddress) {
        uint256 amountInWithFee = amountIn.mul(10000 - SWAP_FEE).div(10000);
        uint256 amountOut = reservesB.sub(amountInWithFee.mul(reservesB).div(reservesA.add(amountInWithFee)));
        return amountOut;
    } else if (tokenIn == tokenB.tokenAddress) {
        uint256 amountInWithFee = amountIn.mul(10000 - SWAP_FEE).div(10000);
        uint256 amountOut = reservesA.sub(amountInWithFee.mul(reservesA).div(reservesB.add(amountInWithFee)));
        return amountOut;
    } else {
        revert("Invalid input token");
    }
}

function claimReward(address pairAddress) external {
    require(isPair[pairAddress], "Pair does not exist");
    Pair storage pair = pairs[pairAddress];

    uint256 blocksPassed = block.number.sub(pair.lastBlock[msg.sender]);
    uint256 rewards = blocksPassed.mul(REWARD_PER_BLOCK);

    pair.rewards[msg.sender] = pair.rewards[msg.sender].add(rewards);
    pair.lastBlock[msg.sender] = block.number;

    IERC20(pairAddress).safeTransfer(msg.sender, rewards);

    emit ClaimReward(msg.sender, pairAddress, rewards);
}

function sqrt(uint256 x) internal pure returns (uint256) {
    uint256 z = (x + 1) / 2;
    uint256 y = x;
    while (z < y) {
        y = z;
        z = (x / z + z) / 2;
    }
    return y;
}

function createPair(address tokenA, address tokenB, uint256 weightA, uint256 weightB) external onlyOwner {
    require(tokenA != address(0) && tokenB != address(0), "Invalid token address");
    require(weightA > 0 && weightB > 0, "Invalid token weight");
    address pairAddress = getPairAddress(tokenA, tokenB);
    require(!isPair[pairAddress], "Pair already exists");

    Pair storage pair = pairs[pairAddress];
    pair.tokenA = Token(tokenA, weightA);
    pair.tokenB = Token(tokenB, weightB);
    pair.totalSupply = 0;
    pair.lpAmount = 0;
    pair.lastBlock[msg.sender] = block.number;

    isPair[pairAddress] = true;

    emit PairCreated(pairAddress, tokenA, tokenB, weightA, weightB);
}
function getPairAddress(address tokenA, address tokenB) public pure returns (address) {
    require(tokenA != address(0) && tokenB != address(0), "Invalid token address");

    address pairAddress;
    bytes32 salt;
    if (tokenA < tokenB) {
        salt = keccak256(abi.encodePacked(tokenA, tokenB));
    } else {
        salt = keccak256(abi.encodePacked(tokenB, tokenA));
    }

    pairAddress = address(uint160(bytes20(salt)));
    return pairAddress;
}




function emergencyWithdraw(address tokenAddress, uint256 amount) external onlyOwner {
    require(tokenAddress != address(0), "Invalid token address");

    IERC20(tokenAddress).safeTransfer(owner(), amount);

    emit EmergencyWithdraw(msg.sender, tokenAddress, amount);
}

function emergencyWithdrawETH(uint256 amount) external onlyOwner {
    require(amount <= address(this).balance, "Insufficient ETH balance");

    payable(owner()).transfer(amount);

    emit EmergencyWithdrawETH(msg.sender, amount);
}
