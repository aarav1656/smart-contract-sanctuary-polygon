    bytes32 public MINT_PROMPT;
    bytes32 public BURN_PROMPT;

    modifier onlyOwner() {
        require(msg.sender == owner, "Unauthorized");
        _;
    }

    event ownerChanged(address indexed newowner, address indexed oldowner);
    event accessManagerChanged(
        AccessManager indexed oldAccessManager,
        AccessManager indexed newAccessManager
    );

    constructor(
        string memory name_,
        string memory symbol_,
        AccessManager accessManager_
    ) ERC20(name_, symbol_) {
        owner = msg.sender;
        accessManager = accessManager_;

        // defaults
        BURN_PROMPT = keccak256(abi.encodePacked("BURN_CCL_TOKEN"));
        MINT_PROMPT = keccak256(abi.encodePacked("MINT_CCL_TOKEN"));
    }

    function changeOwner(address newOwner_) external onlyOwner {
        owner = newOwner_;
        emit ownerChanged(newOwner_, msg.sender);
    }

    function changeAccessManager(AccessManager newAccessManager)
        external
        onlyOwner
    {
        AccessManager oldAccessManager = accessManager;
        accessManager = newAccessManager;
        emit accessManagerChanged(oldAccessManager, newAccessManager);
    }

    function chanegMintPrompt(string memory mintPrompt) public onlyOwner {
        MINT_PROMPT = keccak256(abi.encodePacked(mintPrompt));
    }

    function chanegBurnPrompt(string memory burnPrompt) public onlyOwner {
        BURN_PROMPT = keccak256(abi.encodePacked(burnPrompt));
    }

    function mintCCL(address account, uint256 amount) external {
        bool isAuthorised = accessManager.roles(MINT_PROMPT, msg.sender);
        require(isAuthorised == true, "Invalid minter");
        _mint(account, amount);
    }

    function burnCCL(address account, uint256 amount) external {
        bool isAuthorised = accessManager.roles(BURN_PROMPT, msg.sender);
        require(isAuthorised == true, "Invalid burner");
        _burn(account, amount);
    }
}

