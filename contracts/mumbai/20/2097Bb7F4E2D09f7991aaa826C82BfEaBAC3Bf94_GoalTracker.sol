pragma solidity 0.8.17;
import "../tokens/cclERC20.sol";
import "hardhat/console.sol";

contract GoalTracker {
    address public owner;
    CCL public CCLToken;

    struct GoalData {
        uint256 createdAt;
        uint256 totalReward;
        uint256 milestone;
        uint256 milestoneAccomplised;
        address assignedTo;
        address assigner;
        string goalhash;
    }

    struct UserData {
        bool isManager;
        bool doesExits;
        string email;
        address[] reportingTeamMembers;
        GoalData[] goalsCreated;
        GoalData[] myGoals;
    }

    uint256 constant MINIMUM_REWARD_CONSTRAINT = 1000000000;

    mapping(address => UserData) public registeredUserData;
    mapping(address => mapping(address => bool)) public reportingMemberExist;
    mapping(address => mapping(string => bool)) public goalExist;
    mapping(bytes32 => bool) private emailExists;

    event ownerChanged(address indexed newowner, address indexed oldowner);

    modifier onlyOwner() {
        require(msg.sender == owner, "Unauthorized");
        _;
    }

    modifier userShouldExits(address user) {
        require(
            registeredUserData[user].doesExits == true,
            "User is not registered"
        );
        _;
    }

    modifier onlyManager(address user) {
        require(
            registeredUserData[user].isManager == true,
            "Manager is not registered"
        );
        _;
    }

    constructor(CCL cclToken) {
        owner = msg.sender;
        CCLToken = cclToken;
    }

    function changeOwner(address newOwner_) external onlyOwner {
        owner = newOwner_;
        emit ownerChanged(newOwner_, msg.sender);
    }

    function registerUser(string calldata email) external {
        require(
            registeredUserData[msg.sender].doesExits == false,
            "User already registered"
        );

        bytes32 emailBytes = keccak256(abi.encodePacked(email));
        require(emailExists[emailBytes] == false, "email already exits");

        address[] memory fellowTeamMember_ = new address[](0);

        registeredUserData[msg.sender].email = email;
        registeredUserData[msg.sender].doesExits = true;
        registeredUserData[msg.sender].reportingTeamMembers = fellowTeamMember_;

        emailExists[emailBytes] = true;
    }

    function updateUser(string calldata newEmail)
        external
        userShouldExits(msg.sender)
    {
        bytes32 newEmailBytes = keccak256(abi.encodePacked(newEmail));
        UserData storage user = registeredUserData[msg.sender];
        bytes32 oldEmailBytes = keccak256(abi.encode(user.email));

        require(emailExists[newEmailBytes] == false, "email already exits");
        registeredUserData[msg.sender].email = newEmail;

        emailExists[oldEmailBytes] = false;
        emailExists[newEmailBytes] = true;
    }

    function appointManager(address wallet)
        external
        userShouldExits(wallet)
        onlyOwner
    {
        require(
            registeredUserData[wallet].isManager == false,
            "Manager already appointed"
        );

        registeredUserData[wallet].isManager = true;
    }

    function revokeManager(address wallet)
        external
        userShouldExits(wallet)
        onlyOwner
    {
        require(
            registeredUserData[wallet].isManager == true,
            "Manager already removed"
        );

        address[] memory reportingMembers_ = new address[](0);
        UserData storage manager = registeredUserData[wallet];
        manager.isManager = false;
        manager.reportingTeamMembers = reportingMembers_;
    }

    function assignReportingManager(address newManager, address reportingMember)
        external
        onlyOwner
        onlyManager(newManager)
        userShouldExits(reportingMember)
    {
        require(
            reportingMemberExist[newManager][reportingMember] == false,
            "reporting member already assigned"
        );

        reportingMemberExist[newManager][reportingMember] = true;

        UserData storage manager = registeredUserData[newManager];
        manager.reportingTeamMembers.push(reportingMember);
    }

    function updateReportingManger(
        address oldManager,
        address newManager,
        address reportingMember,
        uint256 reportingMemberIndex
    ) external onlyManager(newManager) onlyOwner {
        require(
            reportingMemberExist[oldManager][reportingMember] == true,
            "Invalid reporing member"
        );

        reportingMemberExist[oldManager][reportingMember] = false;
        reportingMemberExist[newManager][reportingMember] = true;

        UserData storage oldManagerData = registeredUserData[oldManager];

        for (
            uint256 i = reportingMemberIndex;
            i < oldManagerData.reportingTeamMembers.length - 1;
            i++
        ) {
            oldManagerData.reportingTeamMembers[i] = oldManagerData
                .reportingTeamMembers[i + 1];
        }

        oldManagerData.reportingTeamMembers.pop();
        UserData storage newManagerData = registeredUserData[newManager];
        newManagerData.reportingTeamMembers.push(reportingMember);
    }

    function removeReportingMember(
        address manager_,
        address reportingMember,
        uint256 reportingMemberIndex
    ) external onlyManager(manager_) onlyOwner {
        require(
            reportingMemberExist[manager_][reportingMember] == true,
            "Invalid reporing member"
        );

        UserData storage manager = registeredUserData[manager_];

        reportingMemberExist[manager_][reportingMember] = false;

        for (
            uint256 i = reportingMemberIndex;
            i < manager.reportingTeamMembers.length - 1;
            i++
        ) {
            manager.reportingTeamMembers[i] = manager.reportingTeamMembers[
                i + 1
            ];
        }

        manager.reportingTeamMembers.pop();
    }

    function createGoal(
        string memory ipfsHash,
        address assignee,
        uint256 totalReward,
        uint256 milestone
    ) external onlyManager(msg.sender) userShouldExits(assignee) {
        require(goalExist[msg.sender][ipfsHash] == false, "Goal already exist");
        require(
            totalReward >= MINIMUM_REWARD_CONSTRAINT,
            "minimum reward constraint"
        );
        require(
            reportingMemberExist[msg.sender][assignee] == true,
            "Invalid Reporting member"
        );

        goalExist[msg.sender][ipfsHash] = true;

        GoalData memory goal = GoalData(
            block.timestamp,
            totalReward,
            milestone,
            0,
            assignee,
            msg.sender,
            ipfsHash
        );

        UserData storage manager = registeredUserData[msg.sender];
        manager.goalsCreated.push(goal);

        UserData storage user = registeredUserData[assignee];
        user.myGoals.push(goal);
    }

    function updateMilestone(uint256 goalIndex, string calldata ipfsHash)
        external
        onlyManager(msg.sender)
    {
        require(goalExist[msg.sender][ipfsHash] == true, "Goal doesnt exist");

        UserData storage manager = registeredUserData[msg.sender];
        manager.goalsCreated[goalIndex].milestoneAccomplised += 1;

        address userAddr = manager.goalsCreated[goalIndex].assignedTo;

        UserData storage user = registeredUserData[userAddr];

        user.myGoals[goalIndex].milestoneAccomplised += 1;

        require(
            manager.goalsCreated[goalIndex].milestoneAccomplised <=
                manager.goalsCreated[goalIndex].milestone,
            "All milestone already completed"
        );

        require(
            keccak256(
                abi.encodePacked(manager.goalsCreated[goalIndex].goalhash)
            ) == keccak256(abi.encodePacked(ipfsHash)),
            "Incorrect goal index"
        );

        CCLToken.mintCCL(
            manager.goalsCreated[goalIndex].assignedTo,
            manager.goalsCreated[goalIndex].totalReward /
                manager.goalsCreated[goalIndex].milestone
        );
    }

    function getReportingTeamMembers(address user)
        external
        view
        returns (address[] memory)
    {
        return registeredUserData[user].reportingTeamMembers;
    }

    function getCreatedGoals(address user)
        external
        view
        returns (GoalData[] memory)
    {
        return registeredUserData[user].goalsCreated;
    }

    function getAssignedGoals(address user)
        external
        view
        returns (GoalData[] memory)
    {
        return registeredUserData[user].myGoals;
    }
}

