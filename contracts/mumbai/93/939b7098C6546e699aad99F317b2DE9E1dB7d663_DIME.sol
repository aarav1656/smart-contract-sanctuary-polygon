//SPDX-License-Identifier: GPL-3.0
pragma solidity ^0.8.2;

import "hardhat/console.sol";

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interfaces/IUserProfiles.sol";
import "./interfaces/IKUtils.sol";
import "./interfaces/IHashtags.sol";
import "./interfaces/ITagged.sol";
import "./interfaces/IPosts.sol";
import "./interfaces/ILikes.sol";
import "./interfaces/IMessageData.sol";
import "./interfaces/IDOOM.sol";
import "./interfaces/IGroups.sol";
import "./interfaces/ITips.sol";
import "./interfaces/IBlocking.sol";

contract DIME is Pausable, ReentrancyGuard {

    // Admins
    mapping (address => bool) admins;

    // Total messages posted ever
    uint256 public msgCounter;

    // Total messages posted ever
    uint256 public commentCounter;

    // Total allowed tagged accounts per post
    uint256 public maxTaggedAccounts;

    // Total allowed hashtags per post
    uint256 public maxHashtags;

    // Max length of messages to save (UTF-8 single byte characters only)
    uint256 public maxMessageLength;

    // ERC20 Receiver for payment via ERC20
    IERC20 public paymentToken;

    // Cost to post a message
    uint256 public costToPost;

    // Cost to like aa post
    uint256 public costToLike;

    // The max number of messages that can be returned at once
    uint256 public maxMsgReturnCount;

    // Keep a ledger of all tips owed for claiming
    mapping (address => uint256) tipsOwed;

    // Percentage cut of tips for service costs
    uint256 cut = 1;

    // Vault address for cut
    address vaultAddress;

    // Link the User Profiles contract
    IUserProfiles public userProfiles;

    // Link to the KUtils
    IKUtils public KUtils;

    // Link to the Hashtags
    IHashtags public Hashtags;

    // Link to the Tagged Accounts
    ITagged public Tagged;

    // Link to the Posts
    IPosts public Posts;

    // Link to the Likes
    ILikes public Likes;

    // Link to the Message Data
    IMessageData public MessageData;

    // Link to the DOOM Token
    IDOOM public DOOM;

    // Link to the Groups
    IGroups public Groups;

    // Link to the Tips
    ITips public Tips;

    // Link to the Blocking
    IBlocking public Blocking;


    constructor(IERC20 _paymentToken, address _userProfiles, address _kutils) {
        //    constructor(IERC20 _paymentToken, uint256 _costToPost, uint256 _msgCounter, uint256 _maxMessageLength, address _prevContract, uint256 _maxMsgReturnCount, uint256 maxTaggedAccounts, uint256 maxHashtags, uint256 _maxHashtagLength, uint256 _contractRelease) {

        // Setup the payment token
        paymentToken = IERC20(_paymentToken);

        // Setup link to User Profiles
        userProfiles = IUserProfiles(_userProfiles);

        // Setup link to KUtils
        KUtils = IKUtils(_kutils);

        // Setup the default Admin
        admins[msg.sender] = true;

        costToPost = 1 ether;   // In wei
        msgCounter = 0;
        maxMessageLength = 420;
        maxMsgReturnCount = 25;
        maxTaggedAccounts = 5;
        maxHashtags = 10;
        costToLike = 0.1 ether; // In wei
    }


    /*

    MODIFIERS

    */

    modifier onlyAdmins() {
        require(admins[msg.sender], "Only admins can call this function.");
        _;
    }


    /*

    ADMIN FUNCTIONS

    */

    function addAdmin(address newAdmin) public onlyAdmins{
        admins[newAdmin] = true;
    }

    function removeAdmin(address oldAdmin) public onlyAdmins{
        admins[oldAdmin] = false;
    }

    function pause() public onlyAdmins {
        _pause();
    }

    function unpause() public onlyAdmins {
        _unpause();
    }

    function setVaultAddress(address _newVault) public onlyAdmins{
        vaultAddress = _newVault;
    }

    function setParams(uint256 _maxMsgReturnCount, uint256 _maxMessageLength) public onlyAdmins {
        maxMsgReturnCount = _maxMsgReturnCount;
        maxMessageLength = _maxMessageLength;
    }

    function updateCosts(uint256 _costToPost, uint256 _costToLike) public onlyAdmins {
        //  Update the cost of DOOM in wei to post a new message
        costToPost = _costToPost;

        //  Update the cost of DOOM in wei to like a message
        costToLike = _costToLike;
    }


    // Contract Addresses
    // 0 = _userProfiles
    // 1 = _hashtags
    // 2 = _tagged
    // 3 = _posts
    // 4 = _likes
    // 5 = _messageData
    // 6 = _doom
    // 7 = _groups
    // 8 = _tips
    // 9 = _blocking
    function updateContracts(IERC20 _payments, address[] calldata contracts) public onlyAdmins {
        // Update the contract address of the ERC20 token to be used as payment
        paymentToken = IERC20(_payments);

        // Update the User Profiles contract address
        userProfiles = IUserProfiles(contracts[0]);

        // Update the Hashtags address
        Hashtags = IHashtags(contracts[1]);

        // Update the Tagged addresses
        Tagged = ITagged(contracts[2]);

        // Update the Posts addresses
        Posts = IPosts(contracts[3]);

        // Update the Likes addresses
        Likes = ILikes(contracts[4]);

        // Update the Message Data addresses
        MessageData = IMessageData(contracts[5]);

        // Update the DOOM Token address
        DOOM = IDOOM(contracts[6]);

        // Update the Comments
        Groups = IGroups(contracts[7]);

        // Update the Tips
        Tips = ITips(contracts[8]);

        // Update Blocking
        Blocking = IBlocking(contracts[9]);
    }

    /*

    PUBLIC FUNCTIONS

    */

    // Comment Attributes
    // 0 = Comment Level Allowed (0 = No comments Allowed, 1 = Comments Allowed)
    // 1 = Message ID of the post it is a comment to
    // 2 = Message ID of post if it's a repost of another post
    // 3 = Group ID to be posted as

    // Groups
    // inGroups; The Group IDs that this message is being posted into

    function postMsg(string calldata message, string[] memory _hashtags, address[] calldata taggedAccounts, string calldata uri, uint256[4] calldata attribs, uint256[] calldata inGroups) public payable nonReentrant whenNotPaused {
        // require the approval of the sending of the token to the contract
        require(paymentToken.approve(msg.sender, costToPost), "Must approve payment tokens to be transferred");

        // Transfer the Payment Token to the contract
        require(paymentToken.transferFrom(msg.sender, address(this), costToPost), "Didn't receive the Payment Token");

        // Make sure the message is within length limits or that it's a repost
        require(bytes(message).length <= maxMessageLength && (bytes(message).length > 0 || attribs[2] > 0), "Message not within length limits");

        // Initialize the poster address
        address posterAddress = msg.sender;

        // Check for group membership
        if (attribs[3] > 0){
            require(Groups.isMemberOfGroupByID(attribs[3], msg.sender), "You are not a member of that group");

            // Set the poster address to be the group
            posterAddress = Groups.getGroupAddressFromID(attribs[3]);
        }

        // Check if blocked from posting in a group
        for (uint g=0; g < inGroups.length; g++) {
            if (inGroups[g] > 0){
//                    require(Groups.getOwnerOfGroupByID(inGroups[g]) != address(0), "That group doesn't exist");
                require(Blocking.isAllowed(Groups.getGroupAddressFromID(inGroups[g]), msg.sender), "You are blocked from posting in that group");
            }
        }

        // Check if they're being blocked from tagging a user
        for (uint t=0; t < taggedAccounts.length; t++) {
            require(Blocking.isAllowed(taggedAccounts[t], msg.sender), "You are blocked by a tagged user / group");
        }

        // If there are tips, transfer them to the new owners
        uint256 tipPerTag = 0;
        if (msg.value > 0){
            require(taggedAccounts.length > 0, "You need to have tagged accounts to send the tips to");
            // Calculate how much to tip each person tagged
            tipPerTag = msg.value / taggedAccounts.length;
            for (uint t=0; t < taggedAccounts.length; t++) {
                tipsOwed[taggedAccounts[t]] += tipPerTag;
            }
        }


        // Check if the message is posted by contract or person
        uint256 postedByContract = tx.origin == msg.sender ? 0 : 1;

        // Set the proxy poster address (if posted as a group)
        address postProxy = msg.sender == posterAddress ? address(0) : msg.sender;


        // If the new message is a comment
        if (attribs[1] > 0){

            // check if the original message is allowing comments
            require(MessageData.getMsgCommentLevel(attribs[1]) == 1, "Comments not allowed");

            // Increment the amount of comments we have posted
            commentCounter++;

        }

        // Increment the amount of messages we have posted
        msgCounter++;

        // Update the hashtag Mapping with this message ID and hashtag
        if (_hashtags.length > 0){
            Hashtags.addHashtags(msgCounter, _hashtags);
        }

        // Update the tagged accounts with this message ID and tagged user address
        if (taggedAccounts.length > 0){
            Tagged.addTags(msgCounter, taggedAccounts);
        }

        IMessageData.MsgData memory newMsg;

        newMsg.msgID = msgCounter;
        newMsg.postedBy = [posterAddress, postProxy];
        newMsg.message = message;
        newMsg.paid = costToPost;
        newMsg.hashtags = _hashtags;
        newMsg.taggedAccounts = taggedAccounts;
        newMsg.asGroup = attribs[3];
        newMsg.inGroups = inGroups;
        newMsg.uri = uri;
        newMsg.commentLevel = attribs[0];
        newMsg.isCommentOf = attribs[1];
        newMsg.isRepostOf = attribs[2];
        newMsg.msgStats.postByContract = postedByContract;
        newMsg.msgStats.time = block.timestamp;
        newMsg.msgStats.block = block.number;
        newMsg.msgStats.tipsReceived = msg.value;

        MessageData.saveMsg(newMsg);

        // Log the message post
        userProfiles.logPost(posterAddress, tipPerTag, taggedAccounts, attribs[1]);

    }

    function eraseMsg(uint256 msgID) public whenNotPaused nonReentrant {

        // Erase the message
        MessageData.removeMsg(msgID, msg.sender);

        // TODO: I'm sure there's more to do here

    }

    function checkTips(address addr) public view whenNotPaused returns (uint256){
        return tipsOwed[addr];
    }

    function claimTips() public whenNotPaused nonReentrant {
        // Check how much they're owed
        uint256 balance = checkTips(msg.sender);

        // Only run if they're owed anything
        require(balance > 0, "You don't have any tips to claim");

        // Close out what's owed before sending
        tipsOwed[msg.sender] = 0;

        // Transfer tips to their account
        sendValue(payable(msg.sender), balance);
    }

    function addTip(uint256 msgID) public payable nonReentrant whenNotPaused {

        // Calculate tips and sf
        uint256 sf = 0;
        if (cut > 0){
            sf = msg.value - ((msg.value * (100 - cut)) / 100)  ;
            tipsOwed[vaultAddress] += sf;
        }

        uint256 tip = msg.value - sf;

        // Add the tip to their owed balance
        tipsOwed[MessageData.getPoster(msgID)] += tip;

        // Record the tip
        Tips.addTip(msgID, msg.sender, tip);
    }

    function toggleLike(uint256 msgID) public whenNotPaused nonReentrant {
        // Check if this user already liked the post
        if (Likes.checkUserLikeMsg(msg.sender, msgID)) {
            // Unlike a post if so
            Likes.removeLike(msgID, msg.sender);
        } else {
            // require the approval of the sending of the token to the contract
            require(paymentToken.approve(msg.sender, costToLike), "Must approve payment tokens to be transferred");

            // Transfer the Payment Token to the contract
            require(paymentToken.transferFrom(msg.sender, MessageData.getPoster(msgID), costToLike), "Didn't receive the Payment Token");

            // Like a post
            Likes.addLike(msgID, msg.sender);
        }
    }

    function followUser(address userToFollow) public whenNotPaused nonReentrant {
        // Follow the user
        userProfiles.followUser(msg.sender,userToFollow);
    }

    function unfollowUser(address userToUnfollow) public whenNotPaused nonReentrant {
        // Follow the user
        userProfiles.unfollowUser(msg.sender, userToUnfollow);
    }

    function getMsgIDsByAddress(address usrAddress, uint256 startFrom, bool getUserComments, bool getUserReposts) public view whenNotPaused returns (uint256[] memory) {

        // Initialize the array as 256 to sent to Posts
        uint256[] memory whatToGet = new uint256[](3);

        // If we're getting posts from a user for a specific post, swap the flag
        if (getUserComments){
            whatToGet[0] = 1;
        } else if (getUserReposts){
            whatToGet[2] = 1;
        }

        return Posts.getMsgIDsByAddress(usrAddress, startFrom, whatToGet);
    }


    // Returns a list of comment or repost IDs of a given message ID
    function getSubIDsByPost(uint256 msgID, uint256 startFrom, bool isRepost) public view whenNotPaused returns (uint256[] memory) {
        // Initialize the array as 256 to sent to Posts
        uint256[] memory whatToGet = new uint256[](3);

        // Set the vars
        if (isRepost){
            // Get Reposts
            whatToGet[2] = 2;
        } else {
            // Get Comments
            whatToGet[0] = 2;
        }
        whatToGet[1] = msgID;

        return Posts.getMsgIDsByAddress(address(0), startFrom, whatToGet);
    }

    function getMsgIDsByHashtag(string memory hashtag, uint256 startFrom) public view whenNotPaused returns (uint256[] memory) {
        return Hashtags.getMsgIDsFromHashtag(hashtag, startFrom);
    }

    function getMsgIDsByTag(address taggedUser, uint256 startFrom) public view whenNotPaused returns (uint256[] memory) {
        return Tagged.getTaggedMsgIDs(taggedUser, startFrom);
    }

    function getMsgsByIDs(uint256[] calldata msgIDs) public view whenNotPaused returns (string[][] memory) {
        require(msgIDs.length <= maxMsgReturnCount , "Requested too many messages. Lower to the limit.");

        return MessageData.getMsgsByIDs(msgIDs);
    }

    function sendValue(address payable recipient, uint256 amount) internal {
        require(address(this).balance >= amount, "Address: insufficient balance");

        (bool success, ) = recipient.call{value: amount}("");
        require(success, "Address: unable to send value, recipient may have reverted");
    }

    function append(string memory a, string memory b, string memory c, string memory d, string memory e) internal pure returns (string memory) {
        return string(abi.encodePacked(a, b, c, d, e));
    }
}

