    struct GroupDetails {
        address ownerAddress;
        address[] members;
        string groupName;
        string baseURI;
        address groupAddress;
    }

    // Function to set group details on initial mint
    function setInitialDetails(uint256 _groupID, address _owner, string memory groupName, string memory _baseURI, address setInitialDetails) external;

    // Get group owner address by Group ID
    function getOwnerOfGroupByID(uint256 groupID) external view returns (address);

