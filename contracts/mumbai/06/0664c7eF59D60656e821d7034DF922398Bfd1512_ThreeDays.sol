pragma solidity ^0.8.17;

import "@openzeppelin/contracts/utils/structs/DoubleEndedQueue.sol";

library FixedQueueLib {
    struct FixedQueue {
        uint256 maxLength;
        DoubleEndedQueue.Bytes32Deque queue;
    }

    function length(FixedQueue storage queue) internal view returns (uint256) {
        return DoubleEndedQueue.length(queue.queue);
    }

    function push(FixedQueue storage queue, uint256 data) internal returns (bool, uint256) {
        require(queue.maxLength > 0, "NO_ROOM");

        bool dropped = false;
        uint256 _data = 0;

        if (DoubleEndedQueue.length(queue.queue) == queue.maxLength) {
            dropped = true;
            _data = uint256(DoubleEndedQueue.popFront(queue.queue));
        }

        DoubleEndedQueue.pushBack(queue.queue, bytes32(data));

        return (dropped, _data);
    }

    function pop(FixedQueue storage queue) internal returns (uint256) {
        require(DoubleEndedQueue.length(queue.queue) > 0, "NO_ITEMS");
        return uint256(DoubleEndedQueue.popFront(queue.queue));
    }

    function contains(FixedQueue storage queue, uint256 data) internal view returns (bool, uint256) {
        for (uint256 i = 0; i < DoubleEndedQueue.length(queue.queue); i++) {
            if (data == uint256(DoubleEndedQueue.at(queue.queue, i))) {
                return (true, i);
            }
        }
        return (false, 0);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.17;

import "./libraries/FixedQueueLib.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

import "operator-filter-registry/src/DefaultOperatorFilterer.sol";

contract ThreeDays is ERC721, ERC2981, Pausable, Ownable, ReentrancyGuard, DefaultOperatorFilterer {
    using Strings for uint256;

    enum UriLevel {
        DEFAULT,
        SAFE,
        ONE_DAYS,
        TWO_DAYS,
        THREE_DAYS,
        DEAD
    }

    uint256 public constant MAX_PER_MINT = 20;
    uint256 public constant MAX_SUPPLY = 3333;
    uint256 public constant ENDING_SUPPLY = 10;
    uint256 public constant MINT_PRICE = 15 ether;
    uint256 public constant THREE_DAYS = 3 seconds;

    string public baseURI;
    string public prerevealURI;
    bool public revealed = false;

    bytes32 public merkleRoot;
    bool public burnStarted = false;
    mapping(uint256 => address) public burnedLastOwners;

    bool private mintLocked = false;
    uint256 private tokenIdCounter = 1;
    uint256 private burnCounter;
    mapping(address => uint256) private numUserTokensMinted;

    FixedQueueLib.FixedQueue private tokenQueue;
    uint256 private defaultTokenTTL;
    mapping(uint256 => uint256) private tokenTTLs;
    mapping(uint256 => bool) private safeTokenMap;
    mapping(uint256 => uint256) private tokenMintTimestamps;
    mapping(uint256 => uint256) private tokenDeathTimestamps;
    address private salesContract;

    constructor() ERC721("3 Days 2 Live", "3D2L") {
        tokenQueue.maxLength = ENDING_SUPPLY;
        _pause();
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function setBaseURI(string memory _baseURI) external onlyOwner {
        baseURI = _baseURI;
    }

    function setPrerevealURI(string memory _prerevealURI) external onlyOwner {
        prerevealURI = _prerevealURI;
    }

    function setMerkleRoot(bytes32 _merkleRoot) external onlyOwner {
        merkleRoot = _merkleRoot;
    }

    function setSalesContract(address _salesContract) external onlyOwner {
        require(_salesContract != address(0), "INVALID_ADDRESS");
        salesContract = _salesContract;
    }

    function startBurn() external onlyOwner {
        require(totalSupply() >= ENDING_SUPPLY, "NOT_ENOUGH_MINTED");
        require(!burnStarted, "ALREADY_STARTED");
        mintLocked = true;
        burnStarted = true;
        defaultTokenTTL = block.timestamp + THREE_DAYS;
    }

    function reveal() external onlyOwner {
        revealed = true;
    }

    function purge(uint256 _limit) external onlyOwner {
        require(burnStarted, "BURN_NOT_STARTED");
        require(totalSupply() > ENDING_SUPPLY, "ALL_PURGED");

        if (_limit == 0) {
            _limit = tokenIdCounter - 1;
        }

        uint256 _burnCounter = 0;
        uint256 tokenId = 1;

        while (_burnCounter < _limit && tokenId < tokenIdCounter) {
            if (_tokenIsExpired(tokenId)) {
                _burn(tokenId);
                _burnCounter += 1;
            }
            tokenId += 1;
        }
    }

    function mint(uint256 _amount, bytes32[] calldata _merkleProof) external payable whenNotPaused nonReentrant {
        require(_amount > 0, "INVALID_AMOUNT");
        require(tokenIdCounter + _amount - 1 <= MAX_SUPPLY, "ALL_MINTED");
        require(_isWhitelisted(msg.sender, _merkleProof), "NOT_WHITELISTED");
        require(!mintLocked, "MINT_LOCKED");

        // if (merkleRoot == 0) {
        //     require(_amount <= MAX_PER_MINT, "OVER_MINT");
        //     require(msg.value == MINT_PRICE * _amount, "INVALID_PAYMENT");
        //     (bool sent, ) = owner().call{value: MINT_PRICE * _amount}("");
        //     require(sent, "TRANSFER_FAILED");
        // } else {
        //     require(numUserTokensMinted[msg.sender] + _amount == 1, "MAX_MINTED");
        // }

        numUserTokensMinted[msg.sender] += _amount;

        uint256 tokenIdStart = tokenIdCounter;
        for (uint256 i = 0; i < _amount; i++) {
            uint256 tokenId = tokenIdStart;
            tokenIdStart += 1;

            _safeMint(msg.sender, tokenId, "");

            tokenMintTimestamps[tokenId] = block.timestamp;

            if (FixedQueueLib.length(tokenQueue) < ENDING_SUPPLY) {
                FixedQueueLib.push(tokenQueue, tokenId);
                safeTokenMap[tokenId] = true;
            }
        }
        tokenIdCounter = tokenIdStart;
    }

    function setApprovalForAll(
        address operator,
        bool approved
    ) public override(ERC721) onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function approve(
        address operator,
        uint256 tokenId
    ) public override(ERC721) onlyAllowedOperatorApproval(operator) {
        super.approve(operator, tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721) onlyAllowedOperator(from) {
        super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public override(ERC721) onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, data);
    }

    function tokenTTL(uint256 _tokenId) public view returns (uint256) {
        require(_exists(_tokenId), "INVALID_TOKEN_ID");
        if (safeTokenMap[_tokenId]) return 0;
        uint256 _tokenTTL = tokenTTLs[_tokenId];
        if (_tokenTTL == 0) {
            _tokenTTL = defaultTokenTTL;
        }
        return _tokenTTL;
    }

    function isTokenSafe(uint256 _tokenId) public view returns (bool) {
        require(_exists(_tokenId), "INVALID_TOKEN_ID");
        return safeTokenMap[_tokenId];
    }

    function positionInSafeList(uint256 _tokenId) public view returns (uint256) {
        require(_exists(_tokenId), "INVALID_TOKEN_ID");
        require(safeTokenMap[_tokenId], "NOT_IN_SAFE_LIST");

        uint256 pos;
        (, pos) = FixedQueueLib.contains(tokenQueue, _tokenId);
        return pos;
    }

    function daysSurvived(uint256 _tokenId) public view returns (uint256) {
        require(tokenMintTimestamps[_tokenId] > 0, "NOT_MINTED");

        uint256 deathTimestamp = tokenDeathTimestamps[_tokenId];
        if (deathTimestamp == 0) {
            deathTimestamp = block.timestamp;
        }

        return (deathTimestamp - tokenMintTimestamps[_tokenId]) / 1 seconds;
    }

    function totalSupply() public view returns (uint256) {
        return (tokenIdCounter - burnCounter) - 1;
    }

    function tokenURI(uint256 _tokenId) public view override returns (string memory) {
        if (!revealed) {
            return prerevealURI;
        } else {
            require(_exists(_tokenId), "INVALID_TOKEN_ID");

            UriLevel level = UriLevel.DEFAULT;
            if (totalSupply() > ENDING_SUPPLY && burnStarted) {
                if (safeTokenMap[_tokenId]) {
                    level = UriLevel.SAFE;
                } else if (_tokenIsExpired(_tokenId)) {
                    level = UriLevel.DEAD;
                } else {
                    uint256 diff = tokenTTL(_tokenId) - block.timestamp;
                    uint256 daysLeft = diff / 1 seconds;
                    if (daysLeft == 3) {
                        level = UriLevel.THREE_DAYS;
                    } else if (daysLeft == 2) {
                        level = UriLevel.TWO_DAYS;
                    } else {
                        level = UriLevel.ONE_DAYS;
                    }
                }
            }

            return
                bytes(baseURI).length > 0
                    ? string(abi.encodePacked(baseURI, _tokenId.toString(), "/", uint256(level).toString(), ".json"))
                    : "";
        }
    }

    function _isWhitelisted(address _owner, bytes32[] calldata _merkleProof) internal view returns (bool) {
        if (merkleRoot == 0) {
            return true;
        }
        bytes32 leaf = keccak256(abi.encodePacked(_owner));
        return MerkleProof.verify(_merkleProof, merkleRoot, leaf);
    }

    function _tokenIsExpired(uint256 _tokenId) internal view returns (bool) {
        if (!_exists(_tokenId)) return false;
        if (defaultTokenTTL == 0 || totalSupply() == ENDING_SUPPLY) return false;
        if (safeTokenMap[_tokenId]) return false;

        return tokenTTL(_tokenId) <= block.timestamp;
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId,
        uint256 _batchSize
    ) internal override(ERC721) whenNotPaused {
        super._beforeTokenTransfer(_from, _to, _tokenId, _batchSize);
        if (_to != address(0)) {
            require(!_tokenIsExpired(_tokenId), "TOKEN_EXPIRED");
            if (totalSupply() > ENDING_SUPPLY && msg.sender == salesContract && burnStarted) {
                if (!safeTokenMap[_tokenId]) {
                    bool popped;
                    uint256 poppedTokenId;
                    (popped, poppedTokenId) = FixedQueueLib.push(tokenQueue, _tokenId);
                    safeTokenMap[_tokenId] = true;
                    if (popped) {
                        tokenTTLs[poppedTokenId] = block.timestamp + THREE_DAYS;
                        safeTokenMap[_tokenId] = false;
                    }
                    delete tokenTTLs[_tokenId];
                }
            }
        } else {
            burnCounter += 1;
            burnedLastOwners[_tokenId] = ownerOf(_tokenId);
            tokenDeathTimestamps[_tokenId] = tokenTTL(_tokenId);
        }
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(
        bytes4 _interfaceId
    ) public view override(ERC721, ERC2981) returns (bool) {
        return super.supportsInterface(_interfaceId);
    }
}

