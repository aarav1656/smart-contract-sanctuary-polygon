pragma solidity ^0.8.10;












/**
 * @title CohortNFTv02StandAlone
 * @author Embed
 * @notice NFT contract for brands to be distributed as access to users
 * @dev Only minter can change settings in the NFT and mint
 * @dev this contract is stand alone and doesn't use the protocol data base or any other contract than the interface
 */

contract CohortNFTv02StandAlone is
    ICohortNFTv02StandAlone,
    Initializable,
    ContextUpgradeable,
    ERC1155Upgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    AccessControlUpgradeable
{
    ///Contract Variable & mappings definition
    using StringsUpgradeable for uint256;

    string internal baseURI;
    string internal TokenIndex;
    bool public useSingleUri = true;
    string internal baseExtension = ".json";
    uint256[] internal tokenTypeIndecies;
    mapping(uint256 => string) public tokenURI;
    mapping(uint256 => ICohortNFTv02StandAlone.CohortTokenType) internal tokenTypes;
    string[] internal cohorts;
    //cohort index -> array of token type indecies it holdes
    mapping(uint256 => uint256[]) cohortTokenTypeList;
    //cohort index -> count of token type indecies it holdes
    mapping(uint256 => uint256) cohortTokenTypeCount;
    string[] internal rarities;
    bool internal _initialized = false;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    ///Contract Initializer
    ///@param _initBaseURI the initial base uri to be used in the contract
    function initialize(string memory _initBaseURI) external initializer {
        require(!_initialized, "CONTRACT_ALREADY_INITIALIAZED");
        _initialized = true;
        baseURI = "/";
        baseExtension = ".json";
        useSingleUri = true;
        __AccessControl_init();
        __ReentrancyGuard_init();
        _setBaseURI(_initBaseURI);
        __ERC1155_init("");
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(MINTER_ROLE, _msgSender());
        cohorts.push("Embed");
        cohorts.push("Sneakers");
        rarities.push("A");
        rarities.push("B");
        rarities.push("C");
        rarities.push("D");
    }

    //Functions for access control
    function addAdmin(address account) external virtual onlyRole(DEFAULT_ADMIN_ROLE) {
        grantRole(DEFAULT_ADMIN_ROLE, account);
    }

    function removeAdmin(address account) external virtual onlyRole(DEFAULT_ADMIN_ROLE) {
        revokeRole(DEFAULT_ADMIN_ROLE, account);
    }

    function addMinter(address account) external virtual onlyRole(DEFAULT_ADMIN_ROLE) {
        grantRole(MINTER_ROLE, account);
    }

    function removeMinter(address account) external virtual onlyRole(DEFAULT_ADMIN_ROLE) {
        revokeRole(MINTER_ROLE, account);
    }

    ///@dev function supports interface for external contracts check
    ///@param interfaceId interface id to check
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155Upgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    ///@dev internal function to return the base uri
    ///@return string of base uri
    function _baseURI() internal view virtual returns (string memory) {
        return baseURI;
    }

    ///@dev function to add a new Token id
    ///@param _supply the max supply of the token id
    ///@param _cohort the cohort index
    ///@param _rarity the rarity of token id
    function addTokenType(uint256 _supply, uint256 _cohort, uint256 _rarity) external onlyRole(MINTER_ROLE) {
        require(_rarity < rarities.length, "Rarity index exceeded");
        require(_cohort < cohorts.length, "Rarity index exceeded");
        cohortTokenTypeList[_cohort].push(tokenTypeIndecies.length);
        tokenTypes[tokenTypeIndecies.length] = ICohortNFTv02StandAlone.CohortTokenType(
            cohortTokenTypeList[_cohort].length -1,
            _supply,
            0,
            _cohort,
            _rarity,
            true
        );
        tokenTypeIndecies.push(tokenTypeIndecies.length);
    }

    ///@dev function to update new Token id
    ///@param _index the id of the token
    ///@param _supply the max supply of the token id
    ///@param _cohort the cohort index
    ///@param _rarity the rarity of token id
    ///@param _status the rarity of token id
    function setTokenType(uint _index, uint256 _supply, uint256 _cohort, uint256 _rarity, bool _status)
        external
        onlyRole(MINTER_ROLE)
    {
        require(_rarity < rarities.length, "Rarity index exceeded");
        require(_cohort < cohorts.length, "Rarity index exceeded");
        require(_index < tokenTypeIndecies.length, "TOKEN_TYPE_INDEX_EXCEEDED");
        //Search for the index of the token in the old cohort list and remove it by putting it last and poping the array
        if (_cohort != tokenTypes[_index].cohort) {
            if (tokenTypes[_index].index < cohortTokenTypeList[tokenTypes[_index].cohort].length) {
                cohortTokenTypeList[tokenTypes[_index].cohort][tokenTypes[_index].index] = cohortTokenTypeList[tokenTypes[_index].cohort][
                    cohortTokenTypeList[tokenTypes[_index].cohort].length - 1
                ];
            }
            cohortTokenTypeList[tokenTypes[_index].cohort].pop();
            //Then add the index of the token to the new cohort
            cohortTokenTypeList[_cohort].push(_index);
        }
        tokenTypes[_index] = ICohortNFTv02StandAlone.CohortTokenType(
            cohortTokenTypeList[_cohort].length -1,
            _supply,
            tokenTypes[_index].minted,
            _cohort,
            _rarity,
            _status
        );
    }


    function getCohortIndexByName(string calldata _cohort) public view returns(uint256)
    {
        for(uint256 i = 0; i < cohorts.length; i++)
        {
            if(keccak256(abi.encodePacked(cohorts[i])) == keccak256(abi.encodePacked(_cohort)))
            {
                return i;
            }
        }
        require(false,"cannot find cohort");
    }

    function getAllTokenIdsFromCohort(uint256 _cohort) public view returns(uint256[] memory) {
        return cohortTokenTypeList[_cohort];
    }
    
    function getAllTokenIdsFromCohortName(string calldata _cohort) public view returns(uint256[] memory)
    {
        for(uint256 i = 0; i < cohorts.length; i++)
        {
            if(keccak256(abi.encodePacked(cohorts[i])) == keccak256(abi.encodePacked(_cohort)))
            {
                return cohortTokenTypeList[i];
            }
        }
        return new uint256[](0);
    }
    /// @dev function to view the token type of id _index
    /// @return ICohortNFTv02StandAlone.CohortTokenType (token of id _index)
    function getTokenTypeInfo(uint _index)
        public
        view
        virtual
        returns (ICohortNFTv02StandAlone.CohortTokenType memory)
    {
        ICohortNFTv02StandAlone.CohortTokenType memory theToken = tokenTypes[_index];
        return theToken;
    }

    /// @dev function to view all token indecies
    /// @return uint256[] array of all token type indecies
    function getAllTokenTypeIndecies() public view virtual returns (uint256[] memory) {
        return tokenTypeIndecies;
    }

    /// @dev function to add a new rarity
    /// @param _name name of the rarity to add
    function addRarity(string memory _name) external virtual onlyRole(MINTER_ROLE) {
        rarities.push(_name);
    }

    /// @dev function to set the name of an existing rarity
    /// @param _name New name
    /// @param _index index of the rarity to update
    function setRarity(string memory _name, uint256 _index) external virtual onlyRole(MINTER_ROLE) {
        rarities[_index] = _name;
    }

    /// @dev function to remove a rarity from the array and rearrange the array
    /// @param _index of the rarity to remove
    function removeRarity(uint256 _index) external virtual onlyRole(MINTER_ROLE) {
        require(rarities.length > _index, "Out of bounds");
        for (uint256 i = _index; i < rarities.length - 1; i++) {
            rarities[i] = rarities[i + 1];
        }
        rarities.pop();
    }

    /// @dev function to add a new cohort
    /// @param _name name of the cohort to add
    function addCohort(string memory _name) external virtual onlyRole(MINTER_ROLE) {
        cohorts.push(_name);
    }

    /// @dev function to set the name of an existing cohort
    /// @param _name New name
    /// @param _index index of the cohort to update
    function setCohort(string memory _name, uint256 _index) external virtual onlyRole(MINTER_ROLE) {
        cohorts[_index] = _name;
    }

    /// @dev function to remove a cohort from the array and rearrange the array
    /// @param _index of the cohort to remove
    function removeCohort(uint256 _index) external virtual onlyRole(MINTER_ROLE) {
        require(cohorts.length > _index, "Out of bounds");
        for (uint256 i = _index; i < cohorts.length - 1; i++) {
            cohorts[i] = cohorts[i + 1];
        }
        cohorts.pop();
    }

    /// @dev Internal function to set the base uri of the NFT (used in initialization)
    /// @param _newBaseURI the new uri string
    function _setBaseURI(string memory _newBaseURI) internal virtual {
        baseURI = _newBaseURI;
    }

    /// @dev External function to set the base uri of the NFT
    /// @param _newBaseURI the new uri string
    function setBaseURI(string memory _newBaseURI) external virtual onlyRole(MINTER_ROLE) {
        baseURI = _newBaseURI;
    }

    /// @dev External function to set the uris of the NFT to single instead of Base uri
    /// @param _useSingleUri boolean
    function setUseSingleUri(bool _useSingleUri) external virtual onlyRole(MINTER_ROLE) {
        useSingleUri = _useSingleUri;
    }

    /// @dev External function to set the extension string to be used with base uri
    /// @param _extension extension string
    function setExtension(string memory _extension) public virtual onlyRole(MINTER_ROLE) {
        baseExtension = _extension;
    }

    /// @dev function to mint "1" amount token to 1 address of same token id
    /// @param _to address of receiver
    /// @param _tokenIndex the token id to mint under it
    function mint(address _to, uint256 _tokenIndex) external virtual onlyRole(MINTER_ROLE) {
        require(tokenTypes[_tokenIndex].supply > 0, "Supply is 0");
        require(tokenTypes[_tokenIndex].supply > 1 + tokenTypes[_tokenIndex].minted, "Supply exceeded");
        _mint(_to, _tokenIndex, 1, "");
        tokenTypes[_tokenIndex].minted += 1;
        emit Minted(_to, _tokenIndex);
    }

    /// @dev function to mint multiple "1" amount tokens to multiple addresses of same token id
    /// @param _tos addresses of receivers
    /// @param _tokenIndex the token id to mint under it
    function mintBatch(address[] memory _tos, uint256 _tokenIndex) external virtual onlyRole(MINTER_ROLE) {
        require(_tos.length > 0, "tos array is zero");
        require(tokenTypes[_tokenIndex].supply > 0, "Supply is 0");
        for (uint256 i = 0; i < _tos.length; i++) {
            require(tokenTypes[_tokenIndex].supply > 1 + tokenTypes[_tokenIndex].minted, "Supply exceeded");
            _mint(_tos[i], _tokenIndex, 1, "");
            tokenTypes[_tokenIndex].minted += 1;
        }
        emit MintedBatch(_tos, _tokenIndex);
    }

    /// @dev External function to burn 1 amount of token id owned by the sender
    /// @param _id token id to burn 1 amount of (only owned by the sender)
    function burnOne(uint256 _id) external virtual {
        _burn(msg.sender, _id, 1);
        tokenTypes[_id].minted -= 1;
        emit BurnedOne(msg.sender, _id);
    }

    /// @dev function to set uri for a single token id
    /// @param _id token id to update its uri
    /// @param _uri string uri to update the token id
    function setURI(uint256 _id, string memory _uri) public virtual onlyRole(MINTER_ROLE) {
        tokenURI[_id] = _uri;
        emit UriUpdated(_id, _uri);
    }

    /// @dev function to view the uri of a token id, it will check the base uri or single uri of that token id if useSingleUri == true
    /// @param _id id of the token
    /// @return string uri of the token id
    function uri(uint256 _id) public view virtual override returns (string memory) {
        string memory tempTokenURI = "";
        string memory currentBaseURI = _baseURI();
        if (useSingleUri == true) {
            tempTokenURI = bytes(currentBaseURI).length > 0
                ? string(abi.encodePacked(currentBaseURI, _id.toString(), baseExtension))
                : "";
        } else {
            tempTokenURI = tokenURI[_id];
        }
        return tempTokenURI;
    }

    /// @dev function to set uris of multiple token ids
    /// @param _ids token ids arrays to update their uris
    /// @param _uris string array with the uris to update the token ids
    function setBatchURI(uint256[] memory _ids, string[] memory _uris) external virtual onlyRole(MINTER_ROLE) {
        for (uint256 i = 0; i < _ids.length; i++) {
            setURI(_ids[i], _uris[i]);
        }
        emit UriBatchUpdated(_ids, _uris);
    }
}