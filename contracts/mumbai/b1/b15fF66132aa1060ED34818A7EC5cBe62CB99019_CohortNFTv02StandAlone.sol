pragma solidity ^0.8.10;


///@title interface of Cohort nft: The commenting is on the NFT contract itself

interface ICohortNFTv02StandAlone {
    struct CohortTokenType {
        uint256 index; //index inside the cohort list
        uint256 supply;
        uint256 minted;
        uint256 cohort;
        uint256 rarity;
        bool disabled;
    }
    event UriUpdated(uint256 indexed _id, string indexed _uri);
    event UriBatchUpdated(uint256[] indexed ids, string[] indexed uris);
    event BurnedOne(address indexed add, uint256 indexed _id);
    event MintedBatch(address[] indexed _tos, uint256 indexed _tokenIndex);
    event Minted(address indexed _to, uint256 indexed _tokenIndex);

    function initialize(string memory _initBaseURI) external;

    function getTokenTypeInfo(uint _index) external view returns (CohortTokenType memory);

    function getAllTokenTypeIndecies() external view returns (uint256[] memory);

    function addRarity(string memory _name) external;

    function setRarity(string memory _name, uint256 _index) external;

    function removeRarity(uint256 _index) external;

    function addCohort(string memory _name) external;

    function setCohort(string memory _name, uint256 _index) external;

    function removeCohort(uint256 _index) external;

    function setBaseURI(string memory _newBaseURI) external;

    function setUseSingleUri(bool _useSingleUri) external;

    function mint(address _to, uint256 _tokenIndex) external;

    function mintBatch(address[] memory _tos, uint256 _tokenIndex) external;

    function burnOne(uint256 _id) external;

    function setURI(uint256 _id, string memory _uri) external;

    function setBatchURI(uint256[] memory _ids, string[] memory _uris) external;
}

