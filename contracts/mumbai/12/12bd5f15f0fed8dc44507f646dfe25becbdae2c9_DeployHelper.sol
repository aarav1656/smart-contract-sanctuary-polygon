pragma solidity ^0.8.17;

import {IImmutableCreate2Factory} from "./IImmutableCreate2Factory.sol";
import {TransparentUpgradeableProxy} from "@openzeppelin/contracts/proxy/transparent/TransparentUpgradeableProxy.sol";
import {RoyaltyRegistry} from "../RoyaltyRegistry.sol";
import {RoyaltyEngineV1} from "../RoyaltyEngineV1.sol";

contract DeployHelper {
    IImmutableCreate2Factory constant CREATE2_FACTORY =
        IImmutableCreate2Factory(0x0000000000FFe8B47B3e2130213B802212439497);

    ///@dev salts used for deterministically deploying the registry and engine contracts
    /// salts are shifted by 160 bits since those must match msg.sender if they are non-zero
    bytes32 immutable REGISTRY_SALT;
    ///@dev royalty engine proxy requires a distinct salt since it has the same init code as the registry proxy
    bytes32 immutable ENGINE_SALT;

    address immutable INITIAL_OWNER;
    address immutable INITIAL_ADMIN;
    address immutable INITIAL_REGISTRY_IMPLEMENTATION;
    address immutable INITIAL_ENGINE_IMPLEMENTATION;

    struct Create2Params {
        bytes32 salt;
        bytes initCode;
    }

    constructor(
        address initialOwner,
        address initialAdmin,
        address initialRegistryImplementation,
        address initialEngineImplementation
    ) {
        INITIAL_OWNER = initialOwner;
        INITIAL_ADMIN = initialAdmin;
        REGISTRY_SALT = bytes32((uint256(uint160(address(this))) << 96) | uint96(bytes12("REGISTRY")));
        ENGINE_SALT = bytes32((uint256(uint160(address(this))) << 96) | uint96(bytes12("ENGINE")));
        INITIAL_REGISTRY_IMPLEMENTATION = initialRegistryImplementation;
        INITIAL_ENGINE_IMPLEMENTATION = initialEngineImplementation;
    }

    function deploy() external returns (address registry, address engine) {
        address initialImplementation = _deployMinimumViableInitialImplementation();
        address registryProxy = _deployTransparentUpgradeableProxy(REGISTRY_SALT, initialImplementation);
        address engineProxy = _deployTransparentUpgradeableProxy(ENGINE_SALT, initialImplementation);

        _upgradeProxyAndTransferAdmin(
            registryProxy,
            INITIAL_REGISTRY_IMPLEMENTATION,
            abi.encodeWithSignature("initialize(address)", INITIAL_OWNER)
        );
        _upgradeProxyAndTransferAdmin(
            engineProxy,
            INITIAL_ENGINE_IMPLEMENTATION,
            abi.encodeWithSignature("initialize(address,address)", INITIAL_OWNER, registryProxy)
        );

        return (registryProxy, engineProxy);
    }

    /**
     * @notice Deploys a smart contract with a single STOP (00) opcode to a deterministic address.
     *         TransparentUpgradeableProxies require the initial implementation address to be a smart contract on
     *         creation, so this is used as a stand-in until the real implementations are deployed.
     */
    function _deployMinimumViableInitialImplementation() internal returns (address) {
        // deploy a contract with a single STOP (00) opcode (stopcode)
        bytes memory initCode = hex"600b5981380380925939f300";
        address expectedAddress = _expectedAddress(bytes32(0), initCode);
        if (!CREATE2_FACTORY.hasBeenDeployed(expectedAddress)) {
            CREATE2_FACTORY.safeCreate2(bytes32(0), initCode);
        }
        return expectedAddress;
    }

    function _expectedAddress(bytes32 salt, bytes memory initCode) internal pure returns (address) {
        return address(
            uint160( // downcast to match the address type.
                uint256( // convert to uint to truncate upper digits.
                    keccak256( // compute the CREATE2 hash using 4 inputs.
                        abi.encodePacked( // pack all inputs to the hash together.
                            hex"ff", // start with 0xff to distinguish from RLP.
                            address(CREATE2_FACTORY), // this contract will be the caller.
                            salt, // pass in the supplied salt value.
                            keccak256( // pass in the hash of initialization code.
                            abi.encodePacked(initCode))
                        )
                    )
                )
            )
        );
    }

    /**
     * @notice Deploys a TransparentUpgradeableProxy to a deterministic address, pointing to the "minimum viable
     *         initial implementation," and the INITIAL_PROXY_ADMIN specified by the environment variable.
     *         The function takes a single bytes32 salt parameter, which is used to CREATE2 the same init code
     *         to the different addresses, since we need to deploy two proxies: one for RoyaltyRegistry and one for
     *         RoyaltyEngine.
     *         Note that we don't want to intially deploy the proxies pointing to actual implementations of either
     *         contract: since they are upgradeable, their creationCode may change over time, which will affect the
     *         deterministic address CREATE2 deploys the proxy to.
     */
    function _deployTransparentUpgradeableProxy(bytes32 salt, address initialImplementation)
        internal
        returns (address)
    {
        bytes memory proxyInitCode = abi.encodePacked(
            type(TransparentUpgradeableProxy).creationCode, abi.encode(initialImplementation, address(this), "")
        );
        return CREATE2_FACTORY.safeCreate2(salt, proxyInitCode);
    }

    /**
     * @notice Deploys the RoyaltyRegistry implementation contract, and upgrades the RoyaltyRegistry proxy to point to it.
     */
    function _upgradeProxyAndTransferAdmin(address _proxy, address implementation, bytes memory upgradeCalldata)
        internal
    {
        TransparentUpgradeableProxy proxy = TransparentUpgradeableProxy(payable(_proxy));
        proxy.upgradeToAndCall(implementation, upgradeCalldata);
        proxy.changeAdmin(INITIAL_ADMIN);
    }
}

