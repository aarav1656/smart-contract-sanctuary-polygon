pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "erc721a/contracts/ERC721A.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract Box is ERC721A, VRFConsumerBaseV2, Ownable {

    enum Rarity {
        CLASSIC,
        RARE,
        EPIC,
        LEGENDARY,
        INSANE
    }

    enum Type {
        A,
        B,
        C,
        D
    }

    struct Metadata {
        Rarity rarity;
        Type _type;
    }

    uint256 constant private MAX_CLASSIC = 10500;
    uint256 constant private MAX_RARE = 2325;
    uint256 constant private MAX_EPIC = 1425;
    uint256 constant private MAX_LEGENDARY = 675;
    uint256 constant private MAX_INSANE = 75;

    uint256 constant private MAX_INITIAL_CLASSIC = 1400;
    uint256 constant private MAX_INITIAL_RARE = 310;
    uint256 constant private MAX_INITIAL_EPIC = 190;
    uint256 constant private MAX_INITIAL_LEGENDARY = 90;
    uint256 constant private MAX_INITIAL_INSANE = 10;

    uint256 constant private MAX_A = 4800;
    uint256 constant private MAX_B = 4500;
    uint256 constant private MAX_C = 3450;
    uint256 constant private MAX_D = 2250;

    uint256 constant private MAX_INITIAL_A = 640;
    uint256 constant private MAX_INITIAL_B = 600;
    uint256 constant private MAX_INITIAL_C = 460;
    uint256 constant private MAX_INITIAL_D = 300;

    uint256 constant private OPENED_SUPPLY = 2000;
    uint256 constant private MAX_SUPPLY = 15000;

    mapping(Rarity => uint256) private raritySupply;
    mapping(Rarity => uint256) private rarityBPs;

    mapping(Type => uint256) private typeSupply;
    mapping(Type => uint256) private typeBPs;

    mapping(uint256 => bool) private revealedIds;
    mapping(uint256 => Metadata) private metadata;
    mapping(uint256 => uint256) private revealRequest;

    string private baseURI;

    VRFCoordinatorV2Interface private immutable vrfCoordinator;
    uint64 private immutable subscriptionId;
    bytes32 private immutable gasLane;
    uint32 private immutable callbackGasLimit;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint32 private constant NUM_WORDS = 2;

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _baseURI,
        address initialReceiver,
        address secondaryReceiver,
        address vrfCoordinatorV2,
        uint64 _subscriptionId,
        bytes32 keyHash,
        uint32 _callbackGasLimit
    ) ERC721A(_name, _symbol) VRFConsumerBaseV2(vrfCoordinatorV2) {
        baseURI = _baseURI;
        vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorV2);
        gasLane = keyHash;
        subscriptionId = _subscriptionId;
        callbackGasLimit = _callbackGasLimit;

        _mint(initialReceiver, OPENED_SUPPLY);
        _initRarities();
        _initTypes();
    }


    function mint(address receiver, uint256 quantity) external onlyOwner {
        _mint(receiver, quantity);
    }

    function setBaseURI(string memory uri) external onlyOwner {
        baseURI = uri;
    }

    function reveal(uint256 id) external {
        require(ownerOf(id) == msg.sender, "[reveal]: caller is not a owner");
        require(!_isRevealed(id), "[reveal]: token is already revealed");

        uint256 requestId = vrfCoordinator.requestRandomWords(gasLane, subscriptionId, REQUEST_CONFIRMATIONS, callbackGasLimit, NUM_WORDS);
        console.log('request id', requestId, id);
        revealRequest[requestId] = id;
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
        Rarity[] memory availableRarities = _availableRarity();
        Type[] memory availableTypes = _availableType();

        uint256 rarityTotalBPs;
        for (uint8 i = 0; i < availableRarities.length;) {
            rarityTotalBPs += rarityBPs[availableRarities[i]];
            unchecked {
                i++;
            }
        }

        uint256 typeTotalBPs;
        for (uint8 i = 0; i < availableTypes.length;) {
            typeTotalBPs += typeBPs[availableTypes[i]];
            unchecked {
                i++;
            }
        }

        uint256 randomRarity = randomWords[0] % rarityTotalBPs;
        uint256 randomType = randomWords[1] % typeTotalBPs;

        Rarity chosenRarity;
        Type chosenType;

        rarityTotalBPs = 0;
        for (uint8 i = 0; i < availableRarities.length;) {
            rarityTotalBPs += rarityBPs[availableRarities[i]];
            if (randomRarity <= rarityTotalBPs) {
                chosenRarity = availableRarities[i];
                break;
            }
            unchecked {
                i++;
            }
        }

        typeTotalBPs = 0;
        for (uint8 i = 0; i < availableTypes.length;) {
            typeTotalBPs += typeBPs[availableTypes[i]];
            if (randomType <= typeTotalBPs) {
                chosenType = availableTypes[i];
                break;
            }
            unchecked {
                i++;
            }
        }

        uint256 tokenId = revealRequest[requestId];
        console.log(tokenId);
        metadata[tokenId] = Metadata(chosenRarity, chosenType);
        revealedIds[tokenId] = true;
        revealRequest[requestId] = 0;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        if (!_exists(tokenId)) revert URIQueryForNonexistentToken();

        string memory baseURI = _baseURI();
        string memory metadataPointerId = !_isRevealed(tokenId) ? 'unrevealed' : string(abi.encodePacked(Strings.toString(uint(_tokenType(tokenId))), '/', Strings.toString(uint(_tokenRarity(tokenId)))));
        string memory result = string(abi.encodePacked(baseURI, metadataPointerId, '.json'));

        return bytes(baseURI).length != 0 ? result : '';
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function _availableRarity() internal view returns (Rarity[] memory) {
        Rarity[] memory availableRarity = new Rarity[](
            (MAX_CLASSIC - raritySupply[Rarity.CLASSIC] > 0 ? 1 : 0)
            + (MAX_RARE - raritySupply[Rarity.RARE] > 0 ? 1 : 0)
            + (MAX_EPIC - raritySupply[Rarity.EPIC] > 0 ? 1 : 0)
            + (MAX_LEGENDARY - raritySupply[Rarity.LEGENDARY] > 0 ? 1 : 0)
            + (MAX_INSANE - raritySupply[Rarity.INSANE] > 0 ? 1 : 0)
        );

        uint256 idx = 0;

        if (MAX_CLASSIC - raritySupply[Rarity.CLASSIC] > 0) { availableRarity[idx] = Rarity.CLASSIC; unchecked{idx++;} }
        if (MAX_RARE - raritySupply[Rarity.RARE] > 0) { availableRarity[idx] = Rarity.RARE; unchecked{idx++;} }
        if (MAX_EPIC - raritySupply[Rarity.EPIC] > 0) { availableRarity[idx] = Rarity.EPIC; unchecked{idx++;} }
        if (MAX_LEGENDARY - raritySupply[Rarity.LEGENDARY] > 0) { availableRarity[idx] = Rarity.LEGENDARY; unchecked{idx++;} }
        if (MAX_INSANE - raritySupply[Rarity.INSANE] > 0) { availableRarity[idx] = Rarity.INSANE; }

        return availableRarity;
    }

    function _availableType() internal view returns (Type[] memory) {
        Type[] memory availableType = new Type[](
            (MAX_A - typeSupply[Type.A] > 0 ? 1 : 0)
            + (MAX_B - typeSupply[Type.B] > 0 ? 1 : 0)
            + (MAX_C - typeSupply[Type.C] > 0 ? 1 : 0)
            + (MAX_D - typeSupply[Type.D] > 0 ? 1 : 0)
        );

        uint256 idx = 0;

        if (MAX_A - typeSupply[Type.A] > 0) { availableType[idx] = Type.A; unchecked{idx++;} }
        if (MAX_B - typeSupply[Type.B] > 0) { availableType[idx] = Type.B; unchecked{idx++;} }
        if (MAX_C - typeSupply[Type.C] > 0) { availableType[idx] = Type.C; unchecked{idx++;} }
        if (MAX_D - typeSupply[Type.D] > 0) { availableType[idx] = Type.D; }

        return availableType;
    }

    function _initRarities() internal {
        rarityBPs[Rarity.CLASSIC] = 700;
        rarityBPs[Rarity.RARE] = 155;
        rarityBPs[Rarity.EPIC] = 95;
        rarityBPs[Rarity.LEGENDARY] = 45;
        rarityBPs[Rarity.INSANE] = 5;

        raritySupply[Rarity.CLASSIC] = MAX_INITIAL_CLASSIC;
        raritySupply[Rarity.RARE] = MAX_INITIAL_RARE;
        raritySupply[Rarity.EPIC] = MAX_INITIAL_EPIC;
        raritySupply[Rarity.LEGENDARY] = MAX_INITIAL_LEGENDARY;
        raritySupply[Rarity.INSANE] = MAX_INITIAL_INSANE;
    }

    function _initTypes() internal {
        typeBPs[Type.A] = 32;
        typeBPs[Type.B] = 30;
        typeBPs[Type.C] = 23;
        typeBPs[Type.D] = 15;

        typeSupply[Type.A] = MAX_INITIAL_A;
        typeSupply[Type.B] = MAX_INITIAL_B;
        typeSupply[Type.C] = MAX_INITIAL_C;
        typeSupply[Type.D] = MAX_INITIAL_D;
    }

    function _isRevealed(uint256 tokenId) internal view returns(bool) {
        return tokenId < OPENED_SUPPLY ? true : revealedIds[tokenId];
    }

    function _tokenRarity(uint256 tokenId) internal view returns(Rarity) {
        if (tokenId >= OPENED_SUPPLY) return metadata[tokenId].rarity;

        if (tokenId < MAX_INITIAL_CLASSIC) {
            return Rarity.CLASSIC;
        }

        if (tokenId < (MAX_INITIAL_CLASSIC + MAX_INITIAL_RARE)) {
            return Rarity.RARE;
        }

        if (tokenId < (MAX_INITIAL_CLASSIC + MAX_INITIAL_RARE + MAX_INITIAL_EPIC)) {
            return Rarity.EPIC;
        }

        if (tokenId < (MAX_INITIAL_CLASSIC + MAX_INITIAL_RARE + MAX_INITIAL_EPIC + MAX_INITIAL_LEGENDARY)) {
            return Rarity.LEGENDARY;
        }

        return Rarity.INSANE;
    }

    function _tokenType(uint256 tokenId) internal view returns(Type) {
        if (tokenId >= OPENED_SUPPLY) return metadata[tokenId]._type;

        if (tokenId < MAX_INITIAL_A) {
            return Type.A;
        }

        if (tokenId < (MAX_INITIAL_A + MAX_INITIAL_B)) {
            return Type.B;
        }

        if (tokenId < (MAX_INITIAL_A + MAX_INITIAL_B + MAX_INITIAL_C)) {
            return Type.C;
        }

        return Type.D;
    }

}

