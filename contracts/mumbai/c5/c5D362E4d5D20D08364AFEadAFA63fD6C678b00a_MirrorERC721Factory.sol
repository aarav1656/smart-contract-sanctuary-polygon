pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

library StateRootValidator {
    using ECDSA for bytes32;

    function isValidSignature(
        address signer,
        bytes32 stateRoot,
        bytes memory signature
    ) public pure returns (bool) {
        // 因为JavaScript中使用了hashPersonalMessage，我们需要在Solidity中使用keccak256生成相同的哈希
        bytes32 hash = keccak256(
            abi.encodePacked("\x19Ethereum Signed Message:\n32", stateRoot)
        );

        // 使用ECDSA库从签名中恢复地址
        address recoveredAddress = hash.recover(signature);

        // 检查恢复的地址是否与预期的签名者地址匹配
        return recoveredAddress == signer;
    }
}

