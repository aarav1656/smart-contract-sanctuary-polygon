import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "./Interfaces/IGovernance.sol";
import "./Interfaces/IMirrorERC721Factory.sol";

contract MirrorERC721 is ERC721Upgradeable {
    address public immutable governance;

    uint256 public mirrorId;

    constructor(address _governance) {
        governance = _governance;
    }

    function initialize(
        string memory _name,
        string memory _symbol,
        uint256 _mirrorId
    ) public initializer {
        __ERC721_init(_name, _symbol);
        mirrorId = _mirrorId;
    }

    function claim(address to, uint256 tokenId) public onlyFactory {
        if (_exists(tokenId)) {
            _transfer(ownerOf(tokenId), to, tokenId);
        } else {
            _mint(to, tokenId);
        }
    }

    function tokenURI(
        uint256 tokenId
    ) public view override returns (string memory) {
        return
            IMirrorERC721Factory(IGovernance(governance).factoryContract())
                .tokenURI(mirrorId, tokenId);
    }

    modifier onlyFactory() {
        require(
            msg.sender == IGovernance(governance).factoryContract(),
            "not allowed"
        );
        _;
    }
}

