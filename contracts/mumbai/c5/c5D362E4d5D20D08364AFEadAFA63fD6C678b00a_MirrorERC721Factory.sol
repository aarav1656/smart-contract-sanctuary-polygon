import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "./libs/StateRootValidator.sol";
import "./libs/StorageProofValidator.sol";
import "./InitializedProxy.sol";
import "./MirrorERC721.sol";
import "./Interfaces/IMirrorERC721.sol";
import "./Interfaces/IMirrorERC721Factory.sol";

contract MirrorERC721Factory is IMirrorERC721Factory, Ownable, Pausable {
    uint256 chain_id;

    address public stateRootSigner;

    /// @notice the number of ERC721 vaults
    uint256 public mirrorsCount;

    /// @notice the mapping of vault number to vault contract
    mapping(uint256 => NFTData) public mirrors;

    mapping(uint256 => uint256) public mirrorIds;

    /// @notice a settings contract controlled by governance
    address public immutable governance;
    /// @notice the TokenVault logic contract
    address public immutable logic;

    struct NFTConfigParams {
        bytes32[] proof;
        uint256 chain_id;
        address addr;
        string name;
        string symbol;
        uint256 owner_slot_type;
        uint256 owner_slot_index;
        uint256 owner_unpack_type;
    }

    event MirrorCollectionDeployed(
        uint256 indexed chain_id,
        address indexed token,
        address mirrorToken
    );

    event MirrorNFTClaimed(
        uint256 indexed chain_id,
        address indexed token,
        address indexed mirrorToken,
        address to,
        uint256 tokenId
    );

    constructor(uint256 _chain_id, address _governance) {
        chain_id = _chain_id;
        governance = _governance;
        logic = address(new MirrorERC721(_governance));
    }

    function getMirrorData(
        uint256 mirrorId
    ) public view returns (NFTData memory) {
        return mirrors[mirrorId];
    }

    function tokenURI(
        uint256 mirrorId,
        uint256 tokenId
    ) public view returns (string memory) {
        console.log(mirrors[mirrorId].token);
        return
            string(
                abi.encodePacked(
                    IGovernance(governance).tokenuriproxy(),
                    "/",
                    Strings.toString(mirrors[mirrorId].chain_id),
                    "/",
                    Strings.toHexString(
                        uint256(uint160(mirrors[mirrorId].token)),
                        20
                    ),
                    "/",
                    Strings.toString(tokenId)
                )
            );
    }

