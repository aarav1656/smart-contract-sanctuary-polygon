        NFTConfigParams memory params
    ) public view returns (bool) {
        bytes32 leaf = keccak256(
            bytes.concat(
                keccak256(
                    abi.encode(
                        params.chain_id,
                        params.addr,
                        params.name,
                        params.symbol,
                        params.owner_slot_type,
                        params.owner_slot_index,
                        params.owner_unpack_type
                    )
                )
            )
        );

