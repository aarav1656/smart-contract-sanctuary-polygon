        bytes32 mappingSlot = getPositionSlot(
            nftConfigParams.owner_slot_type,
            nftConfigParams.owner_slot_index,
            tokenId
        );

        uint256 value = StorageProofValidator.verifyStorageProof(
            stateRoot,
            nftConfigParams.addr,
            mappingSlot,
            storageProof
        );

        address owner = getUnpackOwner(
            nftConfigParams.owner_unpack_type,
            value
        );

        uint256 mirrorIdKey = packAddressAndChainId(
            nftConfigParams.addr,
            nftConfigParams.chain_id
        );
        address mirror;
        if (mirrorIds[mirrorIdKey] == 0) {
            bytes memory _initializationCalldata = abi.encodeWithSignature(
                "initialize(string,string,uint256)",
                nftConfigParams.name,
                nftConfigParams.symbol,
                mirrorsCount
            );
            mirror = address(
                new InitializedProxy(logic, _initializationCalldata)
            );
            mirrors[mirrorsCount] = NFTData(
                nftConfigParams.chain_id,
                nftConfigParams.addr,
                mirror
            );
            mirrorsCount++;

            emit MirrorCollectionDeployed(
                nftConfigParams.chain_id,
                nftConfigParams.addr,
                mirror
            );
        } else {
            mirror = mirrors[mirrorIdKey].mirrorToken;
        }

        IMirrorERC721(mirror).claim(owner, tokenId);
        emit MirrorNFTClaimed(
            nftConfigParams.chain_id,
            nftConfigParams.addr,
            mirror,
            owner,
            tokenId
        );
    }

    function getPositionSlot(
        uint256 owner_slot_type,
        uint256 owner_slot_index,
        uint256 tokenId
    ) public pure returns (bytes32 slotPosition) {
        if (owner_slot_type == 1) {
            slotPosition = calculateMappingSlot(tokenId, owner_slot_index);
        } else if (owner_slot_type == 2) {
            slotPosition = calculateERC721AStorageSlot(
                tokenId,
                owner_slot_index
            );
        } else {
            slotPosition = calculateDynamicArraySlot(tokenId, owner_slot_index);
        }
    }

    function calculateMappingSlot(
        uint256 tokenId,
        uint256 mappingSlot
    ) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(tokenId, mappingSlot));
    }

    function calculateERC721AStorageSlot(
        uint256 tokenId,
        uint256 mappingSlot
    ) public pure returns (bytes32) {
        bytes32 storageSlot = keccak256(
            abi.encodePacked("ERC721A.contracts.storage.ERC721A")
        );
        uint256 baseStoragePosition = uint256(storageSlot) + mappingSlot;
        bytes32 combinedData = bytes32(tokenId + baseStoragePosition);

        return keccak256(abi.encodePacked(combinedData));
    }

    function calculateDynamicArraySlot(
        uint256 tokenId,
        uint256 arrayPosition
    ) public pure returns (bytes32) {
        bytes32 firstElementSlot = keccak256(
            abi.encodePacked(bytes32(arrayPosition))
        );
        uint256 combinedSlot = uint256(firstElementSlot) + tokenId;

        return bytes32(combinedSlot);
    }

    function getUnpackOwner(
        uint256 owner_unpack_type,
        uint256 value
    ) public pure returns (address) {
        if (owner_unpack_type == 1) {
            return get721AOwner(value);
        } else {
            return address(uint160(value));
        }
    }

    function get721AOwner(uint256 packedData) public pure returns (address) {
        uint256 ownerBigInt = packedData &
            0x00FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF;

        return address(uint160(ownerBigInt));
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function packAddressAndChainId(
        address token,
        uint256 chainId
    ) public pure returns (uint256) {
        require(chainId < (2 ** 96), "chainId is too large"); // Assuming chainId uses up to 96 bits (to fit in a uint256 with an address)

        uint256 packedData = (uint256(uint160(token)) << 96) | chainId;
        return packedData;
    }
}

