abstract contract UsableErc1155 {
    event Used(uint256 tokenId, uint256 amount);
    event Unused(uint256 tokenId, uint256 amount);

    mapping(uint256 => mapping(address => uint256)) private used;

    function inUse(uint256 tokenId, address owner) public view virtual returns (uint256) {
        return used[tokenId][owner];
    }

    modifier whenNotInUse(uint256[] memory tokenIDs, uint256[] memory amounts, address owner) {
        for (uint256 i = 0; i < tokenIDs.length; i++) {
//            require(used[tokenIDs[i]][owner] <= amounts[i], "UsableErc1155: some tokens are used");
        }
        _;
    }

    function _use(uint256 tokenId, uint256 amount, address owner) internal virtual {
        used[tokenId][owner] += amount;
        emit Used(tokenId, amount);
    }

    function _unuse(uint256 tokenId, uint256 amount, address owner) internal virtual {
        require(used[tokenId][owner] >= amount, "UsableErc1155: big amount to unuse");
        used[tokenId][owner] -= amount;
        emit Unused(tokenId, amount);
    }
}

