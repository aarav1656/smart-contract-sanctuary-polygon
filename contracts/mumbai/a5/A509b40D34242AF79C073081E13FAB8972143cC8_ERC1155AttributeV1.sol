        _symbol = symbol;
        factory = IFactory(_factory);
        capacity = _capacity;
        copyCapacity = _copyCapacity;
        _setDefaultRoyalty(_royaltyReceiver, _royaltyFee);
    }

    function attachAttribute(address _avatarCollection, uint256 _tokenId, uint256 _attrTokenID) public {
        _approve(_msgSender(), _avatarCollection, _attrTokenID);
        IAttachable(_avatarCollection).attachAttribute(_tokenId, address(this), _attrTokenID, false);
    }

    function mint(
        address receiver,
        uint256 tokenID,
        uint256 amount,
        uint256 attribute,
        bytes memory data
    ) public onlyTrustedSender onlyOwner returns (uint256)  {
        _tokenIdCounter.increment();
        require(_tokenIdCounter.current() <= capacity, "capacity exceeded");
        require(amount <= copyCapacity, "copy capacity exceeded");
        require(!_minted[tokenID], "already minted");
        require(_isValidAttribute(attribute), "invalid attribute");

        _mint(receiver, tokenID, amount, data);
        _minted[tokenID] = true;
        _attrs[tokenID] = attribute;

        emit AttributeUpdated(tokenID, attribute);

        return tokenID;
    }

    function batchMint(
        address receiver,
        uint256[] calldata tokenIDs,
        uint256[] calldata amount,
        uint256[] calldata attribute,
        bytes memory data
    )
    public onlyTrustedSender onlyOwner {
        require(tokenIDs.length == amount.length, "token amount and copies amount is not eq");
        uint256 _now = _tokenIdCounter.current();
        uint256 _after = _now + tokenIDs.length;
        require(_after <= capacity, "capacity exceeded");
        for (uint256 i = 0; i < tokenIDs.length; i++) {
            uint256 id = tokenIDs[i];

            require(!_minted[id], "already minted");
            require(amount[i] <= copyCapacity, "copy capacity is exceeded");
            require(_isValidAttribute(attribute[i]), "invalid attribute");

            _minted[id] = true;
            _attrs[id] = attribute[i];
            emit AttributeUpdated(tokenIDs[i], attribute[i]);
        }
        _mintBatch(receiver, tokenIDs, amount, data);
    }

    function getAttribute(uint256 id) external view returns (uint256) {
        return _attrs[id];
    }

    function use(uint256 tokenId, uint256 amount) public {
        uint256 balance = balanceOf(_msgSender(), tokenId);
        require(balance >= amount, "low balance for this token id");
        require(inUse(tokenId, _msgSender()) + amount <= balance, "locked amount can't be more then balance");
        _use(tokenId, amount, _msgSender());
    }

    function unuse(uint256 tokenId, uint256 amount) public {
        uint256 balance = balanceOf(_msgSender(), tokenId);
        require(balance > 0, "low balance for unuse this token id");
        _unuse(tokenId, amount, _msgSender());
    }

    function pause() public onlyFactoryOwner {
        _pause();
    }

    function unpause() public onlyFactoryOwner {
        _unpause();
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
    internal
    whenNotPaused
    whenNotInUse(ids, amounts, _msgSender())
    override
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function _msgSender() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (address) {
        return super._msgSender();
    }

    function _msgData() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable) returns (bytes calldata) {
        return super._msgData();
    }

    function uri(uint256 tokenId) public view override(ERC1155Upgradeable) returns (string memory)
    {
        return string(
            abi.encodePacked(
                factory.baseURI(),
                "/",
                Strings.toHexString(uint256(uint160(address(this))), 20),
                "/",
                Strings.toString(tokenId)
            )
        );
    }

    function contractURI() public view returns (string memory)
    {
        return string(
            abi.encodePacked(
                factory.baseURI(),
                "/",
                Strings.toHexString(uint256(uint160(address(this))), 20)
            )
        );
    }

    function burn(address from, uint256 id, uint256 amount) public onlyOwner {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        _burn(from, id, amount);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public virtual override {
        require(
            from == _msgSender() ||
            isApprovedForAll(from, _msgSender()) ||
            this.isApproved(from, _msgSender(), id),
            "ERC1155: caller is not owner nor approved"
        );
        _safeTransferFrom(from, to, id, amount, data);
    }

    function batchApprove(address from, address to, uint256[] calldata tokenIds) external {
        require(to != address(0), "spender can't be zero address");
        require(tokenIds.length > 0, "zero len array illegal");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            approve(from, to, tokenIds[i]);
        }
    }

    function batchRevoke(address from, address to, uint256[] calldata tokenIds) external {
        require(tokenIds.length > 0, "zero len array illegal");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            revoke(from, to, tokenIds[i]);
        }
    }

    function _approve(address owner, address spender, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId][owner][spender] = true;
        emit Approval(owner, spender, tokenId);
    }

    function _revoke(address owner, address spender, uint256 tokenId) internal virtual {
        _tokenApprovals[tokenId][owner][spender] = false;
        emit RevokeApproval(owner, spender, tokenId);
    }

    function revoke(address from, address to, uint256 tokenId) public {
        require(ERC1155Upgradeable.balanceOf(_msgSender(), tokenId) > 0, "balance should be non zero");

        _revoke(from, to, tokenId);
    }

    function approve(address from, address to, uint256 tokenId) public {
        require(ERC1155Upgradeable.balanceOf(_msgSender(), tokenId) > 0, "balance should be non zero");
        require(_msgSender() == from, "invalid sender");

        _approve(from, to, tokenId);
    }

    function isApproved(address from, address to, uint256 tokenId) public view returns (bool) {
        return _tokenApprovals[tokenId][from][to];
    }

    function totalSupply() public view returns (uint256) {
        return capacity;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC2981, ERC1155Upgradeable) returns (bool)
    {
        return
        interfaceId == type(IERC2981).interfaceId ||
        super.supportsInterface(interfaceId);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    mapping(uint256 => bool) private _minted;
}