contract pseudoRand {
	
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    constructor() 
    {}



    function getRandom() public view returns (uint256) {
        
        // pseudo random as the random result is not crucial for anything
        uint256 randomNum = uint256(
            keccak256(
                abi.encode(
                    block.number + 1000,
                    tx.gasprice,
                    block.number,
                    block.timestamp,
                    block.difficulty,
                    blockhash(block.number - 1),
                    address(this),
                    block.number - 1000
                )
            )
        );

        return randomNum;

    }

    function getRandomwModulo() public view returns (uint256) {
        
        // pseudo random as the random result is not crucial for anything
        uint256 randomNum = uint256(
            keccak256(
                abi.encode(
                    block.number + 1000,
                    tx.gasprice,
                    block.number,
                    block.timestamp,
                    block.difficulty,
                    blockhash(block.number - 1),
                    address(this),
                    block.number - 1000
                )
            )
        );

        uint256 randomMod = randomNum % 4;

        return randomMod;

    }


}

