pragma solidity ^0.8.0;

import {IERC721} from 'openzeppelin-contracts/contracts/token/ERC721/ERC721.sol';
import {Ownable} from 'openzeppelin-contracts/contracts/access/Ownable.sol';
import {IERC721Receiver} from 'openzeppelin-contracts/contracts/interfaces/IERC721Receiver.sol';
import {Initializable} from 'openzeppelin-contracts/contracts/proxy/utils/Initializable.sol';
import {IRaffleManager, RaffleConfig} from './interfaces/IRaffleManager.sol';
import {IRaffle} from './interfaces/IRaffle.sol';
import {RaffleTicket, IRaffleTicket} from './RaffleTicket.sol';
import {IPaymentSystem} from './interfaces/IPaymentSystem.sol';
import {IRandomnessRegistry} from './interfaces/IRandomnessRegistry.sol';

contract RaffleManager is
  IRaffleManager,
  IERC721Receiver,
  Ownable,
  Initializable
{
  address public immutable TICKET_SVG_COMPOSER;
  address public immutable PAYMENT_SYSTEM;
  address public immutable RANDOMNESS_REGISTRY;

  // flag indicating if contract is blocked from creating future raffles
  mapping(address => bool) internal _enabledRaffles;

  mapping(address => mapping(uint256 => RaffleConfig.RaffleConfiguration))
    internal _raffleConfigs;

  constructor(
    address ticketSVGComposer,
    address paymentSystem,
    address randomnessRegistry
  ) {
    TICKET_SVG_COMPOSER = ticketSVGComposer;
    PAYMENT_SYSTEM = paymentSystem;
    RANDOMNESS_REGISTRY = randomnessRegistry;
  }

  function initialize() external initializer {}

  /// @inheritdoc IERC721Receiver
  function onERC721Received(
    address operator,
    address,
    uint256,
    bytes calldata data
  ) external pure returns (bytes4) {
    return IERC721Receiver.onERC721Received.selector;
  }

  function enableRaffle(address[] memory raffles) external onlyOwner {
    _updateRaffles(raffles, true);
  }

  function disableRaffle(address[] memory raffles) external onlyOwner {
    _updateRaffles(raffles, false);
  }

  function isRaffleEnabled(address raffle) external view returns (bool) {
    return _enabledRaffles[raffle];
  }

  function cancelRaffle(address raffle, uint256 raffleNftId) external {
    RaffleConfig.RaffleConfiguration storage raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];
    require(
      IERC721(raffle).ownerOf(raffleNftId) == msg.sender,
      'NOT_THE_OWNER'
    );

    // TODO: more checks here??
    require(
      block.timestamp <
        raffleConfig.creationTimestamp + RaffleConfig.RAFFLE_START_BUFFER ||
        IRaffleTicket(raffleConfig.raffleTicket).ticketsSold() == 0
    );

    raffleConfig.canceled = true;
  }

  function getRaffleConfiguration(address raffle, uint256 raffleNftId)
    external
    view
    returns (RaffleConfig.RaffleConfiguration memory)
  {
    return _raffleConfigs[raffle][raffleNftId];
  }

  function createRaffle(
    address raffle,
    uint256 prizeNftId,
    address prizeNftAddress,
    uint40 raffleDuration,
    uint40 minTickets,
    uint40 maxTickets,
    uint256 pricePerTicket,
    address onBehalfOf
  ) external returns (uint256) {
    require(_enabledRaffles[raffle], 'RAFFLE_NOT_ENABLED');
    require(minTickets <= maxTickets, 'INCORRECT_NUMBER_OF_TICKETS');
    require(pricePerTicket > 0, 'TICKET_PRICE_SHOULD_BE_GRT_0');

    IERC721(prizeNftAddress).safeTransferFrom(
      msg.sender,
      address(this),
      prizeNftId
    );

    uint256 raffleNftId = IRaffle(raffle).create(onBehalfOf);

    RaffleConfig.RaffleConfiguration storage raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];

    // like num of tickets must be exact etc
    raffleConfig.raffleTicket = address(
      new RaffleTicket(
        IRaffleTicket.RaffleTicketConfiguration({
          raffleId: raffleNftId,
          pricePerTicket: pricePerTicket,
          maxTickets: maxTickets,
          ticketSVGComposer: TICKET_SVG_COMPOSER,
          paymentTokenDecimals: IRaffle(raffle).PAYMENT_TOKEN_DECIMALS(),
          paymentTokenSymbol: IRaffle(raffle).getPaymentTokenSymbol()
        })
      )
    );

    raffleConfig.raffleId = raffleNftId;
    raffleConfig.prizeNftId = prizeNftId;
    raffleConfig.prizeNftAddress = prizeNftAddress;
    raffleConfig.raffleDuration = raffleDuration;
    raffleConfig.expirationDate =
      uint40(block.timestamp) +
      uint40(RaffleConfig.RAFFLE_START_BUFFER) +
      raffleDuration;
    raffleConfig.maxTickets = maxTickets;
    raffleConfig.minTickets = minTickets;
    raffleConfig.pricePerTicket = pricePerTicket;
    raffleConfig.creationTimestamp = uint40(block.timestamp);

    emit RaffleCreated(
      raffle,
      raffleNftId,
      msg.sender,
      raffleConfig.raffleTicket,
      prizeNftId,
      prizeNftAddress,
      raffleDuration,
      maxTickets,
      minTickets,
      pricePerTicket,
      onBehalfOf
    );

    return raffleNftId;
  }

  function claimExpiredRafflePrizeNFT(
    address raffle,
    uint256 raffleNftId,
    address onBehalfOf
  ) external {
    RaffleConfig.RaffleConfiguration memory raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];

    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.EXPIRED,
      'RAFFLE_NOT_EXPIRED'
    );
    // Owner of the raffle
    require(
      IERC721(raffle).ownerOf(raffleNftId) == msg.sender,
      'NOT_THE_OWNER'
    );
    // Return NFT
    IERC721(raffleConfig.prizeNftAddress).safeTransferFrom(
      address(this),
      onBehalfOf,
      raffleConfig.prizeNftId
    );

    IRaffle(raffle).unsafeBurn(raffleNftId);

    emit ClaimedExpiredRafflePrizeNft(
      raffle,
      raffleNftId,
      msg.sender,
      onBehalfOf,
      raffleConfig.prizeNftAddress,
      raffleConfig.prizeNftId
    );
  }

  function claimExpiredRaffleTicketsAmount(
    address raffle,
    uint256 raffleNftId,
    address onBehalfOf,
    uint256[] memory raffleTicketIds
  ) external {
    RaffleConfig.RaffleConfiguration memory raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];
    // Need to be expired to claim the money
    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.EXPIRED,
      'RAFFLE_NOT_EXPIRED'
    );

    for (uint256 i = 0; i < raffleTicketIds.length; i++) {
      // Check the ownership of the token
      require(
        IERC721(raffleConfig.raffleTicket).ownerOf(raffleTicketIds[i]) ==
          msg.sender,
        'NOT_THE_OWNER'
      );

      // WARNING: This burn dones't check the approval
      IRaffleTicket(raffleConfig.raffleTicket).destroyTicket(
        raffleTicketIds[i]
      );
    }

    // we can do this directly as if not owner it will revert previously so we can assume that all are owned
    uint256 totalTicketsToClaimAmount = raffleTicketIds.length *
      raffleConfig.pricePerTicket;

    (bool success, bytes memory returnData) = PAYMENT_SYSTEM.delegatecall(
      abi.encodeWithSelector(
        IPaymentSystem.payTicketsAmount.selector,
        onBehalfOf,
        totalTicketsToClaimAmount
      )
    );

    emit ClaimedExpiredRaffleTicketsAmount(
      raffle,
      raffleNftId,
      totalTicketsToClaimAmount,
      msg.sender,
      onBehalfOf,
      raffleTicketIds
    );
  }

  function destroyRaffle(address raffle, uint256 raffleNftId) external {
    RaffleConfig.RaffleConfiguration memory raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];
    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.FINISHED,
      'RAFFLE_NOT_FINISHED'
    );

    // Owner of the raffle
    require(
      IERC721(raffle).ownerOf(raffleNftId) == msg.sender,
      'NOT_THE_OWNER'
    );

    require(raffleConfig.ticketSalesCollected, 'TICKET_SALES_NOT_COLLECTED');
    require(raffleConfig.prizeNftCollected, 'PRIZE_NOT_COLLECTED');

    IRaffle(raffle).unsafeBurn(raffleNftId);
  }

  function claimPrizeNftTicketSales(
    address raffle,
    uint256 raffleNftId,
    address onBehalfOf
  ) external {
    RaffleConfig.RaffleConfiguration storage raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];
    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.FINISHED,
      'RAFFLE_NOT_FINISHED'
    );

    // Owner of the raffle
    address raffleOwner = IERC721(raffle).ownerOf(raffleNftId);
    require(raffleOwner == msg.sender, 'NOT_THE_OWNER');

    uint256 raffleRaisedAmount = raffleConfig.maxTickets *
      raffleConfig.pricePerTicket;
    uint256 ticketsSold = IRaffleTicket(raffleConfig.raffleTicket)
      .ticketsSold();
    if (ticketsSold > 1) {
      raffleRaisedAmount = ticketsSold * raffleConfig.pricePerTicket;
    }

    raffleConfig.ticketSalesCollected = true;

    // TODO: maybe do something with results?
    (bool success, bytes memory returnData) = PAYMENT_SYSTEM.delegatecall(
      abi.encodeWithSelector(
        IPaymentSystem.payRaffleAmount.selector,
        raffleRaisedAmount,
        onBehalfOf
      )
    );

    emit ClaimedPrizeNftTicketSales(
      raffle,
      raffleNftId,
      raffleRaisedAmount,
      raffleOwner,
      onBehalfOf
    );
  }

  function claimPrizeNft(
    address raffle,
    uint256 raffleNftId,
    address onBehalfOf
  ) external {
    RaffleConfig.RaffleConfiguration memory raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];
    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.FINISHED,
      'RAFFLE_NOT_FINISHED'
    );

    // This accounts for the case when winner is user that bought all tickets (as this doesnt mint the tickets)
    address winner = IERC721(raffleConfig.raffleTicket).ownerOf(
      raffleConfig.ticketWinner
    );

    require(winner == msg.sender, 'CLAIMER_IS_NOT_WINNER');

    raffleConfig.prizeNftCollected = true;

    // transfer nft
    IERC721(raffleConfig.prizeNftAddress).safeTransferFrom(
      address(this),
      onBehalfOf,
      raffleConfig.prizeNftId
    );

    emit ClaimedPrizeNft(
      raffle,
      raffleNftId,
      raffleConfig.prizeNftId,
      raffleConfig.prizeNftAddress,
      winner,
      onBehalfOf
    );
  }

  function requestRandomWinner(address raffle, uint256 raffleNftId)
    external
    returns (uint256)
  {
    RaffleConfig.RaffleConfiguration storage raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];

    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.RAFFLE_SUCCESSFUL,
      'NOT_ACTIVE'
    );

    return
      IRandomnessRegistry(RANDOMNESS_REGISTRY).requestRandomWord(raffleNftId);
  }

  function selectWinner(address raffle, uint256 raffleNftId) external {
    RaffleConfig.RaffleConfiguration storage raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];

    require(
      raffleConfig.ticketWinnerSelected == false,
      'WINNER_ALREADY_SELECTED'
    );

    uint256 numberOfTicketsSold = IRaffleTicket(raffleConfig.raffleTicket)
      .ticketsSold();

    IRandomnessRegistry.RandomnessRequest
      memory randomnessRequest = IRandomnessRegistry(RANDOMNESS_REGISTRY)
        .getVRFRequestIdFromRaffleNftId(raffleNftId);

    require(randomnessRequest.randomWordFulfilled, 'RANDOM_WORD_NOT_FULFILLED');

    uint256 tokenIdWinner = (randomnessRequest.randomResponseWord %
      numberOfTicketsSold);

    raffleConfig.ticketWinner = tokenIdWinner;
    raffleConfig.ticketWinnerSelected = true;

    emit WinnerSelected(raffle, raffleNftId, tokenIdWinner);
  }

  function buyTickets(
    address raffle,
    uint256 raffleNftId,
    uint256 ticketsToDistribute,
    uint256 buyerFunds,
    address buyer,
    address onBehalfOf
  ) external payable {
    RaffleConfig.RaffleConfiguration storage raffleConfig = _raffleConfigs[
      raffle
    ][raffleNftId];

    // Check state
    require(
      RaffleConfig.getRaffleState(raffleConfig) ==
        RaffleConfig.RaffleStates.ACTIVE,
      'NOT_ACTIVE'
    );
    require(
      block.timestamp >
        raffleConfig.creationTimestamp + RaffleConfig.RAFFLE_START_BUFFER,
      'BUY_NOT_ENABLED_YET'
    );
    // Check not surpass the number max of tickets
    require(
      IRaffleTicket(raffleConfig.raffleTicket).ticketsSold() +
        ticketsToDistribute <=
        raffleConfig.maxTickets,
      'MAX_TICKETS'
    );

    uint256 neededFunds = ticketsToDistribute * raffleConfig.pricePerTicket;

    require(
      buyerFunds == neededFunds && msg.value == neededFunds,
      'PAYMENT_AMOUNT_NOT_ENOUGH'
    );

    // bypass ticket emit if someone buy all tickets to not pass through vrf
    if (ticketsToDistribute == raffleConfig.maxTickets) {
      raffleConfig.ticketWinnerSelected = true;

      raffleConfig.ticketWinner = 0;

      IRaffleTicket(raffleConfig.raffleTicket).createTickets(onBehalfOf, 1);

      emit TicketsDistributed(raffle, raffleNftId, 1, buyer, onBehalfOf);
      emit WinnerSelected(raffle, raffleNftId, 0);
    } else {
      IRaffleTicket(raffleConfig.raffleTicket).createTickets(
        onBehalfOf,
        ticketsToDistribute
      );

      emit TicketsDistributed(
        raffle,
        raffleNftId,
        ticketsToDistribute,
        buyer,
        onBehalfOf
      );
    }
  }

  function _updateRaffles(address[] memory raffles, bool enable) internal {
    for (uint256 i; i < raffles.length; i++) {
      _enabledRaffles[raffles[i]] = enable;

      emit RaffleEnabled(raffles[i], enable);
    }
  }
}

