contract RaffleTicket is ERC721A, IRaffleTicket {
  using Strings for uint256;

  /// @inheritdoc IRaffleTicket
  address public immutable RAFFLE;

  /// @inheritdoc IRaffleTicket
  address public immutable TICKET_SVG_COMPOSER;

  /// @inheritdoc IRaffleTicket
  uint256 public immutable RAFFLE_ID;

  /// @inheritdoc IRaffleTicket
  uint256 public immutable PRICE_PER_TICKET;

  /// @inheritdoc IRaffleTicket
  uint256 public immutable MAX_TICKETS;

  /// @inheritdoc IRaffleTicket
  uint8 public immutable PAYMENT_TOKEN_DECIMALS;

  string internal _paymentTokenSymbol;

  /// @notice checks that the caller is the associated raffle nft
  modifier onlyRaffle() {
    require(RAFFLE == msg.sender, 'CALLER_MUST_BE_RAFFLE');
    _;
  }

  /**
   * @param raffleTicketConfig object containing the associated raffle configuration
   */
  constructor(RaffleTicketConfiguration memory raffleTicketConfig)
    ERC721A('RaffleTicket', 'RT')
  {
    RAFFLE = msg.sender;
    RAFFLE_ID = raffleTicketConfig.raffleId;
    PRICE_PER_TICKET = raffleTicketConfig.pricePerTicket;
    TICKET_SVG_COMPOSER = raffleTicketConfig.ticketSVGComposer;
    MAX_TICKETS = raffleTicketConfig.maxTickets;
    PAYMENT_TOKEN_DECIMALS = raffleTicketConfig.paymentTokenDecimals;
    _paymentTokenSymbol = raffleTicketConfig.paymentTokenSymbol;
  }

  /// @inheritdoc IRaffleTicket
  function getPaymentTokenSymbol() external view returns (string memory) {
    return _paymentTokenSymbol;
  }

  /// @inheritdoc IRaffleTicket
  function createTickets(address receiver, uint256 quantity)
    external
    onlyRaffle
  {
    _mint(receiver, quantity);
  }

  /// @inheritdoc IRaffleTicket
  function ticketsSold() external view returns (uint256) {
    return _totalMinted();
  }

  /// @inheritdoc IRaffleTicket
  function destroyTicket(uint256 ticketId) external onlyRaffle {
    _burn(ticketId, false);
  }

  /// @inheritdoc IERC721A
  function tokenURI(uint256 tokenId)
    public
    view
    override
    returns (string memory)
  {
    string memory json = Base64.encode(
      abi.encodePacked(
        '{"name": " Ticket #',
        Strings.toString(tokenId),
        '", "description": "", "image": "data:image/svg+xml;base64,',
        Base64.encode(
          bytes(
            ISVGTicket(TICKET_SVG_COMPOSER).getSvg(
              RAFFLE_ID.toString(),
              tokenId.toString(),
              string(
                abi.encodePacked(
                  NumberUtils.numToFixedLengthStr(
                    PAYMENT_TOKEN_DECIMALS,
                    PRICE_PER_TICKET
                  ),
                  ' ',
                  _paymentTokenSymbol
                )
              ),
              _totalMinted().toString(),
              MAX_TICKETS.toString(),
              (PRICE_PER_TICKET * MAX_TICKETS).toString()
            )
          )
        ),
        '"}'
      )
    );
    return string(abi.encodePacked('data:application/json;base64,', json));
  }
}

