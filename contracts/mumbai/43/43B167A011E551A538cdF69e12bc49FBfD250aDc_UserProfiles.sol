pragma solidity ^0.8.2;

import "hardhat/console.sol";

import "./interfaces/IKUtils.sol";
import "./interfaces/IFollowers.sol";
import "./interfaces/IGroups.sol";

contract UserProfiles {

    // Admins
    mapping (address => bool) admins;

    // Trusted Contracts
    mapping (address => bool) trustedContracts;

    // User Stats
    struct UserStats {
        uint256 postCount;
        uint256 followerCount;
        uint256 followingCount;
        uint256 tipsReceived;
        uint256 tipsSent;
    }

    // The User data struct
    struct UserData {
        string handle;
        string location;
        uint256 joinBlock;
        uint256 joinTime;
        string avatar;
        string uri;
        string bio;
        uint256 followLimit;
        uint16 verified;
        address[] following;
        bool isGroup;
        UserStats userStats;
    }

    // Map the User Address => User Data
    mapping (address => UserData) public usrProfileMap;

    // Set Max URI Length
    uint256 public maxURILength = 255;

    // Set Max Bio Length
    uint256 public maxBioLength = 255;

    // Set Following Limit
    uint256 public maxFollowing = 5000;

    // Maximum amount of Following records to return on query
    uint16 maxFollowReturn = 10;

    // Link to the KUtils Contracts
    IKUtils public KUtils;

    // Link to the Followers Contract
    IFollowers public Followers;

    // Link to the Groups Contract
    IGroups public Groups;

    constructor(address _kutils) {
        // Setup the default Admin
        admins[msg.sender] = true;
        trustedContracts[msg.sender] = true;

        maxFollowReturn = 100;

        // Setup link to KUtils
        KUtils = IKUtils(_kutils);
    }

    /*

    EVENTS

    */

    event logProfileUpdated1(address indexed requester, string uri, string avatar);
    event logProfileUpdated2(address indexed requester, string handle, string location, string bio);
    event logNewUser(address indexed requester, bool isGroup);




    /*

    MODIFIERS

    */

    modifier onlyAdmins() {
        require(admins[msg.sender], "Only admins can call this function.");
        _;
    }

    modifier onlyTrustedContracts() {
        require(trustedContracts[msg.sender], "Only trusted contracts can call this function.");
        _;
    }


    /*

    ADMIN FUNCTIONS

    */

    function addAdmin(address newAdmin) public onlyAdmins{
        admins[newAdmin] = true;
    }

    function removeAdmin(address oldAdmin) public onlyAdmins{
        admins[oldAdmin] = false;
    }

    function addContract(address newContract) public onlyAdmins{
        trustedContracts[newContract] = true;
    }

    function removeContract(address oldContract) public onlyAdmins{
        trustedContracts[oldContract] = false;
    }



    /**
    * Updates the maximum length of a Bio to store in a user profile
    * @param _newLen : the maximum amount of characters to allow for a Bio in a user profile
    */
    function updateBioLength(uint256 _newLen) public onlyAdmins {
        // Make sure the Bio length is within limits
        require(_newLen > 0, "New length is too small");

        maxBioLength = _newLen;
    }

    /**
    * Updates the maximum length of a URI to store in a user profile
    * @param _newLen : the maximum amount of characters to allow for a URI in a user profile
    */
    function updateURILength(uint256 _newLen) public onlyAdmins {
        // Make sure the URI length is within limits
        require(_newLen > 0, "New length is too small");

        maxURILength = _newLen;
    }

    /**
    * Updates the user followers contract in case of updates to functionality
    * @param _followers : the address to make the following checks go to
    */
    function updateContracts(address _followers, address _kutils, address _groups) public onlyAdmins {
        // Update the Followers contract address
        Followers = IFollowers(_followers);

        // Update the KUtils address
        KUtils = IKUtils(_kutils);

        // Update the Groups address
        Groups = IGroups(_groups);
    }



    /*

    Post Handling

    */

    /**
    * Updates the profile with +/- post count
    * @param posterAddress : the address of the post count to update
    * @param postCount : the amount of posts to add or remove to their count
    * @param add : add or remove the postCount
    */
    function updatePostCount(address posterAddress, uint16 postCount, bool add) public onlyAdmins {
        // Make sure we have a valid address
        require(posterAddress != address(0x0), "Can't update 0 address");

        // Post count must be a number > 0
        require(postCount > 0, "Invalid Post Count");

        if (add){
            usrProfileMap[posterAddress].userStats.postCount += postCount;
        } else {
            require(usrProfileMap[posterAddress].userStats.postCount > 0, "User doesn't have any posts to remove");
            usrProfileMap[posterAddress].userStats.postCount -= postCount;
        }
    }


    /**
    * Updates the profile with contracts posted in
    */
    function logPost(address posterAddress, uint256 tipPerTag, address[] calldata tipReceivers) public onlyAdmins {

        // Require a valid address
        require(posterAddress != address(0), "Bad Poster Address");

        // If this poster doesn't have a profile setup yet, start it
        if (usrProfileMap[posterAddress].joinBlock == 0){
            // Check if this is a group
            if (Groups.getGroupIDFromAddress(posterAddress) > 0){
                joinUser(posterAddress, true);
            } else {
                joinUser(posterAddress, false);
            }
        }

        bool _added = false;

        // Add the post to their total count
        usrProfileMap[posterAddress].userStats.postCount += 1;

        // Add the total tips sent to their count
        usrProfileMap[posterAddress].userStats.tipsSent += (tipPerTag * tipReceivers.length);

        // Update the tips received to each account tagged
        for (uint i=0; i < tipReceivers.length; i++) {
            usrProfileMap[tipReceivers[i]].userStats.tipsReceived += tipPerTag;
        }
    }


    /*

    PROFILE

    */


    /**
    * Updates user profile
    * @param _uri : the URI value to update for the sender
    */
    function updateProfile(string calldata handle, string calldata location, string calldata avatar, string calldata _uri, string calldata _bio, bool isGroup) public {

        address profileAddress = msg.sender;

        // If this is a group get group details needed
        if (isGroup){
            // Validate that this group exists
            require(!Groups.isGroupAvailable(handle), "This group has not been created yet");

            // Get the group Address
            profileAddress = Groups.getGroupAddressFromID(Groups.getGroupID(handle));

            // Validate that they are the owner
            require(msg.sender == Groups.getOwnerOfGroup(Groups.getGroupIDFromAddress(profileAddress)), "You are not the owner of this group");
        }

        // If it's a new user, setup their initial profile
        if (usrProfileMap[profileAddress].joinBlock == 0){
            joinUser(profileAddress, isGroup);
        }

        // If they're not a group validate the handle, otherwise the group name is the handle
        if (!isGroup) {
            // Make sure the Handle length is within limits
            require(bytes(handle).length <= 50, "Your Handle is too long");

            // Require basic characters only
            require(KUtils.isValidString(handle), "Bad characters in Handle");
        }

        // Make sure the Location length is within limits
        require(bytes(location).length <= 25, "Your Location is too long");

        // Require basic characters only for the location
        require(KUtils.isValidString(location), "Bad characters in Location");

        // Make sure the Avatar length is within limits
        require(bytes(avatar).length <= maxURILength, "Your Avatar is too long");

        // Require a Avatar with RCF compliant characters only
        require(KUtils.isValidURI(avatar), "Bad characters in Avatar");

        // Make sure the URI length is within limits
        require(bytes(_uri).length <= maxURILength, "Your URI is too long");

        // Require a URI with RCF compliant characters only
        require(KUtils.isValidURI(_uri), "Bad characters in URI");

        // Make sure the Bio length is within limits
        require(bytes(_bio).length <= maxBioLength, "Your Bio is too long");

        // Require a Bio with RCF compliant characters only
        require(KUtils.isValidURI(_bio), "Bad characters in Bio");


        // Update the URI
        usrProfileMap[profileAddress].uri = _uri;

        // Update the Avatar
        usrProfileMap[profileAddress].avatar = avatar;

        // Update the Handle
        usrProfileMap[profileAddress].handle = handle;

        // Update the Location
        usrProfileMap[profileAddress].location = location;

        // Update the Bio
        usrProfileMap[profileAddress].bio = _bio;

        // Emit to the logs for external reference
        emit logProfileUpdated1(profileAddress, _uri, avatar);
        emit logProfileUpdated2(profileAddress, handle, location, _bio);
    }


    // Setup a new user profile
    function joinUser(address newUser, bool isGroup) internal {
        require(usrProfileMap[msg.sender].joinBlock == 0, "User already registered");

        usrProfileMap[newUser].followLimit = maxFollowing;
        usrProfileMap[newUser].joinBlock = block.number;
        usrProfileMap[newUser].joinTime = block.timestamp;
        usrProfileMap[newUser].isGroup = isGroup;

        // Add them to the bucket
        Followers.addFollower(newUser, newUser);

        // Emit to the logs for external reference
        emit logNewUser(msg.sender, isGroup);
    }


    /**
    * Returns the user details by address in JSON string
    * @param usrAddress : the address to retrieve the details for
    * 0 = Handle
    * 1 = Post Count
    * 2 = Number of users they are following
    * 3 = Number of users that are following them
    * 4 = User Verification level
    * 5 = Avatar URI
    * 6 = URI
    * 7 = Bio
    * 8 = Location
    * 9 = Block number when joined
    * 10 = Block timestamp when joined
    * 11 = Limit of number of users they can follow
    * 12 = Total Tips Received
    * 13 = Total Tips Sent
    */
    function getUserDetails(address usrAddress) public view returns(string[] memory){

        // Initialize the return array of users details
        string[] memory userDetails = new string[](14);


        userDetails[0] = usrProfileMap[usrAddress].handle;
        userDetails[1] = KUtils.toString(usrProfileMap[usrAddress].userStats.postCount);
        userDetails[2] = KUtils.toString(usrProfileMap[usrAddress].userStats.followingCount);
        userDetails[3] = KUtils.toString(usrProfileMap[usrAddress].userStats.followerCount);
        userDetails[4] = KUtils.toString(uint256(usrProfileMap[usrAddress].verified));
        userDetails[5] = usrProfileMap[usrAddress].avatar;
        userDetails[6] = usrProfileMap[usrAddress].uri;
        userDetails[7] = usrProfileMap[usrAddress].bio;
        userDetails[8] = usrProfileMap[usrAddress].location;
        userDetails[9] = KUtils.toString(usrProfileMap[usrAddress].joinBlock);
        userDetails[10] = KUtils.toString(usrProfileMap[usrAddress].joinTime);
        userDetails[11] = KUtils.toString(usrProfileMap[usrAddress].followLimit);
        userDetails[12] = KUtils.toString(usrProfileMap[usrAddress].userStats.tipsReceived);
        userDetails[13] = KUtils.toString(usrProfileMap[usrAddress].userStats.tipsSent);

        return userDetails;
    }


    /**
    * Returns a list of users that a user is following
    * @param posterAddress : the address to retrieve the details for
    * @param startFrom : the number to start getting records from
    */
    function getFollowings(address posterAddress, uint256 startFrom) public view returns(address[] memory){

        // Initialize the return array of users they're following
        address[] memory usersFollowing = new address[](maxFollowReturn);

        for (uint i=startFrom; i < (startFrom + maxFollowReturn); i++) {
            usersFollowing[i] = usrProfileMap[posterAddress].following[startFrom + i];
        }

        return usersFollowing;
    }


    function followUser(address addressRequester, address addressToFollow) public onlyAdmins {
        // If this poster doesn't have a profile setup yet, start it
        if (usrProfileMap[addressRequester].joinBlock == 0){
            // Check if this is a group
            if (Groups.getGroupIDFromAddress(addressRequester) > 0){
                joinUser(addressRequester, true);
            } else {
                joinUser(addressRequester, false);
            }
        }

        // Check to make sure they're under their max follow count
        require(usrProfileMap[addressToFollow].userStats.followerCount < usrProfileMap[addressRequester].followLimit, "You are following the maximum amount of accounts");

        // Add the follower to the followers lists
        Followers.addFollower(addressRequester, addressToFollow);

        // Add to Following
        usrProfileMap[addressRequester].following.push(addressToFollow);

        // Add a follower to the users profile follower count
        usrProfileMap[addressToFollow].userStats.followerCount += 1;
    }


    function unfollowUser(address addressRequester, address addressToUnfollow) public onlyAdmins {
        // Stringify the address
        string memory addressString = KUtils._toLower(KUtils.addressToString(addressToUnfollow));

        // Make sure it's not themselves
        require(!KUtils.stringsEqual(addressString, (KUtils._toLower(KUtils.addressToString(addressRequester)))), "not a thing");

        // Remove the follower from the followers lists
        Followers.removeFollower(addressRequester, addressToUnfollow);

        // Remove the following from the array by first finding the place of the address to remove
        uint256 place = 0;
        string memory addressToUnfollowStr = KUtils._toLower(KUtils.addressToString(addressToUnfollow));
        for (uint i=0; i < usrProfileMap[addressRequester].following.length; i++) {
            if (KUtils.stringsEqual(KUtils._toLower(KUtils.addressToString(usrProfileMap[addressRequester].following[i])), addressToUnfollowStr)){
                place = i;
                break;
            }
        }

        // Swap the last entry with this one
        usrProfileMap[addressRequester].following[place] = usrProfileMap[addressRequester].following[usrProfileMap[addressRequester].following.length-1];

        // Remove the last element
        usrProfileMap[addressRequester].following.pop();

        // Subtract a follower count from the dropped user
        usrProfileMap[addressToUnfollow].userStats.followerCount -= 1;
    }

}

