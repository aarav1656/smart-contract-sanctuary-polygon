pragma solidity >=0.8.17;



/********************************************************************************************/
/*                                       Gyidi Factory                                      */
/********************************************************************************************/
/**
 * @title Gyidi Crowdfunding Project
 * @author Yaw Botwe
 * @notice This creates a central storage space for funds and allows users to contribute, withdraw
 * and perform other functions.
 **/
contract Gyidi {
    using strings for *;
    /********************************************************************************************/
    /*                                       DATA VARIABLES                                     */
    /********************************************************************************************/

    address payable public creator; // Address of the creator of the project
    address payable GyidiTeam =
        payable(address(0x3B4bDFB3f86f5d382D1654C3e53C4A66d3B166AC)); //Address of Gyidi Team
    uint256 public goal; // Amount in wei that the project must reach to be successful
    uint256 public completedAt; // Time that project was successful
    uint256 public raisedBy; // Time of required for project to expire
    uint256 public currentBalance; // Amount currently raised
    uint256 public totalRaised; // Amount raised at the end of the project
    string public title; // Title of the project
    string public description; // Description of the project
    State public state = State.Fundraising; // State of the project (default state: Fundraising)
    uint256 public fee = 3; // Fee Gyidi takes for successful crowdfunding
    uint256 public minContribution; // Minimum amount that can be contributed
    uint256 public numOfContributors; //total number of contributors for the project
    bool public USDCType; // Boolean to determine if creator selected USDC as currency type
    bool public NFTType; // Boolean to determine if creator selected to fundraise with nft

    GyidiNFT public token; //Gyidi NFT token
    IERC20 public USDc; //USDC Token
    uint256 private tokenId = 0; //Token ID of NFT
    string public imageHash;

    mapping(address => uint256) public contributions; //Mapping to track the contributors of the project with
    //the amount contributed.
    uint256 private counter = 1; //Counter for reentrancy

    enum State {
        Fundraising,
        Successful,
        Expired,
        Funded
    } // States of the project

    struct Request {
        string description;
        address payable recipient;
        uint256 value;
        bool completed;
        uint256 noOfVoters;
        mapping(address => bool) voters;
    } // Spending request struct

    mapping(uint256 => Request) public requests; //Mapping to keep track of spending requests
    uint256 public numRequests; //Total number of requests

    /********************************************************************************************/
    /*                      FUNCTION MODIFIERS & CONSTRUCTOR                                    */
    /********************************************************************************************/


    /** 
        @dev Modifier to check for reentrancy.
    */
    modifier entrancyGuard() {
        counter++;
        uint256 localCounter = counter;
        _;
        require(counter == localCounter, "Reentrancy not permitted");
    }

    /**
     * @notice This function creates an instance of a Gyidi Project
     * @dev Constructor function for Gyidi contract. Sets necessary state variables to required values.
     * @param projectCreator the deployer of the Ether wallet
     */
    constructor(
        address projectCreator,
        string memory projectTitle,
        string memory projectDescription,
        uint256 durationInDays,
        uint256 projectGoal,
        uint256 _minContribution,
        bool _usdcType,
        bool _nftType
    ) {
        creator = payable(projectCreator);
        title = projectTitle;
        description = projectDescription;
        raisedBy = block.timestamp + durationInDays * 1 days;
        uint256 decimalsToDivideBy = 0;
        USDCType = _usdcType;
        NFTType = _nftType;
        if (USDCType) {
            decimalsToDivideBy = 12;
            USDc = IERC20(0x0FA8781a83E46826621b3BC094Ea2A0212e71B23);
        }
        goal = projectGoal / 10**decimalsToDivideBy;
        minContribution = _minContribution / 10**decimalsToDivideBy;
        if (NFTType) {
            string memory symbol = symbolize(projectTitle);
            uint256 maxSupply = goal / minContribution;
            token = new GyidiNFT(projectTitle, symbol, maxSupply);
        }
    }

    /********************************************************************************************/
    /*                     EVENTS & CONTRACT FUNCTIONS                                          */
    /********************************************************************************************/

    /**
     * @notice This event will be emitted whenever funding will be received
     * @param contributor The contributor of the project.
     * @param amount The amount contributed by the contributor.
     * @param currentTotal The current total amount contributed to the project
     */
    event FundingReceived(
        address contributor,
        uint256 amount,
        uint256 currentTotal
    );

    /**
     * @notice This event will be emitted when the creator of the project has withdrawn funds
     * @param recipient The creator of the project.
     * @param amount The amount withdrawn by the creator.
     */
    event CreatorFunded(address recipient, uint256 amount);

    /**
     * @notice This event will be emitted when the contributor of the project is refunded
     * @param recipient The contributor of the project.
     * @param amount The amount refunded to the contributor.
     */
    event ContributorRefunded(address recipient, uint256 amount);


    /**
    * @notice Function to contribute funds to a project.
    * @dev This function has a modifier for checking if the project is in the Fundraising state.
    */
    function contribute(uint256 _numOfTokens)
    public
    payable
    {
        require(state == State.Fundraising, "Contract must be in the Fundraising state");
        require(msg.sender != creator, "Creator of project cannot contribute to it");
        require(_numOfTokens >= 0, "Number of tokens cannot be negative");

        // Check if the sender is a new contributor
        if (contributions[msg.sender] == 0) {
            numOfContributors++;
        }

        // Check if the contribution is USDC
        if (USDCType) {
            require(msg.value >= minContribution, "USDC: Value sent is less than minimum contribution");
            // Transfer USDC from sender to the contract
            bool transferSuccessful = USDc.transferFrom(msg.sender, address(this), msg.value);
            require(transferSuccessful, "USDC: Transfer failed");
            // Update the contributions mapping
            contributions[msg.sender] = contributions[msg.sender] + msg.value;
            // Update the current balance
            currentBalance += msg.value;
        } 
        // Handle ether contributions
        else {
            require(msg.value >= minContribution, "Ether: Amount contributed is less than minimum value");
            // Update the contributions mapping
            contributions[msg.sender] = contributions[msg.sender] + msg.value;
            // Update the current balance
            currentBalance += msg.value;
        }

        // Check if the project is using NFT as currency type
        if (NFTType) {
            require(_numOfTokens > 0, "You need to input an accurate number of tokens");
            require(_numOfTokens == msg.value/minContribution, "You need to input an accurate amount of money for the tokens requested");
            for (uint256 i = 0; i < _numOfTokens; i++) {
                token.mint(msg.sender, tokenId);
                tokenId++;
            }
        } 

        // Check if the project has reached its goal
        if (currentBalance >= goal) {
            state = State.Successful;
            completedAt = block.timestamp;
            totalRaised = currentBalance;
        } 
        // Check if the project has expired
        else if (block.timestamp > raisedBy) {
            state = State.Expired;
            completedAt = block.timestamp;
        }

        emit FundingReceived(msg.sender, msg.value, currentBalance);
    }




    /**
     * @notice Function to retrieve donated amount when a project expires.
     * @dev This function has a modifier for checking if the project is in the Expired state.
     * As well as a modifier to check if the caller of the function is a contributor.
     */
    function getRefund() external {
        require(state == State.Expired, "Contract must be in the Expired state");
        require(
            contributions[msg.sender] > 0,
            "Only the project contributor can call this function"
        );
        uint256 amountToRefund = contributions[msg.sender];
        contributions[msg.sender] = 0;
        if (USDCType) {
            USDc.transfer(msg.sender, amountToRefund);
        } else {
            payable(msg.sender).transfer(amountToRefund);
        }
        emit ContributorRefunded(msg.sender, amountToRefund);
    }

    /**
     * @notice Function to get specific information about the project.
     */
    function getDetails()
        public
        view
        returns (
            address payable projectStarter,
            string memory projectTitle,
            string memory projectDesc,
            uint256 deadline,
            uint256 balance,
            uint256 projectGoal,
            State currentState
        )
    {
        projectStarter = creator;
        projectTitle = title;
        projectDesc = description;
        deadline = raisedBy;
        balance = currentBalance;
        projectGoal = goal;
        currentState = state;
    }

    /**
     * @notice Function to check balance of project.
     */
    function getBalance() public view returns (uint256) {
        if(USDCType){
            return USDc.balanceOf(address(this));
        }else{
            return address(this).balance;
        }
    }

    /**
     * @notice Function to create a spending request for a successful project.
     * @dev This function has a modifier for checking if the project is in the Successful state.
     * As well as a modifier to check if the caller of the function is the creator.
     */
    function createRequests(string memory _description, uint256 _value)
        public
        
    {
        require(state == State.Successful, "Contract must be in the Successful state");
        require(
            msg.sender == creator,
            "Only the project creator can call this function"
        );
        require(_value <= goal/3, "You can't withdraw more than one-third of your goal.");
        Request storage newRequest = requests[numRequests];
        numRequests++;
        newRequest.description = _description;
        newRequest.recipient = payable(msg.sender);
        newRequest.value = _value;
        newRequest.completed = false;
        newRequest.noOfVoters = 0;
    }

    /**
     * @notice Function to vote for a spending request of a successful project.
     * @dev This function has a modifier for checking if the project is in the Successful state.
     * As well as a modifier to check if the caller of the function is a contributor.
     */
    function voteForRequest(uint256 requestNo)
        public
        
    {
        require(state == State.Successful, "Contract must be in the Successful state");
        require(
            contributions[msg.sender] > 0,
            "Only the project contributor can call this function"
        );
        require(requestNo < numRequests, "Request does not exist");
        require(requests[requestNo].completed == false, "Request has already been completed");
        Request storage thisRequest = requests[requestNo];
        require(
            thisRequest.voters[msg.sender] == false,
            "You have already voted"
        );
        thisRequest.voters[msg.sender] = true;
        thisRequest.noOfVoters++;
    }

    /**
     * @notice Function to withdraw funds based on a spending request of a successful project.
     * @dev This function has a modifier for checking if the project is in the Successful state.
     * As well as a modifier to check if the caller of the function is the creator.
     * @param requestNo The id of the spending request.
     */
    function withdraw(uint256 requestNo)
        public
    {
        require(state == State.Successful, "Contract must be in the Successful state");
        require(
            msg.sender == creator,
            "Only the project creator can call this function"
        );
        require(
            totalRaised >= goal,
            "You can't withdraw from this because project did not reach goal"
        );
        Request storage thisRequest = requests[requestNo];
        require(
            thisRequest.completed == false,
            "This request has already been fulfilled"
        );
        require(
            thisRequest.noOfVoters > numOfContributors / 2,
            "This request did not pass the minimal number of votes by contributors"
        );
        thisRequest.completed = true;
        uint256 feeToBeSent = (fee / 100) * thisRequest.value;
        thisRequest.value -= feeToBeSent;
        if(USDCType){
            USDc.transfer(GyidiTeam, feeToBeSent);
            USDc.transfer(thisRequest.recipient, thisRequest.value);
        }else{
            payable(GyidiTeam).transfer(feeToBeSent);
            payable(thisRequest.recipient).transfer(thisRequest.value);
        }
        emit CreatorFunded(thisRequest.recipient, thisRequest.value);
    }

    /**
     * @notice Function to cancel a campaign.
     * @dev This function has a modifier for checking if the project is in the Fundraising state.
     * As well as a modifier to check if the caller of the function is the creator.
     */
    function cancelCampaign() public  {
        require(state == State.Fundraising, "Contract must be in the Fundraising state");
        require(
            msg.sender == creator,
            "Only the project creator can call this function"
        );
        state = State.Expired;
        completedAt = block.timestamp;
    }

    /**
     * @notice Function to store ipfsHash of campaign image.
     */
    function storeImage(string memory _ipfsHash) public {
        imageHash = _ipfsHash;
    }


    /**
     * @notice Function to change the project title into a symbol for NFTs.
     */
    function symbolize(string memory text) public pure returns (string memory) {
        strings.slice memory s = text.toSlice();
        strings.slice memory delim = " ".toSlice();
        string[] memory parts = new string[](s.count(delim) + 1);
        for (uint256 i = 0; i < parts.length; i++) {
            parts[i] = s.split(delim).toString();
        }

        bytes memory charByte = new bytes(parts.length);
        bytes memory character;

        for (uint256 i = 0; i < parts.length; i++) {
            character = bytes(parts[i]);
            charByte[i] = bytes1(character);
        }

        return string(charByte);
    }

    receive() external payable {
        if (NFTType) {
            require(
                msg.value % minContribution == 0,
                "Amount sent isn't divisible by minimum contribution amount"
            );
            uint256 numofTkns = msg.value / minContribution;
            if (USDCType){
                contribute(numofTkns);
            }
            
        } else {
            contribute(0);
        }
    }
}

