pragma solidity ^0.8.17;

contract GyidiFactory {
    // List of existing projects
    Gyidi[] private projects;

    // Event that will be emitted whenever a new project is started
    event ProjectStarted(
        address contractAddress,
        address projectStarter,
        string projectTitle,
        string projectDesc,
        uint256 deadline,
        uint256 goalAmount
    );

    /** @dev Function to start a new project.
     * @param title Title of the project to be created
     * @param description Brief description about the project
     * @param durationInDays Project deadline in days
     * @param amountToRaise Project goal in wei
     */
    function startProject(
        string calldata title,
        string calldata description,
        uint256 durationInDays,
        uint256 amountToRaise,
        uint256 minContribution, 
        bool _usdcType,
        bool _nftType
    ) external {
        Gyidi newProject = new Gyidi(
            msg.sender,
            title,
            description,
            durationInDays,
            amountToRaise,
            minContribution,
            _usdcType,
            _nftType
        );
        projects.push(newProject);
        emit ProjectStarted(
            address(newProject),
            msg.sender,
            title,
            description,
            durationInDays,
            amountToRaise
        );
    }

    /** @dev Function to get all projects' contract addresses.
     * @return A list of all projects' contract addreses
     */
    function returnAllProjects() external view returns (Gyidi[] memory) {
        return projects;
    }

    function getAllProjectsContributed()
        external
        view
        returns (Gyidi[] memory)
    {
        uint256 count = projects.length;
        Gyidi[] memory projectsContributed = new Gyidi[](count);
        uint256 countProjects = 0;

        for (uint256 i = 0; i < count; i++) {
            if (projects[i].contributions(msg.sender) > 0) {
                projectsContributed[countProjects] = projects[i];
                countProjects++;
            }
        }

        Gyidi[] memory result = new Gyidi[](countProjects);
        for (uint256 i = 0; i < countProjects; i++) {
            result[i] = projectsContributed[i];
        }

        return result;
    }

    function getAllProjectsCreated() external view returns (Gyidi[] memory) {
        uint256 count = projects.length;
        Gyidi[] memory projectsCreated = new Gyidi[](count);
        uint256 countProjects = 0;

        for (uint256 i = 0; i < projects.length; i++) {
            if (projects[i].creator() == payable(msg.sender)) {
                projectsCreated[countProjects] = projects[i];
                countProjects++;
            }
        }

        Gyidi[] memory result = new Gyidi[](countProjects);
        for (uint256 i = 0; i < countProjects; i++) {
            result[i] = projectsCreated[i];
        }

        return result;
    }
}