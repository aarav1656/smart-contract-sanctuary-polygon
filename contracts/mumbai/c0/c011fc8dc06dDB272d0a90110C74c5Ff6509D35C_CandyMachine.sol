pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./CandyMachineStorage.sol";

contract CandyMachine is Ownable, CandyMachineStorage {
    struct Payment {
        string name;
        IERC20 tokenInterface;
        bool isActive;
        uint256 price;
    }

    Payment[] public paymentTokens;

    event mintedLandsEvent(uint256[] indexed mintedArray, uint256[] mintedLands);

    // /**
    //  * @dev Magic value to be returned upon successful reception of an NFT
    //  *  Equals to `bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"))`,
    //  *  which can be also obtained as `ERC721Receiver(0).onERC721Received.selector`
    //  */
    // bytes4 internal constant ERC721_RECEIVED = 0x150b7a02;

    constructor(uint256 MATICPrice, uint256 USDTPrice, address whitelistRegistry) {
        counter = 0;
        lastIndex = 1;
        isInitialized = false;
        if (whitelistRegistry != address(0)) {
            whitelist = IWhitelist(whitelistRegistry);
            isWhitelistActive = true;
        } else {
            isWhitelistActive = false;
        }
        paymentTokens.push(
            Payment({
                name: "MATIC",
                tokenInterface: IERC20(address(0)),
                isActive: false,
                price: MATICPrice
            })
        );

        paymentTokens.push(
            Payment({
                name: "SoulboundLand",
                tokenInterface: IERC20(address(0)),
                isActive: true,
                price: 1
            })
        );

        paymentTokens.push(
            Payment({
                name: "USDT",
                tokenInterface: IERC20(0xE097d6B3100777DC31B34dC2c58fB524C2e76921),
                isActive: true,
                price: USDTPrice
            })
        );
    }

    function addPaymentToken(
        string calldata _name,
        address _tokenInterface,
        bool _isActive,
        uint256 _price
    ) public onlyOwner {
        paymentTokens.push(
            Payment({
                name: _name,
                tokenInterface: IERC20(_tokenInterface),
                isActive: _isActive,
                price: _price
            })
        );
        emit PaymentTokenAdded(_name, _tokenInterface, _isActive, _price);
    }

    function setPrice(uint256 index, uint256 newPrice) public onlyOwner {
        require(newPrice != 0, "Price can't be 0");

        paymentTokens[index].price = newPrice;

        emit PriceSet(index, newPrice);
    }

    function getCandyMachineStatus() public view returns (bool) {
        return isActive;
    }

    function setCandyMachineStatus(bool _newStatus) public onlyOwner {
        isActive = _newStatus;

        emit CandyMachineStatus(_newStatus);
    }

    function setIsPaymentActive(uint256 index, bool _isActive) public onlyOwner {
        paymentTokens[index].isActive = _isActive;

        emit PaymentActiveSet(index, _isActive);
    }

    function setPaymentInterface(uint256 index, address _tokenInterface) public onlyOwner {
        paymentTokens[index].tokenInterface = IERC20(_tokenInterface);

        emit PaymentInterfaceSet(index, _tokenInterface);
    }

    function getPaymentToken(uint256 index) public view returns (Payment memory) {
        return (paymentTokens[index]);
    }

    function getNumberOfPaymentToken() public view returns (uint256) {
        return (paymentTokens.length);
    }

    function setCollector(address _collector) public onlyOwner {
        require(collector != address(this), "Collector is contract address.");
        collector = _collector;

        emit CollectorSet(_collector);
    }

    function setLandRegistry(address registry) external onlyOwner {
        landRegistry = ILANDRegistry(registry);

        emit LandRegistrySet(registry);
    }

    function setSoulboundToken(address registry) external onlyOwner {
        soulboundRegistry = ISoulboundToken(registry);

        emit SoulboundTokenSet(registry);
    }

    function getLandIdsLen() public view returns (uint256) {
        return landIds.length;
    }

    function getOwnedLand(address receiver) public view returns (uint256) {
        return ownedLands[receiver];
    }

    function initializeArray() public onlyOwner {
        require(isInitialized == false, "Array already intialized");
        uint256 landIdsIndex;
        console.log("Accessing array with index: ", landIdsIndex);
        for (uint i = 1; i <= X_WIDTH; i++) {
            for (uint j = lastIndex; j <= (lastIndex + 4); j++) {
                if (landIsAvailable(i, j)) {
                    uint256 tokenId = landRegistry.getTokenId(i, j);
                    landIdsIndex = _getIndexFromCoords(i, j);
                    landIds[landIdsIndex] = tokenId;
                    remainingLands.push(tokenId);
                    landIdsMap[landIdsIndex] = remainingLands.length - 1;
                }
            }
        }
        lastIndex += 5;
        if (lastIndex > X_WIDTH) {
            isInitialized = true;
        }
    }

    function getArrayLength() public view returns (uint256) {
        return remainingLands.length;
    }

    function sortedArray(uint256 _start, uint256 _end) public view returns (bool) {
        for (uint256 i = _start; i < _end; i++) {
            if (!(remainingLands[i] < remainingLands[i + 1])) {
                return false;
            }
        }
        return true;
    }

    function landIsAvailable(uint256 x, uint256 y) public view returns (bool) {
        return !occupiedLands[x][y];
    }

    function setOccupiedLands(
        uint256[] calldata x,
        uint256[] calldata y,
        bool[] calldata isOccupied
    ) public onlyOwner {
        require(x.length == y.length && x.length == isOccupied.length, "Lengths are different.");
        for (uint256 i = 0; i < x.length; i++) {
            occupiedLands[x[i]][y[i]] = isOccupied[i];
        }
    }

    function random() private returns (uint) {
        counter++;
        uint randomHash = uint(
            keccak256(abi.encodePacked(block.difficulty, block.timestamp, counter))
        );
        return randomHash;
    }

    function mintRandomLand() private returns (uint256) {
        uint256 landIndex = random() % remainingLands.length;
        uint256 tokenId = remainingLands[landIndex];
        if (!landRegistry.canMint(tokenId)) {
            _markLandAsUnavailableWithIndex(tokenId, landIndex);
            return mintRandomLand();
        }
        mintLand(tokenId);
        uint256 landIdsIndex = getIndexFromTokenId(tokenId);
        _markLandAsUnavailableWithIndex(tokenId, landIndex);
        return landIdsIndex;
    }

    function safeMintLand(uint256 tokenId) private {
        mintLand(tokenId);
        _markLandAsUnavailable(tokenId);
    }

    function mintLand(uint256 tokenId) private {
        landRegistry.mintToken(msg.sender, tokenId);
    }

    function _markLandAsUnavailable(uint256 tokenId) private {
        uint landIndex = landIdsMap[getIndexFromTokenId(tokenId)];
        _markLandAsUnavailableWithIndex(tokenId, landIndex);
    }

    function markLandAsUnavailable(uint256[] calldata tokenId) public {
        require(msg.sender == address(landRegistry) || msg.sender == owner());
        for (uint i = 0; i < tokenId.length; i++) {
            _markLandAsUnavailable(tokenId[i]);
        }
    }

    function markLandAsUnavailableAirDrop(uint256[] calldata tokenId, address receiver) public {
        require(msg.sender == address(landRegistry) || msg.sender == owner());
        require(ownedLands[receiver] + tokenId.length <= 36, "You can't mint more than 36 lands.");
        ownedLands[receiver] += tokenId.length;
        for (uint i = 0; i < tokenId.length; i++) {
            _markLandAsUnavailable(tokenId[i]);
        }
    }

    function _markLandAsUnavailableWithIndex(uint256 tokenId, uint256 landIndex) private {
        uint256 landIdsIndex = getIndexFromTokenId(tokenId);
        console.log("Accessing array with index: ", landIdsIndex);
        require(landIds[landIdsIndex] != 0, "this token has already been removed");
        landIds[landIdsIndex] = 0;
        _removeFromArrayByIndex(remainingLands, landIndex);
        if (!(landIndex == remainingLands.length)) {
            landIdsMap[getIndexFromTokenId(remainingLands[landIndex])] = landIndex;
        }
    }

    function reclaimMultipleLand(
        uint256 quantity,
        uint256 paymentTokenIndex
    ) public payable returns (uint256[] memory) {
        require(isActive == true, "CandyMachine is not active.");
        require(isInitialized == true, "Candy Machine has not been initialized yet");
        require(quantity <= remainingLands.length, "Not enough lands.");
        require(quantity + ownedLands[msg.sender] <= 36, "You can't mint more than 36 lands.");
        require(
            !isWhitelistActive || (!whitelist.isActive() || whitelist.isWhitelisted(msg.sender)),
            "Whitelist is active and the current user is not whitelisted"
        );
        ownedLands[msg.sender] += quantity;

        _processPayment(quantity, paymentTokenIndex);

        uint256[] memory mintedLands = new uint256[](quantity);

        if (quantity < FIRST_TIER) {
            for (uint256 i = 0; i < quantity; i++) {
                uint256 landIdsIndex = mintRandomLand();
                console.log("Accessing array with index: ", landIdsIndex);
                mintedLands[i] = landIds[landIdsIndex];
            }

            emit mintedLandsEvent(mintedLands, mintedLands);
            return mintedLands;
        } else if (quantity < SECOND_TIER) {
            uint256 firstLandIndex = mintRandomLand();
            console.log("Accessing array with index: ", firstLandIndex);
            mintedLands[0] = landIds[firstLandIndex];
            for (uint256 i = 1; i < quantity; i++) {
                firstLandIndex = mintLandFirstTier(
                    randomDistanceFirstTier(),
                    firstLandIndex,
                    mintedLands[i]
                );
                console.log("Accessing array with index: ", mintedLands[i]);
                mintedLands[i] = landIds[mintedLands[i]];
            }

            emit mintedLandsEvent(mintedLands, mintedLands);
            return mintedLands;
        } else {
            uint256 firstLandIndex = mintRandomLand();
            console.log("Accessing array with index: ", firstLandIndex);
            mintedLands[0] = landIds[firstLandIndex];
            for (uint256 i = 1; i < quantity; i++) {
                firstLandIndex = mintLandSecondTier(
                    randomDistanceSecondTier(),
                    firstLandIndex,
                    mintedLands[i]
                );
                console.log("Accessing array with index: ", mintedLands[i]);
                mintedLands[i] = landIds[mintedLands[i]];
            }

            emit mintedLandsEvent(mintedLands, mintedLands);
            return mintedLands;
        }
    }

    function _processPayment(uint256 _quantity, uint256 _paymentTokenIndex) internal {
        require(
            paymentTokens[_paymentTokenIndex].isActive,
            "This payment methon is not currently active"
        );
        if (_paymentTokenIndex == MATIC_TOKEN_INDEX) {
            require(
                _quantity * paymentTokens[_paymentTokenIndex].price == msg.value,
                "Price is not correct."
            );
            payable(collector).transfer(msg.value);
        } else if (_paymentTokenIndex == COIN_TOKEN_INDEX) {
            require(msg.value == 0, "You can't send MATIC while paying with ERC-20.");
            require(address(soulboundRegistry) != address(0), "Soulbound contract is not set.");
            soulboundRegistry.spendTokens(msg.sender, _quantity);
        } else {
            require(msg.value == 0, "You can't send MATIC while paying with ERC-20.");
            paymentTokens[_paymentTokenIndex].tokenInterface.transferFrom(
                msg.sender,
                collector,
                _quantity * paymentTokens[_paymentTokenIndex].price
            );
        }
    }

    function mintLandFirstTier(
        uint256 distance,
        uint256 firsLandIndex,
        uint256 mintedLandIndex
    ) public returns (uint256) {
        if (distance == 1) {
            mintedLandIndex = mintCloseLand(firsLandIndex);
            console.log("Minted close land");
            return mintedLandIndex;
        } else {
            mintedLandIndex = mintRandomLand();
            console.log("Minted random land");
            return firsLandIndex;
        }
    }

    function mintLandSecondTier(
        uint256 distance,
        uint256 firsLandIndex,
        uint256 mintedLandIndex
    ) public returns (uint256) {
        if (distance == 1) {
            mintedLandIndex = mintCloseLand(firsLandIndex);
            console.log("Minted close land");
            return mintedLandIndex;
        } else {
            mintedLandIndex = mintRandomLand();
            console.log("Minted random land");
            return firsLandIndex;
        }
    }

    function mintLandAtDistance(uint256 firsLandIndex, uint256 distance) private returns (uint256) {
        uint256 direction = getRandomDirection();
        for (uint256 i = 0; i < 4; i++) {
            uint256 possibleLandIndex = getLandAtDirection(
                firsLandIndex,
                distance,
                (direction + i) % 4
            );
            if (possibleLandIndex != 0) {
                console.log("Accessing array with index: ", possibleLandIndex);
                safeMintLand(landIds[possibleLandIndex]);
                return possibleLandIndex;
            }
        }
        if (distance < MAX_DISTANCE) {
            return mintLandAtDistance(firsLandIndex, distance + 1);
        }
        return mintRandomLand();
    }

    function getLandAtDirection(
        uint256 firsLandIndex,
        uint256 distance,
        uint direction
    ) private view returns (uint256) {
        uint256 landX = _getXFromIndex(firsLandIndex);
        uint256 landY = _getYFromIndex(firsLandIndex);
        if (direction == UP) {
            return getLandUP(landX, landY, distance);
        }
        if (direction == RIGHT) {
            return getLandRIGHT(landX, landY, distance);
        }
        if (direction == DOWN) {
            return getLandDOWN(landX, landY, distance);
        }
        if (direction == LEFT) {
            return getLandLEFT(landX, landY, distance);
        }
        return 0;
    }

    function getLandUP(
        uint256 firstLandX,
        uint256 firstLandY,
        uint256 distance
    ) private view returns (uint256) {
        for (uint256 i = 0; i < distance; i++) {
            if (!(firstLandY < distance + 1) && !(firstLandX < distance + 1)) {
                uint256 newX = firstLandX - i;
                uint256 newY = firstLandY - distance + i;
                if (landIsInBound(newX, newY) && landIds[_getIndexFromCoords(newX, newY)] != 0) {
                    console.log("Accessing array with index: ", _getIndexFromCoords(newX, newY));
                    return _getIndexFromCoords(newX, newY);
                }
            }
        }
        return 0;
    }

    function getLandRIGHT(
        uint256 firstLandX,
        uint256 firstLandY,
        uint256 distance
    ) private view returns (uint256) {
        for (uint256 i = 0; i < distance; i++) {
            if (!(firstLandY < distance + 1)) {
                uint256 newX = firstLandX + distance - i;
                uint256 newY = firstLandY - i;
                if (landIsInBound(newX, newY) && landIds[_getIndexFromCoords(newX, newY)] != 0) {
                    console.log("Accessing array with index: ", _getIndexFromCoords(newX, newY));
                    return _getIndexFromCoords(newX, newY);
                }
            }
        }
        return 0;
    }

    function getLandDOWN(
        uint256 firstLandX,
        uint256 firstLandY,
        uint256 distance
    ) private view returns (uint256) {
        for (uint8 i = 0; i < distance; i++) {
            uint256 newX = firstLandX + i;
            uint256 newY = firstLandY + distance - i;
            if (landIsInBound(newX, newY) && landIds[_getIndexFromCoords(newX, newY)] != 0) {
                console.log("Accessing array with index: ", _getIndexFromCoords(newX, newY));
                return _getIndexFromCoords(newX, newY);
            }
        }
        return 0;
    }

    function getLandLEFT(
        uint256 firstLandX,
        uint256 firstLandY,
        uint256 distance
    ) private view returns (uint256) {
        for (uint256 i = 0; i < distance; i++) {
            if (!(firstLandX < distance + 1)) {
                uint256 newX = firstLandX - distance + i;
                uint256 newY = firstLandY + i;

                if (landIsInBound(newX, newY) && landIds[_getIndexFromCoords(newX, newY)] != 0) {
                    console.log("Accessing array with index: ", _getIndexFromCoords(newX, newY));
                    return _getIndexFromCoords(newX, newY);
                }
            }
        }
        return 0;
    }

    function landIsInBound(uint256 landX, uint256 landY) private pure returns (bool) {
        return (landX > 0 && landX <= X_WIDTH && landY > 0 && landY <= Y_WIDTH);
    }

    function getRandomDirection() private returns (uint256) {
        return uint256(random() % 4);
    }

    function mintCloseLand(uint256 firsLandIndex) private returns (uint256) {
        return mintLandAtDistance(firsLandIndex, 1);
    }

    function randomDistanceFirstTier() public returns (uint256) {
        uint256 _random = random() % 100;
        if (_random < 40) {
            return 1;
        }
        return 2;
    }

    function randomDistanceSecondTier() public returns (uint256) {
        uint256 _random = random() % 100;
        if (_random < 70) {
            return 1;
        }
        return 2;
    }

    function _removeFromArrayByIndex(uint256[] storage array, uint256 index) internal {
        uint256 app = array[array.length - 1];
        array[array.length - 1] = array[index];
        array[index] = app;
        array.pop();
    }

    function min(uint256 a, uint256 b) internal pure returns (uint256) {
        if (a < b) {
            return a;
        }
        return b;
    }

    function abs(uint256 x, uint256 y) internal pure returns (uint256) {
        if (x < y) {
            return y - x;
        }
        return x - y;
    }

    function _getXFromIndex(uint256 i) internal pure returns (uint256) {
        return (i % X_WIDTH) + 1;
    }

    function _getYFromIndex(uint256 i) internal pure returns (uint256) {
        return (i + 1) / Y_WIDTH + 1;
    }

    function _getIndexFromCoords(uint256 x, uint256 y) internal view returns (uint256) {
        if (y == 0 || x == 0) {
            console.log("DING DONG!!! TROVATO L'ERRORE");
        }
        return (y - 1) * X_WIDTH + x - 1;
    }

    function _getDistanceFromCoords(
        uint256 x1,
        uint256 y1,
        uint256 x2,
        uint256 y2
    ) internal pure returns (uint256) {
        return abs(x1, x2) + abs(y1, y2);
    }

    function _getDistanceFromIndex(uint256 i, uint256 j) internal pure returns (uint256) {
        return
            _getDistanceFromCoords(
                _getXFromIndex(i),
                _getYFromIndex(i),
                _getXFromIndex(j),
                _getYFromIndex(j)
            );
    }

    function setWhitelist(address whitelistAddress) public onlyOwner {
        require(whitelistAddress != address(0), "Whitelist address can't be 0");
        whitelist = IWhitelist(whitelistAddress);
        isWhitelistActive = true;
    }

    function disableWhitelist() public onlyOwner {
        isWhitelistActive = false;
    }

    function enableWhitelist() public onlyOwner {
        isWhitelistActive = true;
    }

    function getWhitelistStatus() public view returns (bool) {
        return isWhitelistActive;
    }

    function getIndexFromTokenId(uint256 tokenId) public view returns (uint256) {
        return _getIndexFromCoords(landRegistry.getX(tokenId), landRegistry.getY(tokenId));
    }

    // EVENTS

    event PaymentTokenAdded(string name, address tokenInterface, bool isActive, uint256 price);

    event PriceSet(uint256 tokenIndex, uint256 newPrice);

    event CandyMachineStatus(bool newStatus);

    event PaymentActiveSet(uint256 tokenIndex, bool isActive);

    event PaymentInterfaceSet(uint256 tokenIndex, address tokeInterface);

    event CollectorSet(address newCollecotr);

    event LandRegistrySet(address newLandRegistry);

    event SoulboundTokenSet(address newSoulBoundRegistry);



}

