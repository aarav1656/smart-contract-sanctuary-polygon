pragma solidity ^0.8.2;

import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interfaces/IKUtils.sol";
import "./interfaces/IUserProfiles.sol";
import "./interfaces/IGroups.sol";
import "./interfaces/IDOOM.sol";

contract UserHandles {

    // Admins
    mapping (address => bool) admins;

    // Approved Wallets
    mapping (address => bool) approved;

    // Map the Handle => User Address for uniqueness (stored in lowercase)
    mapping (string => address) public usrHandleMap;

    // Set token cost for name change
    uint256 public costForNameChange = 69 ether;

    // Link to the KUtils Contracts
    IKUtils public KUtils;

    // Link to Random NFT Contract
    IUserProfiles public UserProfiles;

    // Link to the DOOM Token
    IGroups public Groups;

    // Link to the DOOM Token
    IDOOM public DOOM;

    // ERC20 Receiver for payment via ERC20
    IERC20 private paymentToken;

    constructor(IERC20 _paymentToken, address _kutils) {
        // Setup the payment token
        paymentToken = IERC20(_paymentToken);

        // Setup the default Admin
        admins[msg.sender] = true;

        // Setup default approved wallet
        approved[msg.sender] = true;

        // Setup link to KUtils
        KUtils = IKUtils(_kutils);
    }

    /*

    EVENTS

    */

    event logProfileUpdated1(address indexed requester, string uri, string avatar);
    event logProfileUpdated2(address indexed requester, string handle, string location, string bio);
    event logNewUser(address indexed requester, bool isGroup);




    /*

    MODIFIERS

    */

    modifier onlyAdmins() {
        require(admins[msg.sender], "Only admins can call this function.");
        _;
    }

    modifier onlyApproved() {
        require(approved[msg.sender], "Only approved wallets can call this function.");
        _;
    }


    /*

    ADMIN FUNCTIONS

    */

    function addAdmin(address newAdmin) public onlyAdmins{
        admins[newAdmin] = true;
    }

    function removeAdmin(address oldAdmin) public onlyAdmins{
        admins[oldAdmin] = false;
    }

    function addApproved(address newAddress) public onlyAdmins{
        approved[newAddress] = true;
    }

    function removeApproved(address oldAddress) public onlyAdmins{
        approved[oldAddress] = false;
    }


    /**
    * Updates the cost to change a name in DOOM tokens
    * @param _costToChangeName : the cost in DOOM tokens to update a users handle
    */
    function updateLimits(uint256 _costToChangeName) public onlyAdmins {

        costForNameChange = _costToChangeName;
    }


    // Update contracts attached to this contract
    function updateContracts(IERC20 _payments, address _userProfiles, address _kutils, address _doom, address _groups) public onlyAdmins {
        // Update the contract address of the ERC20 token to be used as payment
        paymentToken = IERC20(_payments);

        // Update the Followers contract address
        UserProfiles = IUserProfiles(_userProfiles);

        // Update the KUtils address
        KUtils = IKUtils(_kutils);

        // Update the DOOM Token address
        DOOM = IDOOM(_doom);

        // Update the Groups address
        Groups = IGroups(_groups);
    }



    /*

    Public Functions

    */


    // Update a users (self) handle
    function updateUserHandle(string calldata handle) public {

        // require the approval of the sending of the token to the contract
        require(paymentToken.approve(msg.sender, costForNameChange), "Must approve payment tokens to be transferred");

        // Transfer the Payment Token to the contract
        require(paymentToken.transferFrom(msg.sender, address(this), costForNameChange), "Didn't receive the Payment Token");

        string memory handleLower = KUtils._toLower(handle);

        // Check if anyone already registered that handle
        require(usrHandleMap[handleLower] == address(0), "Handle already taken");

        // Make sure the Handle length is within limits
        require(KUtils.strlen(handle) <= 50, "Your Handle is too long");

        // Require basic characters only
        require(KUtils.isValidString(handle), "Bad characters in Handle");

        // Release old handle
        delete usrHandleMap[KUtils._toLower(UserProfiles.getUserDetails(msg.sender)[0])];

        // Update the Handle in the users profile
        UserProfiles.updateHandle(msg.sender, handle);

        // Update the handle mapping to address to block it
        usrHandleMap[handleLower] = msg.sender;
    }

}

