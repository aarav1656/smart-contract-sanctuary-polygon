import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "./utils/DefaultOperatorFilterer.sol";

contract SynchroverseNFTContract is ERC1155, Pausable, Ownable, ReentrancyGuard, DefaultOperatorFilterer, ERC2981 {
    using Counters for Counters.Counter;

    error IncorrectAmountError();
    error MaxTokensError();
    error ExceedsMaxSupplyError();
    error IncorrectReferralCodeError();
    error InsufficientBalance();
    error DeadlineError();
    error NotActiveSale();

    Counters.Counter private _tokenIdCounter;
    Counters.Counter private _referralCodeIds;
    Counters.Counter private _whiteListCounter;

    uint256 public initialPrice = 100000000000000;
    uint256 public maxTokens = 100;
    uint256 public deadline = 1690121521;
    uint256 private constant REFERRAL_FEE_PERCENTAGE = 5;
    uint256 public constant MAX_MINT_PER_TX = 5;

    string public uriPrefix;
    string uriSuffix;

    bool public activeDeadline = true;
    bool public activeSale = true;
    bool public operatorFilteringEnabled = true;

    uint256[] private _randomIds;

    constructor(string memory _uriPrefix, string memory _uriSuffix) ERC1155("https://ipfs.io/ipfs/QmRoa5ikAdo1ksdvsKriauiFzbufbf2bHtH42U5cSE91Ro/{id}.json")
    {
        uriPrefix = _uriPrefix;
        uriSuffix = _uriSuffix;
        setRoyaltyInfo(payable(0x0f4011d3905E85dd017C011300190e9Fc9838AC4), 1000);
        _registerForOperatorFiltering(address(0), false);

        for(uint i = 1; i <= maxTokens; i++) {
            _randomIds.push(i);
        }
    }

    struct referralUser {
        address user;
    }

    struct WhiteList {
        address user;
        bool active;
    }

    struct ReferralCode {
        address user;
        string code;
    }

    mapping(uint256 => ReferralCode) public  referralCode;
    mapping(address => uint256)      public  mintedPerWallet;
    mapping(uint256 => WhiteList)    public  whiteList;
    mapping(address => ReferralCode) private _referralsUsers;
    mapping(uint256 => string)       private _tokenURIs;
    mapping(address => uint256)      private _usersBalance;

    event Mint (
        uint256[] tokenIds,
        address recipient
    );

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
   
    // Modifiers
    modifier verifyWalletAvailableSupply(uint256 amount) {
        if (
            mintedPerWallet[msg.sender] + amount > MAX_MINT_PER_TX &&
            msg.sender != owner()
        ) revert ExceedsMaxSupplyError();
        _;
    }

    //Minting
    function safeMint(
        uint256 mintAmount
    ) public onlyOwner {
        _mintLoop(msg.sender, mintAmount);
    }

    function randomMintNFTToRecipient(
        uint256 mintAmount,
        address recipient
    ) 
        public 
        payable 
    {
        if (
            mintedPerWallet[recipient] + mintAmount > MAX_MINT_PER_TX &&
            recipient != owner()
        ) revert ExceedsMaxSupplyError();
        if (mintAmount < 1) revert IncorrectAmountError();
        if (msg.value != (initialPrice * mintAmount)) revert IncorrectAmountError();

        _mintLoop(recipient, mintAmount);
        mintedPerWallet[recipient] += mintAmount;
    }

    function randomMint(
        uint256 mintAmount
    ) 
        public 
        payable 
        verifyWalletAvailableSupply(mintAmount) 
    {
        if (mintAmount < 1) revert IncorrectAmountError();
        if (msg.value != (initialPrice * mintAmount)) revert IncorrectAmountError();

        _mintLoop(msg.sender, mintAmount);
        mintedPerWallet[msg.sender] += mintAmount;
    }

    function randomMintWithCode(
        uint256 mintAmount,
        address _referralCodeAddress
    ) 
        public 
        payable
        verifyWalletAvailableSupply(mintAmount) 
    {
        if (mintAmount < 1) revert IncorrectAmountError();

        uint256 codeIndex = getReferralCodeIndex(_referralCodeAddress);

        if (codeIndex == 256) revert IncorrectReferralCodeError();
        
        _referralsUsers[msg.sender] = ReferralCode (
            referralCode[codeIndex].user,
            referralCode[codeIndex].code
        );

        _mintLoop(msg.sender, mintAmount);
        mintedPerWallet[msg.sender] += mintAmount;
        _usersBalance[referralCode[codeIndex].user] = _usersBalance[referralCode[codeIndex].user] + ((msg.value * REFERRAL_FEE_PERCENTAGE) / 100);
    }

    // OperatorFilterer
    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, uint256 amount, bytes memory data)
        public
        override
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, amount, data);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual override onlyAllowedOperator(from) {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    // IERC2981
    function setRoyaltyInfo(address payable receiver, uint96 numerator) public onlyOwner {
        _setDefaultRoyalty(receiver, numerator);
    }

    //White List
    function addInWhiteList(
        address user
    ) public onlyOwner {
        uint256 whiteListId = _whiteListCounter.current();
        _whiteListCounter.increment();

        whiteList[whiteListId] = WhiteList(user, true);
    }

    function getUserInWhiteList(address user) 
        public 
        view
        returns (WhiteList memory) 
    {
        WhiteList memory item;
        for (uint i = 0; i < _whiteListCounter.current(); i++) {
            WhiteList storage currentItem = whiteList[i];

            if (currentItem.user == user) {
                item = currentItem;
                break;
            }
        }
        return item;
    }

    function deleteUserInWhiteList(
        address user
    ) public onlyOwner {
        for (uint i = 0; i < _whiteListCounter.current(); i++) {
            if((whiteList[i].user == user)) {
                delete whiteList[i];
                break;
            }
        }
    }

    //Referral Code
    function addReferralCode(
        address user,
        string memory code
    ) public onlyOwner {
        uint256 referralCodeId = _referralCodeIds.current();
        _referralCodeIds.increment();

        referralCode[referralCodeId] =  ReferralCode(
            user,
            code
        );
    }

    function deleteReferralCode(
        address user,
        string memory code
    ) public onlyOwner {
        for (uint i = 0; i < _referralCodeIds.current(); i++) {
            if((referralCode[i].user == user) && (keccak256(bytes(referralCode[i].code)) == keccak256(bytes(code)))) {
                delete referralCode[i];
            }
        }
    }

    function getReferralCode() 
        public 
        view
        returns (ReferralCode[] memory) 
    {
        ReferralCode[] memory ret = new ReferralCode[](_referralCodeIds.current());
        for (uint i = 0; i < _referralCodeIds.current(); i++) {
            ReferralCode storage currentItem = referralCode[i];
            ret[i] = currentItem;
        }
        return ret;
    }

    function getReferralCodeIndex(address ref) 
        public 
        view
        returns (uint256) 
    {
        uint256 indexRef = 256;
        for (uint i = 0; i < _referralCodeIds.current(); i++) {
            if (referralCode[i].user == ref) {
                indexRef = i;
                break;
            }
        }
        return indexRef;
    }

    function uint2str(uint _i) internal pure returns (string memory _uintAsString) {
        if (_i == 0) {
            return "0";
        }
        uint j = _i;
        uint len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint k = len;
        while (_i != 0) {
            k = k-1;
            uint8 temp = (48 + uint8(_i - _i / 10 * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function uri(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {

        string memory customTokenUri = _tokenURIs[_tokenId];
        string memory currentBaseURI = uriPrefix;

        return
            bytes(customTokenUri).length > 0
                ? customTokenUri
                : string(
                    abi.encodePacked(
                        currentBaseURI,
                        uint2str(_tokenId),
                        uriSuffix
                    )
                );
    }

    function getRemaining() public view returns(uint256) {
        return maxTokens - _tokenIdCounter.current();
    }

    function setTokenURI(
        uint256 tokenId, 
        string memory newuri
    ) public onlyOwner {
        _tokenURIs[tokenId] = newuri;
    }

    function setMaxTokens(uint256 _maxTokens) public onlyOwner {
        if (_maxTokens < _tokenIdCounter.current()) revert MaxTokensError();

        maxTokens = _maxTokens;

        for(uint i = _randomIds.length; i <= maxTokens; i++) {
            _randomIds.push(i);
        }
    }

    function setDeadline(uint256 _deadline) public onlyOwner {
        deadline = _deadline;
    }

    function setActiveSale(bool _activeSale) public onlyOwner {
        activeSale = _activeSale;
    }

    function setActiveDeadline(bool _activeDeadline) public onlyOwner {
        activeDeadline = _activeDeadline;
    }

    function setUriPrefix(string memory _uriPrefix) public onlyOwner {
        uriPrefix = _uriPrefix;
    }

    function withdraw() public nonReentrant {
        uint256 userBalance = _usersBalance[msg.sender];
        if (userBalance <= 0) revert InsufficientBalance();
        uint256 balance = address(this).balance;
        if (balance < userBalance) revert InsufficientBalance();
        
        payable(msg.sender).transfer(userBalance);
    }

    function _mintLoop(
        address _receiver, 
        uint256 _mintAmount
    ) internal {
        if (maxTokens < (_tokenIdCounter.current() + _mintAmount)) revert ExceedsMaxSupplyError();

        if (activeDeadline) {
            if (deadline < block.timestamp) revert DeadlineError();
        }

        if(!activeSale) {
            if (getUserInWhiteList(msg.sender).active != true) revert NotActiveSale();
        }

        uint256[] memory tokenIds = new uint[](_mintAmount);
        for (uint256 i = 0; i < _mintAmount; i++) {
            _tokenIdCounter.increment();

            uint256 randomNumber = createRandom(_randomIds.length);
            uint256 randomIndex = randomNumber % _randomIds.length;
            uint256 resultNumber = _randomIds[randomIndex];
            _randomIds[randomIndex] = _randomIds[_randomIds.length - 1];
            _randomIds.pop();

            _mint(_receiver, resultNumber, 1, "");
            tokenIds[i] = resultNumber;
        }

        emit Mint(tokenIds, _receiver);
    }

    function createRandom(uint256 number) internal view returns(uint256){
        return uint256(blockhash(block.number-1)) % number;
    }
}

