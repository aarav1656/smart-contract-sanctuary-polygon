    function dailyFlexibledInterest(uint256 _val) public pure returns(uint256 interest) {
        uint256  interestDaily = _val.div(365);
        return interest = interestDaily;
    }
    
    // 80 %
    function eightyPercent(uint256 _val) public pure returns (uint256){
       uint256 _eightyPercent = 8000; // 80 %
       uint256 result = _val.mul(_eightyPercent).div(10000); // 80% of cumulated interest (1467)
       return result;
    }

    // 85 %
    function FlexibleDynamicPercentage(uint256 _val,  uint256 _apr) public pure returns (uint256){
       uint256 result = _val.div(100).mul(_apr); // 85% of cumulated interest 5950
       return result;
    }
    
