    function dailyFixedInterest (uint256 _val, uint256 _apr) public view returns(uint256) {
        uint256 interestDaily = _val.div(100).mul(_apr).div(365); // 5950/365 => 85%
        console.log("apr: ", interestDaily);
        return interestDaily;
