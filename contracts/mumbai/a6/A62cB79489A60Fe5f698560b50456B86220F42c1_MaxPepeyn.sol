// SPDX-License-Identifier: MIT
pragma solidity ^0.8.15;

import 'erc721a-upgradeable/contracts/ERC721AUpgradeable.sol';
import 'erc721a-upgradeable/contracts/extensions/ERC721AQueryableUpgradeable.sol';
import 'erc721a-upgradeable/contracts/extensions/ERC721ABurnableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import "@openzeppelin/contracts-upgradeable/utils/Base64Upgradeable.sol";
import '@openzeppelin/contracts-upgradeable/utils/cryptography/MerkleProofUpgradeable.sol';
import "../YRewardsV2.sol";
import "./interface/IMaxPepeyn.sol";
import {DefaultOperatorFiltererUpgradeable} from 'operator-filter-registry/src/upgradeable/DefaultOperatorFiltererUpgradeable.sol';

contract MaxPepeyn is 
  IMaxPepeyn,
  ERC721AUpgradeable,
  ERC721AQueryableUpgradeable,
  ERC721ABurnableUpgradeable,
  OwnableUpgradeable,
  DefaultOperatorFiltererUpgradeable 
{
  Nft public mp;

  mapping(Phases => Phase) public phaseData;
  mapping(uint256 => string) public metadata;
  mapping(address => User) public userData;
  mapping(address => bool) public hasMintedBurnerz;

  /**
   * Modier
   */

  modifier validMint(bytes32[] memory _proof, uint256 _points) {
    YRewardsV2 yrewards = YRewardsV2(mp.rewards);
    (uint256 reward, uint256 burnt) = yrewards.userData(msg.sender);

    require(mp.currPhase != Phases.ALLOWLIST, "MaxPepeyn :: Other phase is on going");
    require((totalSupply() + 1) <= mp.maxSupply, "MaxPepeyn :: Beyond max supply");
    require(_points <= reward, "MaxPepeyn :: Not enough points!");

    if(phaseData[mp.currPhase].maxSupply > 0) {
      require(userData[msg.sender].mintCount[mp.currPhase] >= phaseData[mp.currPhase].maxSupply, "MaxPepeyn :: Beyond phase supply");
    }

    if(phaseData[mp.currPhase].price > 0) {
      require(msg.value >= phaseData[mp.currPhase].price, "MaxPepeyn :: Not enough payment");
    }

    if(mp.currPhase == Phases.BURNERZ || mp.currPhase == Phases.HOLDERS) {
      bytes32 sender = keccak256(abi.encodePacked(msg.sender));
      require(MerkleProofUpgradeable.verify(_proof, phaseData[mp.currPhase].merkleRoot, sender));
    }
    _;
  }

  modifier validBurnerz(bytes32[] memory _proof, uint256 _qty) {
    YRewardsV2 yrewards = YRewardsV2(mp.rewards);
    (uint256 reward, uint256 burnt) = yrewards.userData(msg.sender);

    require(mp.currPhase == Phases.BURNERZ, "MaxPepeyn :: Other phase is on going");
    require(!hasMintedBurnerz[msg.sender], "MaxPepeyn :: Already minted");
    require((totalSupply() + _qty) <= mp.maxSupply, "MaxPepeyn :: Beyond max supply");
    require(userData[msg.sender].mintCount[mp.currPhase] >= phaseData[mp.currPhase].maxSupply, "MaxPepeyn :: Beyond phase supply");
    _;
  }

  modifier enoughPoints(uint256 _points) {
    YRewardsV2 yrewards = YRewardsV2(mp.rewards);
    (uint256 reward, uint256 burnt) = yrewards.userData(msg.sender);

    require(_points <= reward, "MaxPepeyn :: Not enough point balance");
    _;
    yrewards.usePoints(msg.sender, _points);
  }

  /**
   * Initialize
   */

  function initialize() external initializerERC721A initializer {
    __ERC721A_init('MaxPepeyn', 'MAXPEPEYN');
    __ERC721AQueryable_init();
    __ERC721ABurnable_init();
    __Ownable_init();
    __DefaultOperatorFilterer_init();
  }

  /**
   * Main
   */

  function mint(string memory _metadata, bytes32[] memory _proof, uint256 _points) external payable validMint(_proof, _points){
    YRewardsV2 yrewards = YRewardsV2(mp.rewards);
    uint256 currTokenId = totalSupply();

    _mint(msg.sender, 1);
    setTokenUri(currTokenId, _metadata);

    userData[msg.sender].mintCount[mp.currPhase]++;
    yrewards.usePoints(msg.sender, _points);
  }

  function burnerzMint(bytes32[] memory _proof, uint256 _qty) external validBurnerz(_proof, _qty){
    _mint(msg.sender, _qty);
    userData[msg.sender].mintCount[mp.currPhase] += _qty;
  }

  function setTokenUri(uint256 _tokenID, string memory _metadata) internal {
    require(ownerOf(_tokenID) == msg.sender, 'MaxPepeyn :: You are not the owner!');
    metadata[_tokenID] = _metadata;
  }

  function setTokenUriWithPoints(uint256 _tokenID, string memory _metadata, uint256 _points) external enoughPoints(_points){
    setTokenUri(_tokenID, _metadata);
  }

  /**
   * Admin 
   */

  function setRewards(address _rewards) external onlyOwner {
    mp.rewards = _rewards;
  }

  function setTreasury(address _treasury) external onlyOwner {
    mp.treasury = _treasury;
  }

  function setCurrPhase(Phases _phase) external onlyOwner {
    mp.currPhase = _phase;
  }

  function setMaxSupply(uint256 _maxSupply) external onlyOwner {
    mp.maxSupply = _maxSupply;
  }

  function setPhaseData(Phases _phase, uint256 _price, uint256 _maxSupply, uint256 _maxWallet) external onlyOwner {
    phaseData[_phase].price = _price;
    phaseData[_phase].maxSupply = _maxSupply;
    phaseData[_phase].maxWallet = _maxWallet;
  }

  function setMerkleRoot(Phases _phase, bytes32 _merkle) external onlyOwner {
    phaseData[_phase].merkleRoot = _merkle;
  }

  /**
   * View 
   */

  function tokenURI(
    uint256 _tokenID
  )
    public
    view
    virtual
    override(ERC721AUpgradeable, IERC721AUpgradeable)
    returns (string memory)
  {
    // uint256 trueId = _tokenID + 1;
    return
      bytes(metadata[_tokenID]).length > 0
        ? string(
          abi.encodePacked("data:application/json;base64,", metadata[_tokenID])
        )
        : '';
  }

  function batchTokenURI(
    uint256[] memory _tokenIDs
  ) external view returns (string[] memory) {
    uint256 length = _tokenIDs.length;
    string[] memory uris = new string[](length);

    for (uint256 index = 0; index < length; index++) {
      uris[index] = tokenURI(_tokenIDs[index]);
    }

    return uris;
  }

  function getMintCount(Phases _phase) external view returns(uint256) {
    return userData[msg.sender].mintCount[_phase];
  }

  /**
   *  Opensea Operator Filter Registry Impl
   */
  function setApprovalForAll(
    address operator,
    bool approved
  )
    public
    override(ERC721AUpgradeable, IERC721AUpgradeable)
    onlyAllowedOperatorApproval(operator)
  {
    super.setApprovalForAll(operator, approved);
  }

  function approve(
    address operator,
    uint256 tokenId
  )
    public
    payable
    override(ERC721AUpgradeable, IERC721AUpgradeable)
    onlyAllowedOperatorApproval(operator)
  {
    super.approve(operator, tokenId);
  }

  function transferFrom(
    address from,
    address to,
    uint256 tokenId
  )
    public
    payable
    override(ERC721AUpgradeable, IERC721AUpgradeable)
    onlyAllowedOperator(from)
  {
    super.transferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId
  )
    public
    payable
    override(ERC721AUpgradeable, IERC721AUpgradeable)
    onlyAllowedOperator(from)
  {
    super.safeTransferFrom(from, to, tokenId);
  }

  function safeTransferFrom(
    address from,
    address to,
    uint256 tokenId,
    bytes memory data
  )
    public
    payable
    override(ERC721AUpgradeable, IERC721AUpgradeable)
    onlyAllowedOperator(from)
  {
    super.safeTransferFrom(from, to, tokenId, data);
  }
}

