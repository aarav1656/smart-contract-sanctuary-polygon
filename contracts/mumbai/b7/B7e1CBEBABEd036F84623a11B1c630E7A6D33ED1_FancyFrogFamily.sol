contract FancyFrogFamily is
    ERC721Enumerable,
    ERC2981,
    Ownable,
    ReentrancyGuard,
    RoyaltiesV2Impl
{
    using Counters for Counters.Counter;

    Counters.Counter private tokenIdTracker;
    address public proxyRegistryAddress;
     
    /*
Rinkeby: 0xf57b2c51ded3a29e6891aba85459d600256cf317
Mainnet: 0xa5409ec958c83c3f309868babaca7c86dcb077c1
Polygon: 0x207Fa8Df3a17D96Ca7EA4f2893fcdCb78a304101
*/
    string public baseURI;
    uint256 private constant MAX_FFF = 8889;
    uint256 private constant MAX_PURCHASE = 31;
    uint256 private constant MAX_NUM_OF_PRESALE_FFF = 4001;
    uint256 private constant MINT_PRICE = 50000000000000000; /*0.05eth*/
    uint256 private TEAM_RESERVE = 889;
    bool private paused = true;
    bool private presaleIsActive = false;
    //breed cooldown
    //breed max time

    struct Frog {
        uint256 lastBreed;
        uint256 birthday;
        uint256 DNA;
    }

    WhitelistContract public myWL;

    modifier onlyFFFD {
        _onlyFFFD();
        _;
    }

    FancyFrogDescendant public FFFD;

    mapping(address => bool) public projectProxy;
    mapping(uint256 => Frog) public WorldFrog;

    constructor()
        ERC721("FancyFrogFamily", "FFF")
    {
        _setDefaultRoyalty(msg.sender, 750);
        myWL = new WhitelistContract();

    }

    function _onlyFFFD() private view{
        require(msg.sender == address(FFFD), "403");
    }
    function SetlastBreed(uint256 id,uint256 _lastBreed) external onlyFFFD {
        WorldFrog[id+1].lastBreed = _lastBreed;
    }

    function setFFFD(FancyFrogDescendant _FFFD) public onlyOwner {
        FFFD = _FFFD;
    }

    function setBaseURI(string memory _baseURI) public onlyOwner {
        baseURI = _baseURI;
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(_tokenId), "not exist");
        return string(abi.encodePacked(baseURI, Strings.toString(_tokenId)));
    }

    function getFrog(uint256 _tokenId) public view returns (uint256,uint256,uint256) {
        return(WorldFrog[_tokenId+1].lastBreed,WorldFrog[_tokenId+1].birthday,WorldFrog[_tokenId+1].DNA);
    }

    function setProxyRegistryAddress(address _proxyRegistryAddress)
        external
        onlyOwner
    {
        proxyRegistryAddress = _proxyRegistryAddress;
    }

    function flipProxyState(address proxyAddress) public onlyOwner {
        projectProxy[proxyAddress] = !projectProxy[proxyAddress];
    }

    function mint(uint256 amount) public payable nonReentrant() {
        require(!paused || presaleIsActive);
        require(amount > 0 && amount < MAX_PURCHASE,"30 Max");
        require(msg.value == MINT_PRICE * amount,"value");
        require(tokenIdTracker.current() + amount < MAX_FFF, "8888!");
        for (uint256 i; i < amount; i++) {
            tokenIdTracker.increment();
            mintFFF(tokenIdTracker.current());
        }
    }    

    function mintFFF(uint256 id) private {
        _mint(msg.sender, id);
        uint256 randomDNA = uint256(
            keccak256(
                abi.encodePacked(
                    block.prevrandao,
                    block.timestamp,
                    tokenIdTracker.current()
                )
            )
        );
        WorldFrog[id].DNA = randomDNA;
        WorldFrog[id].birthday = block.timestamp;
        WorldFrog[id].lastBreed = block.timestamp;
        setRoyalties(tokenIdTracker.current(),payable(owner()),750);
        //emit createFFF(_tokenIdTracker.current(), randomDNA);
    }

    function presaleMintFFF(uint256 numOfFrogs) public payable nonReentrant(){
        require(presaleIsActive, "Presale is not active");
        require(myWL.addressIsPresaleApproved(msg.sender),
            "not wl"
        );
        uint256 total = tokenIdTracker.current();
        require(
            total + numOfFrogs < MAX_NUM_OF_PRESALE_FFF,
            "max presale"
        );
        require(
            myWL.getReservedPresaleQuantity(msg.sender)+ 1 > numOfFrogs,
            "qty"
        );
        require(
            MINT_PRICE * numOfFrogs == msg.value,
            "not correct"
        );
        myWL.refreshQuantity(msg.sender,numOfFrogs);
        for (uint256 i; i < numOfFrogs; i++) {
            tokenIdTracker.increment();
            mintFFF(tokenIdTracker.current());
        }
        
    }

    function reserveFFF(uint256 amount) public onlyOwner {
        require(amount < TEAM_RESERVE, "amount");
        uint256 total = tokenIdTracker.current();
        require(total + amount < MAX_FFF, "tags");
        TEAM_RESERVE = TEAM_RESERVE - amount;
        for (uint256 i; i < amount; i++) {
            tokenIdTracker.increment();
            mintFFF(tokenIdTracker.current());
        }
    }

    function getFFF_DNA(uint256 id) public view returns (uint256) {
        require(id < tokenIdTracker.current()+1, "mint");
        return WorldFrog[id].DNA;
    }

    function flipPause() public virtual onlyOwner {
        paused = !paused;
    }

    function addToWhitelist(address  _adr,uint256 Qty) public virtual onlyOwner {
        myWL.addToWhitelist(_adr, Qty);
    }

    function refreshQuantity(address _address, uint256 usedQty) public virtual onlyOwner {
        myWL.refreshQuantity(_address, usedQty);
    }

      function getReservedPresaleQuantity(address _address)
        public
        view
        returns (uint256)
    {
        return myWL.getReservedPresaleQuantity(_address);
    }

    function flipPresaleState() public onlyOwner {
        presaleIsActive = !presaleIsActive;
    }

    function totalSupply() public view override returns (uint256) {
        return tokenIdTracker.current();
    }

    function withdraw() external onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function walletOfOwner(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 tokenCount = balanceOf(_owner);
        if (tokenCount == 0) return new uint256[](0);

        uint256[] memory tokensId = new uint256[](tokenCount);
        for (uint256 i; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensId;
    }

    function batchSafeTransferFrom(
        address _from,
        address _to,
        uint256[] memory _tokenIds,
        bytes memory data_
    ) public {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            safeTransferFrom(_from, _to, _tokenIds[i], data_);
        }
    }

    function isOwnerOf(address account, uint256[] calldata _tokenIds)
        external
        view
        returns (bool)
    {
        for (uint256 i; i < _tokenIds.length; ++i) {
            if (_owners[_tokenIds[i]] != account) return false;
        }

        return true;
    }

    function isOwnerOfSimple(address account, uint256 _tokenIds)
        public
        view
        returns (bool)
    {
        if (_owners[_tokenIds] != account) return false;
        

        return true;
    }    

    function isApprovedForAll(address _owner, address operator)
        public
        view
        override(ERC721, IERC721)
        returns (bool)
    {
        OpenSeaProxyRegistry proxyRegistry = OpenSeaProxyRegistry(
            proxyRegistryAddress
        );
        if (
            address(proxyRegistry.proxies(_owner)) == operator ||
            projectProxy[operator]
        ) return true;
        return super.isApprovedForAll(_owner, operator);
    }

    function _mint(address to, uint256 tokenId) internal virtual override {
        _owners.push(to);
        emit Transfer(address(0), to, tokenId);
    }

    function setRoyalties(
        uint256 _tokenId,
        address payable _royaltiesReceipientAddress,
        uint96 _percentageBasisPoints
    ) internal {
        LibPart.Part[] memory _royalties = new LibPart.Part[](1);
        _royalties[0].value = _percentageBasisPoints;
        _royalties[0].account = _royaltiesReceipientAddress;
        _saveRoyalties(_tokenId, _royalties);
    }
    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721Enumerable, ERC165, IERC165)
        returns (bool)
    {
        return
            interfaceId == type(IERC721Enumerable).interfaceId ||
            interfaceId == type(IERC2981).interfaceId ||
            interfaceId == LibRoyaltiesV2._INTERFACE_ID_ROYALTIES ||
            super.supportsInterface(interfaceId);
    }

}
 
