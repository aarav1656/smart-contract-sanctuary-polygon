pragma solidity ^0.8.13;

import "v2-periphery/interfaces/IUniswapV2Router01.sol";
import "v2-core/interfaces/IUniswapV2Factory.sol";
import "v2-core/interfaces/IUniswapV2Pair.sol";
import "solmate/tokens/ERC20.sol";
import "solmate/utils/SafeTransferLib.sol";

contract TPSwapManager {
    using SafeTransferLib for ERC20;

    IUniswapV2Router01 private _router;
    address private _treasury;
    address private _stable;
    uint256 private _feePercentage;

    constructor(address router, address treasury, address stable) {
        _router = IUniswapV2Router01(router);
        _treasury = treasury;
        _stable = stable;
        _feePercentage = 100;
    }

    function getAmountIn(uint256 amountOut, address[] calldata path)
        external
        view
        returns (uint256 amountIn, uint256 fee)
    {
        IUniswapV2Pair pair = IUniswapV2Pair(IUniswapV2Factory(_router.factory()).getPair(path[0], path[1]));
        (uint112 reserve0, uint112 reserve1,) = pair.getReserves();
        uint256 reserveIn = pair.token0() == path[0] ? reserve0 : reserve1;
        uint256 reserveOut = reserveIn == reserve0 ? reserve1 : reserve0;
        fee = getFeeTokensForExactTokens(amountOut, path);

        if (path[0] == _stable) {
            //amountIn is stable
            amountIn = _router.getAmountIn(amountOut, reserveIn, reserveOut);
            amountIn += fee;
        } else {
            //amountOut is stable
            amountIn = _router.getAmountIn(amountOut + fee, reserveIn, reserveOut);
        }
    }

    function getAmountOut(uint256 amountIn, address[] calldata path)
        external
        view
        returns (uint256 amountOut, uint256 fee)
    {
        IUniswapV2Pair pair = IUniswapV2Pair(IUniswapV2Factory(_router.factory()).getPair(path[0], path[1]));
        (uint112 reserve0, uint112 reserve1,) = pair.getReserves();
        uint256 reserveIn = pair.token0() == path[0] ? reserve0 : reserve1;
        uint256 reserveOut = reserveIn == reserve0 ? reserve1 : reserve0;
        fee = getFeeExactTokensForTokens(amountIn, path);

        if (path[0] == _stable) {
            //amountIn is stable
            amountOut = _router.getAmountOut(amountIn - fee, reserveIn, reserveOut);
        } else {
            //amountOut is stable
            amountOut = _router.getAmountOut(amountIn, reserveIn, reserveOut);
            amountOut -= fee;
        }
    }

    function getFeeExactTokensForTokens(uint256 amountIn, address[] calldata path) public view returns (uint256 fee) {
        if (path[0] == _stable) {
            fee = _calculateFeeExtra(amountIn);
        } else if (path[path.length - 1] == _stable) {
            uint256[] memory amounts = _router.getAmountsOut(amountIn, path);
            fee = _calculateFeeIncluded(amounts[amounts.length - 1]);
        }
    }

    function getFeeTokensForExactTokens(uint256 amountOut, address[] calldata path) public view returns (uint256 fee) {
        if (path[0] == _stable) {
            uint256[] memory swapAmounts = _router.getAmountsIn(amountOut, path);
            fee = _calculateFeeExtra(swapAmounts[0]);
        } else if (path[path.length - 1] == _stable) {
            fee = _calculateFeeExtra(amountOut);
        }
    }

    function swapExactTokensForTokens(uint256 amountIn, uint256 amountOutMin, address[] calldata path, uint256 deadline)
        external
        returns (uint256[] memory amounts)
    {
        require(path[0] == _stable || path[path.length - 1] == _stable, "");
        _preSwap(path[0], msg.sender, amountIn); //TBD: use _msgSender?

        uint256 fee;

        if (path[0] == _stable) {
            fee = _calculateFeeExtra(amountIn);
            // amountIn is exact-swap-amount so fee goes to treasury directly
            ERC20(path[0]).safeTransfer(_treasury, fee);
        }

        // TODO: to could be msg.sender instead of address(this) when stable == path0
        amounts = _swapExactTokensForTokens((amountIn - fee), amountOutMin, path, address(this), deadline);

        if (path[path.length - 1] == _stable) {
            fee = _calculateFeeIncluded(amounts[amounts.length - 1]);
            // amounts[amounts.length - 1] is exact-swap-amount so fee goes to treasury directly
            ERC20(path[path.length - 1]).safeTransfer(_treasury, fee);
        }

        _postSwap(path, msg.sender);
    }

    function swapTokensForExactTokens(uint256 amountOut, uint256 amountInMax, address[] calldata path, uint256 deadline)
        external
        returns (uint256[] memory amounts)
    {
        require(path[0] == _stable || path[path.length - 1] == _stable, "");
        _preSwap(path[0], msg.sender, amountInMax); //TBD: use _msgSender?
        uint256 fee;
        if (path[0] == _stable) {
            uint256[] memory swapAmounts = _router.getAmountsIn(amountOut, path);
            fee = _calculateFeeExtra(swapAmounts[0]);
            // swapAmounts[0] is exact-swap-amount so fee goes to treasury directly
            ERC20(path[0]).safeTransfer(_treasury, fee);
        }

        if (path[path.length - 1] == _stable) {
            fee = _calculateFeeExtra(amountOut);
            amountOut += fee;
        }
        amounts = _swapTokensForExactTokens(amountOut, amountInMax, path, address(this), deadline);

        if (path[path.length - 1] == _stable) {
            // stableAmount is exact-swap-amount so fee goes to treasury directly
            ERC20(path[path.length - 1]).safeTransfer(_treasury, fee);
        }

        _postSwap(path, msg.sender);
    }

    function _preSwap(address tokenIn, address account, uint256 amountIn) internal {
        _chargeToken(tokenIn, account, amountIn);
        _approveToken(tokenIn, address(_router), amountIn);
    }

    function _postSwap(address[] calldata path, address to) internal {
        _emptyTokens(path, to);
        _approveToken(path[0], address(_router), 0);
    }

    // **** SWAP ****
    // TBD: check we dont want ETH-swaps
    function _swapExactTokensForTokens(
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address to,
        uint256 deadline
    ) internal returns (uint256[] memory amounts) {
        return _router.swapExactTokensForTokens(amountIn, amountOutMin, path, to, deadline);
    }

    function _swapTokensForExactTokens(
        uint256 amountOut,
        uint256 amountInMax,
        address[] calldata path,
        address to,
        uint256 deadline
    ) internal returns (uint256[] memory amounts) {
        return _router.swapTokensForExactTokens(amountOut, amountInMax, path, to, deadline);
    }

    function _emptyTokens(address[] calldata path, address to) internal {
        for (uint256 i; i < path.length; i++) {
            ERC20 token = ERC20(path[i]);
            uint256 balance = token.balanceOf(address(this));
            if (balance > 0) ERC20(path[i]).safeTransfer(to, balance);
        }
    }

    function _approveToken(address token, address spender, uint256 amount) internal {
        ERC20(token).safeApprove(spender, amount);
    }

    function _chargeToken(address token, address from, uint256 amount) internal {
        ERC20(token).safeTransferFrom(from, address(this), amount);
    }

    function _calculateFeeExtra(uint256 amount) internal view returns (uint256 fee) {
        fee = (amount * _feePercentage) / 10000;
    }

    function _calculateFeeIncluded(uint256 amount) internal view returns (uint256 fee) {
        fee = (_feePercentage * amount) / (10000 + _feePercentage);
    }
}

