pragma solidity ^0.8.4;

import "@semaphore-protocol/contracts/interfaces/ISemaphore.sol";
import {ByteHasher} from "./ByteHasher.sol";
import "hardhat/console.sol";

interface IVerifier {
    function verifyProof(
        uint[2] memory a,
        uint[2][2] memory b,
        uint[2] memory c,
        uint[2] memory input
    ) external view returns (bool r);
}

contract Forum {
    using ByteHasher for bytes;
    
    uint256 public adminCount;
    uint256 public moderatorCount;
    address[] public admins;
    address[] public moderators;
    mapping(address => address[]) removeAdminApprovals;

    enum ItemKind {
        POST,
        COMMENT
    }

    enum VoteKind {
        UP,
        DOWN
    }

    struct Requirement {
        address tokenAddress;
        uint256 minAmount;
    }

    struct Group {
        string name;
        Requirement[] requirements;
        uint256 id;
        uint256 creatorIdentityCommitment;
        uint256 userCount;
        uint256 chainId;
        uint256[] posts;
        bool removed;
        mapping(uint256 => bytes32) users;
    }

    struct Item {
        /// @notice what kind of item (post or comment)
        ItemKind kind;

        /// @notice Unique item id, assigned at creation time.
        uint256 id;

        /// @notice Id of parent item. Posts have parentId == 0.
        uint256 parentId;

        uint256 groupId;
        /// @notice block number when item was submitted
        uint256 createdAtBlock;

        uint256[] childIds;
        uint256 upvote;
        uint256 downvote;
        uint256 note;
        bytes32 contentCID;
        bool removed;
    }

    IVerifier public verifier;
    ISemaphore public semaphore;

    /// @dev counter for issuing group ids
    uint256 public groupCount;

    /// @dev counter for issuing item ids
    uint256 public itemCount;
    
    /// @dev maps group id to Group
    mapping(uint256 => Group) public groups;

    /// @dev maps item id to Group
    mapping(uint256 => Item) public items;

    event NewUser(uint256 groupId, uint256 identityCommitment, bytes32 username);
    event NewGroupCreated(uint256 groupId, string name, uint256 creatorIdentityCommitment);
    event NewItem(ItemKind itemType, uint256 groupId, uint256 id, uint256 parentId, bytes32 contentCID, uint256 note);
    event UpdateItem(ItemKind itemType, uint256 itemId, bytes32 newCID, uint256 newNote);
    event VoteItem(VoteKind voteType, ItemKind itemType, uint256 itemId, uint256 upvote, uint256 downvote);

    modifier onlyAdmin() {
        bool _isAdmin = false;
        for (uint256 i = 0; i < adminCount; ++i) {
            if(admins[i] == msg.sender) {
                _isAdmin = true;
            }
        }
        require(_isAdmin == true, "caller is not admin");
        _;
    }

    modifier onlyModerator() {
        bool _isModerator = false;
        for (uint256 i = 0; i < adminCount; ++i) {
            if(admins[i] == msg.sender) {
                _isModerator = true;
                break;
            }
        }

        if(!_isModerator) {
            for (uint256 i = 0; i < moderatorCount; ++i) {
                if(moderators[i] == msg.sender) {
                    _isModerator = true;
                    break;
                }
            }
        }
        require(_isModerator == true, "caller is not moderator");
        _;
    }

    constructor(address semaphoreAddress, address _verifier, address[] memory _admins, address[] memory _moderators) {
        semaphore = ISemaphore(semaphoreAddress);
        verifier = IVerifier(_verifier);
        for(uint256 i = 0; i < _admins.length; i++) {
            admins.push(_admins[i]);
        }

        for(uint256 i = 0; i < _moderators.length; i++) {
            moderators.push(_moderators[i]);
        }
        adminCount = _admins.length;
        moderatorCount = _moderators.length;
    }

    function isAdmin(address item) public view returns(bool) {
        for (uint256 i = 0; i < adminCount; ++i) {
            if(admins[i] == item) {
                return true;
            }
        }
        return false;
    }

    function isModerator(address item) public view returns(bool) {
        for (uint256 i = 0; i < adminCount; ++i) {
            if(admins[i] == item) {
                return true;
            }
        }

        for (uint256 i = 0; i < moderatorCount; ++i) {
            if(moderators[i] == item) {
                return true;
            }
        }
        return false;
    }

    function getAdmins() public view returns(address[] memory) {
        return admins;
    }

    function getModerators() public view returns(address[] memory) {
        return moderators;
    }

    function addAdmins(address[] memory _admins) public onlyAdmin {
        for(uint256 i = 0; i < _admins.length; i++) {
            admins.push(_admins[i]);
        }
        adminCount += _admins.length;
    }

    function addModerators(address[] memory _moderators) public onlyAdmin {
        for(uint256 i = 0; i < _moderators.length; i++) {
            moderators.push(_moderators[i]);
        }
        moderatorCount += _moderators.length;
    }

    function removeArrayItem(address item, address[] storage array) internal {
        for (uint256 i = 0; i < array.length; ++i) {
            if(array[i] == item) {
                for (uint256 j = i; j < array.length - 1; j++) {
                    array[j] = array[j + 1];
                }
                array.pop();
                break;
            }
        }
    }

    function revokeMyAdminRole() public onlyAdmin {
        removeArrayItem(msg.sender, admins);
        adminCount = admins.length;
        if(removeAdminApprovals[msg.sender].length > 0) {
            delete removeAdminApprovals[msg.sender];
        }
    }

    function removeAdmin(address _admin) public onlyAdmin {
        bool _isAdmin = false;
        for (uint256 i = 0; i < adminCount; ++i) {
            if(admins[i] == _admin) {
                _isAdmin = true;
                break;
            }
        }
        require(_isAdmin == true, "It's not an admin");
        for(uint256 i = 0; i < removeAdminApprovals[_admin].length; i++) {
            require(removeAdminApprovals[_admin][i] != msg.sender, "remove request double submitted");
        }
        removeAdminApprovals[_admin].push(msg.sender);
        if(removeAdminApprovals[_admin].length >= adminCount / 2) {
            removeArrayItem(_admin, admins);
            delete removeAdminApprovals[_admin];
            adminCount = admins.length;
        }
    }

    function removeModerators(address[] memory _moderators) public onlyAdmin {
        for(uint256 i = 0; i < _moderators.length; i++) {
            removeArrayItem(_moderators[i], moderators);
        }
        moderatorCount = _moderators.length;
    }

    function removeGroup(uint256 groupId) public onlyAdmin {
        require(groupId >= 0 && groupId < groupCount, "Invalid groupId");
        groups[groupId].removed = true;
        uint256[] memory posts;
        groups[groupId].posts = posts;
    }

    function removeItem(uint256 itemId) public onlyModerator {
        require(itemId >= 0 && itemId < itemCount, "Invalid itemId");
        items[itemId].removed = true;
        uint256[] memory childIds;
        items[itemId].childIds = childIds;
        items[itemId].contentCID = "";
    }

    function createGroup(string calldata groupName, uint256 chainId, Requirement[] calldata requirements, uint256 identityCommitment) external {
        semaphore.createGroup(groupCount, 20, 0, address(this));
        uint256[] memory posts;
        Group storage group = groups[groupCount];
        group.id = groupCount;
        group.name = groupName;
        group.chainId = chainId;
        group.creatorIdentityCommitment = identityCommitment;
        group.posts = posts;
        group.removed = false;
        unchecked {
            for(uint256 i = 0; i < requirements.length; i++) {
                group.requirements.push(requirements[i]);
            }
            groupCount += 1;
        }
        emit NewGroupCreated(group.id, groupName, identityCommitment);
    }

    function joinGroup(uint256 groupId, uint256 identityCommitment, bytes32 username) external {
        require(groupId >= 0 && groupId < groupCount, "Invalid groupId");
        require(isGroupRemoved(groupId) == false, "Removed group");
        semaphore.addMember(groupId, identityCommitment);

        groups[groupId].users[identityCommitment] = username;
        unchecked {
            groups[groupId].userCount = groups[groupId].userCount + 1;
        }
        
        emit NewUser(groupId, identityCommitment, username);
    }

    function getUserName(uint256 groupId, uint256 identityCommitment) public view returns (bytes32) {
        require(groupId >= 0 && groupId < groupCount, "Invalid groupId");
        return groups[groupId].users[identityCommitment];
    }

    function getPostIdList(uint256 groupId) public view returns (uint256[] memory) {
        require(groupId >= 0 && groupId < groupCount, "Invalid groupId");
        if (groups[groupId].removed) {
            uint256[] memory posts;
            return posts;
        } else {
            return groups[groupId].posts;   
        }
    }

    function getCommentIdList(uint256 postId) public view returns (uint256[] memory) {
        require(postId >= 0 && postId < itemCount, "Invalid postId");
        require(isItemRemoved(postId) == false, "post removed");

        if (items[postId].removed) {
            uint256[] memory childIds;
            return childIds;
        } else {
            return items[postId].childIds;
        }
    }

    function getGroupRequirements(uint256 groupId) public view returns (Requirement[] memory) {
        require(groupId >= 0 && groupId < groupCount, "Invalid groupId");
        return groups[groupId].requirements;
    }

    function addPost(
        uint256 groupId,
        bytes32 contentCID,
        uint256 merkleTreeRoot,
        uint256 nullifierHash,
        uint256 note,
        uint256[8] calldata proof
    ) external {
        require(isGroupRemoved(groupId) == false, "Removed group");

        uint256 externalNullifierHash = abi.encodePacked(contentCID).hashToField();
        semaphore.verifyProof(groupId, merkleTreeRoot, contentCID, nullifierHash, externalNullifierHash, proof);

        uint256[] memory childIds;
        items[itemCount] = Item(ItemKind.POST, itemCount, 0, groupId, block.number, childIds, 0, 0, note, contentCID, false);
        groups[groupId].posts.push(itemCount);

        emit NewItem(ItemKind.POST, groupId, items[itemCount].id, 0, contentCID, note);

        unchecked {
            itemCount += 1;
        }
    }

    function addComment(
        uint256 groupId,
        uint256 parentId,
        bytes32 contentCID,
        uint256 merkleTreeRoot,
        uint256 nullifierHash,
        uint256 note,
        uint256[8] calldata proof
    ) external {
        require(isGroupRemoved(groupId) == false, "Removed group");
        require(isItemRemoved(parentId) == false, "parent item removed");

        uint256 externalNullifierHash = abi.encodePacked(contentCID).hashToField();
        semaphore.verifyProof(groupId, merkleTreeRoot, contentCID, nullifierHash, externalNullifierHash, proof);

        uint256[] memory childIds;
        items[itemCount] = Item(ItemKind.COMMENT, itemCount, parentId, groupId, block.number, childIds, 0, 0, note, contentCID, false);
        items[parentId].childIds.push(items[itemCount].id);

        emit NewItem(ItemKind.COMMENT, groupId, items[itemCount].id, parentId, contentCID, note);
        unchecked {
            itemCount += 1;
        }
    }

    function editItem(
        uint256[2] memory _a,
        uint256[2][2] memory _b,
        uint256[2] memory _c, 
        uint256 itemId,
        bytes32 newCID,
        uint256 newNote
    ) external {
        require(itemId >= 0 && itemId < itemCount, "Invalid itemId");
        uint256 oldCID = abi.encodePacked(items[itemId].contentCID).hashToField();
        require(verifier.verifyProof(
            _a,
            _b,
            _c,
            [
                oldCID, 
                items[itemId].note
            ]
        ), "Invalid Proof");
        items[itemId].contentCID = newCID;
        items[itemId].note = newNote;
        emit UpdateItem(items[itemId].kind, itemId, newCID, newNote);
    }

    function vote(
        uint256 groupId,
        uint256 itemId, //id of post or comment
        VoteKind voteType, //0: up, 1: down
        uint256 merkleTreeRoot,
        uint256 nullifierHash,
        uint256[8] calldata proof
    ) external {
        require(isGroupRemoved(groupId) == false, "Removed group");
        require(isItemRemoved(itemId) == false, "item removed");
        require(voteType == VoteKind.UP || voteType == VoteKind.DOWN, "Invalid voteType");

        uint256 signal = abi.encodePacked(itemId, "vote").hashToField();
        semaphore.verifyProof(groupId, merkleTreeRoot, bytes32(signal), nullifierHash, signal, proof);

        unchecked {
            if(voteType == VoteKind.UP)
                items[itemId].upvote = items[itemId].upvote + 1;
            else 
                items[itemId].downvote = items[itemId].downvote + 1;
        }

        emit VoteItem(voteType, items[itemId].kind, itemId, items[itemId].upvote, items[itemId].downvote);
    }

    function isGroupRemoved(uint256 groupId) public view returns(bool) {
        require(groupId >= 0 && groupId < groupCount, "Invalid groupId");
        return groups[groupId].removed;
    }

    function isItemRemoved(uint256 itemId) public view returns(bool) {
        require(itemId >= 0 && itemId < itemCount, "Invalid itemId");
        return items[itemId].removed;
    }
}

