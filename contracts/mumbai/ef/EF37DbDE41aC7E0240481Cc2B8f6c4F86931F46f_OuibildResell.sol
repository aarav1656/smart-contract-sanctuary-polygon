pragma solidity ^0.8.4;

import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {Pausable} from "@openzeppelin/contracts/security/Pausable.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import {ERC1155Holder} from "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {FixedPointMathLib} from "./solmate/utils/FixedPointMathLib.sol";
import {IOuibildPair} from "./interfaces/IOuibildPair.sol";
import {IOuibild} from "./interfaces/IOuibild.sol";
import {IOuibildRentPayment} from "./interfaces/IOuibildRentPayment.sol";
import {SafeMath} from "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

contract OuibildResell is Ownable, Pausable, ERC1155Holder, ReentrancyGuard {

    using FixedPointMathLib for uint256;
    using SafeMath for uint256;

    /// @notice reference to the OUIBILD / USDT pair
    IOuibildPair public ouibildPair;
    /// @notice reference to the OUIBILD erc1155 token
    IOuibild public ouibildToken;
    /// @notice reference to rent contract payment 
    IOuibildRentPayment public rentPayment;
    /// @notice reference to $usdt contract
    IERC20 public USDT;
    /// @notice reference to $usdc contract
    IERC20 public USDC;
    /// @notice wallet were fund will be transfered
    address private ouibildMultisig;
    /// @notice ouibild sale fee : 5% 0.5 6 decimals 
    uint256 public ouibildFee = 50000;
    
    mapping(address => uint256) public amountAllocated;
    mapping(uint32 => uint256) public numTokenAvailableById;
    mapping(uint32 => uint256[]) public sharesTimestamps;
    mapping(address => bool) public controlerAddr;

    /*** EVENT ***/

    event TokenBought(uint32 propertyID, uint256 amount, address who);

    /*** ERROR ***/

    error PropertySoldOut();
    error NotOwner();
    error Unauthorized();

    constructor(
        address _ouibildPair,
        address _ouibildToken,
        address _rentPayment,
        address _stable1,
        address _stable2
    ) {
        ouibildPair = IOuibildPair(_ouibildPair);
        ouibildToken = IOuibild(_ouibildToken);
        rentPayment = IOuibildRentPayment(_rentPayment);
        USDC = IERC20(_stable1);
        USDT = IERC20(_stable2);
        setControlerAddr(_ouibildPair, true);
    }

    // TODO Only Owners can buy tokens
    // TODO Check if property has been created in OuibildPair 
    function buyTokenWithStable (
        uint32 propertyID, 
        uint256 amount, 
        uint8 stable
    ) public {
        require(tx.origin == _msgSender(), "Only EOA");
        /// safety check, we check if the user has a balance of token for the property @Id superior at 1 only previous owners can buy in resell.
        if(ouibildToken.balanceOf(_msgSender(), propertyID) < 1) revert NotOwner();
        /// safety check, we check the balance of token for the property @Id
        if(ouibildToken.balanceOf(address(this), propertyID) < amount) revert PropertySoldOut();


        /// choose the right token according to the parameter
        IERC20 stableCoin = stable == 1 ? USDT : USDC;


        /// amount to pay in stablecoin, we have to change the decimal 18 -> 6 for USDT & USDC, so we divide the amount by 1e12.
        uint256 amountStableToPay = amount.mul(ouibildToken.getStablePriceByID(propertyID).mul(1e6));

        // TODO Check if it's in 6 decimals or 18 decimals
        uint256 rentAmountInStable = rentAmountSinceDeposit(propertyID).mul(1e6);
        amountStableToPay.add(rentAmountInStable);

        /// take ouibild fee
        uint256 amountFee = (amountStableToPay * ouibildFee) / 1e6;

        /// we transfer stableCoin to this contract

        stableCoin.transferFrom(_msgSender(), address(this), amountStableToPay);

        /// we tranfer fee to the multisig wallet
        stableCoin.transferFrom(address(this), ouibildToken.getOuibildMultisig(), amountFee);
        /// we tranfer the rest to the pair for the liquidity
        stableCoin.transferFrom(address(this), address(ouibildPair), amountStableToPay.sub(amountFee));

        /// we claim all pending rewards for locked tokens


        if(ouibildToken.checkIsKYC(_msgSender())){
            ouibildToken.safeTransferFrom(address(this), _msgSender(), propertyID, amount, "");
        }
        else {
            amountAllocated[_msgSender()] += amount;
        }
        /// updating the spot price according the amount sell
        ouibildPair.computeNewSpotPrice(propertyID, amount);
        removelastShareTimestamp(propertyID);
        emit TokenBought(propertyID, amount, _msgSender());
    }

    /// returns a 0 decimals result 
    function rentAmountSinceDeposit(uint32 propertyID) public view returns (uint256) {
        uint256 lastTimestamp = sharesTimestamps[propertyID].length.min(1);
        uint256 timeSince = block.timestamp.min(lastTimestamp);
        uint256 months =  timeSince.div(30 days);
        // modulo 30 days
        if (months == 0) return 0;
        return rentPayment.getRentPrice(propertyID).mul(months);
    }

    function hasOuiBildToken(address who, uint32 propertyID) public view returns (bool) {
        return ouibildToken.balanceOf(who, propertyID) >= 1;
    }
    
    function setOuibildFee(uint256 _ouibildFee) public onlyOwner {
        ouibildFee = _ouibildFee.mul(1e4);
    }

    function setOuibildPair(address _ouibildPair) public onlyOwner {
        ouibildPair = IOuibildPair(_ouibildPair);
    }

    function getSharesTimestamps(uint32 propertyID) public view returns (uint256[] memory) {
        return sharesTimestamps[propertyID];
    }

    function addShareTimestamp(uint32 propertyID, uint256 timestamp) external onlyController {
        sharesTimestamps[propertyID].push(timestamp);
    }

    function removelastShareTimestamp(uint32 propertyID) internal {
        sharesTimestamps[propertyID].pop();
    }

    function setControlerAddr(address who, bool value) public onlyOwner {
        controlerAddr[who] = value;
    }
    
    modifier onlyController() {
        require(controlerAddr[msg.sender], "Not controller");
        _;
    }
    /// TODO : create receive resell function from pair for the numTokenAvailableById incrementation
}

