pragma solidity ^0.8.13;

import "@openzeppelin/contracts/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";

import "../lib/IPoVFactory.sol";
import "./PoV5192Metadata.sol";

contract PoV5192MetadataFactory is IPoVFactory {
    // ================================================================
    //  user functions
    // ================================================================
    /**
     * @dev create new pov
     * @notice you can register PoV for your resolver.
     * @param povOwner pov owner address
     * @param resolver address, resolver address
     * @param poVInfo pov info
     * @return address, new pov address
     */
    function createNewPoV(
        address povOwner,
        address resolver,
        PoVInfo memory poVInfo
    ) external override returns (address) {
        PoV5192Metadata pov = new PoV5192Metadata(povOwner, resolver, poVInfo);
        emit CreatePoV(address(pov), povOwner);
        return address(pov);
    }
}

