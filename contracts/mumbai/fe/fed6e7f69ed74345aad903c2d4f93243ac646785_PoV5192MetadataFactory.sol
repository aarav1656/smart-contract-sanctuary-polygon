pragma solidity ^0.8.13;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";

import "../lib/PoV5192.sol";

contract PoV5192Metadata is PoV5192, Ownable {
    // ================================================================
    //  usings
    // ================================================================
    using Counters for Counters.Counter;
    using Strings for uint256;

    // ================================================================
    //  variables
    // ================================================================

    // total supply count
    Counters.Counter private _totalSupply;

    /// PoV - Info
    PoVInfo private _povInfo;

    /// latitude and longitude precision
    int256 public constant POSITION_PRECISION = 100000000;

    // ================================================================
    //  constructors
    // ================================================================

    constructor(
        address spotOwner,
        address resolverContractAddress,
        PoVInfo memory povInfo
    ) {
        _transferOwnership(spotOwner);
        super._povInit(resolverContractAddress);
        _povInfo = povInfo;
    }

    // ================================================================
    //  resolver functions
    // ================================================================
    function mint(
        address to,
        uint256 tokenId,
        bytes32 userData
    ) external override {
        super._mint(to, tokenId, userData);
        _totalSupply.increment();
    }

    function burn(uint256 tokenId) external override {
        super._burn(tokenId);
        _totalSupply.decrement();
    }

    // ================================================================
    //  user functions
    // ================================================================

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        if (address(0) == super.ownerOf(tokenId)) revert("PoV: URI query for nonexistent token");

        // separate metadata for avoiding stack too deep error
        bytes memory _metadata1 = abi.encodePacked(
            '{"name":"',
            string(abi.encodePacked(_povInfo.name)),
            '",',
            '"description":"',
            string(abi.encodePacked(_povInfo.description)),
            '",',
            '"image":"',
            string(abi.encodePacked(_povInfo.imageUrl)),
            '",'
        );

        bytes memory _metadata2 = abi.encodePacked(
            '"url":"',
            string(abi.encodePacked(_povInfo.url)),
            '",'
            '"provider":"',
            string(abi.encodePacked(_povInfo.provider)),
            '",',
            '"owner":"',
            Strings.toHexString(uint160(super.ownerOf(tokenId)), 20),
            '",'
        );

        bytes memory _metadata3 = abi.encodePacked('"extra":"', string(abi.encodePacked(_povInfo.extra)), '",', '"userData":"', Strings.toHexString(uint256(super.getUserData(tokenId))), '",');

        string memory _latitude = _int2string(_povInfo.latitude);
        string memory _longitude = _int2string(_povInfo.longitude);

        bytes memory _metadata4 = abi.encodePacked('"latitude":', _latitude, ",", '"longitude":', _longitude, ",", '"spot":"', Strings.toHexString(uint160(super.getResolver()), 20), '"}');

        return string(abi.encodePacked("data:application/json;base64,", Base64.encode(abi.encodePacked(_metadata1, _metadata2, _metadata3, _metadata4))));
    }

    /**
     * @dev Returns PoV of latitude and longitude
     * @return latitude int256, latitude of PoV
     * @return longitude int256, longitude of PoV
     */
    function getPosition() external view returns (int256, int256) {
        return (_povInfo.latitude, _povInfo.longitude);
    }

    // ================================================================
    //  owner functions
    // ================================================================

    /**
     * @dev set Name of pov
     * @notice msg.sender should be pov owner or admin.
     * @param __name bytes32, name of pov
     */
    function setName(bytes32 __name) public onlyOwner {
        _povInfo.name = __name;
    }

    /**
     * @dev set Description of pov
     * @notice msg.sender should be pov owner or admin.
     * @param description bytes32[4] memory, description of pov
     */
    function setDescription(bytes32[4] memory description) public onlyOwner {
        _povInfo.description = description;
    }

    /**
     * @dev set Image Url of pov
     * @notice msg.sender should be pov owner or admin.
     * @param imageUrl bytes32[4] memory, image of pov
     */
    function setImage(bytes32[4] memory imageUrl) public onlyOwner {
        _povInfo.imageUrl = imageUrl;
    }

    /**
     * @dev set Url of pov
     * @notice msg.sender should be pov owner or admin.
     * @param url bytes32[2] memory, url of pov
     */
    function setUrl(bytes32[2] memory url) public onlyOwner {
        _povInfo.url = url;
    }

    /**
     * @dev set provider of pov
     * @notice msg.sender should be pov owner or admin.
     * @param provider bytes32, provider of pov
     */
    function setProvider(bytes32 provider) public onlyOwner {
        _povInfo.provider = provider;
    }

    /**
     * @dev set Extra of pov
     * @notice msg.sender should be pov owner or admin.
     * @param extra bytes32 memory, extra of pov
     */
    function setExtra(bytes32 extra) public onlyOwner {
        _povInfo.extra = extra;
    }

    function changePosition(int256 latitude, int256 longitude) public onlyOwner {
        if (latitude < -9000000000 || latitude > 9000000000) revert("PoV: invalid latitude");
        if (longitude < -18000000000 || longitude > 18000000000) revert("PoV: invalid longitude");
        _povInfo.latitude = latitude;
        _povInfo.longitude = longitude;
    }

    // ================================================================
    //  internal functions
    // ================================================================
    /**
     * @dev Returns initial name
     */
    function _initialName() internal pure override returns (string memory) {
        return "ProofOfVisitMetadata";
    }

    /**
     * @dev Returns initial symbol
     */
    function _initialSymbol() internal pure override returns (string memory) {
        return "POVM";
    }

    function _int2string(int256 i) internal pure returns (string memory) {
        if (i == 0) return "0";
        bool negative = i < 0;
        uint256 abs = uint256(negative ? -i : i);

        return string(abi.encodePacked(negative ? "-" : "", Strings.toString(abs)));
    }
}

