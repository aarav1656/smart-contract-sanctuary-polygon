pragma solidity ^0.8.9;

import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@thirdweb-dev/contracts/extension/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@thirdweb-dev/contracts/extension/ContractMetadata.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";

struct UserBalance {
    address User;
    uint256 Balance;
}

contract PubGameUserAccount is Ownable, Pausable, ContractMetadata {
    using EnumerableMap for EnumerableMap.AddressToUintMap;

    address payable public admin;
    address public deployer;
    address private _owner;
    address payable public recipient;

    EnumerableMap.AddressToUintMap private _balances;

    IERC20 public PGC;

    constructor(address _pgc) {
        admin = payable(msg.sender);
        deployer = msg.sender;
        recipient = payable(address(this));
        PGC = IERC20(_pgc);
        _setupOwner(msg.sender);
    }

    /** Events */

    event Deposit(address indexed from, uint256 value);
    event Withdraw(address indexed from, uint256 value);
    event Use(address indexed from, uint256 value, string reason);
    event Add(address indexed from, uint256 value, string reason);
    event Set(address indexed from, uint256 value, string reason);

    /** Game Functions */

    function depositPgc(
        uint256 _amount
    ) external payable whenNotPaused returns (bool success) {
        bool txSuccess = false;

        require(
            PGC.allowance(msg.sender, address(this)) >= _amount,
            "You need to approve the contract"
        );
        require(PGC.balanceOf(msg.sender) >= _amount, "Insufficient funds");

        txSuccess = PGC.transferFrom(msg.sender, address(this), _amount);

        _increaseBalance(msg.sender, _amount);

        emit Deposit(msg.sender, _amount);

        return txSuccess;
    }

    function withdrawPgc(
        uint256 _amount
    ) external payable whenNotPaused returns (bool success) {
        bool txSuccess = false;

        require(
            PGC.allowance(msg.sender, address(this)) >= _amount,
            "You need to approve the contract"
        );
        require(
            PGC.balanceOf(address(this)) >= _amount,
            "Insufficient funds, contact the support"
        );

        require(
            _balances.contains(msg.sender),
            "User doesn't have any balance"
        );

        require(
            _balances.get(msg.sender) >= _amount,
            "Not enough balance in your account"
        );

        _decreaseBalance(msg.sender, _amount);

        txSuccess = PGC.transfer(msg.sender, _amount);

        emit Withdraw(msg.sender, _amount);

        return txSuccess;
    }

    function usePgc(
        address _user,
        uint256 _amount,
        string memory _reason
    ) public onlyOwner returns (bool success) {
        success = _decreaseBalance(_user, _amount);
        emit Use(_user, _amount, _reason);
    }

    function usePgc(
        address _user,
        uint256 _amount
    ) public onlyOwner returns (bool success) {
        success = _decreaseBalance(_user, _amount);
        emit Use(_user, _amount, "");
    }

    function addPgc(
        address _user,
        uint256 _amount,
        string memory _reason
    ) public onlyOwner returns (bool success) {
        success = _increaseBalance(_user, _amount);
        emit Add(_user, _amount, _reason);
    }

    function addPgc(
        address _user,
        uint256 _amount
    ) public onlyOwner returns (bool success) {
        success = _increaseBalance(_user, _amount);
        emit Add(_user, _amount, "");
    }

    function _increaseBalance(
        address _user,
        uint256 _amount
    ) internal returns (bool success) {
        if (_balances.contains(_user)) {
            uint256 currentBalance = _balances.get(_user);
            _balances.set(_user, currentBalance + _amount);
        } else {
            _balances.set(_user, _amount);
        }

        return true;
    }

    function _decreaseBalance(
        address _user,
        uint256 _amount
    ) internal returns (bool success) {
        require(_balances.contains(_user), "User doesn't have any balance");
        require(
            _balances.get(_user) >= _amount,
            "Not enough balance in your account"
        );

        uint256 currentBalance = _balances.get(_user);
        _balances.set(_user, currentBalance - _amount);

        return true;
    }

    function setBalance(
        address _user,
        uint256 _amount,
        string memory _reason
    ) public onlyOwner returns (bool success) {
        _balances.set(_user, _amount);
        emit Set(_user, _amount, _reason);
        return true;
    }

    function setBalance(
        address _user,
        uint256 _amount
    ) public onlyOwner returns (bool success) {
        _balances.set(_user, _amount);
        emit Set(_user, _amount, "");
        return true;
    }

    /** Read Infos */

    function getPgcBalance(
        address _user
    ) public view onlyOwner returns (uint256 balance) {
        if (_balances.contains(_user)) {
            balance = _balances.get(_user);
        } else {
            balance = 0;
        }

        return balance;
    }

    function getPgcBalance() public view returns (uint256 balance) {
        if (_balances.contains(msg.sender)) {
            balance = _balances.get(msg.sender);
        } else {
            balance = 0;
        }

        return balance;
    }

    function getAllBalances()
        public
        view
        onlyOwner
        returns (UserBalance[] memory)
    {
        uint256 numUsers = _balances.length();

        UserBalance[] memory _uBalances = new UserBalance[](numUsers);

        for (uint256 index = 0; index < numUsers; index++) {
            (address _uAddress, uint256 _uBalance) = _balances.at(index);

            UserBalance memory _cBalance;
            _cBalance.User = _uAddress;
            _cBalance.Balance = _uBalance;

            _uBalances[index] = _cBalance;
        }

        return _uBalances;
    }

    function getTotalBalances() public view onlyOwner returns (uint256) {
        uint256 numUsers = _balances.length();

        uint256 _totalBalance = 0;

        for (uint256 index = 0; index < numUsers; index++) {
            (, uint256 _uBalance) = _balances.at(index);

            _totalBalance += _uBalance;
        }

        return _totalBalance;
    }

    function getDiffBalances()
        public
        view
        onlyOwner
        returns (uint256, string memory)
    {
        uint256 _totalBalance = getTotalBalances();
        uint256 _contractBalance = pgcBalance();

        uint256 _result = 0;
        string memory _position = "Equal";

        if (_totalBalance > _contractBalance) {
            _result = _totalBalance - _contractBalance;
            _position = "Missing";
        }
        if (_totalBalance < _contractBalance) {
            _result = _contractBalance - _totalBalance;
            _position = "Remaining";
        }

        return (_result, _position);
    }

    /** Extensions */

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function _canSetContractURI()
        internal
        view
        virtual
        override
        returns (bool)
    {
        return msg.sender == deployer;
    }

    function _canSetOwner() internal view virtual override returns (bool) {
        return msg.sender == owner();
    }

    fallback() external payable {}

    receive() external payable {}

    function setRecipient(address _recipient) public onlyOwner {
        recipient = payable(_recipient);
    }

    /** Contract Management */

    function withdrawToken(address _token, uint256 _amount) external onlyOwner {
        IERC20 tokenContract = IERC20(_token);
        tokenContract.approve(address(this), _amount);
        tokenContract.transfer(payable(admin), _amount);
    }

    function withdraw() public onlyOwner {
        uint256 amount = address(this).balance;

        (bool success, ) = admin.call{value: amount}("");
        require(success, "Failed to send Matic");
    }

    function setPGC(address _pgc) public onlyOwner {
        PGC = IERC20(_pgc);
    }

    function pgcBalance() public view onlyOwner returns (uint256 _balance) {
        return PGC.balanceOf(address(this));
    }
}

