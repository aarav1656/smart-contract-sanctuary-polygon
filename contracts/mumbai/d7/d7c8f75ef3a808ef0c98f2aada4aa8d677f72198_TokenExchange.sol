// File: credit/TokenExchange.sol



pragma solidity 0.8.17;





contract TokenExchange {    
    /**
    * The `StatusSwap` enumeration represents the different possible states of a swap.
    * - `PENDING` indicates that the swap has been proposed but not yet confirmed.
    * - `PROCESSING` indicates that the swap is in progress.
    * - `CLOSED` indicates that the swap has been completed.
    */
    enum StatusSwap { PENDING, PROCESSING, CLOSED }
    
    /**
    * The `TokenExchangeInfo` struct contains information about a token exchange.
    * - `TOKEN_EXCHANGE_OWNER` is the address of the owner of the token exchange.
    * - `SOURCE_TOKEN` is the address of the token being sold in the exchange.
    * - `DESTINATION_TOKEN` is the address of the token being bought in the exchange.
    * - `EXCHANGE_RATE` is the rate at which the SOURCE_TOKEN can be exchanged for the DESTINATION_TOKEN.
    * - `MAX_SPEND_SOURCE_TOKEN_PER_BUYER` is the maximum amount of SOURCE_TOKEN that a single buyer can spend.
    * - `MAX_SPEND_DESTINATION_TOKEN_PER_BUYER` is the maximum amount of DESTINATION_TOKEN that a single buyer can receive.
    * - `AMOUNT_SOURCE_TOKEN_FOR_SWAP` is the amount of SOURCE_TOKEN that is available for the exchange.
    * - `AMOUNT_DESTINATION_TOKEN_FOR_SWAP` is the amount of DESTINATION_TOKEN that is available for the exchange.
    * - `START_TIME` is the start time of the exchange.
    * - `END_TIME` is the end time of the exchange.
    */
    struct TokenExchangeInfo {
        address payable TOKEN_EXCHANGE_OWNER; 
        IERC20 SOURCE_TOKEN;
        IERC20 DESTINATION_TOKEN; 
        uint256 EXCHANGE_RATE; 
        uint256 MAX_SPEND_SOURCE_TOKEN_PER_BUYER; 
        uint256 MAX_SPEND_DESTINATION_TOKEN_PER_BUYER; 
        uint256 AMOUNT_SOURCE_TOKEN_FOR_SWAP; 
        uint256 AMOUNT_DESTINATION_TOKEN_FOR_SWAP; 
        uint256 START_TIME; 
        uint256 END_TIME; 
    }
    
    /**
    * The `SwapStatus` struct contains information about the status of a swap.
    * - `FORCE_FAILED` indicates whether the swap has been forcibly failed by the owner.
    * - `IS_OWNER_WITHDRAWN` indicates whether the owner has withdrawn their funds from the swap.
    * - `NUM_BUYERS` is the number of buyers who have participated in the swap.
    */
    struct SwapStatus {
        bool FORCE_FAILED;
        bool IS_OWNER_WITHDRAWN;
        uint256 NUM_BUYERS;
    }
    
    /**
    * The `UserInfo` struct contains information about a user participating in a swap.
    * - `sourceTokenDeposit` is the amount of SOURCE_TOKEN that the user has deposited into the swap.
    * - `destinationTokenDeposit` is the amount of DESTINATION_TOKEN that the user has received in exchange for their SOURCE_TOKEN.
    * - `lastBlockSwapToken` is the block number at which the user last participated in a swap.
    * - `countTimeJoined` is the number of times the user has participated in the swap.
    */
    struct UserInfo {
        uint256 sourceTokenDeposit;
        uint256 destinationTokenDeposit;
        uint256 lastBlockSwapToken;
        uint256 countTimeJoined;
    }
    
    /**
    * The `onlyOwner` modifier restricts access to a function to the owner of the token exchange.
    * Throws an error if the caller is not the owner.
    */
    modifier onlyOwner() {
        require(tokenExchangeInfo.TOKEN_EXCHANGE_OWNER == msg.sender, "Only token exchange owner");
        _;
    }
    
    /**
    * The `checkIsOwnerWithDraw` modifier checks whether the owner has already withdrawn their funds from the swap.
    * Throws an error if the owner has already withdrawn their funds.
    */
    modifier checkIsOwnerWithDraw() {
        require(!swapStatus.IS_OWNER_WITHDRAWN, "Admin already withdraw token");
        _;
    }

    // The `INITIALIZED` boolean flag indicates whether the contract has been initialized.
    bool private INITIALIZED;

    // The `tokenExchangeInfo` variable is a `TokenExchangeInfo` struct that contains information about the token exchange.
    TokenExchangeInfo public tokenExchangeInfo;

    // The `userData` mapping stores information about users participating in the swap.
    mapping(address => UserInfo) public userData;

    // The `sollarSettings` variable is an `ISollarSettings` contract that provides access to global settings.
    ISollarSettings public sollarSettings;

    // The `swapStatus` variable is a `SwapStatus` struct that contains information about the status of the swap.
    SwapStatus public swapStatus;

    /**
    * The `Swapped` event is emitted when tokens are swapped.
    * It contains the `fromToken` and `toToken` addresses, as well as the `amountIn` and `amountOut` values.
    * The `WithdrawToken` event is emitted when tokens are withdrawn from the contract.
    * It contains the `caller` address, as well as the `sourceToken`, `destinationToken`, `amountSourceToken`, and `amountDestinationToken` values.
    */
    event Swapped(
        IERC20 indexed fromToken,
        IERC20 indexed toToken,
        uint256 amountIn,
        uint256 amountOut
    );
    
    /*
    * The `WithdrawToken` event Emitted when tokens are withdrawn from the contract.
    * @param caller The address of the caller who initiated the withdrawal.
    * @param sourceToken The address of the token being withdrawn from.
    * @param destinationToken The address of the token being withdrawn to.
    * @param amountSourceToken The amount of `sourceToken` being withdrawn.
    * @param amountDestinationToken The amount of `destinationToken` being withdrawn.
    */
    event WithdrawToken(
        address indexed caller,
        IERC20 indexed sourceToken,
        IERC20 indexed detinationToken,
        uint256 amountSourceToken,
        uint256 amountDetinationToken
    );
    // ["0xA20D55DEFDA036fE158cF9B720Da55F48652261b", "0x3F7FD60C32ee007dd23F27e490fEE016148A8FA8", "0x3F7FD60C32ee007dd23F27e490fEE016148A8FA8", 200, 10000000000000000000000, 50000000000000000000, 10000000000000000000000000, 50000000000000000000000, 1680729098, 1680901778]
    // 0xEA02E5F64730745a8E94D27AE49e53Fb560d4480
    /**
    The `initialize` function initializes the contract with the specified _tokenExchangeInfo and _sollarSettings.
    @param _tokenExchangeInfo The TokenExchangeInfo struct containing exchange-related information such as the exchange owner, source and destination tokens, exchange rate, and more.
    @param _sollarSettings The ISollarSettings contract that determines certain exchange settings, such as the minimum and maximum deposit amounts.
    */
    function initialize(
        TokenExchangeInfo calldata _tokenExchangeInfo,
        ISollarSettings _sollarSettings
    ) external {
        // Ensure that the contract has not been initialized already
        require(!INITIALIZED, "Contract is already initialized");
        INITIALIZED = true;

        // Ensure that the token exchange owner is not the zero address
        require(
            _tokenExchangeInfo.TOKEN_EXCHANGE_OWNER != address(0),
            "The exchange owner is zero address"
        );

        // Ensure that the source token address is not the zero address
        require(
            address(_tokenExchangeInfo.SOURCE_TOKEN) != address(0),
            "The source token is zero address"
        );

        // Ensure that the destination token address is not the zero address
        require(
            address(_tokenExchangeInfo.SOURCE_TOKEN) != address(0),
            "The destination token is zero address"
        );

        // Ensure that the exchange rate is greater than zero
        require(
            _tokenExchangeInfo.EXCHANGE_RATE > 0,
            "The exchange rate must be greater than zero"
        );

        // Ensure that the start time and end time are valid
        require(
            _tokenExchangeInfo.START_TIME < _tokenExchangeInfo.END_TIME &&
            _tokenExchangeInfo.START_TIME >= block.timestamp,
            "The start time and end time is invalid"
        );

        // Ensure that the amount for swap and max spend per user are valid
        require(
            _tokenExchangeInfo.AMOUNT_SOURCE_TOKEN_FOR_SWAP >= _tokenExchangeInfo.MAX_SPEND_SOURCE_TOKEN_PER_BUYER &&
            _tokenExchangeInfo.AMOUNT_DESTINATION_TOKEN_FOR_SWAP >= _tokenExchangeInfo.MAX_SPEND_DESTINATION_TOKEN_PER_BUYER,
            "The amount for swap and max spend per user is invalid"
        );
        
        // Set the tokenExchangeInfo variable to the input _tokenExchangeInfo value
        tokenExchangeInfo = _tokenExchangeInfo;
        
        // The require statement checks that the _sollarSettings address is not the zero address, which would indicate an invalid contract.
        require(address(_sollarSettings) != address(0), "Sollar settings is zero address");
        // The sollarSettings variable is set to the input _sollarSettings value, which is used to access various settings related to the Sollar token.
        sollarSettings = _sollarSettings;
        
        // TransferHelper.safeTransferFrom is used to transfer the specified amount source of tokens 
        TransferHelper.safeTransferFrom(
            address(_tokenExchangeInfo.SOURCE_TOKEN), 
            msg.sender, 
            address(this),
            _tokenExchangeInfo.AMOUNT_SOURCE_TOKEN_FOR_SWAP
        );
        
        // TransferHelper.safeTransferFrom is used to transfer the specified amount destination of tokens 
        TransferHelper.safeTransferFrom(
            address(_tokenExchangeInfo.DESTINATION_TOKEN), 
            msg.sender, 
            address(this),
            _tokenExchangeInfo.AMOUNT_DESTINATION_TOKEN_FOR_SWAP
        );    
    }
    
    /**
    * The `checkSwapStatus()` function is a public view function that returns the current status of the swap.
    * The return value is of the `StatusSwap` enum type.
    */
    function checkSwapStatus() public view returns(StatusSwap) {
        TokenExchangeInfo memory tokenExchange = tokenExchangeInfo;
        
        // If the current block timestamp is before the start time of the token exchange,
        // return a PENDING status indicating that the exchange has not yet started.
        if(tokenExchange.START_TIME > block.timestamp) {
            return StatusSwap.PENDING;
        }
        
        // Check if the current time is within the start and end times of the token exchange
        if(tokenExchange.END_TIME >= block.timestamp) {
            return StatusSwap.PROCESSING;
        }
        
        // If none of the previous conditions are met, it means that the token exchange has ended
        return StatusSwap.CLOSED;
    }
    
    /**
    @dev Allows the contract owner to swap an input amount of tokens for an output amount of tokens according to the exchange rate and other parameters specified during contract initialization.
    @param _tokenIn The address of the token to swap.
    @param _amountIn The amount of tokens to swap.
    */
    function swap(IERC20 _tokenIn, uint256 _amountIn) external payable 
        checkIsOwnerWithDraw 
    {
        TokenExchangeInfo memory tokenExchange = tokenExchangeInfo;

        // Get the user data from the userData mapping
        UserInfo storage uData = userData[msg.sender];
        
        // Check if the input token is either the source token or the destination token
        require(
            _tokenIn == tokenExchange.SOURCE_TOKEN || _tokenIn == tokenExchange.DESTINATION_TOKEN,
            "Token is not supported"
        );
        
        // Check the current status of the token swap, and only allow swaps during the processing period
        require(checkSwapStatus() == StatusSwap.PROCESSING, "Not time for swap");

        uint256 amountOut;
        IERC20 tokenOut;
        uint256 fee;
        uint actualAmountIn = _amountIn;

        // // If the input token is the source token for the swap
        if (_tokenIn == tokenExchange.SOURCE_TOKEN) {
            // If the input token is the SOURCE_TOKEN, then the output token is the DESTINATION_TOKEN.
            tokenOut = tokenExchange.DESTINATION_TOKEN;
            
            // Calculate the actual amount to be exchanged based on the exchange rate
            actualAmountIn = _amountIn - _amountIn % tokenExchange.EXCHANGE_RATE;

            // Check if the actual amount to be exchanged is greater than zero
            require(actualAmountIn > 0, "Insufficient input amount for exchange");

            // Calculate the amount of output tokens to be received based on the input token amount and the exchange rate.
            amountOut = actualAmountIn * 
                (10 ** tokenExchange.DESTINATION_TOKEN.decimals()) / 
                (10 ** tokenExchange.SOURCE_TOKEN.decimals()) / 
                (tokenExchange.EXCHANGE_RATE);
            
            // Check if the destination token deposit plus the calculated amount out is less than or equal to the maximum limit per user
            require(
                uData.destinationTokenDeposit + amountOut <= 
                tokenExchange.AMOUNT_DESTINATION_TOKEN_FOR_SWAP,
                "The swap amount is greater than the limit per user"
            );
            
            // Check if the amount out is less than or equal to the available destination token balance for the swap
            require(
                tokenExchange.AMOUNT_DESTINATION_TOKEN_FOR_SWAP >= amountOut,
                "Not enough amount token out for swap"
            );
            
            // Update the user's source and destination token deposits
            uData.sourceTokenDeposit = uData.sourceTokenDeposit + actualAmountIn;
            if(uData.destinationTokenDeposit >= amountOut) {
                uData.destinationTokenDeposit = uData.destinationTokenDeposit - amountOut;
            } else {
                uData.destinationTokenDeposit = 0;
            }
            
            // Update tokenExchangeInfo
            tokenExchangeInfo.AMOUNT_SOURCE_TOKEN_FOR_SWAP = tokenExchange.AMOUNT_SOURCE_TOKEN_FOR_SWAP + actualAmountIn;
            tokenExchangeInfo.AMOUNT_DESTINATION_TOKEN_FOR_SWAP = tokenExchange.AMOUNT_DESTINATION_TOKEN_FOR_SWAP - amountOut;
        }
        
        // If the input token is the SOURCE_TOKEN, set the output token as DESTINATION_TOKEN
        // and calculate the output amount based on the exchange rate
        if (_tokenIn == tokenExchange.DESTINATION_TOKEN) {
            tokenOut = tokenExchange.SOURCE_TOKEN;
            amountOut = actualAmountIn * (10 ** tokenExchange.SOURCE_TOKEN.decimals()) *
                (tokenExchange.EXCHANGE_RATE) /
                (10 ** tokenExchange.DESTINATION_TOKEN.decimals());
            
            // Check if the user's destination token deposit plus the output amount is less than or equal to the limit
            // per user and if the token exchange has enough destination token for the swap
            require(
                uData.sourceTokenDeposit + amountOut <= 
                tokenExchange.AMOUNT_SOURCE_TOKEN_FOR_SWAP,
                "The swap amount is greater than the limit per user"
            );

            require(
                tokenExchange.AMOUNT_SOURCE_TOKEN_FOR_SWAP >= amountOut,
                "Not enough amount token out for swap"
            );
            
            // Update the user's source and destination token deposits
            uData.destinationTokenDeposit = uData.destinationTokenDeposit + actualAmountIn;
            if(uData.sourceTokenDeposit >= amountOut) {
                uData.sourceTokenDeposit = uData.sourceTokenDeposit - amountOut;
            } else {
                uData.sourceTokenDeposit = 0;
            }
            
            // Update the token exchange's source and destination token amounts for swap
            tokenExchangeInfo.AMOUNT_DESTINATION_TOKEN_FOR_SWAP = tokenExchange.AMOUNT_DESTINATION_TOKEN_FOR_SWAP + (actualAmountIn);
            tokenExchangeInfo.AMOUNT_SOURCE_TOKEN_FOR_SWAP = tokenExchange.AMOUNT_SOURCE_TOKEN_FOR_SWAP - (amountOut);
        }

        // Calculate the actual amount of tokens to be swapped after deducting fees
        uint256 realAmountOut = amountOut;
        
        // If there's a token fee, deduct it from the output amount and transfer it to the fee address
        uint256 tokenFee = sollarSettings.getTokenFee();
        if (tokenFee > 0) {
            fee = amountOut * tokenFee / 1e4;
            realAmountOut = realAmountOut - fee;

            // Transfer the fee to the Sollar token address
            TransferHelper.safeTransfer(
                address(tokenOut),
                sollarSettings.getTokenAddress(),
                fee
            );
        }
        
        // If there is a base fee, check that the transaction value is correct and transfer the base token
        uint256 baseFee = sollarSettings.getBaseFee();
        if (baseFee > 0) {
            require(msg.value == baseFee, "Amount base token not right");
            TransferHelper.saveTransferBaseToken(
                sollarSettings.getEthAddress(),
                msg.value
            );
        }
        
        // Transfer the tokens from the user to this contract
        TransferHelper.safeTransferFrom(
            address(_tokenIn),
            msg.sender,
            address(this),
            actualAmountIn
        );
        
        // Transfer the swapped tokens from this contract to the user
        TransferHelper.safeTransfer(
            address(tokenOut),
            msg.sender,
            realAmountOut
        );
        
        // Update user's swap info
        uData.lastBlockSwapToken = block.number;
        uData.countTimeJoined += 1;
        
        // Emit swap event
        emit Swapped(_tokenIn, tokenOut, actualAmountIn, amountOut);
    }
    
    /**
    * This `withdrawToken` function allows the owner of the contract to withdraw the remaining tokens after the swap has ended
    @dev Withdraws all tokens from the contract to the owner.
    */
    function withdrawToken() external 
        onlyOwner 
        checkIsOwnerWithDraw 
    {
        TokenExchangeInfo memory tokenExchange = tokenExchangeInfo;

        // Check if the swap status is closed, so it's time for withdraw token
        require(checkSwapStatus() == StatusSwap.CLOSED, "Not time for withdraw token");
        uint256 amountSourceTokenremaining = tokenExchange.SOURCE_TOKEN.balanceOf(address(this));
        uint256 amountDetinationTokenremaining = tokenExchange.DESTINATION_TOKEN.balanceOf(address(this));
        
        // Transfer remaining tokens to contract owner
        TransferHelper.safeTransfer(
            address(tokenExchange.SOURCE_TOKEN),
            msg.sender,
            amountSourceTokenremaining
        );

        TransferHelper.safeTransfer(
            address(tokenExchange.DESTINATION_TOKEN),
            msg.sender,
            amountDetinationTokenremaining
        );
        
        // Set the swap status to owner withdrawn
        swapStatus.IS_OWNER_WITHDRAWN = true;
        
        // Emit an event to indicate the successful withdrawal of tokens
        emit WithdrawToken(
            msg.sender, 
            tokenExchange.SOURCE_TOKEN,
            tokenExchange.DESTINATION_TOKEN,
            amountSourceTokenremaining,
            amountDetinationTokenremaining
        );
    }

    /**
    * @dev Finalize the swap process, force failed swap, and transfer remaining tokens to the specified address.
    * Only the owner of the token exchange contract can call this function.
    */
    function finalize() external onlyOwner {
        // Retrieve the token exchange information
        TokenExchangeInfo memory tokenExchange = tokenExchangeInfo;

        // Get the remaining source and destination tokens balance
        uint256 amountSourceTokenremaining = tokenExchange.SOURCE_TOKEN.balanceOf(address(this));
        uint256 amountDetinationTokenremaining = tokenExchange.DESTINATION_TOKEN.balanceOf(address(this)); 

        // Transfer the remaining source tokens to the SOLLAR contract address
        TransferHelper.safeTransfer(
            address(tokenExchange.SOURCE_TOKEN),
            sollarSettings.getTokenAddress(),
            amountSourceTokenremaining
        );

        // Transfer the remaining destination tokens to the SOLLAR contract address
        TransferHelper.safeTransfer(
            address(tokenExchange.DESTINATION_TOKEN),
            sollarSettings.getTokenAddress(),
            amountDetinationTokenremaining
        );

        // Set the swap status to force failed
        swapStatus.FORCE_FAILED = true;

        // Destroy the contract and transfer the remaining ETH balance to the token exchange owner address
        selfdestruct(tokenExchangeInfo.TOKEN_EXCHANGE_OWNER);
    }
}