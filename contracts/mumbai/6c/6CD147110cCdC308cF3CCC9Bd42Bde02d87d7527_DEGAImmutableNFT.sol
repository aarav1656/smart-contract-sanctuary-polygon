pragma solidity 0.8.17;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import {DEGAErrors} from "./DEGAErrors.sol";

contract DEGAModifiers is DEGAErrors {
    using ECDSA for bytes32;

    modifier checkMultisign(bytes memory _tokenOwnerSign, bool _isMultisign, uint256 _tokenId, address _holder) {
        if (_isMultisign) {
            // Verify approval signature of token owner
            bytes32 _hash = keccak256(abi.encode(address(this), _tokenId, _holder));
            if ((_hash.toEthSignedMessageHash().recover(_tokenOwnerSign) != _holder)) revert InvalidSignature();
        }
        _;
    }
}

