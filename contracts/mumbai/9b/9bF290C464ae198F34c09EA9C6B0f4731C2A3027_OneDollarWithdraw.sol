// 1 Dollar Ferrari Withdrawal Contract V1
pragma solidity ^0.8.9;
pragma abicoder v2;

import '@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol';
import '@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol';
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@uniswap/swap-router-contracts/contracts/interfaces/IV3SwapRouter.sol";


//OneDollarFerrari Interface
interface IODF is IERC721 {
    function canWithdraw(address addr) external view returns (bool);
    function withdrawBalance(address caller, uint256 amount) external returns (bool);
    
}

contract OneDollarWithdraw is Ownable {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    using Address for address;
    uint8 _decimals = 6;
    address _ODF_CONTRACT;
    address USDT;
    address WETH9;
   
    mapping(address => bool) private allowed;

    //Uniswap
    address SwapRouterAddress;
    address SwapRouter02Address;
    ISwapRouter swapRouter;
    IV3SwapRouter swapRouter02;
    uint24 poolFee = 3000;
    //-->

    event WithdrawSuccess(address to, uint256 withdrawAmount, uint256 fees, uint256 recievedAmount, uint256 ethFee);

    constructor() {
        
    }
    function setPoolFee(uint24 fee) public onlyOwner{
        require(fee > 0, "fee must > 0");
        poolFee = fee;
    }
    function setRouter(address _router) public onlyOwner{
        SwapRouterAddress = _router;
        swapRouter = ISwapRouter(_router);
    }
    function setRouter2(address _router) public onlyOwner{
        SwapRouter02Address = _router;
        swapRouter02 = IV3SwapRouter(_router);
    }
    function getRouter() public view returns (ISwapRouter){
        return swapRouter;
    }
    function setUSDTAddress(address addr) public onlyOwner{
        USDT = addr;
    }
    function getUSDTAddress() public view returns (address){
        return USDT;
    }
    function setWETH9Address(address addr) public onlyOwner{
        WETH9 = addr;
    }
    function getWETH9Address() public view returns (address){
        return WETH9;
    }
    
   
    function setNFTContract(address addr) public onlyOwner {
        _ODF_CONTRACT = addr;
    }
    function getNFTContract() public view returns (address){
        return _ODF_CONTRACT;
    }
   
    function setDecimals(uint8 _dec) public onlyOwner{
        _decimals = _dec;
    }
    function decimals() public view virtual returns (uint8) {
        return _decimals;
    }
    function isAllowed(address addr) public view virtual returns (bool){
        if(allowed[addr]) return true;
        return false;
    }
    function addAllowed(address addr) public onlyOwner {
        allowed[addr] = true;
    }
    function removeAllowed(address addr) public onlyOwner {
        allowed[addr] = false;
    }
    
    function withdrawTo(address to, uint256 amount, uint256 fees) public virtual{
       
        require(amount >= 1, "minimum withdrawal is 1 USDT");
        require(fees >= 0, "please define fees");
        require(to != address(0), "destination undefined");
        require(isAllowed(msg.sender),"You are not allowed to execute the withdrawal");
        
        uint256 _transferAmount =  amount - fees;
        
        require(
            IERC20(USDT).balanceOf(_ODF_CONTRACT) >= amount,
            "vault has not enough USDT balance"
        );
        
        require(_transferAmount >= 0, "transfer amount must not lower than 0");
        //these contract executes withdrawal function from ODF contract of _totalAmount
        require(IODF(_ODF_CONTRACT).withdrawBalance(msg.sender, amount),"unable to withdraw balance");
        //swap total fees from executioner wallet to eth, store the eth in the executioner address.
       
        TransferHelper.safeTransferFrom(
            USDT,
            _msgSender(),
            address(this),
            fees
        );
        TransferHelper.safeApprove(USDT, address(swapRouter02), fees);

        IV3SwapRouter.ExactInputSingleParams memory params = IV3SwapRouter
            .ExactInputSingleParams({
                tokenIn: USDT,
                tokenOut: WETH9,
                fee: poolFee,
                recipient: msg.sender,
                amountIn: fees,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0
            });

        uint256 EthRecieved = swapRouter02.exactInputSingle(params);
       
        
        require(EthRecieved > 0, "unable to get eth for gasfee");
        //send remaining _transferAmount to destinated address (to)
        require(IERC20(USDT).transferFrom(msg.sender, to, _transferAmount),"unable to send USDT to requestee.");
        
        emit WithdrawSuccess(to, amount, fees, _transferAmount, EthRecieved);
    }

}

