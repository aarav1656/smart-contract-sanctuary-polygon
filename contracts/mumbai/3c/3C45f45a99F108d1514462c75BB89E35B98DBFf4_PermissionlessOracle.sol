pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

import ".././interfaces/HTokenI.sol";
import ".././utils/ErrorReporter.sol";
import "hardhat/console.sol";

/**
 * @title Chainlink oracle that integrates with upshot
 * @author Honey Finance Labs
 * @custom:coauthor m4rio
 * @custom:contributor BowTiedPickle
 */
contract PermissionlessOracle is AccessControlEnumerable, Pausable {
  bytes32 public constant SUBMITTER_ROLE = keccak256("SUBMITTER_ROLE");
  bytes32 public constant CREATOR_ROLE = keccak256("CREATOR_ROLE");

  struct Collection {
    bool paused;
    uint16 twapRange;
    address collectionAddress;
    string collectionSlug;
  }

  struct FloorPrice {
    uint96 lastUpdated;
    address collectionAddress;
    uint256 price;
    uint256 lastMean;
    address submitter;
  }

  /// @notice weth address
  IERC20 public immutable weth;
  bool public testing;

  mapping(address => Collection) public collections;
  mapping(address => FloorPrice[]) public floorPrices;

  /// @notice price feeds for different ERC20s we support in the markets
  mapping(IERC20 => AggregatorV3Interface) public priceFeeds;

  /// @notice variable used for testing real collections with our own testing collections, testCollection=>realCollection
  mapping(address => address) public testingCollections;

  constructor(IERC20 _weth) {
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(SUBMITTER_ROLE, msg.sender);
    _setupRole(CREATOR_ROLE, msg.sender);
    weth = _weth;
    testing = true;
  }

  function registerCollections(
    address[] calldata addresses,
    string[] calldata collectionSlugs,
    uint16[] calldata twapRanges
  ) external whenNotPaused onlyRole(CREATOR_ROLE) {
    if (addresses.length != collectionSlugs.length && addresses.length != collectionSlugs.length) {
      revert WrongParams();
    }

    for (uint256 i; i < addresses.length; ++i) {
      collections[addresses[i]] = Collection(false, twapRanges[i], addresses[i], collectionSlugs[i]);
    }
  }

  function submitFloors(
    address[] calldata addresses,
    uint256[] calldata floors
  ) external whenNotPaused onlyRole(SUBMITTER_ROLE) {
    if (addresses.length != floors.length) {
      revert WrongParams();
    }
    for (uint256 i; i < addresses.length; ) {
      address col = addresses[i];

      if (collections[col].collectionAddress == address(0)) {
        revert CollectionNotRegistered();
      }

      Collection storage collection = collections[col];

      uint256 lastMean;
      uint256 length = floorPrices[col].length;
      uint256 cachedTwapRange = collection.twapRange;

      // if we don't have enough twap, we just submit data, we don't have mean
      if (length < cachedTwapRange) {
        floorPrices[col].push(FloorPrice(uint96(block.timestamp), col, floors[i], 0, msg.sender));
        emit FloorSubmitted(collection.collectionSlug, floors[i], msg.sender);

        unchecked {
          ++i;
        }
        continue;
      }

      for (uint256 j = 1; j < cachedTwapRange; ) {
        if (length < j) {
          lastMean += floorPrices[col][length - 1].price;
        } else {
          lastMean += floorPrices[col][length - j].price;
        }
        unchecked {
          ++j;
        }
      }

      lastMean += floors[i];
      floorPrices[col].push(
        FloorPrice(uint96(block.timestamp), col, floors[i], lastMean / cachedTwapRange, msg.sender)
      );
      emit FloorSubmitted(collection.collectionSlug, floors[i], msg.sender);

      unchecked {
        ++i;
      }
    }
  }

  function triggerPause(bool _shouldUnpause) external onlyRole(DEFAULT_ADMIN_ROLE) {
    if (_shouldUnpause) _unpause();
    else _pause();
  }

  function _addPriceFeed(IERC20 _erc20, address _oracle) external onlyRole(DEFAULT_ADMIN_ROLE) {
    priceFeeds[_erc20] = AggregatorV3Interface(_oracle);
    emit PriceFeedAdded(_erc20, _oracle);
  }

  function setTesting(bool _activate) external onlyRole(DEFAULT_ADMIN_ROLE) {
    testing = _activate;
  }

  /**
   * @notice this needs to be removed in prod as we use this to map testing collection to real collections used by the oracle
   */
  /// TODO remove this from prod
  function _addTestingCollection(
    address _testCollection,
    address _prodCollection
  ) external onlyRole(DEFAULT_ADMIN_ROLE) {
    testingCollections[_testCollection] = _prodCollection;
  }

  /**
   * @notice returns the underlying price for an individual token id
   * @param _address address of the collection
   * @param _decimals adjust decimals of the returned price
   */
  function getUnderlyingFloorNFTPrice(address _address, uint256 _decimals) external view returns (uint128, uint128) {
    if (testing) _address = testingCollections[_address];

    uint256 lengthOfFloor = floorPrices[_address].length;
    if (lengthOfFloor == 0) return (0, 0);
    FloorPrice storage lastFloor = floorPrices[_address][lengthOfFloor - 1];
    uint256 lastMean = lastFloor.lastMean;
    uint256 oracleDecimals = 18;

    if (oracleDecimals < _decimals) {
      unchecked {
        _decimals -= oracleDecimals;
      }

      return (uint128(lastMean * 10 ** _decimals), lastFloor.lastUpdated);
    } else {
      unchecked {
        _decimals = oracleDecimals - _decimals;
      }
    }

    return (uint128(lastMean / 10 ** _decimals), lastFloor.lastUpdated);
  }

  /**
   * @notice returns the latest price for a given pair
   * @param _erc20 the erc20 we want to get the price for in USD
   * @param _decimals decimals to denote the result in
   */
  function getUnderlyingPriceInUSD(IERC20 _erc20, uint256 _decimals) public view returns (uint256) {
    uint256 oracleDecimals = priceFeeds[_erc20].decimals();
    (uint80 roundID, int256 price, , uint256 updatedAt, uint80 answeredInRound) = priceFeeds[_erc20].latestRoundData();

    if (updatedAt == 0 || price <= 0 || answeredInRound < roundID) {
      revert Unexpected("Price data is stale");
    }

    if (oracleDecimals < _decimals) {
      unchecked {
        _decimals -= oracleDecimals;
      }
      return uint256(price) * 10 ** _decimals;
    } else {
      unchecked {
        _decimals = oracleDecimals - _decimals;
      }
      return uint256(price) / 10 ** _decimals;
    }
  }

  /**
   * @notice get price of eth
   * @param _decimals adjust decimals of the returned price
   */
  function getEthPrice(uint256 _decimals) external view returns (uint256) {
    return getUnderlyingPriceInUSD(weth, _decimals);
  }

  function getFloorPriceForCollection(
    address _address,
    uint256 _start,
    uint256 _length
  ) external view returns (FloorPrice[] memory) {
    if (testing) _address = testingCollections[_address];

    FloorPrice[] memory collectionFloorPrices = new FloorPrice[](_length);

    for (uint256 i = _start; i < _start + _length; ++i) {
      collectionFloorPrices[i] = floorPrices[_address][i];
    }
    return collectionFloorPrices;
  }

  function getNoOfFloors(address _address) external view returns (uint256) {
    return floorPrices[_address].length;
  }

  error CollectionNotRegistered();

  event PriceFeedAdded(IERC20 indexed _erc20, address indexed _oracle);
  event FloorSubmitted(string _collectionSlug, uint256 _floor, address _submitter);
}

