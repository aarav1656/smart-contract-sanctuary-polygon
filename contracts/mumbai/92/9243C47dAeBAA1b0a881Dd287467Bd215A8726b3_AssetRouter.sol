pragma solidity 0.8.15;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/IFeeHandler.sol";
import "./interfaces/IAssetRouter.sol";
import "./interfaces/IAssetV2.sol";
import "./interfaces/IBridge.sol";
import "./../interfaces/IFeeCollector.sol";
import "hardhat/console.sol";

contract AssetRouter is IAssetRouter, AccessControl, ReentrancyGuard {
    using SafeERC20 for IERC20;

    uint256 public constant BP_DENOMINATOR = 10000;

    /// @notice chainpaths per pool poolId=>chainPath
    mapping(uint16 => ChainPath[]) public chainPaths;

    /// @notice bytes of srcPoolId + chainId + dstPoolId => index in the array of chainpaths
    mapping(bytes => uint256) public chainPathIndexLookup;

    /// @notice lookup by poolId => PoolObject
    mapping(uint16 => PoolObject) public poolLookup;

    uint16 chainId; // local chain id

    IFeeHandler public feeHandler; // address for retrieving fee params for swaps
    IFeeCollector public feeCollector;
    IBridge public bridge;

    uint256 public syncDeviation; // % of deviation accepted for triggering syncing

    event Swap(uint16 chainId, uint256 dstPoolId, uint256 amountSD, uint256 fee);
    event SendVouchers(uint16 dstChainId, uint16 dstPoolId, uint256 vouchers, uint256 optimalDstBandwidth);
    event VouchersReceived(uint16 chainId, uint16 srcPoolId, uint256 amountSD, uint256 optimalDstBandwidth);
    event SwapRemote(address to, uint256 amountSD, uint256 fee);
    event ChainPathUpdate(uint16 dstChainId, uint16 dstPoolId, uint256 weight);
    event FeeHandlerUpdated(address oldFeeHandler, address newFeeHandler);
    event SyncDeviationUpdated(uint256 oldDeviation, uint256 newDeviation);
    event FeeCollected(uint256 fee);

    constructor(IFeeHandler _feeHandler, uint16 _chainId, IFeeCollector _feeCollector) {
        chainId = _chainId;
        feeHandler = _feeHandler;
        feeCollector = _feeCollector;
    }

    function getChainPathsLength(uint16 _poolId) public view returns (uint256) {
        return chainPaths[_poolId].length;
    }

    function swap(SwapParams memory swapParams) external payable nonReentrant returns (SwapObj memory) {
        ChainPath storage cp = getAndCheckCP(swapParams.srcPoolId, swapParams.dstChainId, swapParams.dstPoolId);
        PoolObject storage pool = poolLookup[swapParams.srcPoolId];
        require(cp.active, "The chainPath is not active");
        require(cp.actualKbp > swapParams.amount, "Not enough Liquidity");
        SwapObj memory s = feeHandler.getFees(
            swapParams.srcPoolId,
            swapParams.amount,
            cp.actualKbp,
            cp.optimalDstBandwidth,
            cp.actualBandwidth,
            (pool.totalLiquidity * cp.weight) / pool.totalWeight
        );

        /**
         * 
            cp.actualBandwidth,
            (pool.totalLiquidity * cp.weight) / pool.totalWeight,
            cp.actualKbp,
            cp.optimalDstBandwidth
         */

        // console.log("s.amount ", s.amount);
        // console.log("cp.bandwidth ", cp.bandwidth);
        require(s.amount >= swapParams.minAmount, "Slippage too high");

        require(cp.bandwidth >= swapParams.amount, "Src bandwidth too low");
        require(cp.kbp + cp.vouchers >= s.kbpRemove, "Dst bandwidth too low");
        require(cp.bandwidth >= s.kbpRemove, "Dst bandwidth too low");
        cp.bandwidth -= s.kbpRemove;
        cp.actualBandwidth -= s.amount;

        if (swapParams.newLiquidity) {
            cp.actualKbp += s.amount;
            pool.undistributedVouchers += swapParams.amount;
        }

        if (s.amount > swapParams.amount) {
            console.log(s.amount - swapParams.amount);
        } else console.log("-", swapParams.amount - s.amount);

        if (pool.undistributedVouchers >= (pool.totalLiquidity * syncDeviation) / BP_DENOMINATOR) {
            _sync(pool);
        }
        CreditObj memory c = _sendVouchers(swapParams.srcPoolId, swapParams.dstChainId, swapParams.dstPoolId, true);

        IERC20(IAssetV2(pool.poolAddress).token()).safeTransferFrom(msg.sender, pool.poolAddress, swapParams.amount);
        emit Swap(swapParams.dstChainId, swapParams.dstPoolId, s.amount, s.fee);
        bridge.swap{ value: msg.value }(swapParams, c, s);
        return s;
    }

    function sendVouchers(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId,
        address payable _refundAddress
    ) public payable nonReentrant returns (CreditObj memory c) {
        c = _sendVouchers(_srcPoolId, _dstChainId, _dstPoolId, false);
        bridge.sendVouchers{ value: msg.value }(_dstChainId, _srcPoolId, _dstPoolId, _refundAddress, c);
    }

    function _sendVouchers(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId,
        bool _swap
    ) internal returns (CreditObj memory c) {
        ChainPath storage cp = getAndCheckCP(_srcPoolId, _dstChainId, _dstPoolId);
        PoolObject storage pool = poolLookup[_srcPoolId];
        require(cp.active, "counter chainPath is not ready");
        // console.log("Vouchers to send ", cp.vouchers);
        cp.kbp += cp.vouchers;
        if (!_swap) cp.actualKbp += cp.vouchers;
        c.optimalDstBandwidth = (pool.totalLiquidity * cp.weight) / pool.totalWeight;
        c.vouchers = cp.vouchers;
        cp.vouchers = 0;
        c.swap = _swap;
        emit SendVouchers(_dstChainId, _dstPoolId, c.vouchers, c.optimalDstBandwidth);
    }

    function receiveVouchers(
        uint16 _dstPoolId,
        uint16 _srcChainId,
        uint16 _srcPoolId,
        CreditObj memory _c
    ) public nonReentrant {
        ChainPath storage cp = getAndCheckCP(_dstPoolId, _srcChainId, _srcPoolId);
        // console.log("receiveVouchers: %s", _c.vouchers);
        // console.log("_dstPoolId: ", _dstPoolId);
        // console.log("_srcChainId: ", _srcChainId);
        // console.log("_srcPoolId: ", _srcPoolId);
        cp.bandwidth += _c.vouchers;
        if (!_c.swap) {
            cp.actualBandwidth += _c.vouchers;
        }
        if (cp.optimalDstBandwidth != _c.optimalDstBandwidth) {
            cp.optimalDstBandwidth = _c.optimalDstBandwidth;
        }
        emit VouchersReceived(_srcChainId, _srcPoolId, _c.vouchers, _c.optimalDstBandwidth);
    }

    function swapRemote(
        uint16 _dstPoolId,
        uint16 _srcChainId,
        uint16 _srcPoolId,
        address _to,
        SwapObj memory _s
    ) external nonReentrant returns (SwapObj memory) {
        ChainPath storage cp = getAndCheckCP(_dstPoolId, _srcChainId, _srcPoolId);
        PoolObject storage pool = poolLookup[_dstPoolId];

        cp.kbp -= _s.kbpRemove;
        cp.actualKbp -= _s.kbpRemove;
        cp.actualBandwidth += _s.amount;
        IAssetV2(pool.poolAddress).release(_to, _s.amount);
        // console.log("User received ", _s.amount, " WEI");

        if (_s.fee > 0 && address(feeCollector) != address(0)) {
            emit FeeCollected(_s.fee);
            pool.totalLiquidity += _s.fee;
            // feeCollector.collectFees(_to, _s.fee); // TODO this needs to be implemented
        }
        emit SwapRemote(_to, _s.amount, _s.fee);
        return _s;
    }

    function createChainPath(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId,
        uint16 _weight,
        address _poolAddress
    ) external {
        for (uint256 i; i < chainPaths[_srcPoolId].length; ++i) {
            ChainPath memory cp = chainPaths[_srcPoolId][i];
            bool exists = cp.dstChainId == _dstChainId && cp.dstPoolId == _dstPoolId && cp.srcPoolId == _srcPoolId;
            require(!exists, "can't createChainPath of existing dstChainId and _dstPoolId");
        }
        if (poolLookup[_srcPoolId].poolAddress == address(0)) {
            poolLookup[_srcPoolId] = PoolObject(_srcPoolId, _poolAddress, 0, 0, 0);
        }
        poolLookup[_srcPoolId].totalWeight += _weight;
        chainPathIndexLookup[abi.encodePacked(_srcPoolId, _dstChainId, _dstPoolId)] = chainPaths[_srcPoolId].length;
        chainPaths[_srcPoolId].push(
            ChainPath(false, _srcPoolId, _dstChainId, _dstPoolId, _weight, 0, 0, 0, 0, 0, 0, _poolAddress)
        );
        emit ChainPathUpdate(_dstChainId, _dstPoolId, _weight);
    }

    function deposit(address _to, uint16 _poolId, uint256 _amount) external {
        PoolObject storage pool = poolLookup[_poolId];
        pool.totalLiquidity += _amount;
        pool.undistributedVouchers += _amount;

        _sync(pool);

        IAssetV2(pool.poolAddress).mint(_to, _amount);
        IERC20(IAssetV2(pool.poolAddress).token()).safeTransferFrom(msg.sender, pool.poolAddress, _amount);
    }

    function setBridge(IBridge _newBridge) external {
        bridge = _newBridge;
    }

    function setWeightForChainPath(uint16 _srcPoolId, uint16 _dstChainId, uint16 _dstPoolId, uint16 _weight) external {
        ChainPath storage cp = getAndCheckCP(_srcPoolId, _dstChainId, _dstPoolId);
        PoolObject storage pool = poolLookup[_srcPoolId];
        pool.totalWeight = pool.totalWeight - cp.weight + _weight;
        cp.weight = _weight;
        emit ChainPathUpdate(_dstChainId, _dstPoolId, _weight);
    }

    function setFeeLibrary(address _feeHandler) external {
        require(_feeHandler != address(0), "fee library cant be 0x0");
        emit FeeHandlerUpdated(address(feeHandler), _feeHandler);
        feeHandler = IFeeHandler(_feeHandler);
    }

    function setSyncDeviation(uint256 _syncDeviation) external {
        require(_syncDeviation <= BP_DENOMINATOR, "sync deviation is too high");
        emit SyncDeviationUpdated(syncDeviation, _syncDeviation);
        syncDeviation = _syncDeviation;
    }

    function sync(uint16 _poolId) external {
        _sync(poolLookup[_poolId]);
    }

    function activateChainPath(uint16 _srcPoolId, uint16 _dstChainId, uint16 _dstPoolId) external {
        ChainPath storage cp = getAndCheckCP(_srcPoolId, _dstChainId, _dstPoolId);
        require(cp.active == false, "chainPath is already active");
        cp.active = true;
    }

    function getAndCheckCP(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId
    ) internal view returns (ChainPath storage) {
        require(chainPaths[_srcPoolId].length > 0, "no chainpaths exist");
        ChainPath storage cp = chainPaths[_srcPoolId][
            chainPathIndexLookup[abi.encodePacked(_srcPoolId, _dstChainId, _dstPoolId)]
        ];
        require(cp.dstChainId == _dstChainId && cp.dstPoolId == _dstPoolId, "local chainPath does not exist");
        return cp;
    }

    function getChainPath(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId
    ) internal view returns (ChainPath storage) {
        ChainPath storage cp = chainPaths[_srcPoolId][
            chainPathIndexLookup[abi.encodePacked(_srcPoolId, _dstChainId, _dstPoolId)]
        ];
        require(cp.dstChainId == _dstChainId && cp.dstPoolId == _dstPoolId, "local chainPath does not exist");
        return cp;
    }

    function getChainPathPublic(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId
    ) public view returns (ChainPath memory) {
        ChainPath storage cp = chainPaths[_srcPoolId][
            chainPathIndexLookup[abi.encodePacked(_srcPoolId, _dstChainId, _dstPoolId)]
        ];
        require(cp.dstChainId == _dstChainId && cp.dstPoolId == _dstPoolId, "local chainPath does not exist");
        return cp;
    }

    function getCompensatioRatio(
        uint16 _srcPoolId,
        uint16 _dstChainId,
        uint16 _dstPoolId
    ) public view returns (uint256, uint256) {
        ChainPath storage cp = getChainPath(_srcPoolId, _dstChainId, _dstPoolId);
        PoolObject storage pool = poolLookup[_srcPoolId];

        return (
            feeHandler.getCompensatioRatio(cp.bandwidth, (pool.totalLiquidity * cp.weight) / pool.totalWeight),
            feeHandler.getCompensatioRatio(cp.kbp + cp.vouchers, cp.optimalDstBandwidth)
        );
    }

    /**
     * @notice Syncs chainpaths for a specific pool
     * @dev (liquidity * (weight/totalWeight)) - (kbp+vouchers)
     * @param pool the pool to sync for
     */
    function _sync(PoolObject storage pool) internal {
        uint256 undistributedVouchers = pool.undistributedVouchers;

        if (pool.totalWeight > 0) {
            uint256 cpLength = chainPaths[pool.poolId].length;
            uint256[] memory deficit = new uint256[](cpLength);
            uint256 totalDeficit = 0;

            for (uint256 i; i < cpLength; ++i) {
                ChainPath storage cp = chainPaths[pool.poolId][i];

                uint256 balLiq = (pool.totalLiquidity * cp.weight) / pool.totalWeight;

                uint256 currLiq = cp.kbp + cp.vouchers;
                if (balLiq > currLiq) {
                    deficit[i] = balLiq - currLiq;
                    totalDeficit = totalDeficit + deficit[i];
                }
            }
            // console.log("totalDeficit = ", totalDeficit);
            // console.log("undistributedVouchers = ", undistributedVouchers);
            // console.log("cpLength = ", cpLength);
            uint256 spentVouchers;
            if (totalDeficit == 0 && undistributedVouchers > 0) {
                for (uint256 i = 0; i < cpLength; ++i) {
                    ChainPath storage cp = chainPaths[pool.poolId][i];
                    uint256 consumed = (pool.undistributedVouchers * cp.weight) / pool.totalWeight;
                    // console.log("cp.dstChainId ", cp.dstChainId);
                    // console.log("cp.dstPoolId ", cp.dstPoolId);
                    // console.log("cp.srcPoolId ", cp.srcPoolId);
                    // console.log("consumed ", consumed);
                    spentVouchers += consumed;
                    cp.vouchers += consumed;
                }
            } else if (totalDeficit <= undistributedVouchers) {
                for (uint256 i; i < cpLength; ++i) {
                    if (deficit[i] > 0) {
                        ChainPath storage cp = chainPaths[pool.poolId][i];
                        // console.log("cp.dstChainId ", cp.dstChainId);
                        // console.log("cp.dstPoolId ", cp.dstPoolId);
                        // console.log("cp.srcPoolId ", cp.srcPoolId);
                        uint256 consumed = deficit[i];
                        // console.log("consumed ", consumed);

                        spentVouchers += consumed;
                        cp.vouchers = cp.vouchers + consumed;
                    }
                }
            } else {
                for (uint256 i; i < cpLength; ++i) {
                    if (deficit[i] > 0) {
                        ChainPath storage cp = chainPaths[pool.poolId][i];
                        uint256 proportionalDeficit = (deficit[i] * undistributedVouchers) / totalDeficit;

                        spentVouchers += proportionalDeficit;
                        cp.vouchers += proportionalDeficit;
                    }
                }
            }

            pool.undistributedVouchers -= spentVouchers;
        }
    }
}

