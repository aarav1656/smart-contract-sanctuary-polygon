     * @dev Function for updating discounted NFT minting limit per user. Can only be called by the owner.
     * @param _limit The new limit to set. If 0, no discounted minting.
     */
    function setDiscountedPerUserLimit(uint256 _limit) external onlyOwner {
        require(_limit != discountedPerUserLimit, "PSBC: Already set");
        discountedPerUserLimit = _limit;
        emit DiscountedPerUserLimitUpdated(_limit);
    }

    /**
     * @notice Sets the price per NFT to the specified value.
     * This function can only be called by the owner of the contract.
     * @param _price The new price per NFT to set, in 18 decimals.

     */
    function setPricePerNFT(uint256 _price) external onlyOwner {
        require(_price > 0, "PSBC: Price must be greater than 0");
        require(_price != pricePerNFT, "PSBC: Already set");
        pricePerNFT = _price;
        emit PricePerNFTUpdated(_price);
    }

    /**
     * @notice Sets the discounted price per NFT to the specified value.
     * This function can only be called by the owner of the contract.
     * @param _price The new discounted price per NFT to set, in 18 decimals.
     */
    function setDiscountedPricePerNFT(uint256 _price) external onlyOwner {
        require(_price > 0, "PSBC: Price must be greater than 0");
        require(_price != discountedPricePerNFT, "PSBC: Already set");
        discountedPricePerNFT = _price;
        emit DiscountedPricePerNFTUpdated(_price);
    }

    /**
     * @notice Sets the presale status to the specified value.
     * This function can only be called by the owner of the contract.
     * @param _isPresale The new presale status to set.
     */
    function setPresale(bool _isPresale) external onlyOwner {
        require(_isPresale != isPresale, "PSBC: Already set");
        isPresale = _isPresale;
        emit PresaleUpdated(_isPresale);
    }

    /**
     * @notice Sets the base URI to the specified value.
     * This function can only be called by the owner of the contract.
     * @param _baseUri The new base URI to set.
     */
    function setBaseURI(string calldata _baseUri) external onlyOwner {
        require(
            keccak256(bytes(_baseUri)) != keccak256(bytes(baseURI)),
            "PSBC: Already set"
        );
        baseURI = _baseUri;
        emit BaseURIUpdated(_baseUri);
    }

    /**
     * @notice Sets the Merkle root to the specified value for whitelisting.
     * This function can only be called by the owner of the contract.
     * @param _root The new Merkle root to set.
     */
    function setMerkleRoot(bytes32 _root) external onlyOwner {
        require(_root != bytes32(0), "PSBC: Invalid Merkle root");
        require(_root != merkleRoot, "PSBC: Already set");
        merkleRoot = _root;
        emit MerkleRootUpdated(_root);
    }

    /**
     * @notice Sets the price feed oracle to the specified value.
     * This function can only be called by the owner of the contract.
     * @param _oracleAddress The new price feed to set.
     */
    function setPriceFeed(address _oracleAddress) external onlyOwner {
        require(_oracleAddress != address(0), "PSBC: Invalid address");
        require(_oracleAddress != address(priceFeed), "PSBC: Already set");
        priceFeed = AggregatorV3Interface(_oracleAddress);
        emit PriceFeedUpdated(_oracleAddress);
    }

    /**
     * @notice Sets the treasury address to the specified value.
     * This function can only be called by the owner of the contract.
     * @param _treasury The new treasury address to set.
     */
    function setTreasury(address payable _treasury) external onlyOwner {
        require(_treasury != address(0), "PSBC: Invalid address");
        require(_treasury != treasury, "PSBC: Already set");
        treasury = _treasury;
        emit TreasuryUpdated(_treasury);
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }
}