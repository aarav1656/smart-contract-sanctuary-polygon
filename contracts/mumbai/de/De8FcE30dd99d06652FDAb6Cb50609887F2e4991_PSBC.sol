    event AdminUpdated(address _admin, bool _isAdmin);
    event PerUserLimitUpdated(uint256 _limit);
    event DiscountedPerUserLimitUpdated(uint256 _limit);
    event PricePerNFTUpdated(uint256 _price);
    event DiscountedPricePerNFTUpdated(uint256 _price);
    event PresaleUpdated(bool _isPresale);
    event BaseURIUpdated(string _baseUri);
    event MerkleRootUpdated(bytes32 _root);
    event PriceFeedUpdated(address _oracleAddress);
    event TreasuryUpdated(address _treasury);
    event TreasuryReceived(address _sender, uint256 _amount);

