    uint256 public discountedPerUserLimit = 2; // Maximum number of discounted tokens each user can mint
    uint256 public pricePerNFT = 200e18; // The price per NFT in Ether
    uint256 public discountedPricePerNFT = 150e18; // The discounted price per NFT in Ether
    uint256 public usdDecimals = 8; // The number of decimals for USD

